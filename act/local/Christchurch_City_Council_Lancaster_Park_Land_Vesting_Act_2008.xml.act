      <heading>Land of the Crown (subject to the Victory Park Act 1919) vested in Council</heading>
      <schedule.misc>
        <para>
          <table align="center" fontsize="10.5" frame="none" id="DLM1143829" leading="11.5">
            <tgroup cols="3">
              <colspec colname="COLSPEC0" colwidth="23.00mm"/>
              <colspec colname="col1" colwidth="100.00*"/>
              <colspec align="left" colname="col2" colwidth="30.00mm"/>
              <thead>
                <row>
                  <entry colsep="0" rowsep="0" valign="top">
