            <amend>
              <def-para id="DLM81902" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM81903" xml:lang="en-NZ">District</def-term> means the area delineated on SO Plan No 36001 deposited with the Chief Surveyor of the Wellington Land District</text>
                </para>
              </def-para>
            </amend>
            <follow-text space-before="no">.</follow-text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
