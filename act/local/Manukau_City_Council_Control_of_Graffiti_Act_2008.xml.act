<act date.instructed="2004-11-25" id="DLM1035735" in.amend="false" instructing.office="Unknown department" irdnumbering="no" xml:lang="en-NZ" year.imprint="2008" act.no="2" act.type="local" date.assent="2008-04-22" date.first.valid="2008-04-23" stage="in-force" year="2008">
