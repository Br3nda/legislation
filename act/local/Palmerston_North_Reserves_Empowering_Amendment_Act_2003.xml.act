  <schedule.group xml:lang="en-NZ">
    <schedule id="DLM87749" in.amend="false" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" xml:lang="en-NZ" irdnumbering="no" act.no="2" act.type="local" date.as.at="2007-09-03" date.assent="2003-05-19" date.first.valid="2007-09-03" stage="reprint" year="2003">
