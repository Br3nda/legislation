<act id="DLM71743" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="5" act.type="local" date.as.at="2007-09-03" date.assent="1974-09-12" date.first.valid="2007-09-18" stage="in-force" year="1974">
