    <schedule id="DLM122360" in.amend="false" irdnumbering="no" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" xml:lang="en-NZ" act.no="2" act.type="private" date.as.at="2003-05-20" date.assent="2003-05-19" date.first.valid="2007-09-18" stage="reprint" year="2003">
