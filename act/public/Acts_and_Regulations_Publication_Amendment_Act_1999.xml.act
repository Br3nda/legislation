<act id="DLM42785" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="111" act.type="public" date.as.at="2007-09-03" date.assent="1999-09-09" date.first.valid="2007-09-18" stage="in-force" year="1999">
