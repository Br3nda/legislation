<act date.instructed="2007-08-03" id="DLM1074737" in.amend="false" instructing.office="Housing New Zealand Corporation" irdnumbering="no" xml:lang="en-NZ" year.imprint="2008" act.no="67" act.type="public" date.as.at="2008-09-16" date.assent="2008-09-16" date.first.valid="2008-09-17" stage="in-force" year="2008">
