      <prov id="DLM1101360" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">35E</label>
        <heading xml:lang="en-NZ">Persons authorised to issue certificates of compliance</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The Director-General may designate 1 or more persons employed within the Ministry as persons authorised to issue certificates of compliance for the purposes of this Act.</text>
            </para>
          </subprov>
        </prov.body>
