tidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></text>
                </para>
              </def-para>
              <def-para id="DLM415032" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM415033" xml:lang="en-NZ">Place</def-term> includes any building, conveyance, craft, land, or structure</text>
                </para>
              </def-para>
              <def-para id="DLM415034" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM415035" xml:lang="en-NZ">Prescribed</def-term> means prescribed by regulations made under this Act</text>
                </para>
              </def-para>
              <def-para id="DLM415036" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM415037" xml:lang="en-NZ">Primary produce</def-term> includes any plant or animal, or any derivative of any plant or animal, intended for sale</text>
                </para>
              </def-para>
              <def-para id="DLM1100206" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM1100207" xml:lang="en-NZ">public health</def-term> means the health of all of—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>the people of New Zealand; or</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>a community or section of such people</text>
                    </para>
                  </label-para>
                </para>
              </def-para>
              <def-para id="DLM1100208" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM1100209" xml:lang="en-NZ">recognised person</def-term> means a person for the time being appointed as a recognised person under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="721699" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1100210 - prov 62" atidlm:xmlId="DLM1105601" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 62</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="579541" atidlm:sourceXmlId="DLM1105601" atidlm:targetKey="2250617" atidlm:targetXmlId="DLM415141" atidlm:targetName="prov 62" atidlm:sourceName="DLM1105601" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM415128.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM414582.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[27]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para recognised person" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM414576" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="87" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Agricultural Compounds and Veterinary Medicines Act 1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></text>
                </para>
              </def-para>
              <def-para id="DLM415038" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM415039" xml:lang="en-NZ">Registered</def-term> means registered under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="480868" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 21" atidlm:xmlId="DLM924465" atidlm:state="UNCHANGED"><atidlm:linkcontent>21</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="489482" atidlm:sourceXmlId="DLM924465" atidlm:targetKey="2248821" atidlm:targetXmlId="DLM415087" atidlm:targetName="prov 21" atidlm:sourceName="DLM924465" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM415061.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM414582.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[28]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Registered" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM414576" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="87" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Agricultural Compounds and Veterinary Medicines Act 1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="480869" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 27" atidlm:xmlId="DLM924466" atidlm:state="UNCHANGED"><atidlm:linkcontent>27</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="489483" atidlm:sourceXmlId="DLM924466" atidlm:targetKey="2248809" atidlm:targetXmlId="DLM415094" atidlm:targetName="prov 27" atidlm:sourceName="DLM924466" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM415061.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM414582.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[28]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Registered" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM414576" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="87" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Agricultural Compounds and Veterinary Medicines Act 1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></text>
                </para>
              </def-para>
              <def-para id="DLM415040" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM415041" xml:lang="en-NZ">Registrant</def-term> means, in relation to a registered trade name product, the person who applied to register that product or the person to whom a registration is transferred</text>
                </para>
              </def-para>
              <def-para id="DLM415042" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM415043" xml:lang="en-NZ">Regulations</def-term> means regulations in force under this Act</text>
                </para>
              </def-para>
              <def-para id="DLM415044" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM415045" xml:lang="en-NZ">Risk</def-term> includes any costs or potential costs</text>
                </para>
              </def-para>
              <def-para id="DLM415046" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM415047" xml:lang="en-NZ">Sale</def-term> includes barter, and also includes offering, exposing, or attempting to sell, or having in possession for sale, or sending or delivering for sale, or causing or allowing to be sold, offered, or exposed for sale; and also includes—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>Delivering or disposing of by way of gift, loan, or otherwise; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>Giving or distributing, in the course of business, as a sample or otherwise, without charge:</text>
                    </para>
                  </label-para>
                </para>
              </def-para>
              <def-para id="DLM415048" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM415049" xml:lang="en-NZ">Trade name product</def-term> means an agricultural compound identified and packaged under a trade name for a specified use or uses</text>
                </para>
              </def-para>
              <def-para id="DLM415050" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM415051" xml:lang="en-NZ">Use</def-term>, in relation to any agricultural compound, includes its use in such a way that animals, plants, or raw primary produce are exposed to it</text>
                </para>
              </def-para>
              <def-para id="DLM415052" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM415053" xml:lang="en-NZ">Veterinarian</def-term> means a person for the time being registered as a <insertwords>veterinarian or a specialist within the meaning of <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="480870" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 4" atidlm:xmlId="DLM924467" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="489484" atidlm:sourceXmlId="DLM924467" atidlm:targetKey="2195664" atidlm:targetXmlId="DLM363866" atidlm:targetName="prov 4" atidlm:sourceName="DLM924467" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM363864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM414582.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[35]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Veterinarian" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM414576" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="87" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Agricultural Compounds and Veterinary Medicines Act 1997" atidlm:local