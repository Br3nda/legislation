      <prov id="DLM415143" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">63</label>
        <heading xml:lang="en-NZ">Protection of ACVM officers and recognised persons</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>No action or proceedings may be brought against any ACVM officer or recognised person in respect of any actions taken by any such officer or person under this Act unless he or she has acted in bad faith or without reasonable cause</text>
            </para>
          </subprov>
        </prov.body>
        <notes>
