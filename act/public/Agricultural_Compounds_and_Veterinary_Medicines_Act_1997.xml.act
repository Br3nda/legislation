                  </entry>
                  <entry colsep="1">
                    <para deletion-status="repealed" xml:lang="en-NZ"/>
                  </entry>
                </row>
                <row>
                  <entry colsep="1">
                    <citation jurisdiction="nz">1996, No 30—The <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="483114" atidlm:branchLabel="_NOVERSION_" atidlm:name="Hazardous Substances and New Organisms Act 1996" atidlm:xmlId="DLM926909" atidlm:state="UNCHANGED"><atidlm:linkcontent>Hazardous Substances and New Organisms Act 1996</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="491177" atidlm:sourceXmlId="DLM926909" atidlm:targetKey="2038205" atidlm:targetXmlId="DLM381221" atidlm:targetName="Hazardous Substances and New Organisms Act 1996" atidlm:sourceName="DLM926909" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM381221.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM415460.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[2]/schedule.misc[1]/para[1]/table[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM414576" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="87" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Agricultural Compounds and Veterinary Medicines Act 1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
                  </entry>
                  <entry colsep="1">
                    <para xml:lang="en-NZ">
                      <text>By repealing <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="483115" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 55" atidlm:xmlId="DLM926910" atidlm:state="UNCHANGED"><atidlm:linkcontent>55(5)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="491178" atidlm:sourceXmlId="DLM926910" atidlm:targetKey="2044148" atidlm:targetXmlId="DLM384069" atidlm:targetName="prov 55" atidlm:sourceName="DLM926910" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM383157.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM415460.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[2]/schedule.misc[1]/para[1]/table[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM414576" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="87" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Agricultural Compounds and Veterinary Medicines Act 1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="483116" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 55" atidlm:xmlId="DLM926911" atidlm:state="UNCHANGED"><atidlm:linkcontent>(6)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="491179" atidlm:sourceXmlId="DLM926911" atidlm:targetKey="2044148" atidlm:targetXmlId="DLM384069" atidlm:targetName="prov 55" atidlm:sourceName="DLM926911" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM383157.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM415460.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[2]/schedule.misc[1]/para[1]/table[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM414576" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="87" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Agricultural Compounds and Veterinary Medicines Act 1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substituting the following subsections:</text>
                      <amend quote="1">
                        <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes" quote="1">4A</label>
                          <para xml:lang="en-NZ">
                            <text>Where—</text>
                            <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                              <label auto.number="no" denominator="yes" quote="1">a</label>
                              <para xml:lang="en-NZ">
                                <text>Any information is held by the Authority relating to any application made under <citation jurisdiction="nz">section 28 or section 31 or section 47</citation> in respect of a hazardous substance; and</text>
                              </para>
                            </label-para>
                            <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                              <label auto.number="no" denominator="yes" quote="1">b</label>
                              <para xml:lang="en-NZ">
                                <text>That substance is also the subject of an innovative agricultural compound application as defined in <insertwords><citation jurisdiction="nz">Part 6</citation></insertwords> of the Agricultural Compounds and Veterinary Medicines Act 1997; and</text>
                              </para>
                            </label-para>
                            <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                              <label auto.number="no" denominator="yes" quote="1">c</label>
                              <para xml:lang="en-NZ">
                                <text>That information includes trade secrets or information that has commercial value that would be, or would be likely to be, diminished by disclosure,—</text>
                              </para>
                            </label-para>
                            <text>the provisions of <insertwords><citation jurisdiction="nz">Part 6</citation></insertwords> of the Agricultural Compounds and Veterinary Medicines Act 1997, with the necessary modifications, apply to that information as if the information were confidential supporting information as defined in that Part of that Act.</text>
                          </para>
                        </subprov>
                        <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes" quote="1">4B</label>
                          <para xml:lang="en-NZ">
                            <text>The provisions of <insertwords><citation jurisdiction="nz">Part 6</citation></insertwords> of the Agricultural Compounds and Veterinary Medicines Act 1997, with the necessary modifications, apply to the Authority in respect of the information referred to in <citation jurisdiction="nz">subsection (4A)</citation> as if the Authority were the Director-General, and as if references in those sections to applications were references to applications in respect of hazardous substances; but—</text>
                            <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                              <label auto.number="no" denominator="yes" quote="1">a</label>
                              <para xml:lang="en-NZ">
                                <text>The protected period (as defined in <insertwords><citation jurisdiction="nz">Part 6</citation></insertwords> of the Agricultural Compounds and Veterinary Medicines Act 1997) is the same period for which the information is protected under the Agricultural Compounds and Veterinary Medicines Act 1997; and</text>
                              </para>
                            </label-para>
                            <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                              <label auto.number="no" denominator="yes" quote="1">b</label>
                              <para xml:lang="en-NZ">
                                <text>The Authority may disclose the information to any prescribed person or organisation or prescribed class of persons or organisations; and</text>
                              </para>
                            </label-para>
                            <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                              <label auto.number="no" denominator="yes" quote="1">c</label>
                              <para xml:lang="en-NZ">
                                <text>The Authority must provide a summary of the effects of any substance in respect of which <citation jurisdiction="nz">subsection (4A)</citation> applies where an application for approval is required to be publicly notified in accordance with <citation jurisdiction="nz">section 53 of this Act</citation>.</text>
                              </para>
                            </label-para>
                          </para>
                        </subprov>
                      </amend>
                    </para>
                    <para xml:lang="en-NZ">
                      <text>By omitting from <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="483117" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 55" atidlm:xmlId="DLM926912" atidlm:state="UNCHANGED"><atidlm:linkcontent>55(7)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="491180" atidlm:sourceXmlId="DLM926912" atidlm:targetKey="2044148" atidlm:targetXmlId="DLM384069" atidlm:targetName="prov 55" atidlm:sourceName="DLM926912" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM383157.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM415460.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[2]/schedule.misc[1]/para[1]/table[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM414576" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="87" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Agricultural Compounds and Veterinary Medicines Act 1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> the expression <amend.in quote="1">6(b)</amend.in>, and substituting the expression <amend.in quote="1">(4A)(b)</amend.in>.</text>
                    </para>
                  </entry>
                </row>
              </tbody>
            </tgroup>
          </table>
          <notes>
