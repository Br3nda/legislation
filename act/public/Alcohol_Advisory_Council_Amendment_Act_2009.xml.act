        <para>
          <amend quote="1">
            <schedule act.no="4" id="DLM1350704" in.amend="true" irdnumbering="no" prov-type="clause" quote="1" schedule.heading.continue="no" toc="yes" type="schedule" year="2009" year.imprint="2009">
              <label auto.number="no" denominator="yes"/>
              <empowering-prov>
                <citation jurisdiction="nz">
                  <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1626837" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1891814 - prov 2" atidlm:parentFolderId="/NZ/NZAct/A/Alcohol Advisory Council Amendment Act 2009" atidlm:xmlId="DLM1891837" atidlm:state="UNCHANGED">
