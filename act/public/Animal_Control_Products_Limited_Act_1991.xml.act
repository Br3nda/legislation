    <prov deletion-status="repealed" id="DLM229538" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">5</label>
      <heading xml:lang="en-NZ">Incorporation of company to take over poison factory business</heading>
      <prov.body/>
