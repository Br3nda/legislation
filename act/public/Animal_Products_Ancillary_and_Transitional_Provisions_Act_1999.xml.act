    <schedule id="DLM37546" in.amend="false" irdnumbering="no" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" xml:lang="en-NZ" act.no="94" act.type="public" date.as.at="2005-06-01" date.assent="1999-09-08" date.first.valid="1999-09-08" stage="reprint" year="1999">
