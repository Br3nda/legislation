    <part id="DLM37518" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="1999-09-08" act.no="94" act.type="public" date.as.at="2005-06-01" date.assent="1999-09-08" stage="reprint" year="1999">
