  <cover id="DLM342783">
    <title xml:lang="en-NZ">Antarctica (Environmental Protection) Act 1994</title>
    <assent>1994-12-06</assent>
    <cover.reprint-note>
      <para xml:lang="en-NZ"/>
      <admin-office>This Act is administered in the <ministry>Ministry of Foreign Affairs</ministry> and Trade.</admin-office>
    </cover.reprint-note>
  </cover>
  <contents>
    <heading xml:lang="en-NZ">Contents</heading>
    <toc/>
