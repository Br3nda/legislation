              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>Only in accordance with, and subject to the restrictions and conditions set out in, <citation jurisdiction="nz">Annex <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="433608" atidlm:branchLabel="_NOVERSION_" atidlm:name="conv  Conservation of Antarctic Fauna and Flora" atidlm:xmlId="DLM877474" atidlm:state="UNCHANGED"><atidlm:linkcontent>II</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="440908" atidlm:sourceXmlId="DLM877474" atidlm:targetKey="1791020" atidlm:targetXmlId="DLM343805" atidlm:targetName="conv  Conservation of Antarctic Fauna and Flora" atidlm:sourceName="DLM877474" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM343804.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM343259.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/prov[7]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Restrictions on permits to take native fauna and flora" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="31" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM342782" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="119" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Antarctica (Environmental Protection) Act 1994" atidlm:locale="xx_XX" atidlm:state="UN