            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264909" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 32" atidlm:xmlId="DLM710136" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="269151" atidlm:sourceXmlId="DLM710136" atidlm:targetKey="1922006" atidlm:targetXmlId="DLM324785" atidlm:targetName="prov 32" atidlm:sourceName="DLM710136" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM324772.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324772.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324785" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> applies to a levy or fee under the <citation jurisdiction="nz">Fisheries (Cost Recovery Levies for Fisheries Services) Order 2004</citation> payable by the holder of—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>a marine farming permit under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264910" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 67J" atidlm:xmlId="DLM710137" atidlm:state="UNCHANGED"><atidlm:linkcontent>67J</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="269152" atidlm:sourceXmlId="DLM710137" atidlm:targetKey="391605" atidlm:targetXmlId="DLM69750" atidlm:targetName="prov 67J" atidlm:sourceName="DLM710137" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM69745.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324772.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324785" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264911" atidlm:branchLabel="_NOVERSION_" atidlm:name="Fisheries Act 1983" atidlm:xmlId="DLM710138" atidlm:state="UNCHANGED"><atidlm:linkcontent>Fisheries Act 1983</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="269153" atidlm:sourceXmlId="DLM710138" atidlm:targetKey="383200" atidlm:targetXmlId="DLM66581" atidlm:targetName="Fisheries Act 1983" atidlm:sourceName="DLM710138" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM66581.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324772.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324785" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>a spat catching permit under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264912" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 67Q" atidlm:xmlId="DLM710139" atidlm:state="UNCHANGED"><atidlm:linkcontent>67Q(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="269154" atidlm:sourceXmlId="DLM710139" atidlm:targetKey="391633" atidlm:targetXmlId="DLM69778" atidlm:targetName="prov 67Q" atidlm:sourceName="DLM710139" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM69745.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324772.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324785" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264913" atidlm:branchLabel="_NOVERSION_" atidlm:name="Fisheries Act 1983" atidlm:xmlId="DLM710140" atidlm:state="UNCHANGED"><atidlm:linkcontent>Fisheries Act 1983</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="269155" atidlm:sourceXmlId="DLM710140" atidlm:targetKey="383200" atidlm:targetXmlId="DLM66581" atidlm:targetName="Fisheries Act 1983" atidlm:sourceName="DLM710140" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM66581.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324772.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324785" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>A levy or fee that is, immediately before the commencement of this Act, payable but unpaid remains payable even though the permit is deemed to be a coastal permit under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264914" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 20" atidlm:xmlId="DLM710141" atidlm:state="UNCHANGED"><atidlm:linkcontent>20</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="269156" atidlm:sourceXmlId="DLM710141" atidlm:targetKey="1922008" atidlm:targetXmlId="DLM324773" atidlm:targetName="prov 20" atidlm:sourceName="DLM710141" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM324772.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324772.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324785" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264915" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 21" atidlm:xmlId="DLM710142" atidlm:state="UNCHANGED"><atidlm:linkcontent>21</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="269157" atidlm:sourceXmlId="DLM710142" atidlm:targetKey="1922009" atidlm:targetXmlId="DLM324774" atidlm:targetName="prov 21" atidlm:sourceName="DLM710142" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM324772.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324772.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324785" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
    </part>
    <part id="DLM324786" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-18" act.no="109" act.type="public" date.as.at="2007-09-03" date.assent="2004-12-21" stage="reprint" year="2004">
      <heading xml:lang="en-NZ">Revocation</heading>
      <prov id="DLM324787" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">33</label>
        <heading xml:lang="en-NZ">Revocation of <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264916" atidlm:branchLabel="_NOVERSION_" atidlm:name="Freshwater Fish Farming Regulations 1983" atidlm:xmlId="DLM710143" atidlm:state="UNCHANGED"><atidlm:linkcontent>Freshwater Fish Farming Regulations 1983</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="269158" atidlm:sourceXmlId="DLM710143" atidlm:targetKey="3576216" atidlm:targetXmlId="DLM93755" atidlm:targetName="Freshwater Fish Farming Regulations 1983" atidlm:sourceName="DLM710143" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZReg/fragxyxz_DLM93755.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264917" atidlm:branchLabel="_NOVERSION_" atidlm:name="Freshwater Fish Farming Regulations 1983" atidlm:xmlId="DLM710144" atidlm:state="UNCHANGED"><atidlm:linkcontent>Freshwater Fish Farming Regulations 1983</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="269200" atidlm:sourceXmlId="DLM710144" atidlm:targetKey="3576216" atidlm:targetXmlId="DLM93755" atidlm:targetName="Freshwater Fish Farming Regulations 1983" atidlm:sourceName="DLM710144" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZReg/fragxyxz_DLM93755.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> (SR 1983/278)</citation> are revoked.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>All licences granted under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264918" atidlm:branchLabel="_NOVERSION_" atidlm:name="Freshwater Fish Farming Regulations 1983" atidlm:xmlId="DLM710145" atidlm:state="UNCHANGED"><atidlm:linkcontent>Freshwater Fish Farming Regulations 1983</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="269201" atidlm:sourceXmlId="DLM710145" atidlm:targetKey="3576216" atidlm:targetXmlId="DLM93755" atidlm:targetName="Freshwater Fish Farming Regulations 1983" atidlm:sourceName="DLM710145" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZReg/fragxyxz_DLM93755.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> are revoked.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264919" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 2" atidlm:xmlId="DLM710146" atidlm:state="UNCHANGED"><atidlm:linkcontent>2(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="269202" atidlm:sourceXmlId="DLM710146" atidlm:targetKey="2114117" atidlm:targetXmlId="DLM394199" atidlm:targetName="prov 2" atidlm:sourceName="DLM710146" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM394198.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264920" atidlm:branchLabel="_NOVERSION_" atidlm:name="Fisheries Act 1996" atidlm:xmlId="DLM710147" atidlm:state="UNCHANGED"><atidlm:linkcontent>Fisheries Act 1996</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="269203" atidlm:sourceXmlId="DLM710147" atidlm:targetKey="2111006" atidlm:targetXmlId="DLM394191" atidlm:targetName="Fisheries Act 1996" atidlm:sourceName="DLM710147" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM394191.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is amended by repealing paragraph (b) of the definitions of <emphasis style="bold">fish farm</emphasis> and <emphasis style="bold">fish farmer</emphasis>.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264921" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 186ZA" atidlm:xmlId="DLM710148" atidlm:state="UNCHANGED"><atidlm:linkcontent>186ZA</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="269204" atidlm:sourceXmlId="DLM710148" atidlm:targetKey="2129636" atidlm:targetXmlId="DLM398340" atidlm:targetName="prov 186ZA" atidlm:sourceName="DLM710148" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM398307.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264922" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 301" atidlm:xmlId="DLM710149" atidlm:state="UNCHANGED"><atidlm:linkcontent>301</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="269205" atidlm:sourceXmlId="DLM710149" atidlm:targetKey="2138026" atidlm:targetXmlId="DLM399924" atidlm:targetName="prov 301" atidlm:sourceName="DLM710149" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM399598.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264923" atidlm:branchLabel="_NOVERSION_" atidlm:name="Fisheries Act 1996" atidlm:xmlId="DLM710150" atidlm:state="UNCHANGED"><atidlm:linkcontent>Fisheries Act 1996</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="269206" atidlm:sourceXmlId="DLM710150" atidlm:targetKey="2111006" atidlm:targetXmlId="DLM394191" atidlm:targetName="Fisheries Act 1996" atidlm:sourceName="DLM710150" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM394191.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> are repealed.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">5</label>
            <para xml:lang="en-NZ">
              <text>On and from the commencement of this section the holder of a fish-farm licence immediately before the commencement of this section is to be treated as if the holder were registered as a fish farmer under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264924" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 186S" atidlm:xmlId="DLM710151" atidlm:state="UNCHANGED"><atidlm:linkcontent>186S</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="269207" atidlm:sourceXmlId="DLM710151" atidlm:targetKey="2129605" atidlm:targetXmlId="DLM398324" atidlm:targetName="prov 186S" atidlm:sourceName="DLM710151" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM398307.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264925" atidlm:branchLabel="_NOVERSION_" atidlm:name="Fisheries Act 1996" atidlm:xmlId="DLM710152" atidlm:state="UNCHANGED"><atidlm:linkcontent>Fisheries Act 1996</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="269208" atidlm:sourceXmlId="DLM710152" atidlm:targetKey="2111006" atidlm:targetXmlId="DLM394191" atidlm:targetName="Fisheries Act 1996" atidlm:sourceName="DLM710152" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM394191.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">6</label>
            <para xml:lang="en-NZ">
              <text>The chief executive must, within 12 months after the commencement of this section, register without an application or payment of the prescribed fee the holder of a fish-farm licence under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264926" atidlm:branchLabel="_NOVERSION_" atidlm:name="Freshwater Fish Farming Regulations 1983" atidlm:xmlId="DLM710153" atidlm:state="UNCHANGED"><atidlm:linkcontent>Freshwater Fish Farming Regulations 1983</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="269209" atidlm:sourceXmlId="DLM710153" atidlm:targetKey="3576216" atidlm:targetXmlId="DLM93755" atidlm:targetName="Freshwater Fish Farming Regulations 1983" atidlm:sourceName="DLM710153" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZReg/fragxyxz_DLM93755.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> immediately before the commencement of this section as a fish farmer in respect of the space that the licence relates to.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">7</label>
            <para xml:lang="en-NZ">
              <text>An application for a fish-farm licence under <citation jurisdiction="nz">regulation <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264927" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 6" atidlm:xmlId="DLM710154" atidlm:state="UNCHANGED"><atidlm:linkcontent>6</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="269210" atidlm:sourceXmlId="DLM710154" atidlm:targetKey="3576804" atidlm:targetXmlId="DLM94109" atidlm:targetName="prov 6" atidlm:sourceName="DLM710154" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZReg/fragxyxz_DLM94104.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264928" atidlm:branchLabel="_NOVERSION_" atidlm:name="Freshwater Fish Farming Regulations 1983" atidlm:xmlId="DLM710155" atidlm:state="UNCHANGED"><atidlm:linkcontent>Freshwater Fish Farming Regulations 1983</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="269211" atidlm:sourceXmlId="DLM710155" atidlm:targetKey="3576216" atidlm:targetXmlId="DLM93755" atidlm:targetName="Freshwater Fish Farming Regulations 1983" atidlm:sourceName="DLM710155" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZReg/fragxyxz_DLM93755.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> made but not determined at the commencement of this section must be treated as if it were an application to register as a fish farmer under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264929" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 186R" atidlm:xmlId="DLM710156" atidlm:state="UNCHANGED"><atidlm:linkcontent>186R</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="269212" atidlm:sourceXmlId="DLM710156" atidlm:targetKey="2129603" atidlm:targetXmlId="DLM398322" atidlm:targetName="prov 186R" atidlm:sourceName="DLM710156" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM398307.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="264930" atidlm:branchLabel="_NOVERSION_" atidlm:name="Fisheries Act 1996" atidlm:xmlId="DLM710157" atidlm:state="UNCHANGED"><atidlm:linkcontent>Fisheries Act 1996</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="269213" atidlm:sourceXmlId="DLM710157" atidlm:targetKey="2111006" atidlm:targetXmlId="DLM394191" atidlm:targetName="Fisheries Act 1996" atidlm:sourceName="DLM710157" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM394191.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM324787" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
        </prov.body>
        <notes>
          <amends-note>
            <amends-effective>
              <authority>OIC</authority>
            </amends-effective>
            <amends-affect>
              <citation jurisdiction="nz">
                <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1295481" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1649356 - Freshwater Fish Farming Regulations 1983" atidlm:xmlId="DLM1649358" atidlm:state="UNCHANGED">
                  <atidlm:linkcontent>Freshwater Fish Farming Regulations 1983</atidlm:linkcontent>
                  <atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="791407" atidlm:sourceXmlId="DLM1649358" atidlm:targetKey="3576216" atidlm:targetXmlId="DLM93755" atidlm:targetName="Freshwater Fish Farming Regulations 1983" atidlm:sourceName="DLM1649358" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZReg/fragxyxz_DLM93755.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM324786.xml" atidlm:state="UNCHANGED">
