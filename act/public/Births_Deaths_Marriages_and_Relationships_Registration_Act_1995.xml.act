          <part act.no="48" act.type="public" date.as.at="2008-07-24" date.assent="2008-07-24" date.first.valid="2009-01-24" date.instructed="2003-10-09" id="DLM3176503" in.amend="false" instructing.office="Internal Affairs" irdnumbering="no" prospective.consolidation="F" stage="in-force" toc="yes" year="2008">
