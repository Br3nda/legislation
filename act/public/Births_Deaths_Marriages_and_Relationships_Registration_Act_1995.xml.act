  <schedule.group xml:lang="en-NZ">
    <schedule id="DLM364792" in.amend="false" irdnumbering="no" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" xml:lang="en-NZ" act.no="16" act.type="public" date.as.at="2010-07-07" date.assent="1995-03-31" date.first.valid="2007-09-18" stage="reprint" year="1995" prospective.consolidation="F">
      <label auto.number="no" denominator="yes">1A</label>
      <empowering-prov>
        <citation jurisdiction="nz">
          <intref href="DLM364750">s 78A</intref>
