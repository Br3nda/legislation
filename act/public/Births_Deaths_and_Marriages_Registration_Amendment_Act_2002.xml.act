            <table align="center" fontsize="11.5" frame="none" id="DLM167863" leading="13.5" xml:lang="en-NZ">
              <tgroup cols="3">
                <colspec colname="col1" colsep="0" colwidth="0.89*"/>
                <colspec colname="col2" colsep="0" colwidth="0.89*"/>
                <colspec colname="col3" colsep="0" colwidth="1.22*"/>
                <tbody>
                  <row>
                    <entry>Ministry of Education</entry>
                    <entry>Birth information and death information</entry>
                    <entry>To verify and update student information on the National Student Index</entry>
                  </row>
                </tbody>
              </tgroup>
