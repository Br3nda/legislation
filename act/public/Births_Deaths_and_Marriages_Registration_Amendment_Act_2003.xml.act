          <para xml:lang="en-NZ">
            <text>This Act is the Births, Deaths, and Marriages Registration Amendment Act 2003.</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <para xml:lang="en-NZ">
