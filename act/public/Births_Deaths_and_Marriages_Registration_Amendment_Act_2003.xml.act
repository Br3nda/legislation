                    <colspec colname="col1" colwidth="54.12*"/>
                    <colspec colname="col2" colwidth="35.50*"/>
                    <colspec colname="col3" colwidth="54.36*"/>
                    <tbody>
                      <row>
                        <entry>Accident Compensation Corporation</entry>
                        <entry>Birth information</entry>
                        <entry>To verify the identity of a claimant, and a claimant's eligibility or continuing eligibility for a benefit</entry>
                      </row>
                      <row>
                        <entry/>
                        <entry>Marriage information</entry>
                        <entry>To verify a claimant's change of name</entry>
                      </row>
                      <row>
                        <entry/>
                        <entry>Death information</entry>
                        <entry>To identify deceased claimants</entry>
                      </row>
                    </tbody>
