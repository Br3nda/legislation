      <prov id="DLM158020" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">39A</label>
        <heading xml:lang="en-NZ">Funding policies</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The Commission and Te Reo Whakapuaki Irirangi shall, in carrying out their respective functions under this Act, each have regard to the funding policies adopted by the other.</text>
            </para>
          </subprov>
        </prov.body>
        <notes>
