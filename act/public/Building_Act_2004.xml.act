        <prov id="DLM307860" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">217</label>
          <heading xml:lang="en-NZ">Access to certain information kept by territorial authority</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A person—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>has a right to access the information referred to in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240215" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 216" atidlm:xmlId="DLM685175" atidlm:state="UNCHANGED"><atidlm:linkcontent>216(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243640" atidlm:sourceXmlId="DLM685175" atidlm:targetKey="1781022" atidlm:targetXmlId="DLM307859" atidlm:targetName="prov 216" atidlm:sourceName="DLM685175" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307860" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240216" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 216" atidlm:xmlId="DLM685176" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243641" atidlm:sourceXmlId="DLM685176" atidlm:targetKey="1781022" atidlm:targetXmlId="DLM307859" atidlm:targetName="prov 216" atidlm:sourceName="DLM685176" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307860" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>must, on request, be given access to that information by the territorial authority during ordinary office hours.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The right conferred by <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240217" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 217" atidlm:xmlId="DLM685177" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243642" atidlm:sourceXmlId="DLM685177" atidlm:targetKey="1781023" atidlm:targetXmlId="DLM307860" atidlm:targetName="prov 217" atidlm:sourceName="DLM685177" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307860" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>is subject to the provisions of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240218" atidlm:branchLabel="_NOVERSION_" atidlm:name="Local Government Official Information and Meetings Act 1987" atidlm:xmlId="DLM685178" atidlm:state="UNCHANGED"><atidlm:linkcontent>Local Government Official Information and Meetings Act 1987</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="243643" atidlm:sourceXmlId="DLM685178" atidlm:targetKey="665800" atidlm:targetXmlId="DLM122241" atidlm:targetName="Local Government Official Information and Meetings Act 1987" atidlm:sourceName="DLM685178" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM122241.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307860" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>does not extend to a plan or specification that is marked confidential by any or all of the following persons for the reason set out in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240219" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 217" atidlm:xmlId="DLM685179" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243644" atidlm:sourceXmlId="DLM685179" atidlm:targetKey="1781023" atidlm:targetXmlId="DLM307860" atidlm:targetName="prov 217" atidlm:sourceName="DLM685179" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307860" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>the person who submitted the plan or specification:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>the owner of the building to which the plan or specification relates:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para xml:lang="en-NZ">
                        <text>any subsequent owner of that building; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>is subject to any prescribed limits.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>The reason referred to in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240220" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 217" atidlm:xmlId="DLM685180" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243645" atidlm:sourceXmlId="DLM685180" atidlm:targetKey="1781023" atidlm:targetXmlId="DLM307860" atidlm:targetName="prov 217" atidlm:sourceName="DLM685180" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307860" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is any requirement of the owner of the building relating to the security of the building.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">4</label>
              <para xml:lang="en-NZ">
                <text>A territorial authority—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>must make photocopying facilities available to persons who wish to access information under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240221" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 217" atidlm:xmlId="DLM685181" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243646" atidlm:sourceXmlId="DLM685181" atidlm:targetKey="1781023" atidlm:targetXmlId="DLM307860" atidlm:targetName="prov 217" atidlm:sourceName="DLM685181" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307860" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>may charge a reasonable fee for the use of those facilities.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">5</label>
              <para xml:lang="en-NZ">
                <text><citation jurisdiction="nz">Subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240222" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 217" atidlm:xmlId="DLM685182" atidlm:state="UNCHANGED"><atidlm:linkcontent>(4)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243647" atidlm:sourceXmlId="DLM685182" atidlm:targetKey="1781023" atidlm:targetXmlId="DLM307860" atidlm:targetName="prov 217" atidlm:sourceName="DLM685182" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307860" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is subject to <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240223" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 44A" atidlm:xmlId="DLM685183" atidlm:state="UNCHANGED"><atidlm:linkcontent>44A</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243648" atidlm:sourceXmlId="DLM685183" atidlm:targetKey="670008" atidlm:targetXmlId="DLM123065" atidlm:targetName="prov 44A" atidlm:sourceName="DLM685183" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM123057.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307860" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240224" atidlm:branchLabel="_NOVERSION_" atidlm:name="Local Government Official Information and Meetings Act 1987" atidlm:xmlId="DLM685184" atidlm:state="UNCHANGED"><atidlm:linkcontent>Local Government Official Information and Meetings Act 1987</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="243649" atidlm:sourceXmlId="DLM685184" atidlm:targetKey="665800" atidlm:targetXmlId="DLM122241" atidlm:targetName="Local Government Official Information and Meetings Act 1987" atidlm:sourceName="DLM685184" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM122241.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307860" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 27(3), (4)</citation>
          </cf>
        </prov>
        <prov id="DLM307861" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">218</label>
          <heading xml:lang="en-NZ">Territorial authority must provide information to chief executive for purpose of facilitating performance of chief executive's function under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240225" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 169" atidlm:xmlId="DLM685185" atidlm:state="UNCHANGED"><atidlm:linkcontent>169</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243650" atidlm:sourceXmlId="DLM685185" atidlm:targetKey="1780279" atidlm:targetXmlId="DLM307366" atidlm:targetName="prov 169" atidlm:sourceName="DLM685185" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307364.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307861" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>For the purpose of facilitating the performance of the chief executive's function under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240226" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 169" atidlm:xmlId="DLM685186" atidlm:state="UNCHANGED"><atidlm:linkcontent>169</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243651" atidlm:sourceXmlId="DLM685186" atidlm:targetKey="1780279" atidlm:targetXmlId="DLM307366" atidlm:targetName="prov 169" atidlm:sourceName="DLM685186" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307364.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307861" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> (which relates to monitoring current and emerging trends in building design, etc), a territorial authority must provide information to the chief executive in accordance with the regulations.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>However, a territorial authority is only required to provide information under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240227" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 218" atidlm:xmlId="DLM685187" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243652" atidlm:sourceXmlId="DLM685187" atidlm:targetKey="1781024" atidlm:targetXmlId="DLM307861" atidlm:targetName="prov 218" atidlm:sourceName="DLM685187" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307861" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> about its functions, duties, and powers under this Act.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM307862" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">219</label>
          <heading xml:lang="en-NZ">Territorial authority may impose fee or charge and must collect levy</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A territorial authority—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>may impose a fee or charge—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>in relation to a building consent; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>for the performance of any other function or service under this Act; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>must collect the levy which an applicant is liable to pay to the chief executive under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240228" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 53" atidlm:xmlId="DLM685188" atidlm:state="UNCHANGED"><atidlm:linkcontent>53</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243653" atidlm:sourceXmlId="DLM685188" atidlm:targetKey="1772405" atidlm:targetXmlId="DLM306390" atidlm:targetName="prov 53" atidlm:sourceName="DLM685188" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM306360.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307862" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>If a fee, charge, or levy is payable to a territorial authority for the performance of a function or service under this Act, the territorial authority may refuse to perform the function or service, unless the fee, charge, or levy is paid.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <crosshead id="DLM307863" toc="yes" xml:lang="en-NZ">Power of territorial authority to carry out building work on default</crosshead>
        <prov id="DLM307864" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">220</label>
          <heading xml:lang="en-NZ">Territorial authority may carry out building work on default</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>This section applies if—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>a person is required, under this Act, by a building consent authority, territorial authority, or regional authority to carry out any building work on, or in connection with, any building; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>either—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>that person, after being given notice of the requirement, fails to commence to comply with the notice within the time stated in the notice or, if the time is not so stated, within a reasonable time; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>that person, after a certificate from any officer of the territorial authority that the work is of an urgent nature is communicated to him or her, defaults for 24 hours from the time of that communication; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>that person does not immediately proceed with the work with all reasonable speed.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The territorial authority may apply to a District Court for an order authorising the territorial authority to carry out building work.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>Before the territorial authority applies to a District Court under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240229" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 220" atidlm:xmlId="DLM685189" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243654" atidlm:sourceXmlId="DLM685189" atidlm:targetKey="1781027" atidlm:targetXmlId="DLM307864" atidlm:targetName="prov 220" atidlm:sourceName="DLM685189" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307864" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the territorial authority must give the owner of the building not less than 10 days' notice of its intention to do so.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">4</label>
              <para xml:lang="en-NZ">
                <text>If a territorial authority carries out building work under the authority of an order made under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240230" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 220" atidlm:xmlId="DLM685190" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243655" atidlm:sourceXmlId="DLM685190" atidlm:targetKey="1781027" atidlm:targetXmlId="DLM307864" atidlm:targetName="prov 220" atidlm:sourceName="DLM685190" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307864" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>,—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the owner of the building is liable for the costs of the work; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>the territorial authority may recover those costs from the owner; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>the amount recoverable by the territorial authority becomes a charge on the land on which the work was carried out.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">5</label>
              <para xml:lang="en-NZ">
                <text>Any building work required to be carried out under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240231" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 220" atidlm:xmlId="DLM685191" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243656" atidlm:sourceXmlId="DLM685191" atidlm:targetKey="1781027" atidlm:targetXmlId="DLM307864" atidlm:targetName="prov 220" atidlm:sourceName="DLM685191" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307864" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> may include the demolition of all or part of a building.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 74</citation>
          </cf>
        </prov>
        <prov id="DLM307865" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">221</label>
          <heading xml:lang="en-NZ">Recovery of costs when territorial authority carries out work on default</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>If a territorial authority is entitled under this Act to recover the costs of carrying out any building work from the owner of any building or land,—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the money payable becomes a charge on the land; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>the provisions of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240232" atidlm:branchLabel="_NOVERSION_" atidlm:name="Local Government (Rating) Act 2002" atidlm:xmlId="DLM685192" atidlm:state="UNCHANGED"><atidlm:linkcontent>Local Government (Rating) Act 2002</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="243657" atidlm:sourceXmlId="DLM685192" atidlm:targetKey="735800" atidlm:targetXmlId="DLM131393" atidlm:targetName="Local Government (Rating) Act 2002" atidlm:sourceName="DLM685192" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM131393.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307865" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240233" atidlm:branchLabel="_NOVERSION_" atidlm:name="Local Government Act 2002" atidlm:xmlId="DLM685193" atidlm:state="UNCHANGED"><atidlm:linkcontent>Local Government Act 2002</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="243658" atidlm:sourceXmlId="DLM685193" atidlm:targetKey="976802" atidlm:targetXmlId="DLM170872" atidlm:targetName="Local Government Act 2002" atidlm:sourceName="DLM685193" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM170872.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307865" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> apply accordingly.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The territorial authority—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>may destroy, sell, or otherwise dispose of any materials that result from the carrying out of any work by the territorial authority; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>in the case of the sale of any materials,—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>must apply the proceeds of the sale towards payment of the amount payable to the territorial authority under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240234" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 221" atidlm:xmlId="DLM685194" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243659" atidlm:sourceXmlId="DLM685194" atidlm:targetKey="1781028" atidlm:targetXmlId="DLM307865" atidlm:targetName="prov 221" atidlm:sourceName="DLM685194" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307865" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>must pay the surplus (if any) to the owner.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>If the territorial authority exercises the powers conferred by this section, the exercise of those powers does not relieve a person from any penalty for failure to comply with the requirements of a notice under this Act.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">4</label>
              <para xml:lang="en-NZ">
                <text>Any building work that is carried out, or is to be carried out, by a territorial authority under this section is declared to be a public work for the purposes of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240235" atidlm:branchLabel="_NOVERSION_" atidlm:name="Public Works Act 1981" atidlm:xmlId="DLM685195" atidlm:state="UNCHANGED"><atidlm:linkcontent>Public Works Act 1981</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="243660" atidlm:sourceXmlId="DLM685195" atidlm:targetKey="254400" atidlm:targetXmlId="DLM45426" atidlm:targetName="Public Works Act 1981" atidlm:sourceName="DLM685195" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM45426.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307865" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 75</citation>
          </cf>
        </prov>
        <crosshead id="DLM307866" toc="yes" xml:lang="en-NZ">Powers of territorial authority to carry out inspections and enter land</crosshead>
        <prov id="DLM307867" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">222</label>
          <heading xml:lang="en-NZ">Inspections by territorial authority</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>An authorised officer is entitled, at all times during normal working hours or while building work is being carried out,—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>to inspect—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>land on which building work is or is proposed to be carried out; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>building work that has been or is being carried out on or off the building site; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para xml:lang="en-NZ">
                        <text>any building; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>to enter premises for—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>the purpose of inspecting the building; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>the purpose of determining whether the building is dangerous, earthquake prone, or insanitary within the meaning of subpart 6 of <citation jurisdiction="nz">Part <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240236" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 2 Building" atidlm:xmlId="DLM685196" atidlm:state="UNCHANGED"><atidlm:linkcontent>2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243661" atidlm:sourceXmlId="DLM685196" atidlm:targetKey="1768800" atidlm:targetXmlId="DLM306332" atidlm:targetName="part 2 Building" atidlm:sourceName="DLM685196" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM306332.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307867" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>An authorised officer must, on entering private land under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240237" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 222" atidlm:xmlId="DLM685197" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243662" atidlm:sourceXmlId="DLM685197" atidlm:targetKey="1781006" atidlm:targetXmlId="DLM307867" atidlm:targetName="prov 222" atidlm:sourceName="DLM685197" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307867" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, and when requested at any subsequent time, produce to the occupier of the land written evidence of the authorised officer's identity.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>The powers conferred by this section are in addition to, and do not limit, the powers conferred by <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240238" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 173" atidlm:xmlId="DLM685198" atidlm:state="UNCHANGED"><atidlm:linkcontent>173</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243663" atidlm:sourceXmlId="DLM685198" atidlm:targetKey="989835" atidlm:targetXmlId="DLM173448" atidlm:targetName="prov 173" atidlm:sourceName="DLM685198" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM173420.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307867" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240239" atidlm:branchLabel="_NOVERSION_" atidlm:name="Local Government Act 2002" atidlm:xmlId="DLM685199" atidlm:state="UNCHANGED"><atidlm:linkcontent>Local Government Act 2002</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="243664" atidlm:sourceXmlId="DLM685199" atidlm:targetKey="976802" atidlm:targetXmlId="DLM170872" atidlm:targetName="Local Government Act 2002" atidlm:sourceName="DLM685199" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM170872.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307867" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">4</label>
              <para xml:lang="en-NZ">
                <text>In this section and <citation jurisdiction="nz">sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240240" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 223" atidlm:xmlId="DLM685300" atidlm:state="UNCHANGED"><atidlm:linkcontent>223 to 228</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243665" atidlm:sourceXmlId="DLM685300" atidlm:targetKey="1781007" atidlm:targetXmlId="DLM307872" atidlm:targetName="prov 223" atidlm:sourceName="DLM685300" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307867" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>,—</text>
                <def-para id="DLM307868" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM307869" xml:lang="en-NZ">authorised officer</def-term> means an officer of a territorial authority to whom either or both of the following applies</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">a</label>
                      <para xml:lang="en-NZ">
                        <text>he or she is authorised to carry out inspections; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">b</label>
                      <para xml:lang="en-NZ">
                        <text>he or she is authorised to enter land—</text>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">i</label>
                          <para xml:lang="en-NZ">
                            <text>by this Act; or</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">ii</label>
                          <para xml:lang="en-NZ">
                            <text>by an order of the District Court made under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240241" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 227" atidlm:xmlId="DLM685301" atidlm:state="UNCHANGED"><atidlm:linkcontent>227</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243666" atidlm:sourceXmlId="DLM685301" atidlm:targetKey="1781011" atidlm:targetXmlId="DLM307876" atidlm:targetName="prov 227" atidlm:sourceName="DLM685301" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307868" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></text>
                          </para>
                        </label-para>
                      </para>
                    </label-para>
                  </para>
                </def-para>
                <def-para id="DLM307870" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM307871" xml:lang="en-NZ">inspection</def-term> means the taking of all reasonable steps—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">a</label>
                      <para xml:lang="en-NZ">
                        <text>to determine whether—</text>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">i</label>
                          <para xml:lang="en-NZ">
                            <text>building work is being carried out without a building consent; or</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">ii</label>
                          <para xml:lang="en-NZ">
                            <text>building work is being carried out in accordance with a building consent; or</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">iii</label>
                          <para xml:lang="en-NZ">
                            <text>a notice to fix has been complied with:</text>
                          </para>
                        </label-para>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">b</label>
                      <para xml:lang="en-NZ">
                        <text>to ensure that,—</text>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">i</label>
                          <para xml:lang="en-NZ">
                            <text>in relation to buildings for which a compliance schedule is issued, the inspection, maintenance, and reporting procedures stated in the compliance schedule are being complied with; or</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">ii</label>
                          <para xml:lang="en-NZ">
                            <text>in relation to buildings that have specified systems, the requirement for a compliance schedule is being complied with:</text>
                          </para>
                        </label-para>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">c</label>
                      <para xml:lang="en-NZ">
                        <text>to enable a territorial authority to—</text>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">i</label>
                          <para xml:lang="en-NZ">
                            <text>identify dangerous, earthquake-prone, or insanitary buildings within its district; and</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">ii</label>
                          <para xml:lang="en-NZ">
                            <text>carry out its functions or duties in relation to those buildings:</text>
                          </para>
                        </label-para>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">d</label>
                      <para xml:lang="en-NZ">
                        <text>to satisfy a territorial authority as to whether a certificate of acceptance for building work should be issued under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240242" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 96" atidlm:xmlId="DLM685302" atidlm:state="UNCHANGED"><atidlm:linkcontent>96</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243667" atidlm:sourceXmlId="DLM685302" atidlm:targetKey="1774843" atidlm:targetXmlId="DLM306854" atidlm:targetName="prov 96" atidlm:sourceName="DLM685302" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM306845.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307870" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                      </para>
                    </label-para>
                  </para>
                </def-para>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 76(1), (2), (3), (11)</citation>
          </cf>
        </prov>
        <prov id="DLM307872" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">223</label>
          <heading xml:lang="en-NZ">Duty to assist inspections</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The persons specified in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240243" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 223" atidlm:xmlId="DLM685303" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243668" atidlm:sourceXmlId="DLM685303" atidlm:targetKey="1781007" atidlm:targetXmlId="DLM307872" atidlm:targetName="prov 223" atidlm:sourceName="DLM685303" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307872" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must give all reasonable assistance to enable an authorised officer to inspect all or part of a building or building work.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The persons are—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the owner of the building; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>an occupier of the building; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>any person engaged in the building work.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 76(4)</citation>
          </cf>
        </prov>
        <prov id="DLM307873" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">224</label>
          <heading xml:lang="en-NZ">Warrant must be produced</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>An authorised officer must, on entering private land and when requested at any subsequent time, produce to the occupier of the building a written warrant issued under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240244" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 174" atidlm:xmlId="DLM685304" atidlm:state="UNCHANGED"><atidlm:linkcontent>174</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243669" atidlm:sourceXmlId="DLM685304" atidlm:targetKey="989836" atidlm:targetXmlId="DLM173449" atidlm:targetName="prov 174" atidlm:sourceName="DLM685304" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM173420.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307873" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240245" atidlm:branchLabel="_NOVERSION_" atidlm:name="Local Government Act 2002" atidlm:xmlId="DLM685305" atidlm:state="UNCHANGED"><atidlm:linkcontent>Local Government Act 2002</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="243670" atidlm:sourceXmlId="DLM685305" atidlm:targetKey="976802" atidlm:targetXmlId="DLM170872" atidlm:targetName="Local Government Act 2002" atidlm:sourceName="DLM685305" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM170872.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307873" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 76(5)</citation>
          </cf>
        </prov>
        <prov id="DLM307874" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">225</label>
          <heading xml:lang="en-NZ">Offence to impersonate authorised officer</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A person commits an offence if the person impersonates or falsely pretends to be an authorised officer named in a warrant referred to in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240246" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 224" atidlm:xmlId="DLM685306" atidlm:state="UNCHANGED"><atidlm:linkcontent>224</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243671" atidlm:sourceXmlId="DLM685306" atidlm:targetKey="1781008" atidlm:targetXmlId="DLM307873" atidlm:targetName="prov 224" atidlm:sourceName="DLM685306" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307874" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>A person who commits an offence under this section is liable to a fine not exceeding $5,000.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 80(h)</citation>
          </cf>
        </prov>
        <prov id="DLM307875" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">226</label>
          <heading xml:lang="en-NZ">Restriction on entry to household unit</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>Despite <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240247" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 222" atidlm:xmlId="DLM685307" atidlm:state="UNCHANGED"><atidlm:linkcontent>222</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243672" atidlm:sourceXmlId="DLM685307" atidlm:targetKey="1781006" atidlm:targetXmlId="DLM307867" atidlm:targetName="prov 222" atidlm:sourceName="DLM685307" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307875" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, an authorised officer may not enter a household unit that is being used as a household unit without—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the consent of the occupier of the household unit; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>an order of a District Court made under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240248" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 227" atidlm:xmlId="DLM685308" atidlm:state="UNCHANGED"><atidlm:linkcontent>227</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243673" atidlm:sourceXmlId="DLM685308" atidlm:targetKey="1781011" atidlm:targetXmlId="DLM307876" atidlm:targetName="prov 227" atidlm:sourceName="DLM685308" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307875" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text><citation jurisdiction="nz">Subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240249" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 226" atidlm:xmlId="DLM685309" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243674" atidlm:sourceXmlId="DLM685309" atidlm:targetKey="1781010" atidlm:targetXmlId="DLM307875" atidlm:targetName="prov 226" atidlm:sourceName="DLM685309" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307875" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> does not limit the power conferred by <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240250" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 173" atidlm:xmlId="DLM685310" atidlm:state="UNCHANGED"><atidlm:linkcontent>173</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243675" atidlm:sourceXmlId="DLM685310" atidlm:targetKey="989835" atidlm:targetXmlId="DLM173448" atidlm:targetName="prov 173" atidlm:sourceName="DLM685310" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM173420.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307875" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240251" atidlm:branchLabel="_NOVERSION_" atidlm:name="Local Government Act 2002" atidlm:xmlId="DLM685311" atidlm:state="UNCHANGED"><atidlm:linkcontent>Local Government Act 2002</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="243676" atidlm:sourceXmlId="DLM685311" atidlm:targetKey="976802" atidlm:targetXmlId="DLM170872" atidlm:targetName="Local Government Act 2002" atidlm:sourceName="DLM685311" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM170872.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307875" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> (which allows a local authority entry into occupied land or buildings without prior notice in cases of emergency).</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 76(7)</citation>
          </cf>
        </prov>
        <prov id="DLM307876" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">227</label>
          <heading xml:lang="en-NZ">District Court may authorise entry to household unit</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A District Court, on the application of an authorised officer, may make an order authorising the officer to enter a household unit.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The Court may make the order under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240252" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 227" atidlm:xmlId="DLM685312" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243677" atidlm:sourceXmlId="DLM685312" atidlm:targetKey="1781011" atidlm:targetXmlId="DLM307876" atidlm:targetName="prov 227" atidlm:sourceName="DLM685312" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307876" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>only if it is satisfied that—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>the proposed entry is necessary for the purposes of <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240253" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 222" atidlm:xmlId="DLM685313" atidlm:state="UNCHANGED"><atidlm:linkcontent>222</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243678" atidlm:sourceXmlId="DLM685313" atidlm:targetKey="1781006" atidlm:targetXmlId="DLM307867" atidlm:targetName="prov 222" atidlm:sourceName="DLM685313" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307876" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>the authorised officer has taken all reasonable steps to obtain the consent of the occupier to the proposed entry; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>subject to any conditions that it thinks fit.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 76(8), (9)</citation>
          </cf>
        </prov>
        <prov id="DLM307877" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">228</label>
          <heading xml:lang="en-NZ">Authorised officer must give notice to occupier of household unit</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>An authorised officer must,—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>if he or she intends to apply for an order under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240254" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 227" atidlm:xmlId="DLM685314" atidlm:state="UNCHANGED"><atidlm:linkcontent>227</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243680" atidlm:sourceXmlId="DLM685314" atidlm:targetKey="1781011" atidlm:targetXmlId="DLM307876" atidlm:targetName="prov 227" atidlm:sourceName="DLM685314" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307877" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, give the occupier of the household unit to which that application relates not less than 10 days' written notice of his or her intention to do so; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>before he or she enters a household unit under the authority of an order under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240255" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 227" atidlm:xmlId="DLM685315" atidlm:state="UNCHANGED"><atidlm:linkcontent>227</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243681" atidlm:sourceXmlId="DLM685315" atidlm:targetKey="1781011" atidlm:targetXmlId="DLM307876" atidlm:targetName="prov 227" atidlm:sourceName="DLM685315" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307877" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, serve the order on the occupier of the household unit to which that order relates.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 76(10)</citation>
          </cf>
        </prov>
        <crosshead id="DLM307878" toc="yes" xml:lang="en-NZ">Enforcement powers of territorial authority</crosshead>
        <prov id="DLM307879" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">229</label>
          <heading xml:lang="en-NZ">Authorisation of enforcement officers</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A territorial authority—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>may authorise any of its officers to issue infringement notices under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240256" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 372" atidlm:xmlId="DLM685316" atidlm:state="UNCHANGED"><atidlm:linkcontent>372</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243682" atidlm:sourceXmlId="DLM685316" atidlm:targetKey="1788821" atidlm:targetXmlId="DLM309023" atidlm:targetName="prov 372" atidlm:sourceName="DLM685316" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM308695.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307879" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>if it grants an authorisation under <citation jurisdiction="nz">paragraph <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240257" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 229" atidlm:xmlId="DLM685317" atidlm:state="UNCHANGED"><atidlm:linkcontent>(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243683" atidlm:sourceXmlId="DLM685317" atidlm:targetKey="1781014" atidlm:targetXmlId="DLM307879" atidlm:targetName="prov 229" atidlm:sourceName="DLM685317" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307879" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, must supply the enforcement officer with a warrant.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The warrant must clearly state the functions and powers that the enforcement officer has been authorised to carry out under this Act.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 77(1), (2)</citation>
          </cf>
        </prov>
        <prov id="DLM307880" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">230</label>
          <heading xml:lang="en-NZ">Conditions of authorisation</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>An enforcement officer who exercises or purports to exercise a power conferred by or under this Act—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>must carry on him or her—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>the warrant supplied under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240258" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 229" atidlm:xmlId="DLM685318" atidlm:state="UNCHANGED"><atidlm:linkcontent>229</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243684" atidlm:sourceXmlId="DLM685318" atidlm:targetKey="1781014" atidlm:targetXmlId="DLM307879" atidlm:targetName="prov 229" atidlm:sourceName="DLM685318" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307880" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>evidence of his or her identity; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>must produce, if required to do so, the warrant and the evidence referred to in <citation jurisdiction="nz">paragraph <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240259" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 230" atidlm:xmlId="DLM685319" atidlm:state="UNCHANGED"><atidlm:linkcontent>(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243685" atidlm:sourceXmlId="DLM685319" atidlm:targetKey="1781015" atidlm:targetXmlId="DLM307880" atidlm:targetName="prov 230" atidlm:sourceName="DLM685319" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307880" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>An enforcement officer who holds a warrant issued under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240260" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 229" atidlm:xmlId="DLM685320" atidlm:state="UNCHANGED"><atidlm:linkcontent>229</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243686" atidlm:sourceXmlId="DLM685320" atidlm:targetKey="1781014" atidlm:targetXmlId="DLM307879" atidlm:targetName="prov 229" atidlm:sourceName="DLM685320" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307880" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must, on the termination of his or her appointment as an enforcement officer, surrender the warrant to the territorial authority.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 77(3), (4)</citation>
          </cf>
        </prov>
        <prov id="DLM307881" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">231</label>
          <heading xml:lang="en-NZ">Offence to impersonate enforcement officer</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A person commits an offence if the person impersonates or falsely pretends to be an enforcement officer named in a warrant supplied under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240261" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 229" atidlm:xmlId="DLM685321" atidlm:state="UNCHANGED"><atidlm:linkcontent>229</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243687" atidlm:sourceXmlId="DLM685321" atidlm:targetKey="1781014" atidlm:targetXmlId="DLM307879" atidlm:targetName="prov 229" atidlm:sourceName="DLM685321" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307881" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>A person who commits an offence under this section is liable to a fine not exceeding $5,000.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 80(h)</citation>
          </cf>
        </prov>
        <crosshead id="DLM307882" toc="yes" xml:lang="en-NZ">Delegation of power</crosshead>
        <prov id="DLM307883" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">232</label>
          <heading xml:lang="en-NZ">Delegation of powers by territorial authority and its officers</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text><citation jurisdiction="nz">Clause <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240262" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 32" atidlm:xmlId="DLM685322" atidlm:state="UNCHANGED"><atidlm:linkcontent>32</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243688" atidlm:sourceXmlId="DLM685322" atidlm:targetKey="1002812" atidlm:targetXmlId="DLM176007" atidlm:targetName="prov 32" atidlm:sourceName="DLM685322" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM175643.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307883" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of Schedule 7 of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240263" atidlm:branchLabel="_NOVERSION_" atidlm:name="Local Government Act 2002" atidlm:xmlId="DLM685323" atidlm:state="UNCHANGED"><atidlm:linkcontent>Local Government Act 2002</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="243689" atidlm:sourceXmlId="DLM685323" atidlm:targetKey="976802" atidlm:targetXmlId="DLM170872" atidlm:targetName="Local Government Act 2002" atidlm:sourceName="DLM685323" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM170872.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307883" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> applies, with all necessary modifications, in respect of powers conferred by this Act on a territorial authority and its officers.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 78</citation>
          </cf>
        </prov>
        <crosshead id="DLM307884" toc="yes" xml:lang="en-NZ">Transfer of functions, duties, or powers of territorial authority</crosshead>
        <prov id="DLM307885" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">233</label>
          <heading xml:lang="en-NZ">Transfer of functions, duties, or powers of territorial authority</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A territorial authority may transfer 1 or more of its functions, duties, or powers under this Act to another territorial authority, except the power of transfer conferred by this section.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 25(1), (2)</citation>
          </cf>
        </prov>
        <prov id="DLM307886" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">234</label>
          <heading xml:lang="en-NZ">Procedure for transfer</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>If a territorial authority proposes to transfer any of its functions, duties, or powers under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240264" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 233" atidlm:xmlId="DLM685324" atidlm:state="UNCHANGED"><atidlm:linkcontent>233</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243690" atidlm:sourceXmlId="DLM685324" atidlm:targetKey="1781033" atidlm:targetXmlId="DLM307885" atidlm:targetName="prov 233" atidlm:sourceName="DLM685324" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307886" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the territorial authority must—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>use the special consultative procedure in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240265" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 83" atidlm:xmlId="DLM685325" atidlm:state="UNCHANGED"><atidlm:linkcontent>83</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243691" atidlm:sourceXmlId="DLM685325" atidlm:targetKey="983613" atidlm:targetXmlId="DLM172328" atidlm:targetName="prov 83" atidlm:sourceName="DLM685325" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM172317.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307886" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240266" atidlm:branchLabel="_NOVERSION_" atidlm:name="Local Government Act 2002" atidlm:xmlId="DLM685326" atidlm:state="UNCHANGED"><atidlm:linkcontent>Local Government Act 2002</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="243692" atidlm:sourceXmlId="DLM685326" atidlm:targetKey="976802" atidlm:targetXmlId="DLM170872" atidlm:targetName="Local Government Act 2002" atidlm:sourceName="DLM685326" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM170872.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307886" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>serve notice on the Minister of its proposal to transfer the function, duty, or power; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>agree with the other territorial authority to whom the function, duty, or power is to be transferred that the transfer is desirable on either or both of the following grounds:</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>efficiency:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>technical or special capability, or expertise.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 25(3)</citation>
          </cf>
        </prov>
        <prov id="DLM307887" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">235</label>
          <heading xml:lang="en-NZ">Territorial authorities may agree on terms of transfer</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A territorial authority from whom functions, duties, or powers are to be transferred under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240267" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 233" atidlm:xmlId="DLM685327" atidlm:state="UNCHANGED"><atidlm:linkcontent>233</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243693" atidlm:sourceXmlId="DLM685327" atidlm:targetKey="1781033" atidlm:targetXmlId="DLM307885" atidlm:targetName="prov 233" atidlm:sourceName="DLM685327" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307887" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and a territorial authority to whom those functions, duties, or powers are to be transferred—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>must enter into an agreement in respect of the transfer; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>may agree on the terms and conditions of the transfer.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 25(4)</citation>
          </cf>
        </prov>
        <prov id="DLM307888" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">236</label>
          <heading xml:lang="en-NZ">Effect of transfer</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A territorial authority to whom a function, duty, or power is transferred under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240268" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 233" atidlm:xmlId="DLM685328" atidlm:state="UNCHANGED"><atidlm:linkcontent>233</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243694" atidlm:sourceXmlId="DLM685328" atidlm:targetKey="1781033" atidlm:targetXmlId="DLM307885" atidlm:targetName="prov 233" atidlm:sourceName="DLM685328" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307888" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>may perform the function or duty, or exercise the power, as if the function or duty were imposed, or the power were conferred, on that territorial authority under this Act; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>may, unless the agreement in respect of the transfer provides otherwise, cancel, at any time, the transfer in accordance with that agreement.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>A territorial authority from whom a function, duty, or power is transferred may, at any time, change or revoke the transfer by written notice to the other territorial authority concerned.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 25(5), (6), (7)</citation>
          </cf>
        </prov>
      </subpart>
      <subpart id="DLM307889" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" act.no="72" act.type="public" date.as.at="2008-10-16" date.assent="2004-08-24" date.first.valid="2007-09-18" stage="reprint" year="2004">
        <label auto.number="no" denominator="yes">3</label>
        <heading xml:lang="en-NZ">Responsibilities of building consent authority that is not territorial authority <insertwords>or regional authority</insertwords></heading>
        <notes>
          <history>
            <history-note id="DLM1222442"><amended-provision>Subpart 3 heading</amended-provision>: <amending-operation>amended</amending-operation>, on <amendment-date>15 March 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="849786" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1221608 - prov 41" atidlm:xmlId="DLM1222664" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 41</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="625270" atidlm:sourceXmlId="DLM1222664" atidlm:targetKey="11147724" atidlm:targetXmlId="DLM1149808" atidlm:targetName="prov 41" atidlm:sourceName="DLM1222664" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1149746.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1222442" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Building Amendment Act 2008</amending-leg> (2008 No 4).</history-note>
          </history>
        </notes>
        <prov id="DLM307890" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">237</label>
          <heading xml:lang="en-NZ">Application of subpart</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>This subpart applies to a building consent authority that—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>is registered under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240269" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 273" atidlm:xmlId="DLM685329" atidlm:state="UNCHANGED"><atidlm:linkcontent>273</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243695" atidlm:sourceXmlId="DLM685329" atidlm:targetKey="1784202" atidlm:targetXmlId="DLM308255" atidlm:targetName="prov 273" atidlm:sourceName="DLM685329" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM308253.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307890" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>provides services as a building consent authority in a district; but</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>is not—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>the territorial authority for the district, or another territorial authority to which a function, duty, or power is transferred under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240270" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 233" atidlm:xmlId="DLM685330" atidlm:state="UNCHANGED"><atidlm:linkcontent>233</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243696" atidlm:sourceXmlId="DLM685330" atidlm:targetKey="1781033" atidlm:targetXmlId="DLM307885" atidlm:targetName="prov 233" atidlm:sourceName="DLM685330" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307851.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307890" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>the regional authority for the region, or another regional authority to which a function, duty, or power is transferred under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240271" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 244" atidlm:xmlId="DLM685331" atidlm:state="UNCHANGED"><atidlm:linkcontent>244</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243697" atidlm:sourceXmlId="DLM685331" atidlm:targetKey="1782008" atidlm:targetXmlId="DLM308204" atidlm:targetName="prov 244" atidlm:sourceName="DLM685331" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307890" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM307891">Paragraph <amended-provision>(c)</amended-provision> was <amending-operation>substituted</amending-operation>, as from <amendment-date>14 April 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240272" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 3" atidlm:xmlId="DLM685332" atidlm:state="UNCHANGED"><atidlm:linkcontent>3(7)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="243698" atidlm:sourceXmlId="DLM685332" atidlm:targetKey="2034430" atidlm:targetXmlId="DLM340844" atidlm:targetName="prov 3" atidlm:sourceName="DLM685332" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340838.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307891" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Building Amendment Act 2005</amending-leg> (2005 No 31).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM307892" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">238</label>
          <heading xml:lang="en-NZ">Duties of building consent authority</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A building consent authority must, in performing its functions under <citation jurisdiction="nz">Part <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240273" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 2 Building" atidlm:xmlId="DLM685333" atidlm:state="UNCHANGED"><atidlm:linkcontent>2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243699" atidlm:sourceXmlId="DLM685333" atidlm:targetKey="1768800" atidlm:targetXmlId="DLM306332" atidlm:targetName="part 2 Building" atidlm:sourceName="DLM685333" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM306332.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307892" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>,—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>obtain a project information memorandum before granting a building consent; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>provide to the New Zealand Fire Service Commission a copy of every application for a building consent of a kind specified by notice under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240274" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 46" atidlm:xmlId="DLM685334" atidlm:state="UNCHANGED"><atidlm:linkcontent>46</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243700" atidlm:sourceXmlId="DLM685334" atidlm:targetKey="1772424" atidlm:targetXmlId="DLM306382" atidlm:targetName="prov 46" atidlm:sourceName="DLM685334" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM306360.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307892" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>provide to the territorial authority for the relevant district copies of the information referred to in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240275" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 238" atidlm:xmlId="DLM685335" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243701" atidlm:sourceXmlId="DLM685335" atidlm:targetKey="1781602" atidlm:targetXmlId="DLM307892" atidlm:targetName="prov 238" atidlm:sourceName="DLM685335" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307892" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The information is—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>plans and specifications submitted to the building consent authority in relation to an application for a building consent; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>the following information issued or received by the building consent authority in respect of a building:</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>project information memoranda:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>building consents:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para xml:lang="en-NZ">
                        <text>code compliance certificates:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iv</label>
                      <para xml:lang="en-NZ">
                        <text>compliance schedules:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">v</label>
                      <para xml:lang="en-NZ">
                        <text>building warrants of fitness:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">vi</label>
                      <para xml:lang="en-NZ">
                        <text>energy work certificates:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">vii</label>
                      <para xml:lang="en-NZ">
                        <text>notices to fix:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">viii</label>
                      <para xml:lang="en-NZ">
                        <text>any other records that relate to the information referred to in <citation jurisdiction="nz">subparagraphs <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240276" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 238" atidlm:xmlId="DLM685336" atidlm:state="UNCHANGED"><atidlm:linkcontent>(i) to (vii)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243702" atidlm:sourceXmlId="DLM685336" atidlm:targetKey="1781602" atidlm:targetXmlId="DLM307892" atidlm:targetName="prov 238" atidlm:sourceName="DLM685336" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307892" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>records of any information on any land or building received by the building consent authority from a statutory authority; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">d</label>
                  <para xml:lang="en-NZ">
                    <text>details about any levy collected under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240277" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 53" atidlm:xmlId="DLM685337" atidlm:state="UNCHANGED"><atidlm:linkcontent>53</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243703" atidlm:sourceXmlId="DLM685337" atidlm:targetKey="1772405" atidlm:targetXmlId="DLM306390" atidlm:targetName="prov 53" atidlm:sourceName="DLM685337" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM306360.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307892" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">e</label>
                  <para xml:lang="en-NZ">
                    <text>the following information about the building consent authority:</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>the name and contact address of the building consent authority:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>the scope of accreditation of the building consent authority:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para xml:lang="en-NZ">
                        <text>whether the building consent authority has adequate means to cover any civil liabilities that may arise in the performance of its functions, and, if so, what those means are.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>A building consent authority must provide the copies of the information to the territorial authority for the relevant district within 5 working days after the building consent authority issues or receives the information.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">4</label>
              <para xml:lang="en-NZ">
                <text>A building consent authority commits an offence if it fails to comply with <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240278" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 238" atidlm:xmlId="DLM685338" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243704" atidlm:sourceXmlId="DLM685338" atidlm:targetKey="1781602" atidlm:targetXmlId="DLM307892" atidlm:targetName="prov 238" atidlm:sourceName="DLM685338" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307892" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">5</label>
              <para xml:lang="en-NZ">
                <text>A building consent authority that commits an offence under this section is liable to a fine not exceeding $5,000 and, in the case of a continuing offence, to a further fine not exceeding $500 for every day or part of a day during which the offence has continued.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM307893" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">239</label>
          <heading xml:lang="en-NZ">Building consent authority must provide information to chief executive</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A building consent authority must provide information to the chief executive in accordance with the regulations.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM307894" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">240</label>
          <heading xml:lang="en-NZ">Building consent authority may impose fee or charge and must collect levy</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A building consent authority—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>may impose a fee or charge payable by a member of the public—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>in relation to a building consent; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>for the performance of any other function or service under this Act; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>must collect the levy for which an applicant is liable to pay to the chief executive under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240279" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 53" atidlm:xmlId="DLM685339" atidlm:state="UNCHANGED"><atidlm:linkcontent>53</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243705" atidlm:sourceXmlId="DLM685339" atidlm:targetKey="1772405" atidlm:targetXmlId="DLM306390" atidlm:targetName="prov 53" atidlm:sourceName="DLM685339" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM306360.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307894" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>If a fee, charge, or levy is payable to a building consent authority for the performance of a function or service under this Act, the building consent authority may refuse to perform the function or service, unless the fee, charge, or levy is paid.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>A failure by a person to pay a fee, charge, or levy does not affect the duty of a building consent authority under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240280" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 238" atidlm:xmlId="DLM685340" atidlm:state="UNCHANGED"><atidlm:linkcontent>238</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243706" atidlm:sourceXmlId="DLM685340" atidlm:targetKey="1781602" atidlm:targetXmlId="DLM307892" atidlm:targetName="prov 238" atidlm:sourceName="DLM685340" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307889.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307894" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> to provide copies of certain information to the territorial authority.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
      </subpart>
      <subpart id="DLM307895" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" act.no="72" act.type="public" date.as.at="2008-10-16" date.assent="2004-08-24" date.first.valid="2007-09-18" stage="reprint" year="2004">
        <label auto.number="no" denominator="yes">4</label>
        <heading xml:lang="en-NZ">Responsibilities of regional authorities</heading>
        <crosshead id="DLM307896" toc="yes" xml:lang="en-NZ">Functions, duties, and powers of regional authorities generally</crosshead>
        <prov id="DLM307897" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">241</label>
          <heading xml:lang="en-NZ">Regional authority must gain accreditation and be registered</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A regional authority must, for the purpose of ensuring that it performs the function of a building consent authority in relation to dams,—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>apply for, and gain, accreditation under this Part; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>apply to be, and be registered as, a building consent authority under this Part; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>maintain that accreditation and registration at all times.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text><citation jurisdiction="nz">Subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240281" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 241" atidlm:xmlId="DLM685341" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243707" atidlm:sourceXmlId="DLM685341" atidlm:targetKey="1782002" atidlm:targetXmlId="DLM307897" atidlm:targetName="prov 241" atidlm:sourceName="DLM685341" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307897" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>is subject to the regional authority's power under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240282" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 244" atidlm:xmlId="DLM685342" atidlm:state="UNCHANGED"><atidlm:linkcontent>244</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243708" atidlm:sourceXmlId="DLM685342" atidlm:targetKey="1782008" atidlm:targetXmlId="DLM308204" atidlm:targetName="prov 244" atidlm:sourceName="DLM685342" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307897" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> to transfer any of its functions under this Act to another regional authority; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>does not apply to any function it transfers to another regional authority.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM307898">Subsection <amended-provision>(2)</amended-provision> was <amending-operation>inserted</amending-operation>, as from <amendment-date>14 April 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240283" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 18" atidlm:xmlId="DLM685343" atidlm:state="UNCHANGED"><atidlm:linkcontent>18</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="243709" atidlm:sourceXmlId="DLM685343" atidlm:targetKey="2034419" atidlm:targetXmlId="DLM340871" atidlm:targetName="prov 18" atidlm:sourceName="DLM685343" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340838.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM307898" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Building Amendment Act 2005</amending-leg> (2005 No 31).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM307899" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">242</label>
          <heading xml:lang="en-NZ">Regional authority must provide information to chief executive</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A regional authority must provide information to the chief executive in accordance with the regulations.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM308200" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">243</label>
          <heading xml:lang="en-NZ">Regional authorities may impose fee or charge and recover costs<insertwords>, and must collect levy</insertwords></heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A regional authority—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>may impose a fee or charge—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>for issuing a project information memorandum under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240284" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 34" atidlm:xmlId="DLM685344" atidlm:state="UNCHANGED"><atidlm:linkcontent>34</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243710" atidlm:sourceXmlId="DLM685344" atidlm:targetKey="1772404" atidlm:targetXmlId="DLM306365" atidlm:targetName="prov 34" atidlm:sourceName="DLM685344" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM306360.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308200" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>for performing any other function or service under this Act; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>may recover its costs from the owner if it carries out building work under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240285" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 156" atidlm:xmlId="DLM685345" atidlm:state="UNCHANGED"><atidlm:linkcontent>156</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243711" atidlm:sourceXmlId="DLM685345" atidlm:targetKey="1777002" atidlm:targetXmlId="DLM307340" atidlm:targetName="prov 156" atidlm:sourceName="DLM685345" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307312.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308200" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>must collect the levy an applicant is liable to pay to the chief executive under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240286" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 53" atidlm:xmlId="DLM685346" atidlm:state="UNCHANGED"><atidlm:linkcontent>53</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243712" atidlm:sourceXmlId="DLM685346" atidlm:targetKey="1772405" atidlm:targetXmlId="DLM306390" atidlm:targetName="prov 53" atidlm:sourceName="DLM685346" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM306360.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308200" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>If a fee or charge is payable to a regional authority for the performance of a function or service under this Act, the regional authority may decline to perform the function or service, unless the fee or charge is paid.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM308201">The heading to section <amended-provision>243</amended-provision> was <amending-operation>amended</amending-operation>, as from <amendment-date>14 April 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240287" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 19" atidlm:xmlId="DLM685347" atidlm:state="UNCHANGED"><atidlm:linkcontent>19(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="243713" atidlm:sourceXmlId="DLM685347" atidlm:targetKey="2034420" atidlm:targetXmlId="DLM340872" atidlm:targetName="prov 19" atidlm:sourceName="DLM685347" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340838.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308201" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Building Amendment Act 2005</amending-leg> (2005 No 31) <amending-instruction>by adding the words <quote.in quote="1">, and must collect levy</quote.in></amending-instruction>.</history-note>
              <history-note id="DLM308202">Subsection <amended-provision>(1)</amended-provision> was <amending-operation>substituted</amending-operation>, as from <amendment-date>14 April 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240288" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 19" atidlm:xmlId="DLM685348" atidlm:state="UNCHANGED"><atidlm:linkcontent>19(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="243714" atidlm:sourceXmlId="DLM685348" atidlm:targetKey="2034420" atidlm:targetXmlId="DLM340872" atidlm:targetName="prov 19" atidlm:sourceName="DLM685348" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340838.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308202" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Building Amendment Act 2005</amending-leg> (2005 No 31).</history-note>
            </history>
          </notes>
        </prov>
        <crosshead id="DLM308203" toc="yes" xml:lang="en-NZ">Transfer of functions, duties, and powers of regional authority</crosshead>
        <prov id="DLM308204" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">244</label>
          <heading xml:lang="en-NZ">Transfer of functions, duties, and powers of regional authority</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A regional authority may transfer 1 or more of its functions, duties, or powers under this Act to another regional authority, except the power of transfer conferred by this section.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM308205" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">245</label>
          <heading xml:lang="en-NZ">Procedure for transfer</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>If a regional authority proposes to transfer any of its functions, duties, or powers under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240289" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 244" atidlm:xmlId="DLM685349" atidlm:state="UNCHANGED"><atidlm:linkcontent>244</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243715" atidlm:sourceXmlId="DLM685349" atidlm:targetKey="1782008" atidlm:targetXmlId="DLM308204" atidlm:targetName="prov 244" atidlm:sourceName="DLM685349" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308205" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the regional authority must—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>use the special consultative procedure in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240290" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 83" atidlm:xmlId="DLM685350" atidlm:state="UNCHANGED"><atidlm:linkcontent>83</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243716" atidlm:sourceXmlId="DLM685350" atidlm:targetKey="983613" atidlm:targetXmlId="DLM172328" atidlm:targetName="prov 83" atidlm:sourceName="DLM685350" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM172317.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308205" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240291" atidlm:branchLabel="_NOVERSION_" atidlm:name="Local Government Act 2002" atidlm:xmlId="DLM685351" atidlm:state="UNCHANGED"><atidlm:linkcontent>Local Government Act 2002</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="243717" atidlm:sourceXmlId="DLM685351" atidlm:targetKey="976802" atidlm:targetXmlId="DLM170872" atidlm:targetName="Local Government Act 2002" atidlm:sourceName="DLM685351" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM170872.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308205" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>serve notice on the Minister of its proposal to transfer the function, duty, or power; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>agree with the other regional authority to whom the function, duty, or power is to be transferred that the transfer is desirable on either or both of the following grounds:</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>efficiency:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>technical or special capability, or expertise.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM308206" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">246</label>
          <heading xml:lang="en-NZ">Regional authorities may agree on terms of transfer</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A regional authority from whom functions, duties, or powers are to be transferred under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240292" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 244" atidlm:xmlId="DLM685352" atidlm:state="UNCHANGED"><atidlm:linkcontent>244</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243718" atidlm:sourceXmlId="DLM685352" atidlm:targetKey="1782008" atidlm:targetXmlId="DLM308204" atidlm:targetName="prov 244" atidlm:sourceName="DLM685352" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308206" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and a regional authority to whom those functions, duties, or powers are to be transferred—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>must enter into an agreement in respect of the transfer; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>may agree on the terms and conditions of the transfer.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM308207" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">247</label>
          <heading xml:lang="en-NZ">Effect of transfer</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A regional authority to whom a function, duty, or power is transferred under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240293" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 244" atidlm:xmlId="DLM685353" atidlm:state="UNCHANGED"><atidlm:linkcontent>244</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243760" atidlm:sourceXmlId="DLM685353" atidlm:targetKey="1782008" atidlm:targetXmlId="DLM308204" atidlm:targetName="prov 244" atidlm:sourceName="DLM685353" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM307895.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308207" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>may perform the function or duty, or exercise the power, as if the function or duty were imposed, or the power were conferred, on that regional authority under this Act; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>may, unless the agreement in respect of the transfer provides otherwise, cancel, at any time, the transfer in accordance with that agreement.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>A regional authority from whom a function, duty, or power is transferred may, at any time, change or revoke the transfer by written notice to the other regional authority concerned.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
      </subpart>
      <subpart id="DLM308208" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" act.no="72" act.type="public" date.as.at="2008-10-16" date.assent="2004-08-24" date.first.valid="2007-09-18" stage="reprint" year="2004">
        <label auto.number="no" denominator="yes">5</label>
        <heading xml:lang="en-NZ">Responsibilities of building consent accreditation body</heading>
        <crosshead id="DLM308209" toc="yes" xml:lang="en-NZ">Appointment of building consent accreditation body</crosshead>
        <prov id="DLM308210" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">248</label>
          <heading xml:lang="en-NZ">Chief executive may appoint building consent accreditation body</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The chief executive may—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>appoint a person as a building consent accreditation body; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>revoke the appointment at any time.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>A reference in this subpart to a building consent accreditation body is a reference to—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the person appointed under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240294" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 248" atidlm:xmlId="DLM685354" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243761" atidlm:sourceXmlId="DLM685354" atidlm:targetKey="1782601" atidlm:targetXmlId="DLM308210" atidlm:targetName="prov 248" atidlm:sourceName="DLM685354" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308210" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>if no person is appointed, the chief executive.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <crosshead id="DLM308211" toc="yes" xml:lang="en-NZ">Requirements for building consent accreditation body</crosshead>
        <prov id="DLM308212" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">249</label>
          <heading xml:lang="en-NZ">Requirements for building consent accreditation body</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The chief executive must, by notice in the <emphasis style="italic">Gazette</emphasis>, specify—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the minimum frequency of audits that the building consent accreditation body must conduct on accredited building consent authorities<struckoutwords/> (which must be at least once every 3 years); and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>any other matters the chief executive considers necessary or appropriate.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM308213">Paragraph <amended-provision>(a)</amended-provision> was <amending-operation>amended</amending-operation>, as from <amendment-date>14 April 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240295" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 16" atidlm:xmlId="DLM685355" atidlm:state="UNCHANGED"><atidlm:linkcontent>16(2)(n)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="243762" atidlm:sourceXmlId="DLM685355" atidlm:targetKey="2034417" atidlm:targetXmlId="DLM340868" atidlm:targetName="prov 16" atidlm:sourceName="DLM685355" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340838.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308213" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Building Amendment Act 2005</amending-leg> (2005 No 31) <amending-instruction>by omitting the words <quote.in quote="1">and accredited regional authorities</quote.in></amending-instruction>.</history-note>
            </history>
          </notes>
        </prov>
        <crosshead id="DLM1221609" toc="yes">Audit fees</crosshead>
        <notes>
          <history>
            <history-note id="DLM1222443"><amended-provision>Heading</amended-provision>: <amending-operation>inserted</amending-operation>, on <amendment-date>15 March 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="849787" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1221611 - prov 42" atidlm:xmlId="DLM1222665" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 42</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="625271" atidlm:sourceXmlId="DLM1222665" atidlm:targetKey="11147727" atidlm:targetXmlId="DLM1149809" atidlm:targetName="prov 42" atidlm:sourceName="DLM1222665" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1149746.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1222443" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Building Amendment Act 2008</amending-leg> (2008 No 4).</history-note>
          </history>
        </notes>
        <prov id="DLM1221610" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">249A</label>
          <heading>Fees for audits</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>A building consent accreditation body may charge an accredited building consent authority the prescribed fee (if any) for an audit conducted under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="849788" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1221613 - prov 249" atidlm:xmlId="DLM1222666" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 249(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="625272" atidlm:sourceXmlId="DLM1222666" atidlm:targetKey="1782604" atidlm:targetXmlId="DLM308212" atidlm:targetName="prov 249" atidlm:sourceName="DLM1222666" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1221610" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> by the building consent accreditation body on the building consent authority.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1222444"><amended-provision>Section 249A</amended-provision>: <amending-operation>inserted</amending-operation>, on <amendment-date>15 March 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="849789" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1221611 - prov 42" atidlm:xmlId="DLM1222667" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 42</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="625273" atidlm:sourceXmlId="DLM1222667" atidlm:targetKey="11147727" atidlm:targetXmlId="DLM1149809" atidlm:targetName="prov 42" atidlm:sourceName="DLM1222667" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1149746.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1222444" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Building Amendment Act 2008</amending-leg> (2008 No 4).</history-note>
            </history>
          </notes>
        </prov>
        <crosshead id="DLM308214" toc="yes" xml:lang="en-NZ">Accreditation of building consent authorities or regional authorities</crosshead>
        <prov id="DLM308215" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">250</label>
          <heading xml:lang="en-NZ">Accreditation</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The building consent accreditation body may, on the application of a person made in accordance with <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240296" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 253" atidlm:xmlId="DLM685356" atidlm:state="UNCHANGED"><atidlm:linkcontent>253</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243763" atidlm:sourceXmlId="DLM685356" atidlm:targetKey="1782611" atidlm:targetXmlId="DLM308220" atidlm:targetName="prov 253" atidlm:sourceName="DLM685356" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308215" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, <insertwords>and on payment by the person of the prescribed fee (if any),</insertwords> accredit that person to perform the functions of a building consent authority<struckoutwords/> under this Part and <citation jurisdiction="nz">Part <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240297" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 2 Building" atidlm:xmlId="DLM685357" atidlm:state="UNCHANGED"><atidlm:linkcontent>2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243764" atidlm:sourceXmlId="DLM685357" atidlm:targetKey="1768800" atidlm:targetXmlId="DLM306332" atidlm:targetName="part 2 Building" atidlm:sourceName="DLM685357" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM306332.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308215" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1991 No 150 s 51</citation>
          </cf>
          <notes>
            <history>
              <history-note id="DLM1222445"><amended-provision>Section 250</amended-provision>: <amending-operation>amended</amending-operation>, on <amendment-date>15 March 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="849790" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1221614 - prov 43" atidlm:xmlId="DLM1222668" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 43</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="625274" atidlm:sourceXmlId="DLM1222668" atidlm:targetKey="11147728" atidlm:targetXmlId="DLM1149812" atidlm:targetName="prov 43" atidlm:sourceName="DLM1222668" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1149746.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1222445" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Building Amendment Act 2008</amending-leg> (2008 No 4).</history-note>
              <history-note id="DLM308216">Section <amended-provision>250</amended-provision> was <amending-operation>amended</amending-operation>, as from <amendment-date>14 April 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240298" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 16" atidlm:xmlId="DLM685358" atidlm:state="UNCHANGED"><atidlm:linkcontent>16(2)(o)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="243765" atidlm:sourceXmlId="DLM685358" atidlm:targetKey="2034417" atidlm:targetXmlId="DLM340868" atidlm:targetName="prov 16" atidlm:sourceName="DLM685358" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340838.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308216" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Building Amendment Act 2005</amending-leg> (2005 No 31) <amending-instruction>by omitting the words <quote.in quote="1">or a regional authority</quote.in></amending-instruction>.</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM308217" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">251</label>
          <heading xml:lang="en-NZ">Criteria for accreditation</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>Before granting accreditation, the building consent accreditation body must be satisfied that the applicant meets the prescribed criteria and standards for accreditation.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM308218" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">252</label>
          <heading xml:lang="en-NZ">Scope of accreditation of building consent authority that is not territorial authority</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>This section applies to a building consent authority that is not a territorial authority <insertwords>or a regional authority</insertwords>.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The building consent accreditation body must, before granting accreditation to a building consent authority to whom this section applies, determine the scope of accreditation having regard to the prescribed criteria and standards for accreditation.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>The scope of accreditation relates to 1 or more types of building in relation to which the building consent authority concerned is authorised to perform functions under this Part and <citation jurisdiction="nz">Part <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240299" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 2 Building" atidlm:xmlId="DLM685359" atidlm:state="UNCHANGED"><atidlm:linkcontent>2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243766" atidlm:sourceXmlId="DLM685359" atidlm:targetKey="1768800" atidlm:targetXmlId="DLM306332" atidlm:targetName="part 2 Building" atidlm:sourceName="DLM685359" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM306332.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308218" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">4</label>
              <para xml:lang="en-NZ">
                <text>An accredited building consent authority may, at any time, request the building consent accreditation body to change the scope of its accreditation.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM308219">Subsection <amended-provision>(1)</amended-provision> was <amending-operation>amended</amending-operation>, as from <amendment-date>14 April 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240300" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 3" atidlm:xmlId="DLM685360" atidlm:state="UNCHANGED"><atidlm:linkcontent>3(5)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="243767" atidlm:sourceXmlId="DLM685360" atidlm:targetKey="2034430" atidlm:targetXmlId="DLM340844" atidlm:targetName="prov 3" atidlm:sourceName="DLM685360" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340838.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308219" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Building Amendment Act 2005</amending-leg> (2005 No 31) <amending-instruction>by inserting the words <quote.in quote="1">or a regional authority</quote.in> after the words <quote.in quote="1">territorial authority</quote.in></amending-instruction>.</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM308220" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">253</label>
          <heading xml:lang="en-NZ">Application for accreditation</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>An application for accreditation under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240301" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 250" atidlm:xmlId="DLM685361" atidlm:state="UNCHANGED"><atidlm:linkcontent>250</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243768" atidlm:sourceXmlId="DLM685361" atidlm:targetKey="1782607" atidlm:targetXmlId="DLM308215" atidlm:targetName="prov 250" atidlm:sourceName="DLM685361" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308220" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>be made in writing; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>be given in the prescribed manner (if any); and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>contain the prescribed information <insertwords>(if any).</insertwords></text>
                  </para>
                </label-para>
                <label-para deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">d</label>
                  <para xml:lang="en-NZ">
                    <text/>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1222446"><amended-provision>Section 253(c)</amended-provision>: <amending-operation>amended</amending-operation>, on <amendment-date>15 March 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="849791" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1221615 - prov 44" atidlm:xmlId="DLM1222669" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 44(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="625275" atidlm:sourceXmlId="DLM1222669" atidlm:targetKey="11147695" atidlm:targetXmlId="DLM1149813" atidlm:targetName="prov 44" atidlm:sourceName="DLM1222669" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1149746.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1222446" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Building Amendment Act 2008</amending-leg> (2008 No 4).</history-note>
              <history-note id="DLM1222447"><amended-provision>Section 253(d)</amended-provision>: <amending-operation>repealed</amending-operation>, on <amendment-date>15 March 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="849792" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1221615 - prov 44" atidlm:xmlId="DLM1222670" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 44(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="625276" atidlm:sourceXmlId="DLM1222670" atidlm:targetKey="11147695" atidlm:targetXmlId="DLM1149813" atidlm:targetName="prov 44" atidlm:sourceName="DLM1222670" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1149746.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1222447" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Building Amendment Act 2008</amending-leg> (2008 No 4).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM308221" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">254</label>
          <heading xml:lang="en-NZ">Revocation of accreditation</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>An accreditation under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240302" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 250" atidlm:xmlId="DLM685362" atidlm:state="UNCHANGED"><atidlm:linkcontent>250</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243769" atidlm:sourceXmlId="DLM685362" atidlm:targetKey="1782607" atidlm:targetXmlId="DLM308215" atidlm:targetName="prov 250" atidlm:sourceName="DLM685362" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308221" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> may be revoked, or the scope of accreditation may be amended, by—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the building consent accreditation body; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>the chief executive.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>An accreditation may be revoked only if the building consent accreditation body or the chief executive—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>is satisfied that the building consent authority<struckoutwords/> no longer meets the prescribed criteria and standards for accreditation; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>has first given the building consent authority<struckoutwords/> concerned a reasonable opportunity to be heard.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>Despite <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240303" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 254" atidlm:xmlId="DLM685363" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243770" atidlm:sourceXmlId="DLM685363" atidlm:targetKey="1782615" atidlm:targetXmlId="DLM308221" atidlm:targetName="prov 254" atidlm:sourceName="DLM685363" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308221" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the building consent accreditation body or the chief executive must not revoke the accreditation of a building consent authority<struckoutwords/> if—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the prescribed criteria and standards for accreditation are amended; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>the building consent authority or regional authority no longer meets those criteria and standards solely as a result of the amendments.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">4</label>
              <para xml:lang="en-NZ">
                <text>The limit in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240304" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 254" atidlm:xmlId="DLM685364" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243771" atidlm:sourceXmlId="DLM685364" atidlm:targetKey="1782615" atidlm:targetXmlId="DLM308221" atidlm:targetName="prov 254" atidlm:sourceName="DLM685364" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308221" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> applies only during the period of 3 months after the date on which the amendments come into force.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM308222">Subsection <amended-provision>(2)(a)</amended-provision> was <amending-operation>amended</amending-operation>, as from <amendment-date>14 April 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240305" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 16" atidlm:xmlId="DLM685365" atidlm:state="UNCHANGED"><atidlm:linkcontent>16(2)(p)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="243772" atidlm:sourceXmlId="DLM685365" atidlm:targetKey="2034417" atidlm:targetXmlId="DLM340868" atidlm:targetName="prov 16" atidlm:sourceName="DLM685365" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340838.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308222" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Building Amendment Act 2005</amending-leg> (2005 No 31) <amending-instruction>by omitting the words <quote.in quote="1">or the regional authority</quote.in></amending-instruction>.</history-note>
              <history-note id="DLM308223">Subsection <amended-provision>(2)(b)</amended-provision> was <amending-operation>amended</amending-operation>, as from <amendment-date>14 April 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240306" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 16" atidlm:xmlId="DLM685366" atidlm:state="UNCHANGED"><atidlm:linkcontent>16(2)(p)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="243773" atidlm:sourceXmlId="DLM685366" atidlm:targetKey="2034417" atidlm:targetXmlId="DLM340868" atidlm:targetName="prov 16" atidlm:sourceName="DLM685366" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340838.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308223" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Building Amendment Act 2005</amending-leg> (2005 No 31) <amending-instruction>by omitting the words <quote.in quote="1">or the regional authority</quote.in></amending-instruction>.</history-note>
              <history-note id="DLM308224">Subsection <amended-provision>(3)</amended-provision> was <amending-operation>amended</amending-operation>, as from <amendment-date>14 April 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240307" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 16" atidlm:xmlId="DLM685367" atidlm:state="UNCHANGED"><atidlm:linkcontent>16(2)(o)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="243774" atidlm:sourceXmlId="DLM685367" atidlm:targetKey="2034417" atidlm:targetXmlId="DLM340868" atidlm:targetName="prov 16" atidlm:sourceName="DLM685367" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340838.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308224" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Building Amendment Act 2005</amending-leg> (2005 No 31) <amending-instruction>by omitting the words <quote.in quote="1">or a regional authority</quote.in></amending-instruction>.</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM308225" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">255</label>
          <heading xml:lang="en-NZ">Building consent accreditation body must notify chief executive of grant and revocation of accreditation</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The building consent accreditation body must notify the chief executive when it grants or revokes an accreditation of a building consent authority<struckoutwords/> .</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The notification must be given—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>in the manner notified by the chief executive to the building consent accreditation body from time to time; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>within 7 days after the grant or revocation to which it relates.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM308226">Subsection <amended-provision>(1)</amended-provision> was <amending-operation>amended</amending-operation>, as from <amendment-date>14 April 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240308" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 16" atidlm:xmlId="DLM685368" atidlm:state="UNCHANGED"><atidlm:linkcontent>16(2)(o)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="243775" atidlm:sourceXmlId="DLM685368" atidlm:targetKey="2034417" atidlm:targetXmlId="DLM340868" atidlm:targetName="prov 16" atidlm:sourceName="DLM685368" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340838.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308226" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Building Amendment Act 2005</amending-leg> (2005 No 31) <amending-instruction>by omitting the words <quote.in quote="1">or a regional authority</quote.in></amending-instruction>.</history-note>
            </history>
          </notes>
        </prov>
      </subpart>
      <subpart id="DLM308227" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" act.no="72" act.type="public" date.as.at="2008-10-16" date.assent="2004-08-24" date.first.valid="2007-09-18" stage="reprint" year="2004">
        <label auto.number="no" denominator="yes">6</label>
        <heading xml:lang="en-NZ">Responsibilities of dam owner accreditation body</heading>
        <crosshead id="DLM308228" toc="yes" xml:lang="en-NZ">Appointment of dam owner accreditation body</crosshead>
        <prov id="DLM308229" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">256</label>
          <heading xml:lang="en-NZ">Chief executive may appoint dam owner accreditation body</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The chief executive may—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>appoint a person as a dam owner accreditation body; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>revoke the appointment at any time.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>A reference in this subpart to a dam owner accreditation body is a reference to—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>a person appointed under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240309" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 256" atidlm:xmlId="DLM685369" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243776" atidlm:sourceXmlId="DLM685369" atidlm:targetKey="1783000" atidlm:targetXmlId="DLM308229" atidlm:targetName="prov 256" atidlm:sourceName="DLM685369" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM308227.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308227.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308229" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>if no person is appointed, the chief executive.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <crosshead id="DLM308230" toc="yes" xml:lang="en-NZ">Requirements for dam owner accreditation body</crosshead>
        <prov id="DLM308231" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">257</label>
          <heading xml:lang="en-NZ">Requirements for dam owner accreditation body</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The chief executive must, by notice in the <emphasis style="italic">Gazette</emphasis>, specify—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the minimum frequency of audits that the dam owner accreditation body must conduct on accredited dam owners; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>any other matters that the chief executive considers necessary or appropriate.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <crosshead id="DLM1221617" toc="yes">Audit fees</crosshead>
        <notes>
          <history>
            <history-note id="DLM1222448"><amended-provision>Heading</amended-provision>: <amending-operation>inserted</amending-operation>, on <amendment-date>15 March 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="849793" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1221620 - prov 45" atidlm:xmlId="DLM1222671" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 45</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="625277" atidlm:sourceXmlId="DLM1222671" atidlm:targetKey="11147663" atidlm:targetXmlId="DLM1149814" atidlm:targetName="prov 45" atidlm:sourceName="DLM1222671" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1149746.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308227.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1222448" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Building Amendment Act 2008</amending-leg> (2008 No 4).</history-note>
          </history>
        </notes>
        <prov id="DLM1221618" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">257A</label>
          <heading>Fees for audits</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>A dam owner accreditation body may charge an accredited dam owner the prescribed fee (if any) for an audit conducted under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="849794" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1221619 - prov 257" atidlm:xmlId="DLM1222672" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 257(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="625278" atidlm:sourceXmlId="DLM1222672" atidlm:targetKey="1783002" atidlm:targetXmlId="DLM308231" atidlm:targetName="prov 257" atidlm:sourceName="DLM1222672" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308227.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308227.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1221618" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> by the dam owner accreditation body on the accredited dam owner.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1222449"><amended-provision>Section 257A</amended-provision>: <amending-operation>inserted</amending-operation>, on <amendment-date>15 March 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="849795" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1221620 - prov 45" atidlm:xmlId="DLM1222673" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 45</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="625280" atidlm:sourceXmlId="DLM1222673" atidlm:targetKey="11147663" atidlm:targetXmlId="DLM1149814" atidlm:targetName="prov 45" atidlm:sourceName="DLM1222673" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1149746.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308227.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1222449" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Building Amendment Act 2008</amending-leg> (2008 No 4).</history-note>
            </history>
          </notes>
        </prov>
        <crosshead id="DLM308232" toc="yes" xml:lang="en-NZ">Accreditation of dam owners</crosshead>
        <prov id="DLM308233" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">258</label>
          <heading xml:lang="en-NZ">Accreditation</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The dam owner accreditation body may, on the application of an owner of a dam made in accordance with <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240310" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 258" atidlm:xmlId="DLM685370" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243777" atidlm:sourceXmlId="DLM685370" atidlm:targetKey="1783004" atidlm:targetXmlId="DLM308233" atidlm:targetName="prov 258" atidlm:sourceName="DLM685370" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM308227.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308227.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308233" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, <insertwords>and on payment by the person of the prescribed fee (if any),</insertwords> accredit that person under this subpart.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>Before granting accreditation, the dam owner accreditation body must be satisfied that the applicant meets the prescribed criteria and standards.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>An application for accreditation under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="240311" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 258" atidlm:xmlId="DLM685371" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="243778" atidlm:sourceXmlId="DLM685371" atidlm:targetKey="1783004" atidlm:targetXmlId="DLM308233" atidlm:targetName="prov 258" atidlm:sourceName="DLM685371" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM308227.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM308227.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM308233" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>be made in writing; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>be given in the prescribed manner (if any); and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>contain the prescribed information <insertwords>(if any).</insertwords></text>
                  </para>
                </label-para>
                <label-para deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">d</label>
                  <para xml:lang="en-NZ">
                    <text/>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
