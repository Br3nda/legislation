            <amend>
              <prov id="DLM340869" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">191</label>
                <heading xml:lang="en-NZ">Chief executive may enter person's name in register of building consent authorities</heading>
                <prov.body>
                  <subprov skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="no"/>
                    <para xml:lang="en-NZ">
                      <text>The chief executive may, on the application of a person made in accordance with <citation jurisdiction="nz">section 194</citation>, enter the person's name in the register of building consent authorities kept under <citation jurisdiction="nz">section 273(1)(a)</citation>.</text>
                    </para>
                  </subprov>
                </prov.body>
              </prov>
            </amend>
          </para>
        </subprov>
