      <prov id="DLM317975" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">142</label>
        <heading xml:lang="en-NZ">Costs</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>In any proceedings under this Act, the Court may make any order as to costs it thinks fit.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
