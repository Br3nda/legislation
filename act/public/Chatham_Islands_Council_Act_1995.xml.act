        <prov id="DLM368069" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">10</label>
          <heading xml:lang="en-NZ">Rates, dues, and other money</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>All rates, dues, or other money that would have been payable to the former council if it had not been dissolved shall be due and payable to the Chatham Islands Council.</text>
              </para>
            </subprov>
          </prov.body>
