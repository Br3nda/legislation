    <prov id="DLM280684" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">13</label>
      <heading xml:lang="en-NZ">Validation of certain prescribed fees</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text>The imposition, recovery, and application of fees under <citation jurisdiction="nz">regulation 14 of the Citizenship Regulations 1978</citation> in respect of certificates of confirmation of status as a New Zealand citizen and denial of status as a New Zealand citizen are hereby validated and deemed to have been lawfully imposed, recovered, and applied.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
