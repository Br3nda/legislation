          <amend>
            <para xml:lang="en-NZ">
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>a person under <citation jurisdiction="nz">Part 5 of the Civil Defence Emergency Management Act 2002</citation>; or</text>
