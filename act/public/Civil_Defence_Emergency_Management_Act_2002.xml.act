              <def-para id="DLM150561" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM150562" xml:lang="en-NZ">representative</def-term> means an elected member of a local authority, including an elected member of a community board</text>
                </para>
              </def-para>
              <def-para id="DLM150563" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM150564" xml:lang="en-NZ">responsible department</def-term> means the department that is, with the authority of the Prime Minister, for the time being responsible for the administration of this Act</text>
                </para>
              </def-para>
              <def-para id="DLM150565" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM150566" xml:lang="en-NZ">risk</def-term> means the likelihood and consequences of a hazard</text>
                </para>
              </def-para>
