              <def-para id="DLM150511" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM150512" xml:lang="en-NZ">civil defence emergency management plan</def-term> means a national civil defence emergency management plan or a civil defence emergency management group plan</text>
                </para>
              </def-para>
              <def-para id="DLM150513" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM150514" xml:lang="en-NZ">Controller</def-term> means the person who is the National Controller in accordance with <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="91171" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 10" atidlm:xmlId="DLM537684" atidlm:state="UNCHANGED"><atidlm:linkcontent>10</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="92702" atidlm:sourceXmlId="DLM537684" atidlm:targetKey="844409" atidlm:targetXmlId="DLM150593" atidlm:targetName="prov 10" atidlm:sourceName="DLM537684" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM150588.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM149794.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[8]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Controller" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM149788" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="33" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Civil Defence Emergency Management Act 2002" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metad