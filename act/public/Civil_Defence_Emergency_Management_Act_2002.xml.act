                </para>
              </def-para>
              <def-para id="DLM150519" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM150520" xml:lang="en-NZ">district</def-term> means the district of a local authority; and includes a region</text>
                  <notes>
                    <history>
                      <history-note id="DLM150521">district: this definition was <amending-operation>substituted</amending-operation>, as from <amendment-date>1 July 2003</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="91176" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 262" atidlm:xmlId="DLM537689" atidlm:state="UNCHANGED"><atidlm:linkcontent>262</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="92707" atidlm:sourceXmlId="DLM537689" atidlm:targetKey="996432" atidlm:targetXmlId="DLM174088" atidlm:targetName="prov 262" atidlm:sourceName="DLM537689" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM174086.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM149794.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[11]/para[1]/notes[1]/history[1]/history-note[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM149788" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="33" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Civil Defence Emergency Management A