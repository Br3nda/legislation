"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="261751" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 44" atidlm:parentFolderId="/NZ/NZAct/C/Civil Union Act 2004" atidlm:xmlId="DLM706882" atidlm:state="UNCHANGED"><atidlm:linkcontent>(iv)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="265778" atidlm:sourceXmlId="DLM706882" atidlm:targetKey="221465" atidlm:targetXmlId="DLM40424" atidlm:targetName="prov 44" atidlm:sourceName="DLM706882" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM40259.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM323495.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[3]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Amendments to Family Proceedings Act 1980" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="3" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM323384" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="102" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Civil Union Act 2004" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2004" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substitute:</text>
          <amend>
            <label-para skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">iii</label>
              <para xml:lang="en-NZ">
                <text>in any case, on the basis that the spouse or civil union partner has been deserted by his or her spouse or civil union partner, or that one spouse or civil union partner has been deported and the other spouse or partner was, immediately before the desertion or deportation, domiciled in that country; or</text>
              </para>
            </label-para>
            <label-para skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">iv</label>
              <para xml:lang="en-NZ">
                <text>in any case, on the basis that the spouses or civil union partners were legally separated, whether by an order of a competent Court or by agreement, and that one of the spouses or partners was, at the date of the order or agreement, domiciled in that country; or.</text>
              </para>
            </label-para>
          </amend>
          <follow-text space-before="no">.</follow-text>
        </para>
