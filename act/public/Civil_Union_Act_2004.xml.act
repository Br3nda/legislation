          <heading xml:lang="en-NZ">Partners not to be within prohibited degrees of civil union</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
