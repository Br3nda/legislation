<act id="DLM286900" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="128" act.type="public" date.as.at="2007-09-03" date.assent="1992-12-18" date.first.valid="2007-09-18" stage="reprint" year="1992">
  <cover id="DLM286901">
    <title xml:lang="en-NZ">Clerks of Works Act Repeal Act 1992</title>
    <assent>1992-12-18</assent>
    <cover.reprint-note>
      <para xml:lang="en-NZ"/>
      <admin-office>This Act is administered in the <ministry>Ministry of Commerce</ministry>.</admin-office>
    </cover.reprint-note>
