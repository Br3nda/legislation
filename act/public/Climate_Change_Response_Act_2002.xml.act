    <part id="DLM158588" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-18" act.no="40" act.type="public" date.as.at="2008-09-26" date.assent="2002-11-18" stage="reprint" year="2002">
