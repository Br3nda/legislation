idlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662654" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319115" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664101 - prov 191" atidlm:xmlId="DLM1666738" atidlm:state="UNCHANGED"><atidlm:linkcontent>191</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800054" atidlm:sourceXmlId="DLM1666738" atidlm:targetKey="14065013" atidlm:targetXmlId="DLM1662790" atidlm:targetName="prov 191" atidlm:sourceName="DLM1666738" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662654" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319116" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664102 - prov 193" atidlm:xmlId="DLM1666739" atidlm:state="UNCHANGED"><atidlm:linkcontent>193</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800055" atidlm:sourceXmlId="DLM1666739" atidlm:targetKey="14065218" atidlm:targetXmlId="DLM1662795" atidlm:targetName="prov 193" atidlm:sourceName="DLM1666739" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662654" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">g</label>
                  <para>
                    <text>the total number of units surrendered; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">h</label>
                  <para>
                    <text>the total number of New Zealand units transferred for removal activities; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">i</label>
                  <para>
                    <text>the total number of New Zealand units allocated free of charge under any allocation plan.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The chief executive—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>must publish the information specified in subsection (1) by 30 June in each year; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>may publish the information specified in subsection (1), in whole or in part, at any other time and in whatever manner and format that the chief executive considers appropriate.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The chief executive is not required to publish the information required under subsection (1)(e) in respect of an activity if the chief executive is satisfied that publishing the information would result in the disclosure of a participant’s individual emissions, unless—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the participant to whom the information relates has consented to the publication of the information; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the information is already in the public domain.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665900"><amended-provision>Section 89</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319117" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666740" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800056" atidlm:sourceXmlId="DLM1666740" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666740" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665900" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662655" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">90</label>
          <heading>Chief executive may prescribe form of certain documents</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The chief executive may, for the purposes of this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319118" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664103 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666741" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800057" atidlm:sourceXmlId="DLM1666741" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666741" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662655" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, prescribe—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the form and electronic format of any forms, applications, returns, information accompanying any applications or returns, or other documents that are not otherwise prescribed in regulations made under this Act; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>different forms or formats for different classes of participants or different activities; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>the manner in which any application, return, information, or other document must be submitted or notified under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319119" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664104 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666742" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800058" atidlm:sourceXmlId="DLM1666742" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666742" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662655" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> if this is not otherwise prescribed in regulations.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The chief executive must publish any form or format prescribed under subsection (1) via the Internet site of the department of the chief executive.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The production by the chief executive of any document purporting to be a prescribed form or an extract from a prescribed form or a copy of a form or extract is, in all courts and in all proceedings, unless the contrary is proved, sufficient evidence that the form or electronic format was prescribed.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>To avoid doubt, if the chief executive prescribes an electronic form or format under subsection (1), the chief executive may require any signature on that form or that relates to that format to be an electronic signature.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665901"><amended-provision>Section 90</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319120" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666743" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800059" atidlm:sourceXmlId="DLM1666743" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666743" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665901" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662656" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">91</label>
          <heading>Approval of unique emissions factors</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The chief executive may approve the use by a participant of a unique emissions factor when calculating emissions or removals from an activity under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319121" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664105 - prov 62" atidlm:xmlId="DLM1666744" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 62(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800060" atidlm:sourceXmlId="DLM1666744" atidlm:targetKey="14063246" atidlm:targetXmlId="DLM1662492" atidlm:targetName="prov 62" atidlm:sourceName="DLM1666744" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662656" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> if—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>regulations made under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319122" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664106 - prov 164" atidlm:xmlId="DLM1666745" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 164</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800061" atidlm:sourceXmlId="DLM1666745" atidlm:targetKey="14064485" atidlm:targetXmlId="DLM1662741" atidlm:targetName="prov 164" atidlm:sourceName="DLM1666745" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662656" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> provide a mechanism for participants to apply for approval to use a unique emissions factor for the activity; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the chief executive is satisfied that the unique emissions factor that the participant has applied to use meets any requirements prescribed in regulations made under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319123" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664107 - prov 164" atidlm:xmlId="DLM1666746" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 164</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800062" atidlm:sourceXmlId="DLM1666746" atidlm:targetKey="14064485" atidlm:targetXmlId="DLM1662741" atidlm:targetName="prov 164" atidlm:sourceName="DLM1666746" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662656" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>An approval under subsection (1)—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>may be subject to the conditions that the chief executive considers appropriate; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>ceases to have effect on the earliest of the following dates:</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>the date of a material change in any of the information or factors on which the approval is based; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>the date of a material change to this Act or to any regulations to which the approval relates; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>the date on which any of the conditions to which the approval is subject cease to be met or complied with.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>If the chief executive approves the use of a unique emissions factor under subsection (1), the chief executive must—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>notify the applicant of the approval; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>publish in the <emphasis style="italic">Gazette</emphasis>—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>the name of the participant; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>a description of the activity; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>the details of the unique emissions factor the chief executive has approved the participant to use when calculating emissions or removals for the activity.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665902"><amended-provision>Section 91</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319124" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666747" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800063" atidlm:sourceXmlId="DLM1666747" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666747" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662657" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">92</label>
          <heading>Recognition of verifiers</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The chief executive may, in accordance with any regulations made under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319125" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664108 - prov 163" atidlm:xmlId="DLM1666748" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 163</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800064" atidlm:sourceXmlId="DLM1666748" atidlm:targetKey="14064496" atidlm:targetXmlId="DLM1662740" atidlm:targetName="prov 163" atidlm:sourceName="DLM1666748" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662657" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, recognise a person or organisation with the prescribed expertise, technical competence, or qualifications as a person or organisation that may undertake verification functions for the purposes of <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319126" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664109 - prov 62" atidlm:xmlId="DLM1666749" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 62(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800065" atidlm:sourceXmlId="DLM1666749" atidlm:targetKey="14063246" atidlm:targetXmlId="DLM1662492" atidlm:targetName="prov 62" atidlm:sourceName="DLM1666749" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662657" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and (c)</citation> or regulations made under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319127" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664110 - prov 164" atidlm:xmlId="DLM1666750" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 164 </atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800066" atidlm:sourceXmlId="DLM1666750" atidlm:targetKey="14064485" atidlm:targetXmlId="DLM1662741" atidlm:targetName="prov 164" atidlm:sourceName="DLM1666750" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662657" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> relating to the process for approval of a unique emissions factor.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>A person or organisation may be recognised by the chief executive as able to verify information or unique emissions factors in respect of—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>1 or more types of data or information or calculations of types of emissions or removals:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>1 or more activities in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319128" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664111 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1666751" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800067" atidlm:sourceXmlId="DLM1666751" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1666751" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662657" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319129" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664112 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1666752" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800068" atidlm:sourceXmlId="DLM1666752" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1666752" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662657" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The chief executive may suspend or revoke any recognition given under this section in accordance with regulations made under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319130" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664113 - prov 163" atidlm:xmlId="DLM1666753" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 163</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800069" atidlm:sourceXmlId="DLM1666753" atidlm:targetKey="14064496" atidlm:targetXmlId="DLM1662740" atidlm:targetName="prov 163" atidlm:sourceName="DLM1666753" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662657" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665903"><amended-provision>Section 92</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319131" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666754" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800070" atidlm:sourceXmlId="DLM1666754" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666754" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665903" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <crosshead id="DLM1662658" toc="yes">Verification and inquiry</crosshead>
        <notes>
          <history>
            <history-note id="DLM1665904"><amended-provision>Heading</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319132" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666755" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800071" atidlm:sourceXmlId="DLM1666755" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666755" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665904" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
          </history>
        </notes>
        <prov id="DLM1662659" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">93</label>
          <heading>Appointment of enforcement officers</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The chief executive may appoint 1 or more persons as  enforcement officers to exercise all or any of the powers and perform the functions conferred on enforcement officers under this Part.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>A person appointed under subsection (1) must be employed by the chief executive under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319133" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664114 - State Sector Act 1988" atidlm:xmlId="DLM1666756" atidlm:state="UNCHANGED"><atidlm:linkcontent>State Sector Act 1988</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800072" atidlm:sourceXmlId="DLM1666756" atidlm:targetKey="717800" atidlm:targetXmlId="DLM129109" atidlm:targetName="State Sector Act 1988" atidlm:sourceName="DLM1666756" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM129109.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662659" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The chief executive must supply an enforcement officer with a warrant of authorisation that clearly states the powers and functions of the enforcement officer.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>An enforcement officer who exercises, or purports to exercise, a power conferred on the enforcement officer under this Act must carry and produce, if required to do so,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>his or her warrant of authorisation; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>evidence of his or her identity.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>An enforcement officer must, on the termination of the enforcement officer’s appointment, surrender his or her warrant to the chief executive.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">6</label>
              <para>
                <text>To avoid doubt, if the chief executive delegates the appointment power specified in subsection (1) to another chief executive under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319134" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664115 - prov 41" atidlm:xmlId="DLM1666757" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 41</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800073" atidlm:sourceXmlId="DLM1666757" atidlm:targetKey="721420" atidlm:targetXmlId="DLM129566" atidlm:targetName="prov 41" atidlm:sourceName="DLM1666757" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM129546.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662659" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> of the State Sector Act 1988, subsection (2) applies as if the reference to the chief executive were a reference to the chief executive to whom the power specified in subsection (1) is delegated.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665905"><amended-provision>Section 93</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319135" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666758" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800074" atidlm:sourceXmlId="DLM1666758" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666758" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665905" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662660" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">94</label>
          <heading>Power to require information</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The chief executive or an enforcement officer may, by notice, require a person to provide any information that is reasonably necessary for the purposes of—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>ascertaining whether a person is complying, or has complied, with this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319136" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664116 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666759" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800075" atidlm:sourceXmlId="DLM1666759" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666759" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662660" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>ascertaining whether the chief executive should exercise any powers under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319137" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664117 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666760" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800076" atidlm:sourceXmlId="DLM1666760" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666760" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662660" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The information required to be provided under subsection (1) must,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>if required by the chief executive or an enforcement officer, be accompanied by a statutory declaration attesting to the truthfulness of the information provided; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>be provided—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>in the form specified by the chief executive or enforcement officer; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>within any reasonable time specified in the notice requiring the information; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>free of charge.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665906"><amended-provision>Section 94</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319138" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666761" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800077" atidlm:sourceXmlId="DLM1666761" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666761" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665906" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662661" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">95</label>
          <heading>Power to inquire</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>For the purpose of obtaining information for a purpose specified in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319139" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664118 - prov 94" atidlm:xmlId="DLM1666762" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 94(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800078" atidlm:sourceXmlId="DLM1666762" atidlm:targetKey="14063824" atidlm:targetXmlId="DLM1662660" atidlm:targetName="prov 94" atidlm:sourceName="DLM1666762" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662661" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, or any other information required for the purposes of the administration or enforcement of this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319140" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664119 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666763" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800080" atidlm:sourceXmlId="DLM1666763" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666763" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662661" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the chief executive may require a person to—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>appear before the chief executive or an enforcement officer at a time and place that is specified in the notice to give evidence; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>produce any document or class of documents in the person’s possession or under the person’s control that is specified in the notice.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The chief executive or enforcement officer may require the evidence to be given on oath and either orally or in writing, and for that purpose the chief executive or enforcement officer may administer an oath.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665907"><amended-provision>Section 95</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319141" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666764" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800081" atidlm:sourceXmlId="DLM1666764" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666764" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665907" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662662" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">96</label>
          <heading>Inquiry before District Court Judge</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>For the purpose of obtaining information for a purpose specified in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319142" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664120 - prov 94" atidlm:xmlId="DLM1666765" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 94(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800082" atidlm:sourceXmlId="DLM1666765" atidlm:targetKey="14063824" atidlm:targetXmlId="DLM1662660" atidlm:targetName="prov 94" atidlm:sourceName="DLM1666765" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662662" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, or any other information required for the purposes of the administration or enforcement of this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319143" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664121 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666766" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800083" atidlm:sourceXmlId="DLM1666766" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666766" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662662" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the chief executive, if the chief executive considers it necessary, may apply in writing to a District Court Judge to hold an inquiry under this section.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>For the purposes of an inquiry under this section,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the District Court Judge—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>may, with respect to any matter that is relevant to the subject matter of the inquiry, summon and examine on oath all persons whom the chief executive or any other interested person requires to be called and examined; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>has the same jurisdiction and authority regarding the summoning and examination of a person as the Judge would have in respect of a witness in a civil action within the Judge’s ordinary jurisdiction; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the person summoned and examined has all the rights and is subject to all the liabilities that the person would have and be subject to if the person were a witness in a civil action within the Judge’s ordinary jurisdiction.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The chief executive and any person materially affected by the subject matter of the inquiry may be represented by a barrister or solicitor, who may examine, cross-examine, and re-examine, in accordance with ordinary practice, any person summoned under subsection (2).</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>Every examination under this section must take place in chambers.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>The statement of every person examined—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>must be—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>recorded in writing and signed by the person in the presence of the District Court Judge; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>delivered to the chief executive; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>does not form part of the records of the court.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665908"><amended-provision>Section 96</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319144" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666767" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800084" atidlm:sourceXmlId="DLM1666767" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666767" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665908" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662663" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">97</label>
          <heading>No criminal proceedings for statements under section 95 or 96</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>No person summoned or examined under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319145" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664122 - prov 95" atidlm:xmlId="DLM1666768" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 95</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800085" atidlm:sourceXmlId="DLM1666768" atidlm:targetKey="14063842" atidlm:targetXmlId="DLM1662661" atidlm:targetName="prov 95" atidlm:sourceName="DLM1666768" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662663" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319146" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664123 - prov 96" atidlm:xmlId="DLM1666769" atidlm:state="UNCHANGED"><atidlm:linkcontent>96</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800086" atidlm:sourceXmlId="DLM1666769" atidlm:targetKey="14063815" atidlm:targetXmlId="DLM1662662" atidlm:targetName="prov 96" atidlm:sourceName="DLM1666769" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662663" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is excused from answering a question on the ground that the answer may incriminate the person or render the person liable to any penalty or forfeiture.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The testimony of a person examined is not admissible as evidence in criminal proceedings against the person, except on a charge of perjury in relation to the testimony.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665909"><amended-provision>Section 97</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319147" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666770" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800087" atidlm:sourceXmlId="DLM1666770" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666770" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665909" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662664" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">98</label>
          <heading>Expenses in relation to inquiries by chief executive or District Court Judge</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>The chief executive may pay, or a District Court Judge may order the chief executive to pay, to any person who has appeared before the chief executive or an enforcement officer under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319148" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664124 - prov 95" atidlm:xmlId="DLM1666771" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 95</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800088" atidlm:sourceXmlId="DLM1666771" atidlm:targetKey="14063842" atidlm:targetXmlId="DLM1662661" atidlm:targetName="prov 95" atidlm:sourceName="DLM1666771" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662664" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> or the District Court Judge under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319149" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664125 - prov 96" atidlm:xmlId="DLM1666772" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 96</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800089" atidlm:sourceXmlId="DLM1666772" atidlm:targetKey="14063815" atidlm:targetXmlId="DLM1662662" atidlm:targetName="prov 96" atidlm:sourceName="DLM1666772" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662664" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, out of money appropriated by Parliament for the purpose, the sum that in the chief executive’s or Judge’s opinion, as the case may be, is reasonable in respect of that person’s travelling and other expenses.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665910"><amended-provision>Section 98</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319150" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666773" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800090" atidlm:sourceXmlId="DLM1666773" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666773" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665910" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662665" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">99</label>
          <heading>Obligation to maintain confidentiality</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>This section applies—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>to the chief executive, any enforcement officer, and any other person who performs functions or exercises powers of the chief executive or an enforcement officer under this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319151" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664126 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666774" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800091" atidlm:sourceXmlId="DLM1666774" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666774" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662665" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>at the time during which, and any time after which, those functions are performed or those powers are exercised.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>A person to whom this section applies—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>must keep confidential all information that comes into the person’s knowledge when performing any function or exercising any power under this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319152" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664127 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666775" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800092" atidlm:sourceXmlId="DLM1666775" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666775" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662665" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>may not disclose any information specified in paragraph (a), except—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>with the consent of the person to whom the information relates or of the person to whom the information is confidential; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>to the extent that the information is already in the public domain; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>for the purposes of, or in connection with, the exercise of powers conferred by this Part; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iv</label>
                      <para>
                        <text>as provided under this Act or any other Act; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">v</label>
                      <para>
                        <text>in connection with any investigation or inquiry (whether or not preliminary to any proceedings) in respect of, or any proceedings for, an offence against this Act or any other Act; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">vi</label>
                      <para>
                        <text>for the purpose of complying with any obligation under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319153" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1665531 - schedule 2 Kyoto Protocol to the United Nations Framework Convention on Climate Change" atidlm:xmlId="DLM1666776" atidlm:state="UNCHANGED"><atidlm:linkcontent>Protocol</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800093" atidlm:sourceXmlId="DLM1666776" atidlm:targetKey="890614" atidlm:targetXmlId="DLM160087" atidlm:targetName="schedule 2 Kyoto Protocol to the United Nations Framework Convention on Climate Change" atidlm:sourceName="DLM1666776" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM160087.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662665" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>A person to whom this section applies commits an offence under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319154" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664128 - prov 130" atidlm:xmlId="DLM1666777" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 130</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800094" atidlm:sourceXmlId="DLM1666777" atidlm:targetKey="14064133" atidlm:targetXmlId="DLM1662699" atidlm:targetName="prov 130" atidlm:sourceName="DLM1666777" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662665" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> if the person knowingly contravenes this section.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>Nothing in subsection (2) may be treated as prohibiting the chief executive from—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>providing or publishing general guidance in relation to the operation of this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319155" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664129 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666778" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800095" atidlm:sourceXmlId="DLM1666778" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666778" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662665" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>with the prior approval of the Minister, preparing and supplying statistical information to any person in a form that does not identify any individual; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>providing information to any person about whether any forest land is considered by the chief executive to be pre-1990 forest land or post-1989 forest land, or has been declared to be exempt land by the chief executive.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665911"><amended-provision>Section 99</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319156" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666779" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800096" atidlm:sourceXmlId="DLM1666779" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666779" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665911" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662666" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">100</label>
          <heading>Power of entry for investigation</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>An enforcement officer may enter land or premises (excluding any dwellinghouse or marae) at any reasonable time during the ordinary hours of business to investigate whether a person is complying with this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319157" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664130 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666780" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800097" atidlm:sourceXmlId="DLM1666780" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666780" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662666" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>During an investigation, an enforcement officer may—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>require the production of, inspect, and copy any documents:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>take samples of water, air, soil, organic matter, or any other thing:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>carry out surveys, investigations, tests, inspections, or measurements (including those that involve leaving measuring equipment on the land or premises):</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>demand from the occupier any other information that the enforcement officer may reasonably require for the purpose of determining whether a person is complying with this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319158" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664131 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666781" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800098" atidlm:sourceXmlId="DLM1666781" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666781" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662666" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>An enforcement officer who exercises the power of investigation under this section must give the occupier or owner reasonable notice of the enforcement officer’s intention to enter the land or premises, unless doing so would defeat the purpose of the entry.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>A notice given under subsection (3) must specify—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>when entry is to be made; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the purpose for which the entry is required; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>that the entry is authorised under this section.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>An enforcement officer who exercises the power of investigation under this section may be accompanied by any person or persons reasonably necessary to assist the enforcement officer with the investigation.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">6</label>
              <para>
                <text>A person who provides assistance under subsection (5) may exercise the powers provided to enforcement officers under subsection (2)(a) to (c).</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">7</label>
              <para>
                <text>Nothing in this section limits the privilege against self-incrimination.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665912"><amended-provision>Section 100</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319159" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666782" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800099" atidlm:sourceXmlId="DLM1666782" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666782" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665912" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662667" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">101</label>
          <heading>Applications for warrants</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>A District Court Judge, Justice of the Peace, Community Magistrate, or Registrar of any court who, on written application made on oath by an enforcement officer authorised by the chief executive, is satisfied that there are reasonable grounds to believe that there are in or on or under or over any land, premises, dwellinghouse, or marae any documents or other records or things (including samples) that may be evidence of the commission of an offence under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319160" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664132 - prov 129" atidlm:xmlId="DLM1666783" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 129</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800100" atidlm:sourceXmlId="DLM1666783" atidlm:targetKey="14064129" atidlm:targetXmlId="DLM1662698" atidlm:targetName="prov 129" atidlm:sourceName="DLM1666783" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662667" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319161" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664133 - prov 132" atidlm:xmlId="DLM1666784" atidlm:state="UNCHANGED"><atidlm:linkcontent>132</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800101" atidlm:sourceXmlId="DLM1666784" atidlm:targetKey="14064086" atidlm:targetXmlId="DLM1662701" atidlm:targetName="prov 132" atidlm:sourceName="DLM1666784" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662667" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319162" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664134 - prov 133" atidlm:xmlId="DLM1666785" atidlm:state="UNCHANGED"><atidlm:linkcontent>133</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800102" atidlm:sourceXmlId="DLM1666785" atidlm:targetKey="14064098" atidlm:targetXmlId="DLM1662702" atidlm:targetName="prov 133" atidlm:sourceName="DLM1666785" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662667" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> may issue a warrant authorising the entry and search of the land, premises, dwellinghouse, or marae.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>Every search warrant may authorise the enforcement officer executing the warrant to do any of the following things:</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>enter and search the land, premises, dwellinghouse, or marae, at any time that is reasonable in the circumstances during the ordinary hours of business, within—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>14 working days after the date of the warrant; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>if the Judge or other person issuing the warrant is satisfied that special circumstances justify a longer period, any period of up to 28 working days that is specified in the warrant:</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>seize any document or other thing that the enforcement officer has reasonable cause to suspect may be evidence of the commission of an offence under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319163" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664135 - prov 129" atidlm:xmlId="DLM1666786" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 129</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800103" atidlm:sourceXmlId="DLM1666786" atidlm:targetKey="14064129" atidlm:targetXmlId="DLM1662698" atidlm:targetName="prov 129" atidlm:sourceName="DLM1666786" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662667" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319164" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664136 - prov 132" atidlm:xmlId="DLM1666787" atidlm:state="UNCHANGED"><atidlm:linkcontent>132</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800104" atidlm:sourceXmlId="DLM1666787" atidlm:targetKey="14064086" atidlm:targetXmlId="DLM1662701" atidlm:targetName="prov 132" atidlm:sourceName="DLM1666787" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662667" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319165" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664137 - prov 133" atidlm:xmlId="DLM1666788" atidlm:state="UNCHANGED"><atidlm:linkcontent>133</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800105" atidlm:sourceXmlId="DLM1666788" atidlm:targetKey="14064098" atidlm:targetXmlId="DLM1662702" atidlm:targetName="prov 133" atidlm:sourceName="DLM1666788" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662667" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>take samples of water, air, soil, organic matter, or any other thing:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>use the assistance of any person that is reasonably necessary in the circumstances:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">e</label>
                  <para>
                    <text>use any force to enter (whether by breaking doors or otherwise) that is reasonable in the circumstances:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">f</label>
                  <para>
                    <text>carry out surveys, investigations, tests, inspections, or measurements (including those that involve leaving measuring equipment on the land or premises).</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>An enforcement officer may not enter a dwellinghouse or marae unless that enforcement officer is accompanied by a member of the police.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>A person who provides assistance under subsection (2)(d) may exercise the powers provided to enforcement officers under subsection (2)(a), (b), (c), and (f).</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665913"><amended-provision>Section 101</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319166" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666789" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800106" atidlm:sourceXmlId="DLM1666789" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666789" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665913" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662668" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">102</label>
          <heading>Proof of authority must be produced</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>If powers are exercised under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319167" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664138 - prov 100" atidlm:xmlId="DLM1666790" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 100</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800107" atidlm:sourceXmlId="DLM1666790" atidlm:targetKey="14063790" atidlm:targetXmlId="DLM1662666" atidlm:targetName="prov 100" atidlm:sourceName="DLM1666790" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662668" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319168" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664139 - prov 101" atidlm:xmlId="DLM1666791" atidlm:state="UNCHANGED"><atidlm:linkcontent>101</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800108" atidlm:sourceXmlId="DLM1666791" atidlm:targetKey="14063778" atidlm:targetXmlId="DLM1662667" atidlm:targetName="prov 101" atidlm:sourceName="DLM1666791" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662668" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, an enforcement officer must, on initial entry, and if asked by the occupier at any time afterward, produce for inspection—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the enforcement officer’s warrant of authorisation and evidence of his or her identity; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>any notice given under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319169" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664140 - prov 100" atidlm:xmlId="DLM1666792" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 100(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800109" atidlm:sourceXmlId="DLM1666792" atidlm:targetKey="14063790" atidlm:targetXmlId="DLM1662666" atidlm:targetName="prov 100" atidlm:sourceName="DLM1666792" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662668" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> or a search warrant issued under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319170" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664141 - prov 101" atidlm:xmlId="DLM1666793" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 101</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800110" atidlm:sourceXmlId="DLM1666793" atidlm:targetKey="14063778" atidlm:targetXmlId="DLM1662667" atidlm:targetName="prov 101" atidlm:sourceName="DLM1666793" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662668" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, as the case may be.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665914"><amended-provision>Section 102</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319171" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666794" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800111" atidlm:sourceXmlId="DLM1666794" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666794" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665914" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662669" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">103</label>
          <heading>Notice of entry</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>If, when powers are exercised under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319172" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664142 - prov 100" atidlm:xmlId="DLM1666795" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 100</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800112" atidlm:sourceXmlId="DLM1666795" atidlm:targetKey="14063790" atidlm:targetXmlId="DLM1662666" atidlm:targetName="prov 100" atidlm:sourceName="DLM1666795" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662669" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319173" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664143 - prov 101" atidlm:xmlId="DLM1666796" atidlm:state="UNCHANGED"><atidlm:linkcontent>101</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800113" atidlm:sourceXmlId="DLM1666796" atidlm:targetKey="14063778" atidlm:targetXmlId="DLM1662667" atidlm:targetName="prov 101" atidlm:sourceName="DLM1666796" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662669" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the occupier is not present, the enforcement officer must, in a prominent place, attach a written notice that states—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the date and time of the entry or search; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the purpose of the entry or search; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>the name and phone number of the enforcement officer; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>the right, under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319174" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664144 - Official Information Act 1982" atidlm:xmlId="DLM1666797" atidlm:state="UNCHANGED"><atidlm:linkcontent>Official Information Act 1982</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800114" atidlm:sourceXmlId="DLM1666797" atidlm:targetKey="371800" atidlm:targetXmlId="DLM64784" atidlm:targetName="Official Information Act 1982" atidlm:sourceName="DLM1666797" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64784.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662669" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, to access documentation relating to the application for a search warrant and the exercise of the search power; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">e</label>
                  <para>
                    <text>an address at which inquiries may be made.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If the enforcement officer removes, or has removed, any document or other thing from any land, premises, dwellinghouse, or marae, the enforcement officer must hand to the occupier, or attach in a prominent place, a notice that—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>lists all of the items taken; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>states—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>where those items are being held; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>if they are being held in 2 or more places, which items are being held at which place; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>provides information about—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>the procedures to be followed to initiate a claim that privileged or confidential material has been seized; and </text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>access to and the disposition of seized items.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665915"><amended-provision>Section 103</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319175" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666798" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800115" atidlm:sourceXmlId="DLM1666798" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666798" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665915" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662670" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">104</label>
          <heading>Information obtained under section 100 or 101 only admissible in proceedings for alleged breach of obligations imposed under this Part and Part 5</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>No document or other information obtained from a person under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319176" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664145 - prov 100" atidlm:xmlId="DLM1666799" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 100</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800116" atidlm:sourceXmlId="DLM1666799" atidlm:targetKey="14063790" atidlm:targetXmlId="DLM1662666" atidlm:targetName="prov 100" atidlm:sourceName="DLM1666799" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662670" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319177" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664146 - prov 101" atidlm:xmlId="DLM1666800" atidlm:state="UNCHANGED"><atidlm:linkcontent>101</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800117" atidlm:sourceXmlId="DLM1666800" atidlm:targetKey="14063778" atidlm:targetXmlId="DLM1662667" atidlm:targetName="prov 101" atidlm:sourceName="DLM1666800" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662670" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is admissible against that person in any criminal or civil proceedings, other than proceedings for an alleged breach of an obligation imposed under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319178" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664147 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666801" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800118" atidlm:sourceXmlId="DLM1666801" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666801" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662670" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665916"><amended-provision>Section 104</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319179" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666802" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800120" atidlm:sourceXmlId="DLM1666802" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666802" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665916" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662671" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">105</label>
          <heading>Return of items seized</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319180" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664148 - prov 199" atidlm:xmlId="DLM1666803" atidlm:state="UNCHANGED"><atidlm:linkcontent>Section 199</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800121" atidlm:sourceXmlId="DLM1666803" atidlm:targetKey="2048080" atidlm:targetXmlId="DLM314018" atidlm:targetName="prov 199" atidlm:sourceName="DLM1666803" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM314008.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662671" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> of the Summary Proceedings Act 1957 applies, with the necessary modifications, to any property seized or taken by an enforcement officer as if—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>references in that section to a constable were references to an enforcement officer; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the reference in that section to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319181" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664151 - prov 198" atidlm:xmlId="DLM1666804" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 198</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800122" atidlm:sourceXmlId="DLM1666804" atidlm:targetKey="2048073" atidlm:targetXmlId="DLM314010" atidlm:targetName="prov 198" atidlm:sourceName="DLM1666804" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM314008.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662671" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> of that Act were a reference to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319182" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664149 - prov 100" atidlm:xmlId="DLM1666805" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 100</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800123" atidlm:sourceXmlId="DLM1666805" atidlm:targetKey="14063790" atidlm:targetXmlId="DLM1662666" atidlm:targetName="prov 100" atidlm:sourceName="DLM1666805" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662671" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319183" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664150 - prov 101" atidlm:xmlId="DLM1666806" atidlm:state="UNCHANGED"><atidlm:linkcontent>101</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800124" atidlm:sourceXmlId="DLM1666806" atidlm:targetKey="14063778" atidlm:targetXmlId="DLM1662667" atidlm:targetName="prov 101" atidlm:sourceName="DLM1666806" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662671" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> of this Act.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665917"><amended-provision>Section 105</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319184" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666807" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800125" atidlm:sourceXmlId="DLM1666807" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666807" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665917" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662672" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">106</label>
          <heading>Protection of persons acting under authority of this Part</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>No enforcement officer or person called upon to assist an enforcement officer who does an act, or omits to do an act, when performing a function or exercising a power conferred on that person by this Part is under any civil or criminal liability in respect of the act or omission, unless the person has acted, or omitted to act, in bad faith or without reasonable cause.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665918"><amended-provision>Section 106</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319185" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666808" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800126" atidlm:sourceXmlId="DLM1666808" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666808" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665918" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <crosshead id="DLM1662673" toc="yes">Emissions rulings</crosshead>
        <notes>
          <history>
            <history-note id="DLM1665919"><amended-provision>Heading</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319186" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666809" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800127" atidlm:sourceXmlId="DLM1666809" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666809" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665919" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
          </history>
        </notes>
        <prov id="DLM1662674" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">107</label>
          <heading>Applications for emissions rulings</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>A person may apply to the chief executive for an emissions ruling in respect of 1 or more of the following matters:</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>whether something that the person—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>is doing is an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319187" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664152 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1666810" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800128" atidlm:sourceXmlId="DLM1666810" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1666810" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662674" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319188" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664153 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1666811" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800129" atidlm:sourceXmlId="DLM1666811" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1666811" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662674" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>proposes to do would be an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319189" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664154 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1666812" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800130" atidlm:sourceXmlId="DLM1666812" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1666812" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662674" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319190" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664155 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1666813" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800131" atidlm:sourceXmlId="DLM1666813" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1666813" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662674" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>whether the person is a participant in respect of an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319191" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664156 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1666814" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800132" atidlm:sourceXmlId="DLM1666814" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1666814" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662674" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> or is eligible to register as a participant in respect of an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319192" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664157 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1666815" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800133" atidlm:sourceXmlId="DLM1666815" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1666815" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662674" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>the correct application of any provision contained in regulations made under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319193" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664158 - prov 163" atidlm:xmlId="DLM1666816" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 163(1)(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800134" atidlm:sourceXmlId="DLM1666816" atidlm:targetKey="14064496" atidlm:targetXmlId="DLM1662740" atidlm:targetName="prov 163" atidlm:sourceName="DLM1666816" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662674" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> to (c)</citation> in respect of a particular matter specified in the person’s application:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>any other matters prescribed in regulations made under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319194" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664159 - prov 168" atidlm:xmlId="DLM1666817" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 168(1)(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800135" atidlm:sourceXmlId="DLM1666817" atidlm:targetKey="14064468" atidlm:targetXmlId="DLM1662745" atidlm:targetName="prov 168" atidlm:sourceName="DLM1666817" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662674" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>Every application under subsection (1) must—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>be in the prescribed form; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>state the name and address of the applicant; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>specify the matter on which the applicant seeks a ruling; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>specify the applicant’s opinion as to what the ruling should be; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">e</label>
                  <para>
                    <text>contain, or have attached, all information that is relevant to a proper consideration of the application; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">f</label>
                  <para>
                    <text>be accompanied by the prescribed fee (if any).</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The chief executive may request any further information from an applicant that the chief executive considers necessary to assist in the consideration of the application.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665920"><amended-provision>Section 107</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319195" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666818" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800136" atidlm:sourceXmlId="DLM1666818" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666818" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665920" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662675" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">108</label>
          <heading>Matters in relation to which chief executive may decline to make emissions rulings</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The chief executive may not make an emissions ruling with respect to a provision that authorises or requires the chief executive to—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>impose or remit a penalty; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>inquire into the correctness of any return or other information supplied by any person; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>prosecute any person; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>recover any debt owing by any person.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The chief executive may decline to make an emissions ruling if—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the chief executive considers that the correctness of the ruling would depend on which assumptions were made about a future event or other matter; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the matter on which the ruling is sought is subject to a review or appeal, or is the subject of proceedings, whether in relation to the applicant or any other person; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>the applicant has outstanding unpaid fees relating to an earlier emissions ruling application; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>the chief executive considers the application is frivolous or vexatious; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">e</label>
                  <para>
                    <text>the matter on which the ruling is sought concerns an obligation to surrender units that are already due and payable, unless the application is received before the obligation arises; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">f</label>
                  <para>
                    <text>an assessment or amendment relating to the same person, activity, and period to which the proposed ruling would apply has been made (unless the application is received by the chief executive before the date an assessment or amendment is made); or </text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">g</label>
                  <para>
                    <text>in the chief executive’s opinion—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>the chief executive has insufficient information to make the ruling; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>it would be unreasonable to make a ruling in view of the resources available to the chief executive.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665921"><amended-provision>Section 108</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319196" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666819" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800137" atidlm:sourceXmlId="DLM1666819" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666819" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665921" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662676" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">109</label>
          <heading>Making of emissions rulings</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The chief executive must make an emissions ruling regarding the matter in respect of which a ruling is sought under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319197" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664160 - prov 107" atidlm:xmlId="DLM1666820" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 107</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800138" atidlm:sourceXmlId="DLM1666820" atidlm:targetKey="14063757" atidlm:targetXmlId="DLM1662674" atidlm:targetName="prov 107" atidlm:sourceName="DLM1666820" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662676" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> as soon as practicable after the receipt of—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>a properly completed application for a ruling; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>all information that the chief executive considers relevant to the consideration of the application, including information requested under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319198" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664161 - prov 107" atidlm:xmlId="DLM1666821" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 107(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800139" atidlm:sourceXmlId="DLM1666821" atidlm:targetKey="14063757" atidlm:targetXmlId="DLM1662674" atidlm:targetName="prov 107" atidlm:sourceName="DLM1666821" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662676" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>Subject to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319200" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664162 - prov 114" atidlm:xmlId="DLM1666822" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 114(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800140" atidlm:sourceXmlId="DLM1666822" atidlm:targetKey="14063716" atidlm:targetXmlId="DLM1662681" atidlm:targetName="prov 114" atidlm:sourceName="DLM1666822" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662676" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, a ruling comes into effect on the day on which it is made.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>A ruling may be made subject to any conditions that the chief executive thinks fit.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>Subsection (1) is subject to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319201" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664163 - prov 108" atidlm:xmlId="DLM1666823" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 108</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800141" atidlm:sourceXmlId="DLM1666823" atidlm:targetKey="14063762" atidlm:targetXmlId="DLM1662675" atidlm:targetName="prov 108" atidlm:sourceName="DLM1666823" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662676" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665922"><amended-provision>Section 109</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319202" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666824" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800142" atidlm:sourceXmlId="DLM1666824" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666824" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665922" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662677" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">110</label>
          <heading>Notice of emissions rulings</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>The chief executive must, as soon as practicable, notify the applicant of—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>an emissions ruling, together with the reasons for the ruling, and the conditions (if any) to which the ruling is subject; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>a decision to decline to make an emissions ruling, together with the reasons for the decision.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665923"><amended-provision>Section 110</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319203" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666825" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800143" atidlm:sourceXmlId="DLM1666825" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666825" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665923" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662678" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">111</label>
          <heading>Confirmation of basis of emissions rulings</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>At any time after an emissions ruling is made, the chief executive may, by notice, require an applicant to satisfy the chief executive, within 20 working days after receipt of the notice, and in a manner that the chief executive considers appropriate, that—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the information on which the emissions ruling is based remains accurate; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the conditions (if any) to which the ruling is subject, have been, and continue to be, complied with.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665924"><amended-provision>Section 111</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319204" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666826" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800144" atidlm:sourceXmlId="DLM1666826" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666826" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665924" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662679" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">112</label>
          <heading>Notifying chief executive of changes relevant to or failure to comply with emissions rulings</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>A person must, as soon as practicable, notify the chief executive of any material change that is relevant to the application if the person—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>has made an application for an emissions ruling under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319205" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664164 - prov 107" atidlm:xmlId="DLM1666827" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 107</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800145" atidlm:sourceXmlId="DLM1666827" atidlm:targetKey="14063757" atidlm:targetXmlId="DLM1662674" atidlm:targetName="prov 107" atidlm:sourceName="DLM1666827" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662679" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>becomes aware of a material change relating to the application before the emissions ruling is made by the chief executive.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>A person who has obtained an emissions ruling under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319206" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664165 - prov 109" atidlm:xmlId="DLM1666828" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 109</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800146" atidlm:sourceXmlId="DLM1666828" atidlm:targetKey="14063773" atidlm:targetXmlId="DLM1662676" atidlm:targetName="prov 109" atidlm:sourceName="DLM1666828" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662679" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must, as soon as practicable, notify the chief executive of—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>any material change that is relevant to the ruling:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>any failure to comply with any of the conditions of the ruling.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The notification that a person provides under subsection (1) or (2) must state the date on which the person became aware of the material change or the failure to comply.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665925"><amended-provision>Section 112</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319207" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666829" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800147" atidlm:sourceXmlId="DLM1666829" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666829" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665925" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662680" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">113</label>
          <heading>Correction of emissions rulings</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The chief executive may amend an emissions ruling to correct any error that the chief executive is satisfied is contained in the ruling.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The chief executive must, as soon as practicable after making a correction, notify the applicant of the corrected ruling.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The correction to a ruling applies to the applicant from the date on which notice of the corrected ruling is given to the applicant.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>Despite subsection (3), if the corrected ruling has the effect of—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>increasing the number of units that a person is required to surrender, or decreasing the number of New Zealand units that a person is entitled to receive, in respect of a year, then the ruling as given prior to correction under this section must be applied to that year; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>decreasing the number of units that a person is required to surrender, or increasing the number of New Zealand units that a person is entitled to receive, in respect of a year, then the corrected ruling must be applied to that year.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665926"><amended-provision>Section 113</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319208" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666830" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800148" atidlm:sourceXmlId="DLM1666830" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666830" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665926" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662681" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">114</label>
          <heading>Cessation of emissions rulings</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>An emissions ruling ceases to have effect on the earliest of the following dates:</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the date of a material change in any of the information or facts on which the ruling is based; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the date of a material change to this Act or to any regulations relevant to the ruling; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>the date on which any of the conditions to which the ruling is subject cease to be met or complied with; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>the date of a failure to satisfy the requirements of the chief executive under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319209" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664166 - prov 111" atidlm:xmlId="DLM1666831" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 111</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800149" atidlm:sourceXmlId="DLM1666831" atidlm:targetKey="14063728" atidlm:targetXmlId="DLM1662678" atidlm:targetName="prov 111" atidlm:sourceName="DLM1666831" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662681" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>An emissions ruling does not come into effect if any information on which it is based is not accurate in all material respects.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665927"><amended-provision>Section 114</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319210" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666832" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800150" atidlm:sourceXmlId="DLM1666832" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666832" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665927" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662682" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">115</label>
          <heading>Appeal from decisions of chief executive</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>An applicant who is dissatisfied with an emissions ruling, or a decision to decline to make an emissions ruling, may, within 20 working days after the date on which notice of the ruling or decision is given, appeal to a District Court against the ruling or decision.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The District Court may confirm, reverse, or modify the emissions ruling or decision appealed against.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>An emissions ruling or decision appealed against under this section continues in force pending the determination of the appeal, and no person is excused from complying with any of the provisions of this Act on the ground that any appeal is pending.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665928"><amended-provision>Section 115</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319211" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666833" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800151" atidlm:sourceXmlId="DLM1666833" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666833" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665928" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662683" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">116</label>
          <heading>Effect of emissions rulings</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>An emissions ruling is conclusive evidence of the determination of the matter in respect of which a ruling is sought under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319212" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664167 - prov 107" atidlm:xmlId="DLM1666834" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 107</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800152" atidlm:sourceXmlId="DLM1666834" atidlm:targetKey="14063757" atidlm:targetXmlId="DLM1662674" atidlm:targetName="prov 107" atidlm:sourceName="DLM1666834" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662683" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If the chief executive makes an emissions ruling under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319213" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664168 - prov 109" atidlm:xmlId="DLM1666835" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 109</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800153" atidlm:sourceXmlId="DLM1666835" atidlm:targetKey="14063773" atidlm:targetXmlId="DLM1662676" atidlm:targetName="prov 109" atidlm:sourceName="DLM1666835" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662683" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the ruling applies to the matter in relation to which the ruling was sought; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>if the applicant complies with the ruling, the chief executive must apply this Act to that matter in accordance with the ruling.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>This section is subject to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319214" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664169 - prov 113" atidlm:xmlId="DLM1666836" atidlm:state="UNCHANGED"><atidlm:linkcontent>sections 113</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800154" atidlm:sourceXmlId="DLM1666836" atidlm:targetKey="14063708" atidlm:targetXmlId="DLM1662680" atidlm:targetName="prov 113" atidlm:sourceName="DLM1666836" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662683" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319215" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664170 - prov 114" atidlm:xmlId="DLM1666837" atidlm:state="UNCHANGED"><atidlm:linkcontent>114</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800155" atidlm:sourceXmlId="DLM1666837" atidlm:targetKey="14063716" atidlm:targetXmlId="DLM1662681" atidlm:targetName="prov 114" atidlm:sourceName="DLM1666837" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662683" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and any decision of the District Court under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319216" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664171 - prov 115" atidlm:xmlId="DLM1666838" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 115(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800156" atidlm:sourceXmlId="DLM1666838" atidlm:targetKey="14063720" atidlm:targetXmlId="DLM1662682" atidlm:targetName="prov 115" atidlm:sourceName="DLM1666838" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662683" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665929"><amended-provision>Section 116</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319217" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666839" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800157" atidlm:sourceXmlId="DLM1666839" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666839" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665929" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662684" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">117</label>
          <heading>Chief executive may publish certain aspects of emissions rulings</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>For the purpose of providing general guidance about the application of this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319218" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664172 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666840" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800158" atidlm:sourceXmlId="DLM1666840" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666840" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662684" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the chief executive may, after making an emissions ruling, publish information that relates to the ruling.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The chief executive may not publish any information under subsection (1) that identifies any person to whom the ruling relates.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>No person may treat, or rely on, the information published under subsection (1) as an emissions ruling with the effect specified by <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319219" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664173 - prov 116" atidlm:xmlId="DLM1666841" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 116</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800160" atidlm:sourceXmlId="DLM1666841" atidlm:targetKey="14063730" atidlm:targetXmlId="DLM1662683" atidlm:targetName="prov 116" atidlm:sourceName="DLM1666841" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662684" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665930"><amended-provision>Section 117</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319220" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666842" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800161" atidlm:sourceXmlId="DLM1666842" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666842" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665930" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <crosshead id="DLM1662685" toc="yes">Emissions returns</crosshead>
        <notes>
          <history>
            <history-note id="DLM1665931"><amended-provision>Heading</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319221" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666843" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800162" atidlm:sourceXmlId="DLM1666843" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666843" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665931" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
          </history>
        </notes>
        <prov id="DLM1662686" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">118</label>
          <heading>Submission of final emissions returns</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>Subsection (2) applies to the following persons:</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>a person who the chief executive believes is about to—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>cease carrying out an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319222" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664174 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1666844" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800163" atidlm:sourceXmlId="DLM1666844" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1666844" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662686" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319223" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664175 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1666845" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800164" atidlm:sourceXmlId="DLM1666845" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1666845" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662686" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in relation to which the person is a participant; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>leave New Zealand:</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>a participant who has ceased to carry out any activities in New Zealand:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>the executors or administrators of a deceased participant:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>a participant who has become bankrupt or has been put into liquidation.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The chief executive may, at any time, require a person to whom subsection (1) applies to submit a final emissions return in relation to a specified activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319224" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664176 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1666846" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800165" atidlm:sourceXmlId="DLM1666846" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1666846" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662686" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319225" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664177 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1666847" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800166" atidlm:sourceXmlId="DLM1666847" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1666847" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662686" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>Any of the following persons may, at any time, submit a final emissions return in relation to a specified activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319226" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664176 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1666848" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800167" atidlm:sourceXmlId="DLM1666848" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1666848" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662686" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319227" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664177 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1666849" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800168" atidlm:sourceXmlId="DLM1666849" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1666849" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662686" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>a person who is about to—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>cease carrying out an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319228" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664176 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1666850" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800169" atidlm:sourceXmlId="DLM1666850" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1666850" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662686" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319229" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664177 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1666851" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800170" atidlm:sourceXmlId="DLM1666851" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1666851" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662686" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in relation to which the person is a participant:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>leave New Zealand:</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>a participant who has ceased to carry out any activities in New Zealand:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>the executor or administrator of a deceased participant:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>a participant who has become bankrupt or has been put into liquidation.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>A final emissions return submitted under subsection (2) or (3) must—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>contain all of the information required in an annual emissions return under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319230" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664182 - prov 65" atidlm:xmlId="DLM1666852" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 65(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800171" atidlm:sourceXmlId="DLM1666852" atidlm:targetKey="14063267" atidlm:targetXmlId="DLM1662495" atidlm:targetName="prov 65" atidlm:sourceName="DLM1666852" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662686" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, but only for the period specified by the chief executive, or if the return is made under subsection (3), the period determined by the submitter; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>be submitted in accordance with <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319231" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664183 - prov 65" atidlm:xmlId="DLM1666853" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 65(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800172" atidlm:sourceXmlId="DLM1666853" atidlm:targetKey="14063267" atidlm:targetXmlId="DLM1662495" atidlm:targetName="prov 65" atidlm:sourceName="DLM1666853" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662686" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>Following the submission of a final emissions return under this section, the person submitting the return must, within 20 working days, surrender the number of units in the assessment under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319232" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664184 - prov 65" atidlm:xmlId="DLM1666854" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 65(2)(c)(i)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800173" atidlm:sourceXmlId="DLM1666854" atidlm:targetKey="14063267" atidlm:targetXmlId="DLM1662495" atidlm:targetName="prov 65" atidlm:sourceName="DLM1666854" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662686" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665932"><amended-provision>Section 118</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319233" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666855" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800174" atidlm:sourceXmlId="DLM1666855" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666855" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665932" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662687" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">119</label>
          <heading>Power to extend date for emissions returns</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>The chief executive may extend the time for the submission of an emissions return by a period of no more than 20 working days if—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the participant has applied for an extension before the date upon which the emissions return is due; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the chief executive is satisfied that the participant is unable to submit the required emissions return by the due date.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665933"><amended-provision>Section 119</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319234" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666856" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800175" atidlm:sourceXmlId="DLM1666856" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666856" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665933" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662688" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">120</label>
          <heading>Amendment to emissions returns by chief executive</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>Subject to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319235" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664185 - prov 127" atidlm:xmlId="DLM1666857" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 127</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800176" atidlm:sourceXmlId="DLM1666857" atidlm:targetKey="14063635" atidlm:targetXmlId="DLM1662695" atidlm:targetName="prov 127" atidlm:sourceName="DLM1666857" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662688" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, if the chief executive is satisfied that the information contained in an emissions return is incorrect, the chief executive may, at any time, amend the emissions return and any assessment of the participant’s liability to surrender units or entitlement to receive New Zealand units in the emissions return as the chief executive thinks fit.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665934"><amended-provision>Section 120</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319236" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666858" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800177" atidlm:sourceXmlId="DLM1666858" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666858" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665934" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662689" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">121</label>
          <heading>Assessment if default made in submitting emissions return</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>This section applies if—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>a participant fails to submit an emissions return when required to do so under this Act; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the chief executive has reason to believe that a person is a participant who should have submitted an emissions return, but did not.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If this section applies, the chief executive may make an assessment of the matters that should have been in the person’s emissions return.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665935"><amended-provision>Section 121</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319237" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666859" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800178" atidlm:sourceXmlId="DLM1666859" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666859" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665935" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662690" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">122</label>
          <heading>Amendment or assessment presumed to be correct</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>An amendment made to an emissions return under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319238" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664186 - prov 120" atidlm:xmlId="DLM1666860" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 120</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800179" atidlm:sourceXmlId="DLM1666860" atidlm:targetKey="14063686" atidlm:targetXmlId="DLM1662688" atidlm:targetName="prov 120" atidlm:sourceName="DLM1666860" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662690" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, or an assessment made under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319239" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664187 - prov 121" atidlm:xmlId="DLM1666861" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 121</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800180" atidlm:sourceXmlId="DLM1666861" atidlm:targetKey="14063690" atidlm:targetXmlId="DLM1662689" atidlm:targetName="prov 121" atidlm:sourceName="DLM1666861" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662690" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, must be taken to be correct unless, on review or appeal, a different amendment or assessment is made.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665936"><amended-provision>Section 122</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319240" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666862" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800181" atidlm:sourceXmlId="DLM1666862" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666862" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665936" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662691" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">123</label>
          <heading>Effect of amendment or assessment</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>If the chief executive makes an amendment under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319241" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664188 - prov 120" atidlm:xmlId="DLM1666863" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 120</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800182" atidlm:sourceXmlId="DLM1666863" atidlm:targetKey="14063686" atidlm:targetXmlId="DLM1662688" atidlm:targetName="prov 120" atidlm:sourceName="DLM1666863" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662691" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> or an assessment under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319242" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664189 - prov 121" atidlm:xmlId="DLM1666864" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 121</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800183" atidlm:sourceXmlId="DLM1666864" atidlm:targetKey="14063690" atidlm:targetXmlId="DLM1662689" atidlm:targetName="prov 121" atidlm:sourceName="DLM1666864" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662691" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the chief executive must, as soon as practicable after making the amendment or assessment, notify the participant of—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the particulars of the amendment or assessment; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>any grounds or information upon which the amendment or assessment was based; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>the right of the person to seek a review of the decision under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319243" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664190 - prov 144" atidlm:xmlId="DLM1666865" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 144</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800184" atidlm:sourceXmlId="DLM1666865" atidlm:targetKey="14064210" atidlm:targetXmlId="DLM1662716" atidlm:targetName="prov 144" atidlm:sourceName="DLM1666865" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662691" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>A notice under subsection (1) must, if relevant, be accompanied by a penalty notice under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319244" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664191 - prov 134" atidlm:xmlId="DLM1666866" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 134(3)(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800185" atidlm:sourceXmlId="DLM1666866" atidlm:targetKey="14064044" atidlm:targetXmlId="DLM1662703" atidlm:targetName="prov 134" atidlm:sourceName="DLM1666866" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662691" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>If the amendment or assessment results in a liability for the person to surrender units or any additional units, the participant must surrender those units within 90 days after the date of the notice under subsection (1).</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>If the amendment shows that a participant has surrendered too many units, the chief executive must, within 20 working days after the date of the notice under subsection (1), arrange for reimbursement to the participant, in accordance with <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319245" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664192 - prov 124" atidlm:xmlId="DLM1666867" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 124</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800186" atidlm:sourceXmlId="DLM1666867" atidlm:targetKey="14063645" atidlm:targetXmlId="DLM1662692" atidlm:targetName="prov 124" atidlm:sourceName="DLM1666867" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662691" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, of the number of units incorrectly surrendered.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>If the amendment or assessment results in an entitlement for a participant to receive New Zealand units for the participant’s removal activities, the chief executive must notify the Minister of Finance under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319246" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664193 - prov 64" atidlm:xmlId="DLM1666868" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 64(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800187" atidlm:sourceXmlId="DLM1666868" atidlm:targetKey="14063256" atidlm:targetXmlId="DLM1662494" atidlm:targetName="prov 64" atidlm:sourceName="DLM1666868" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662691" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> of the entitlement.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">6</label>
              <para>
                <text>If the amendment shows that a participant was transferred too many New Zealand units for the participant’s removal activities, the participant must, within 90 days after the date of the notice under subsection (1), repay the number of units to which the amendment shows the participant was not entitled by transferring them to a Crown holding account designated by the chief executive.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">7</label>
              <para>
                <text>Units repaid by any person under subsection (6) must be of a type that may be transferred to a surrender account at the time the unit is repaid.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">8</label>
              <para>
                <text>The chief executive is not required to meet the time frame in subsection (4) if consultation under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319247" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664194 - prov 124" atidlm:xmlId="DLM1666869" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 124(6)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800188" atidlm:sourceXmlId="DLM1666869" atidlm:targetKey="14063645" atidlm:targetXmlId="DLM1662692" atidlm:targetName="prov 124" atidlm:sourceName="DLM1666869" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662691" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> on the units to be reimbursed makes this impracticable.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665937"><amended-provision>Section 123</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319248" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666870" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800189" atidlm:sourceXmlId="DLM1666870" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666870" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665937" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662692" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">124</label>
          <heading>Reimbursement of units by chief executive</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>If the chief executive is required to arrange for the reimbursement of units to a person under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319249" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664195 - prov 123" atidlm:xmlId="DLM1666871" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 123(4)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800190" atidlm:sourceXmlId="DLM1666871" atidlm:targetKey="14063672" atidlm:targetXmlId="DLM1662691" atidlm:targetName="prov 123" atidlm:sourceName="DLM1666871" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662692" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319250" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664196 - prov 126" atidlm:xmlId="DLM1666872" atidlm:state="UNCHANGED"><atidlm:linkcontent>126(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800191" atidlm:sourceXmlId="DLM1666872" atidlm:targetKey="14063663" atidlm:targetXmlId="DLM1662694" atidlm:targetName="prov 126" atidlm:sourceName="DLM1666872" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662692" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319251" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664197 - prov 138" atidlm:xmlId="DLM1666873" atidlm:state="UNCHANGED"><atidlm:linkcontent>138(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800192" atidlm:sourceXmlId="DLM1666873" atidlm:targetKey="14064018" atidlm:targetXmlId="DLM1662708" atidlm:targetName="prov 138" atidlm:sourceName="DLM1666873" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662692" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319252" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664198 - prov 189" atidlm:xmlId="DLM1666874" atidlm:state="UNCHANGED"><atidlm:linkcontent>189(7)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800193" atidlm:sourceXmlId="DLM1666874" atidlm:targetKey="14065045" atidlm:targetXmlId="DLM1662782" atidlm:targetName="prov 189" atidlm:sourceName="DLM1666874" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662692" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the chief executive may satisfy the requirement by giving notice to the Minister of Finance under subsection (2) or (4) in relation to the transfer of units to the person.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If the chief executive wishes to arrange reimbursement of New Zealand units or approved overseas units to a person, the chief executive must notify the Minister of Finance of—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the number of New Zealand units or approved overseas units to be reimbursed to the person from the units surrendered or repaid by that person; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the details of the person's holding account.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>As soon as practicable after receiving notification under subsection (2), the Minister of Finance must direct the Registrar to transfer the New Zealand units or approved overseas units from the appropriate surrender account or Crown holding account to the person's holding account.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>If the chief executive wishes to arrange reimbursement of Kyoto units to a person, the chief executive must notify the Minister of Finance of—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the number and type of Kyoto units to be reimbursed to the person; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the details of the person's holding account.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>As soon as practicable after receiving notification under subsection (4), the Minister of Finance must direct the Registrar to transfer the applicable number and type of Kyoto units from a surrender account or a Crown holding account to the person's holding account.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">6</label>
              <para>
                <text>The chief executive must take into account the views of the person to whom units will be reimbursed about the type of units to be reimbursed when determining what units to reimburse.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665938"><amended-provision>Section 124</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319253" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666875" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800194" atidlm:sourceXmlId="DLM1666875" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666875" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665938" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662693" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">125</label>
          <heading>Repayment of units by persons in case of error</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The chief executive may, if satisfied that as a result of an error, units to which a person is not entitled under a provision in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319254" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664199 - part  4 New Zealand greenhouse gas emissions trading scheme" atidlm:xmlId="DLM1666876" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800195" atidlm:sourceXmlId="DLM1666876" atidlm:targetKey="14063002" atidlm:targetXmlId="DLM1662481" atidlm:targetName="part  4 New Zealand greenhouse gas emissions trading scheme" atidlm:sourceName="DLM1666876" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662481.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662693" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319255" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664200 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666877" atidlm:state="UNCHANGED"><atidlm:linkcontent>5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800196" atidlm:sourceXmlId="DLM1666877" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666877" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662693" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> have been transferred from a Crown holding account or other Crown account to the person's holding account, give a notice to the person requiring that person to repay the units referred to in the notice in accordance with subsection (2).</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>A person who receives a notice under subsection (1) must—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>repay any units transferred in error that are still in the person’s holding account, or are otherwise under the person's control, by transferring those units as soon as practicable to the Crown holding account designated in the notice; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>if not all the units transferred in error are repaid under paragraph (a), repay, within 30 working days of the date of the notice, the outstanding number of units by transferring units to the Crown holding account designated in the notice.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>Units repaid by any person under subsection (2)(b) must be of a type that may be transferred to a surrender account at the time the unit is repaid.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665939"><amended-provision>Section 125</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319256" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666878" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800197" atidlm:sourceXmlId="DLM1666878" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666878" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665939" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662694" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">126</label>
          <heading>Obligation to surrender or repay units not suspended by review or appeal</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The obligation to surrender or repay units under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319257" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664201 - prov 123" atidlm:xmlId="DLM1666879" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 123</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800198" atidlm:sourceXmlId="DLM1666879" atidlm:targetKey="14063672" atidlm:targetXmlId="DLM1662691" atidlm:targetName="prov 123" atidlm:sourceName="DLM1666879" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662694" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319258" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664202 - prov 125" atidlm:xmlId="DLM1666880" atidlm:state="UNCHANGED"><atidlm:linkcontent>125</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800200" atidlm:sourceXmlId="DLM1666880" atidlm:targetKey="14063655" atidlm:targetXmlId="DLM1662693" atidlm:targetName="prov 125" atidlm:sourceName="DLM1666880" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662694" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is not suspended by any review or legal proceedings.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If the applicant for a review or the appellant in proceedings is successful in the review or the proceedings, the chief executive must arrange for the reimbursement to the applicant or appellant of the number of units surrendered or repaid in excess of those that are determined to be required to be surrendered or repaid.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>However, any obligation on the chief executive under subsection (2) is suspended pending the outcome of any appeal filed by the chief executive under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319259" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664203 - prov 146" atidlm:xmlId="DLM1666881" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 146</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800201" atidlm:sourceXmlId="DLM1666881" atidlm:targetKey="14064218" atidlm:targetXmlId="DLM1662718" atidlm:targetName="prov 146" atidlm:sourceName="DLM1666881" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662694" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665940"><amended-provision>Section 126</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319260" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666882" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800202" atidlm:sourceXmlId="DLM1666882" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666882" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665940" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662695" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">127</label>
          <heading>Time bar for amendment of emissions returns</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>If a participant has complied with the participant’s obligation to surrender units in relation to an emissions return submitted under—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>any section except <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319261" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664204 - prov 189" atidlm:xmlId="DLM1666883" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 189</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800203" atidlm:sourceXmlId="DLM1666883" atidlm:targetKey="14065045" atidlm:targetXmlId="DLM1662782" atidlm:targetName="prov 189" atidlm:sourceName="DLM1666883" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662695" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319262" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664205 - prov 193" atidlm:xmlId="DLM1666884" atidlm:state="UNCHANGED"><atidlm:linkcontent>193</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800204" atidlm:sourceXmlId="DLM1666884" atidlm:targetKey="14065218" atidlm:targetXmlId="DLM1662795" atidlm:targetName="prov 193" atidlm:sourceName="DLM1666884" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662695" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the chief executive may not amend the emissions return, or the assessment made by the participant of the units to be surrendered or received, after the expiration of 4 years from the end of the year or other period in respect of which the emissions return was made, or in the case of a return under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319263" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664206 - prov 187" atidlm:xmlId="DLM1666885" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 187</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800205" atidlm:sourceXmlId="DLM1666885" atidlm:targetKey="14065071" atidlm:targetXmlId="DLM1662780" atidlm:targetName="prov 187" atidlm:sourceName="DLM1666885" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662695" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319264" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664207 - prov 191" atidlm:xmlId="DLM1666886" atidlm:state="UNCHANGED"><atidlm:linkcontent>191</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800206" atidlm:sourceXmlId="DLM1666886" atidlm:targetKey="14065013" atidlm:targetXmlId="DLM1662790" atidlm:targetName="prov 191" atidlm:sourceName="DLM1666886" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662695" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, from the date of the submission of the emissions return, if the amendment would—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>increase the number of units required to be surrendered by the participant; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>alter the number of New Zealand units that the participant is entitled to receive for removal activities:</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319265" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664208 - prov 189" atidlm:xmlId="DLM1666887" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 189</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800207" atidlm:sourceXmlId="DLM1666887" atidlm:targetKey="14065045" atidlm:targetXmlId="DLM1662782" atidlm:targetName="prov 189" atidlm:sourceName="DLM1666887" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662695" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319266" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664209 - prov 193" atidlm:xmlId="DLM1666888" atidlm:state="UNCHANGED"><atidlm:linkcontent>193</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800208" atidlm:sourceXmlId="DLM1666888" atidlm:targetKey="14065218" atidlm:targetXmlId="DLM1662795" atidlm:targetName="prov 193" atidlm:sourceName="DLM1666888" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662695" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the chief executive may not amend the emissions return, or the assessment made by the participant of the units to be surrendered or received, after the expiration of 7 years from the end of the year or other period in respect of which the emissions return was made if the amendment would—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>increase the number of units required to be surrendered by the participant; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>alter the number of New Zealand units that the participant is entitled to receive for removal activities.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>However, if the chief executive is satisfied that an emissions return was fraudulent, was wilfully misleading, or deliberately omitted mention of emissions or removals in respect of which an emissions return was required to be submitted, the chief executive may amend the emissions return at any time, under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319267" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664210 - prov 120" atidlm:xmlId="DLM1666889" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 120</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800209" atidlm:sourceXmlId="DLM1666889" atidlm:targetKey="14063686" atidlm:targetXmlId="DLM1662688" atidlm:targetName="prov 120" atidlm:sourceName="DLM1666889" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662695" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, so as to—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>increase the number of units required to be surrendered by the participant:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>decrease the number of New Zealand units to which the participant is entitled in respect of removal activities.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665941"><amended-provision>Section 127</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319268" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666890" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800210" atidlm:sourceXmlId="DLM1666890" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666890" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665941" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662696" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">128</label>
          <heading>Amendments and assessments made by electronic means</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>Any amendment or assessment made by the chief executive for the purpose of this Act that is made automatically by a computer or other electronic means in response to or as a result of information entered or held in the computer or other electronic medium—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>must be treated as an amendment or assessment made by or under the properly delegated authority of the chief executive; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>is not invalid by virtue of the fact that it is made automatically by such means.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665942"><amended-provision>Section 128</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319269" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666891" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800211" atidlm:sourceXmlId="DLM1666891" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666891" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665942" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
      </subpart>
      <subpart id="DLM1662697" in.amend="false" irdnumbering="no" toc="yes" act.no="40" act.type="public" date.as.at="2008-09-26" date.assent="2002-11-18" date.first.valid="2007-09-18" stage="reprint" year="2002">
        <label auto.number="no" denominator="yes">4</label>
        <heading>Offences and penalties</heading>
        <notes>
          <history>
            <history-note id="DLM1665943"><amended-provision>Subpart 4</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319270" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666892" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800212" atidlm:sourceXmlId="DLM1666892" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666892" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665943" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
          </history>
        </notes>
        <prov id="DLM1662698" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">129</label>
          <heading>Strict liability offences</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>A person commits an offence against this Act if the person—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>is a participant in any year and, without reasonable excuse, fails to comply with <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319271" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664211 - prov 62" atidlm:xmlId="DLM1666893" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 62</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800213" atidlm:sourceXmlId="DLM1666893" atidlm:targetKey="14063246" atidlm:targetXmlId="DLM1662492" atidlm:targetName="prov 62" atidlm:sourceName="DLM1666893" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662698" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> (requirement to collect data or other information, calculate emissions and removals, and keep records); or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>without reasonable excuse,—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>fails to notify the chief executive under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319272" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664212 - prov 56" atidlm:xmlId="DLM1666894" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 56</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800214" atidlm:sourceXmlId="DLM1666894" atidlm:targetKey="14063312" atidlm:targetXmlId="DLM1662486" atidlm:targetName="prov 56" atidlm:sourceName="DLM1666894" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662698" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> that the person is carrying out an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319273" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664213 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1666895" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800215" atidlm:sourceXmlId="DLM1666895" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1666895" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662698" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or </text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>fails to submit an emissions return when required to do so under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319274" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664214 - prov 65" atidlm:xmlId="DLM1666896" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 65</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800216" atidlm:sourceXmlId="DLM1666896" atidlm:targetKey="14063267" atidlm:targetXmlId="DLM1662495" atidlm:targetName="prov 65" atidlm:sourceName="DLM1666896" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662698" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319275" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664216 - prov 118" atidlm:xmlId="DLM1666897" atidlm:state="UNCHANGED"><atidlm:linkcontent>118</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800217" atidlm:sourceXmlId="DLM1666897" atidlm:targetKey="14063678" atidlm:targetXmlId="DLM1662686" atidlm:targetName="prov 118" atidlm:sourceName="DLM1666897" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662698" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319276" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664217 - prov 189" atidlm:xmlId="DLM1666898" atidlm:state="UNCHANGED"><atidlm:linkcontent>189</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800218" atidlm:sourceXmlId="DLM1666898" atidlm:targetKey="14065045" atidlm:targetXmlId="DLM1662782" atidlm:targetName="prov 189" atidlm:sourceName="DLM1666898" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662698" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319277" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664218 - prov 191" atidlm:xmlId="DLM1666899" atidlm:state="UNCHANGED"><atidlm:linkcontent>191</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800219" atidlm:sourceXmlId="DLM1666899" atidlm:targetKey="14065013" atidlm:targetXmlId="DLM1662790" atidlm:targetName="prov 191" atidlm:sourceName="DLM1666899" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662698" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319278" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664219 - prov 193" atidlm:xmlId="DLM1666900" atidlm:state="UNCHANGED"><atidlm:linkcontent>193</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800220" atidlm:sourceXmlId="DLM1666900" atidlm:targetKey="14065218" atidlm:targetXmlId="DLM1662795" atidlm:targetName="prov 193" atidlm:sourceName="DLM1666900" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662698" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>fails to keep emissions records as required under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319279" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664215 - prov 67" atidlm:xmlId="DLM1666901" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 67</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800221" atidlm:sourceXmlId="DLM1666901" atidlm:targetKey="14063238" atidlm:targetXmlId="DLM1662497" atidlm:targetName="prov 67" atidlm:sourceName="DLM1666901" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662698" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iv</label>
                      <para>
                        <text>fails to notify the chief executive of a matter that is required to be notified under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319280" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664222 - prov 112" atidlm:xmlId="DLM1666902" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 112</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800222" atidlm:sourceXmlId="DLM1666902" atidlm:targetKey="14063736" atidlm:targetXmlId="DLM1662679" atidlm:targetName="prov 112" atidlm:sourceName="DLM1666902" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662698" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or </text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">v</label>
                      <para>
                        <text>fails to notify the chief executive of the transfer of any post-1989 forest land when required to do so under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319281" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664220 - prov 192" atidlm:xmlId="DLM1666903" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 192</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800223" atidlm:sourceXmlId="DLM1666903" atidlm:targetKey="14065023" atidlm:targetXmlId="DLM1662791" atidlm:targetName="prov 192" atidlm:sourceName="DLM1666903" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662698" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319282" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664221 - prov 193" atidlm:xmlId="DLM1666904" atidlm:state="UNCHANGED"><atidlm:linkcontent>193</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800224" atidlm:sourceXmlId="DLM1666904" atidlm:targetKey="14065218" atidlm:targetXmlId="DLM1662795" atidlm:targetName="prov 193" atidlm:sourceName="DLM1666904" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662698" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>Every person who is convicted of an offence against subsection (1) is liable on summary conviction,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the first time the person is convicted of that offence, to a fine not exceeding $8,000:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the second time the person is convicted of that offence, to a fine not exceeding $16,000:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>on every subsequent occasion that the person is convicted of that offence, to a fine not exceeding $24,000.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665944"><amended-provision>Section 129</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319283" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666905" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800225" atidlm:sourceXmlId="DLM1666905" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666905" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665944" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662699" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">130</label>
          <heading>Offence for breach of section 99</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>Every person to whom <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319284" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664223 - prov 99" atidlm:xmlId="DLM1666906" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 99(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800226" atidlm:sourceXmlId="DLM1666906" atidlm:targetKey="14063783" atidlm:targetXmlId="DLM1662665" atidlm:targetName="prov 99" atidlm:sourceName="DLM1666906" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662699" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> applies who knowingly acts in contravention of section 99 commits an offence and is liable on summary conviction to—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>imprisonment for a term not exceeding 6 months; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>a fine not exceeding $15,000; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>both.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665945"><amended-provision>Section 130</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319285" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666907" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800227" atidlm:sourceXmlId="DLM1666907" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666907" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665945" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662700" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">131</label>
          <heading>Offence for failure to provide information or documents</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>A person commits an offence against this Act if the person, without reasonable excuse,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>fails to provide information to the chief executive or an enforcement officer when required to do so under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319286" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664224 - prov 94" atidlm:xmlId="DLM1666908" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 94</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800228" atidlm:sourceXmlId="DLM1666908" atidlm:targetKey="14063824" atidlm:targetXmlId="DLM1662660" atidlm:targetName="prov 94" atidlm:sourceName="DLM1666908" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662700" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>fails to appear before the chief executive or an enforcement officer, or fails to produce any document or documents, when required to do so under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319287" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664225 - prov 95" atidlm:xmlId="DLM1666909" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 95</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800229" atidlm:sourceXmlId="DLM1666909" atidlm:targetKey="14063842" atidlm:targetXmlId="DLM1662661" atidlm:targetName="prov 95" atidlm:sourceName="DLM1666909" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662700" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>Every person who is convicted of an offence against subsection (1) is liable on summary conviction,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>in the case of an individual, to a fine not exceeding $12,000; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>in the case of a body corporate, to a fine not exceeding $24,000.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665946"><amended-provision>Section 131</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319288" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666910" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800230" atidlm:sourceXmlId="DLM1666910" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666910" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665946" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662701" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">132</label>
          <heading>Other offences</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>A person commits an offence against this Act if the person—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>refuses to take an oath when required to do so under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319289" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664226 - prov 95" atidlm:xmlId="DLM1666911" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 95</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800231" atidlm:sourceXmlId="DLM1666911" atidlm:targetKey="14063842" atidlm:targetXmlId="DLM1662661" atidlm:targetName="prov 95" atidlm:sourceName="DLM1666911" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>refuses to answer any question when required to do so under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319290" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664227 - prov 95" atidlm:xmlId="DLM1666912" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 95</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800232" atidlm:sourceXmlId="DLM1666912" atidlm:targetKey="14063842" atidlm:targetXmlId="DLM1662661" atidlm:targetName="prov 95" atidlm:sourceName="DLM1666912" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>is a participant in any year and knowingly fails to comply with <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319291" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664228 - prov 62" atidlm:xmlId="DLM1666913" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 62</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800233" atidlm:sourceXmlId="DLM1666913" atidlm:targetKey="14063246" atidlm:targetXmlId="DLM1662492" atidlm:targetName="prov 62" atidlm:sourceName="DLM1666913" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> (requirement to collect data or other information, calculate emissions and removals, and keep records); or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>knowingly fails to submit an emissions return when required to do so under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319292" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664229 - prov 65" atidlm:xmlId="DLM1666914" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 65</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800234" atidlm:sourceXmlId="DLM1666914" atidlm:targetKey="14063267" atidlm:targetXmlId="DLM1662495" atidlm:targetName="prov 65" atidlm:sourceName="DLM1666914" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319293" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664230 - prov 118" atidlm:xmlId="DLM1666915" atidlm:state="UNCHANGED"><atidlm:linkcontent>118</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800235" atidlm:sourceXmlId="DLM1666915" atidlm:targetKey="14063678" atidlm:targetXmlId="DLM1662686" atidlm:targetName="prov 118" atidlm:sourceName="DLM1666915" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319294" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664231 - prov 189" atidlm:xmlId="DLM1666916" atidlm:state="UNCHANGED"><atidlm:linkcontent>189</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800236" atidlm:sourceXmlId="DLM1666916" atidlm:targetKey="14065045" atidlm:targetXmlId="DLM1662782" atidlm:targetName="prov 189" atidlm:sourceName="DLM1666916" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319295" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664232 - prov 191" atidlm:xmlId="DLM1666917" atidlm:state="UNCHANGED"><atidlm:linkcontent>191</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800237" atidlm:sourceXmlId="DLM1666917" atidlm:targetKey="14065013" atidlm:targetXmlId="DLM1662790" atidlm:targetName="prov 191" atidlm:sourceName="DLM1666917" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319296" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664233 - prov 193" atidlm:xmlId="DLM1666918" atidlm:state="UNCHANGED"><atidlm:linkcontent>193</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800238" atidlm:sourceXmlId="DLM1666918" atidlm:targetKey="14065218" atidlm:targetXmlId="DLM1662795" atidlm:targetName="prov 193" atidlm:sourceName="DLM1666918" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">e</label>
                  <para>
                    <text>knowingly fails to keep records as required under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319297" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664234 - prov 67" atidlm:xmlId="DLM1666919" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 67</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800240" atidlm:sourceXmlId="DLM1666919" atidlm:targetKey="14063238" atidlm:targetXmlId="DLM1662497" atidlm:targetName="prov 67" atidlm:sourceName="DLM1666919" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">f</label>
                  <para>
                    <text>knowingly provides altered, false, incomplete, or misleading information (including emissions returns) to the chief executive or any other person in respect of any matter in this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319298" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664235 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666920" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800241" atidlm:sourceXmlId="DLM1666920" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666920" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">g</label>
                  <para>
                    <text>wilfully obstructs, hinders, resists, or deceives a person exercising a power conferred on that person under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319299" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664236 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666921" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800242" atidlm:sourceXmlId="DLM1666921" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666921" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">h</label>
                  <para>
                    <text>wilfully interferes with any survey, investigation, test, or measurement carried out by an enforcement officer or a person assisting an enforcement officer under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319300" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664237 - prov 100" atidlm:xmlId="DLM1666922" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 100</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800243" atidlm:sourceXmlId="DLM1666922" atidlm:targetKey="14063790" atidlm:targetXmlId="DLM1662666" atidlm:targetName="prov 100" atidlm:sourceName="DLM1666922" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">i</label>
                  <para>
                    <text>refuses to provide information that an enforcement officer has demanded from that person under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319301" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664238 - prov 100" atidlm:xmlId="DLM1666923" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 100(2)(d)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800244" atidlm:sourceXmlId="DLM1666923" atidlm:targetKey="14063790" atidlm:targetXmlId="DLM1662666" atidlm:targetName="prov 100" atidlm:sourceName="DLM1666923" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>Every person who is convicted of an offence against subsection (1) is liable on summary conviction,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>in the case of an individual, to a fine not exceeding $25,000; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>in the case of a body corporate, to a fine not exceeding $50,000.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665947"><amended-provision>Section 132</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319302" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666924" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800245" atidlm:sourceXmlId="DLM1666924" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666924" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665947" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662702" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">133</label>
          <heading>Evasion or similar offences</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>A person commits an offence against this Act if the person, with intent to deceive and for the purpose of either obtaining any material benefit or avoiding any material detriment,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>fails to comply with any of the requirements specified in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319303" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664239 - prov 62" atidlm:xmlId="DLM1666925" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 62</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800246" atidlm:sourceXmlId="DLM1666925" atidlm:targetKey="14063246" atidlm:targetXmlId="DLM1662492" atidlm:targetName="prov 62" atidlm:sourceName="DLM1666925" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662702" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>fails to submit an emissions return when required to do so under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319304" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664240 - prov 65" atidlm:xmlId="DLM1666926" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 65</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800247" atidlm:sourceXmlId="DLM1666926" atidlm:targetKey="14063267" atidlm:targetXmlId="DLM1662495" atidlm:targetName="prov 65" atidlm:sourceName="DLM1666926" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662702" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319305" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664241 - prov 118" atidlm:xmlId="DLM1666927" atidlm:state="UNCHANGED"><atidlm:linkcontent>118</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800248" atidlm:sourceXmlId="DLM1666927" atidlm:targetKey="14063678" atidlm:targetXmlId="DLM1662686" atidlm:targetName="prov 118" atidlm:sourceName="DLM1666927" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662702" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319306" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664242 - prov 189" atidlm:xmlId="DLM1666928" atidlm:state="UNCHANGED"><atidlm:linkcontent>189</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800249" atidlm:sourceXmlId="DLM1666928" atidlm:targetKey="14065045" atidlm:targetXmlId="DLM1662782" atidlm:targetName="prov 189" atidlm:sourceName="DLM1666928" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662702" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319307" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664243 - prov 191" atidlm:xmlId="DLM1666929" atidlm:state="UNCHANGED"><atidlm:linkcontent>191</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800250" atidlm:sourceXmlId="DLM1666929" atidlm:targetKey="14065013" atidlm:targetXmlId="DLM1662790" atidlm:targetName="prov 191" atidlm:sourceName="DLM1666929" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662702" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319308" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664244 - prov 193" atidlm:xmlId="DLM1666930" atidlm:state="UNCHANGED"><atidlm:linkcontent>193</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800251" atidlm:sourceXmlId="DLM1666930" atidlm:targetKey="14065218" atidlm:targetXmlId="DLM1662795" atidlm:targetName="prov 193" atidlm:sourceName="DLM1666930" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662702" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>fails to keep records as required under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319309" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664245 - prov 67" atidlm:xmlId="DLM1666931" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 67</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800252" atidlm:sourceXmlId="DLM1666931" atidlm:targetKey="14063238" atidlm:targetXmlId="DLM1662497" atidlm:targetName="prov 67" atidlm:sourceName="DLM1666931" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662702" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>fails to provide information to the chief executive or any other person when required to do so under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319310" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664246 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666932" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800253" atidlm:sourceXmlId="DLM1666932" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666932" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662702" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">e</label>
                  <para>
                    <text>provides altered, false, incomplete, or misleading information (including emissions returns) to the chief executive or any other person in respect of a matter in this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319311" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664247 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666933" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800254" atidlm:sourceXmlId="DLM1666933" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666933" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662702" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>Every person who commits an offence against subsection (1) is liable on conviction on indictment to—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>imprisonment for a term not exceeding 5 years; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>a fine not exceeding $50,000; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>both imprisonment and a fine.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665948"><amended-provision>Section 133</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319312" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666934" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800255" atidlm:sourceXmlId="DLM1666934" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666934" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665948" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662703" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">134</label>
          <heading>Penalty for failing to surrender or repay units</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>This section applies if—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>a person fails to surrender units by the due date when required to do so under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319313" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664248 - prov 65" atidlm:xmlId="DLM1666935" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 65(4)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800256" atidlm:sourceXmlId="DLM1666935" atidlm:targetKey="14063267" atidlm:targetXmlId="DLM1662495" atidlm:targetName="prov 65" atidlm:sourceName="DLM1666935" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319314" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664249 - prov 118" atidlm:xmlId="DLM1666936" atidlm:state="UNCHANGED"><atidlm:linkcontent>118(5)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800257" atidlm:sourceXmlId="DLM1666936" atidlm:targetKey="14063678" atidlm:targetXmlId="DLM1662686" atidlm:targetName="prov 118" atidlm:sourceName="DLM1666936" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319315" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664250 - prov 189" atidlm:xmlId="DLM1666937" atidlm:state="UNCHANGED"><atidlm:linkcontent>189</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800258" atidlm:sourceXmlId="DLM1666937" atidlm:targetKey="14065045" atidlm:targetXmlId="DLM1662782" atidlm:targetName="prov 189" atidlm:sourceName="DLM1666937" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319316" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664251 - prov 191" atidlm:xmlId="DLM1666938" atidlm:state="UNCHANGED"><atidlm:linkcontent>191</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800259" atidlm:sourceXmlId="DLM1666938" atidlm:targetKey="14065013" atidlm:targetXmlId="DLM1662790" atidlm:targetName="prov 191" atidlm:sourceName="DLM1666938" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319317" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664252 - prov 193" atidlm:xmlId="DLM1666939" atidlm:state="UNCHANGED"><atidlm:linkcontent>193</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800260" atidlm:sourceXmlId="DLM1666939" atidlm:targetKey="14065218" atidlm:targetXmlId="DLM1662795" atidlm:targetName="prov 193" atidlm:sourceName="DLM1666939" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>an amendment to an emissions return under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319318" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664253 - prov 120" atidlm:xmlId="DLM1666940" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 120</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800261" atidlm:sourceXmlId="DLM1666940" atidlm:targetKey="14063686" atidlm:targetXmlId="DLM1662688" atidlm:targetName="prov 120" atidlm:sourceName="DLM1666940" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> or an assessment made under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319319" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664254 - prov 121" atidlm:xmlId="DLM1666941" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 121</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800262" atidlm:sourceXmlId="DLM1666941" atidlm:targetKey="14063690" atidlm:targetXmlId="DLM1662689" atidlm:targetName="prov 121" atidlm:sourceName="DLM1666941" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> results in a liability for a person—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>to surrender units or additional units under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319320" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664255 - prov 123" atidlm:xmlId="DLM1666942" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 123(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800263" atidlm:sourceXmlId="DLM1666942" atidlm:targetKey="14063672" atidlm:targetXmlId="DLM1662691" atidlm:targetName="prov 123" atidlm:sourceName="DLM1666942" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>to repay units in accordance with <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319321" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664256 - prov 123" atidlm:xmlId="DLM1666943" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 123(6)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800264" atidlm:sourceXmlId="DLM1666943" atidlm:targetKey="14063672" atidlm:targetXmlId="DLM1662691" atidlm:targetName="prov 123" atidlm:sourceName="DLM1666943" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>a person is required under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319322" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664257 - prov 125" atidlm:xmlId="DLM1666944" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 125</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800265" atidlm:sourceXmlId="DLM1666944" atidlm:targetKey="14063655" atidlm:targetXmlId="DLM1662693" atidlm:targetName="prov 125" atidlm:sourceName="DLM1666944" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> to repay units transferred in error.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>Subject to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319323" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664258 - prov 135" atidlm:xmlId="DLM1666945" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 135</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800266" atidlm:sourceXmlId="DLM1666945" atidlm:targetKey="14064060" atidlm:targetXmlId="DLM1662704" atidlm:targetName="prov 135" atidlm:sourceName="DLM1666945" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, if this section applies, the person is liable to—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>surrender or repay the units as required under the relevant section; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>pay to the chief executive an excess emissions penalty of $30 for each unit that,—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>if subsection (1)(a) applies, the person fails to surrender by the due date; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>if subsection (1)(b) applies, the person is required to surrender under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319324" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664259 - prov 123" atidlm:xmlId="DLM1666946" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 123(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800267" atidlm:sourceXmlId="DLM1666946" atidlm:targetKey="14063672" atidlm:targetXmlId="DLM1662691" atidlm:targetName="prov 123" atidlm:sourceName="DLM1666946" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> or repay under section 123(6); or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>if subsection (1)(c) applies, the person fails to repay by the due date.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>If a person is liable to an excess emissions penalty under subsection (2), the chief executive must give a notice to the person that,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>if subsection (1)(a) or (c) applies,—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>refers to the person’s failure to surrender units by the due date as required under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319325" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664260 - prov 65" atidlm:xmlId="DLM1666947" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 65(4)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800268" atidlm:sourceXmlId="DLM1666947" atidlm:targetKey="14063267" atidlm:targetXmlId="DLM1662495" atidlm:targetName="prov 65" atidlm:sourceName="DLM1666947" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319326" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664261 - prov 118" atidlm:xmlId="DLM1666948" atidlm:state="UNCHANGED"><atidlm:linkcontent>118(5)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800269" atidlm:sourceXmlId="DLM1666948" atidlm:targetKey="14063678" atidlm:targetXmlId="DLM1662686" atidlm:targetName="prov 118" atidlm:sourceName="DLM1666948" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319327" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664262 - prov 189" atidlm:xmlId="DLM1666949" atidlm:state="UNCHANGED"><atidlm:linkcontent>189</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800270" atidlm:sourceXmlId="DLM1666949" atidlm:targetKey="14065045" atidlm:targetXmlId="DLM1662782" atidlm:targetName="prov 189" atidlm:sourceName="DLM1666949" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319328" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664263 - prov 191" atidlm:xmlId="DLM1666950" atidlm:state="UNCHANGED"><atidlm:linkcontent>191</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800271" atidlm:sourceXmlId="DLM1666950" atidlm:targetKey="14065013" atidlm:targetXmlId="DLM1662790" atidlm:targetName="prov 191" atidlm:sourceName="DLM1666950" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319329" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664264 - prov 193" atidlm:xmlId="DLM1666951" atidlm:state="UNCHANGED"><atidlm:linkcontent>193</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800272" atidlm:sourceXmlId="DLM1666951" atidlm:targetKey="14065218" atidlm:targetXmlId="DLM1662795" atidlm:targetName="prov 193" atidlm:sourceName="DLM1666951" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, as applicable, or repay units by the due date under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1324802" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1670100 - prov 125" atidlm:xmlId="DLM1670102" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 125</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="802282" atidlm:sourceXmlId="DLM1670102" atidlm:targetKey="14063655" atidlm:targetXmlId="DLM1662693" atidlm:targetName="prov 125" atidlm:sourceName="DLM1670102" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>sets out the number of units required to be surrendered or repaid; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>sets out the amount of the excess emissions penalty to which the person is liable under subsection (2)(b); and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iv</label>
                      <para>
                        <text>requires the person to surrender or repay the units specified in subparagraph (ii), and pay the penalty specified in subparagraph (iii) to the chief executive, within 20 working days of the date of the notice; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">v</label>
                      <para>
                        <text>advises that, unless both the units are surrendered or repaid and the penalty paid in full by the due date, interest on the amount of the penalty will accrue in accordance with <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319330" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664265 - prov 137" atidlm:xmlId="DLM1666952" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 137</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800273" atidlm:sourceXmlId="DLM1666952" atidlm:targetKey="14064017" atidlm:targetXmlId="DLM1662706" atidlm:targetName="prov 137" atidlm:sourceName="DLM1666952" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>if subsection (1)(b) applies,—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>refers to the relevant notice under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319331" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664266 - prov 123" atidlm:xmlId="DLM1666953" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 123(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800274" atidlm:sourceXmlId="DLM1666953" atidlm:targetKey="14063672" atidlm:targetXmlId="DLM1662691" atidlm:targetName="prov 123" atidlm:sourceName="DLM1666953" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>sets out the amount of the excess emissions penalty to which the person is liable under subsection (2)(b); and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>requires the person to pay the penalty specified in subparagraph (ii) within the period in which the person must surrender units under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319332" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664267 - prov 123" atidlm:xmlId="DLM1666954" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 123(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800275" atidlm:sourceXmlId="DLM1666954" atidlm:targetKey="14063672" atidlm:targetXmlId="DLM1662691" atidlm:targetName="prov 123" atidlm:sourceName="DLM1666954" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> or repay units under section 123(6); and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iv</label>
                      <para>
                        <text>advises that, unless both the units are surrendered or repaid and the penalty paid in full by the due date, interest on the amount of the penalty will accrue in accordance with <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319333" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664268 - prov 137" atidlm:xmlId="DLM1666955" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 137</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800276" atidlm:sourceXmlId="DLM1666955" atidlm:targetKey="14064017" atidlm:targetXmlId="DLM1662706" atidlm:targetName="prov 137" atidlm:sourceName="DLM1666955" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662703" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>The amount of the excess emissions penalty, together with any interest that accrues on that penalty, constitutes a debt due to the Crown and is recoverable by the chief executive in a court of competent jurisdiction.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665949"><amended-provision>Section 134</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319334" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666956" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800277" atidlm:sourceXmlId="DLM1666956" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666956" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665949" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662704" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">135</label>
          <heading>Reductions in penalty</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The chief executive may reduce the excess emissions penalty imposed by <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319335" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664269 - prov 134" atidlm:xmlId="DLM1666957" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 134(2)(b)(i)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800278" atidlm:sourceXmlId="DLM1666957" atidlm:targetKey="14064044" atidlm:targetXmlId="DLM1662703" atidlm:targetName="prov 134" atidlm:sourceName="DLM1666957" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662704" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or (iii)</citation> by up to 100%, if the person voluntarily discloses the failure to surrender or repay units before receiving a penalty notice under <citation jurisdiction="nz">section 134</citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The chief executive may reduce the excess emissions penalty imposed by <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319336" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664270 - prov 134" atidlm:xmlId="DLM1666958" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 134(2)(b)(ii)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800280" atidlm:sourceXmlId="DLM1666958" atidlm:targetKey="14064044" atidlm:targetXmlId="DLM1662703" atidlm:targetName="prov 134" atidlm:sourceName="DLM1666958" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662704" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> by up to 100%, if—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the person voluntarily disclosed that an emissions return submitted by the person contained incorrect information, or that the person failed to file a return when required to do so, before the chief executive or an enforcement officer—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>requested any information under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319337" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664271 - prov 94" atidlm:xmlId="DLM1666959" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 94</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800281" atidlm:sourceXmlId="DLM1666959" atidlm:targetKey="14063824" atidlm:targetXmlId="DLM1662660" atidlm:targetName="prov 94" atidlm:sourceName="DLM1666959" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662704" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319338" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664272 - prov 95" atidlm:xmlId="DLM1666960" atidlm:state="UNCHANGED"><atidlm:linkcontent>95</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800282" atidlm:sourceXmlId="DLM1666960" atidlm:targetKey="14063842" atidlm:targetXmlId="DLM1662661" atidlm:targetName="prov 95" atidlm:sourceName="DLM1666960" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662704" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in relation to the return; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>gave notice of an intention to enter land or premises under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319339" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664273 - prov 100" atidlm:xmlId="DLM1666961" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 100(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800283" atidlm:sourceXmlId="DLM1666961" atidlm:targetKey="14063790" atidlm:targetXmlId="DLM1662666" atidlm:targetName="prov 100" atidlm:sourceName="DLM1666961" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662704" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>executed a warrant under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319340" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664274 - prov 101" atidlm:xmlId="DLM1666962" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 101</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800284" atidlm:sourceXmlId="DLM1666962" atidlm:targetKey="14063778" atidlm:targetXmlId="DLM1662667" atidlm:targetName="prov 101" atidlm:sourceName="DLM1666962" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662704" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the chief executive is satisfied that the person formed a view as to the information on which the return was based or as to whether a return was required, that, while incorrect, was reasonable, having regard to the information available to that person at the time the emissions return was required.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665950"><amended-provision>Section 135</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319341" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666963" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800285" atidlm:sourceXmlId="DLM1666963" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666963" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665950" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662705" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">136</label>
          <heading>Additional penalty for knowing failure to comply</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>This section applies to a person who—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>is or was liable following an amendment under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319342" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664275 - prov 120" atidlm:xmlId="DLM1666964" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 120</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800286" atidlm:sourceXmlId="DLM1666964" atidlm:targetKey="14063686" atidlm:targetXmlId="DLM1662688" atidlm:targetName="prov 120" atidlm:sourceName="DLM1666964" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662705" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> or an assessment under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319343" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664276 - prov 121" atidlm:xmlId="DLM1666965" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 121</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800287" atidlm:sourceXmlId="DLM1666965" atidlm:targetKey="14063690" atidlm:targetXmlId="DLM1662689" atidlm:targetName="prov 121" atidlm:sourceName="DLM1666965" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662705" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> to surrender units (or additional units) or to repay units, in respect of any period covered by, or that should have been covered by, an emissions return; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>is convicted of an offence against <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319344" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664277 - prov 132" atidlm:xmlId="DLM1666966" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 132(1)(c)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800288" atidlm:sourceXmlId="DLM1666966" atidlm:targetKey="14064086" atidlm:targetXmlId="DLM1662701" atidlm:targetName="prov 132" atidlm:sourceName="DLM1666966" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662705" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> to (f) or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319345" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664278 - prov 133" atidlm:xmlId="DLM1666967" atidlm:state="UNCHANGED"><atidlm:linkcontent>133</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800289" atidlm:sourceXmlId="DLM1666967" atidlm:targetKey="14064098" atidlm:targetXmlId="DLM1662702" atidlm:targetName="prov 133" atidlm:sourceName="DLM1666967" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662705" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in relation to that period.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If this section applies, the person is liable, in addition to any penalty imposed in respect of the offence, to—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>surrender a number of units equivalent to the number of units determined by the chief executive in the amendment under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319346" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664279 - prov 120" atidlm:xmlId="DLM1666968" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 120</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800290" atidlm:sourceXmlId="DLM1666968" atidlm:targetKey="14063686" atidlm:targetXmlId="DLM1662688" atidlm:targetName="prov 120" atidlm:sourceName="DLM1666968" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662705" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> or the assessment under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319347" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664280 - prov 121" atidlm:xmlId="DLM1666969" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 121</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800291" atidlm:sourceXmlId="DLM1666969" atidlm:targetKey="14063690" atidlm:targetXmlId="DLM1662689" atidlm:targetName="prov 121" atidlm:sourceName="DLM1666969" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662705" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, or in any review or appeal proceedings relating to that determination; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>pay an excess emissions penalty of $30 for each unit the person is liable to surrender under paragraph (a).</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>If a person is liable under subsection (1), the chief executive must give a notice to the person that—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>sets out the—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>number of additional units that the person is required to surrender; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>amount of the excess emissions penalty to which the person is liable; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>requires the person to surrender the additional units and pay the penalty within 90 days after the date of the notice; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>advises that, unless both the units are surrendered and the penalty paid in full by the due date, interest on the amount of the penalty will accrue in accordance with <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319348" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664281 - prov 137" atidlm:xmlId="DLM1666970" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 137</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800292" atidlm:sourceXmlId="DLM1666970" atidlm:targetKey="14064017" atidlm:targetXmlId="DLM1662706" atidlm:targetName="prov 137" atidlm:sourceName="DLM1666970" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662705" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>To avoid doubt, any liability to surrender units or pay a penalty under subsection (2) is additional to, and does not affect, the liability of a person to surrender or repay units under any other section of this Act or to pay a penalty under a penalty notice given by the chief executive under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319349" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664282 - prov 134" atidlm:xmlId="DLM1666971" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 134</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800293" atidlm:sourceXmlId="DLM1666971" atidlm:targetKey="14064044" atidlm:targetXmlId="DLM1662703" atidlm:targetName="prov 134" atidlm:sourceName="DLM1666971" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662705" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>The amount of the excess emissions penalty, together with any interest that accrues on that penalty, constitutes a debt due to the Crown and is recoverable by the chief executive in a court of competent jurisdiction.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665951"><amended-provision>Section 136</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319350" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666972" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800294" atidlm:sourceXmlId="DLM1666972" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666972" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665951" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662706" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">137</label>
          <heading>Interest for late payment</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>This section applies if a person—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>has failed to surrender or repay units when required to do so and is liable to pay an excess emissions penalty in relation to those units under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319351" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664283 - prov 134" atidlm:xmlId="DLM1666973" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 134(2)(b)(i)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800295" atidlm:sourceXmlId="DLM1666973" atidlm:targetKey="14064044" atidlm:targetXmlId="DLM1662703" atidlm:targetName="prov 134" atidlm:sourceName="DLM1666973" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662706" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or (iii)</citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>is required to surrender or repay units under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319352" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664284 - prov 123" atidlm:xmlId="DLM1666974" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 123</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800296" atidlm:sourceXmlId="DLM1666974" atidlm:targetKey="14063672" atidlm:targetXmlId="DLM1662691" atidlm:targetName="prov 123" atidlm:sourceName="DLM1666974" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662706" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and is liable to pay an excess emissions penalty in relation to those units under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319353" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664285 - prov 134" atidlm:xmlId="DLM1666975" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 134(2)(b)(ii)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800297" atidlm:sourceXmlId="DLM1666975" atidlm:targetKey="14064044" atidlm:targetXmlId="DLM1662703" atidlm:targetName="prov 134" atidlm:sourceName="DLM1666975" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662706" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>is required to surrender units and pay an excess emissions penalty under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319354" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664286 - prov 136" atidlm:xmlId="DLM1666976" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 136</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800298" atidlm:sourceXmlId="DLM1666976" atidlm:targetKey="14064073" atidlm:targetXmlId="DLM1662705" atidlm:targetName="prov 136" atidlm:sourceName="DLM1666976" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662706" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>does not comply, or comply in full, with the requirement to surrender or repay units and to pay the penalty by the relevant date.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If this section applies, the person is liable to pay interest on the full amount of the excess emissions penalty—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>at the rate prescribed by the Governor-General by Order in Council; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>for the period from the date by which the penalty was due to be paid until the associated liability to surrender or repay units (or to pay any associated debt under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319355" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664287 - prov 159" atidlm:xmlId="DLM1666977" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 159</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800299" atidlm:sourceXmlId="DLM1666977" atidlm:targetKey="14064583" atidlm:targetXmlId="DLM1662735" atidlm:targetName="prov 159" atidlm:sourceName="DLM1666977" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662706" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>) has been met, and until the penalty and any interest due have been paid in full.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>To avoid doubt, interest accrues under subsection (2) even if the amount of the excess emissions penalty in a penalty notice has been paid in full if the associated requirement to surrender or repay units (or to pay any associated debt under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319356" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664288 - prov 159" atidlm:xmlId="DLM1666978" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 159</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800300" atidlm:sourceXmlId="DLM1666978" atidlm:targetKey="14064583" atidlm:targetXmlId="DLM1662735" atidlm:targetName="prov 159" atidlm:sourceName="DLM1666978" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662706" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>) has not been met in full.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>Despite anything in this section, the chief executive may remit any amount of interest that has accrued under this section, if the chief executive is satisfied that—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the failure of the person to comply with the requirement to surrender or repay units and pay the penalty in full arises as a result of an event or circumstance beyond the control of that person; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>as a consequence of that event or circumstance, the person has a reasonable justification or excuse for the non-compliance; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>the person corrected the failure to comply as soon as practicable.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>Without limiting the chief executive’s discretion under subsection (4), an event or circumstance may include—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>an accident or a disaster; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>illness or emotional or mental distress.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">6</label>
              <para>
                <text>Despite anything in this section, the chief executive may remit part of an amount of interest that has accrued under this section if the chief executive is satisfied that it would be manifestly unfair or unjust to impose the full amount.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">7</label>
              <para>
                <text>For the purposes of this section, an <def-term id="DLM1662707">event or circumstance</def-term> does not include—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>an act or omission of an agent of a person, unless the chief executive is satisfied that the act or omission was caused by an event or circumstance beyond the control of the agent—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>that could not have been anticipated; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>the effect of which could not have been avoided by compliance with accepted standards of business organisation and professional conduct; or</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>a person’s financial position.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665952"><amended-provision>Section 137</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319357" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666979" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800301" atidlm:sourceXmlId="DLM1666979" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666979" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665952" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662708" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">138</label>
          <heading>Obligation to pay penalty not suspended by appeal</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The obligation to pay and the right to receive and recover any excess emissions penalty or interest imposed under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319358" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664289 - prov 134" atidlm:xmlId="DLM1666980" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 134</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800302" atidlm:sourceXmlId="DLM1666980" atidlm:targetKey="14064044" atidlm:targetXmlId="DLM1662703" atidlm:targetName="prov 134" atidlm:sourceName="DLM1666980" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662708" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319359" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664290 - prov 136" atidlm:xmlId="DLM1666981" atidlm:state="UNCHANGED"><atidlm:linkcontent>136</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800303" atidlm:sourceXmlId="DLM1666981" atidlm:targetKey="14064073" atidlm:targetXmlId="DLM1662705" atidlm:targetName="prov 136" atidlm:sourceName="DLM1666981" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662708" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319360" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664291 - prov 137" atidlm:xmlId="DLM1666982" atidlm:state="UNCHANGED"><atidlm:linkcontent>137</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800304" atidlm:sourceXmlId="DLM1666982" atidlm:targetKey="14064017" atidlm:targetXmlId="DLM1662706" atidlm:targetName="prov 137" atidlm:sourceName="DLM1666982" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662708" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, and the obligation to surrender any additional units under section 136, are not suspended by any review or appeal.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If the applicant or appellant is successful in the review or appeal, the amount of any excess emissions penalty or interest paid by the applicant must be refunded to the applicant or appellant by the chief executive, and any units not required to be surrendered must be reimbursed in accordance with the procedure specified in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319361" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664292 - prov 124" atidlm:xmlId="DLM1666983" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 124</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800305" atidlm:sourceXmlId="DLM1666983" atidlm:targetKey="14063645" atidlm:targetXmlId="DLM1662692" atidlm:targetName="prov 124" atidlm:sourceName="DLM1666983" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662708" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>However, any obligation on the chief executive under subsection (2) is suspended pending the outcome of any appeal filed under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319362" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664293 - prov 146" atidlm:xmlId="DLM1666984" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 146</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800306" atidlm:sourceXmlId="DLM1666984" atidlm:targetKey="14064218" atidlm:targetXmlId="DLM1662718" atidlm:targetName="prov 146" atidlm:sourceName="DLM1666984" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662708" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>The chief executive must pay interest on any refunded excess emissions penalty and interest calculated in accordance with the following formula:</text>
                <eqn id="DLM1662709">
                  <eqn-line>((X × Y) ÷ 365) × Z</eqn-line>
                  <para>
                    <text>where—</text>
                  </para>
                  <variable-def>
                    <variable>X</variable>
                    <para>
                      <text>is the number of days in the period that—</text>
                      <label-para skeleton="no">
                        <label auto.number="no" denominator="yes">a</label>
                        <para>
                          <text>commences on the day on which the relevant penalty is lodged to the credit of the chief executive; and</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no">
                        <label auto.number="no" denominator="yes">b</label>
                        <para>
                          <text>ends on the day on which the relevant penalty is refunded by the chief executive; and</text>
                        </para>
                      </label-para>
                    </para>
                  </variable-def>
                  <variable-def>
                    <variable>Y</variable>
                    <para>
                      <text>is the amount of penalty and interest that, having been paid, is caused to be refunded in accordance with the outcome of a successful appeal; and</text>
                    </para>
                  </variable-def>
                  <variable-def>
                    <variable>Z</variable>
                    <para>
                      <text>is the rate of interest specified by the Governor-General by Order in Council made under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319363" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664294 - prov 137" atidlm:xmlId="DLM1666985" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 137(2)(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800307" atidlm:sourceXmlId="DLM1666985" atidlm:targetKey="14064017" atidlm:targetXmlId="DLM1662706" atidlm:targetName="prov 137" atidlm:sourceName="DLM1666985" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662709" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                    </para>
                  </variable-def>
                </eqn>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665953"><amended-provision>Section 138</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319364" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666986" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800308" atidlm:sourceXmlId="DLM1666986" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666986" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665953" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662710" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">139</label>
          <heading>Liability of body corporate</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>If, in the course of proceedings against a body corporate for an offence under this Part, it is necessary to establish the state of mind of the body corporate, it is sufficient to show that a director, employee, or agent of the body corporate, acting within the scope of the person’s actual or apparent authority, had that state of mind.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665954"><amended-provision>Section 139</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319365" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666987" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800309" atidlm:sourceXmlId="DLM1666987" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666987" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665954" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662711" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">140</label>
          <heading>Liability of directors and managers of companies</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>If a body corporate is convicted of an offence under this Part, every director and every person concerned in the management of the body corporate is also guilty of that offence if it is proved that—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the act or omission that constituted the offence took place with the authority, permission, or consent of the director or person; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the director or person knew that the offence was to be, or was being, committed and failed to take all reasonable steps to prevent or stop it.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665955"><amended-provision>Section 140</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319366" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666988" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800310" atidlm:sourceXmlId="DLM1666988" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666988" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665955" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662712" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">141</label>
          <heading>Liability of companies and persons for actions of director, agent, or employee</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>Any act or omission on behalf of a body corporate or other person (the <emphasis style="bold">principal</emphasis>) by a director, agent, or employee of the principal is to be treated for the purposes of this Act as being also the act or omission of the principal.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>Despite subsection (1), if a principal is charged under this Part in relation to the act or omission of an agent for an offence against any of <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319367" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664295 - prov 132" atidlm:xmlId="DLM1666989" atidlm:state="UNCHANGED"><atidlm:linkcontent>sections 132(1)(c)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800311" atidlm:sourceXmlId="DLM1666989" atidlm:targetKey="14064086" atidlm:targetXmlId="DLM1662701" atidlm:targetName="prov 132" atidlm:sourceName="DLM1666989" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662712" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> to (f) or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319368" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664296 - prov 133" atidlm:xmlId="DLM1666990" atidlm:state="UNCHANGED"><atidlm:linkcontent>133</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800312" atidlm:sourceXmlId="DLM1666990" atidlm:targetKey="14064098" atidlm:targetXmlId="DLM1662702" atidlm:targetName="prov 133" atidlm:sourceName="DLM1666990" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662712" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, it is a defence to the charge if the principal proves that the principal took all reasonable steps to prevent the commission of the offence or the commission of offences of that kind.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665956"><amended-provision>Section 141</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319369" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666991" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800313" atidlm:sourceXmlId="DLM1666991" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666991" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665956" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662713" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">142</label>
          <heading>Limitation period for commencement of proceedings</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>Despite <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319370" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664297 - prov 14" atidlm:xmlId="DLM1666992" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 14</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800314" atidlm:sourceXmlId="DLM1666992" atidlm:targetKey="2038878" atidlm:targetXmlId="DLM311191" atidlm:targetName="prov 14" atidlm:sourceName="DLM1666992" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM311185.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662713" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> of the Summary Proceedings Act 1957, an information for an offence against—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319371" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664298 - prov 131" atidlm:xmlId="DLM1666993" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 131</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800315" atidlm:sourceXmlId="DLM1666993" atidlm:targetKey="14064138" atidlm:targetXmlId="DLM1662700" atidlm:targetName="prov 131" atidlm:sourceName="DLM1666993" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662713" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319372" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664299 - prov 132" atidlm:xmlId="DLM1666994" atidlm:state="UNCHANGED"><atidlm:linkcontent>132(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800316" atidlm:sourceXmlId="DLM1666994" atidlm:targetKey="14064086" atidlm:targetXmlId="DLM1662701" atidlm:targetName="prov 132" atidlm:sourceName="DLM1666994" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662713" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, (b), (g), (h), or (i)</citation> may be laid at any time within 2 years from the time when the matter of the information arose:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319373" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664300 - prov 129" atidlm:xmlId="DLM1666995" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 129</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800317" atidlm:sourceXmlId="DLM1666995" atidlm:targetKey="14064129" atidlm:targetXmlId="DLM1662698" atidlm:targetName="prov 129" atidlm:sourceName="DLM1666995" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662713" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319374" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664301 - prov 130" atidlm:xmlId="DLM1666996" atidlm:state="UNCHANGED"><atidlm:linkcontent>130</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800318" atidlm:sourceXmlId="DLM1666996" atidlm:targetKey="14064133" atidlm:targetXmlId="DLM1662699" atidlm:targetName="prov 130" atidlm:sourceName="DLM1666996" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662713" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319375" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664302 - prov 132" atidlm:xmlId="DLM1666997" atidlm:state="UNCHANGED"><atidlm:linkcontent>132(c)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800320" atidlm:sourceXmlId="DLM1666997" atidlm:targetKey="14064086" atidlm:targetXmlId="DLM1662701" atidlm:targetName="prov 132" atidlm:sourceName="DLM1666997" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662713" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> to (f)</citation> may be commenced at any time within 7 years from the time when the matter of the information arose.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665957"><amended-provision>Section 142</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319376" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1666998" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800321" atidlm:sourceXmlId="DLM1666998" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1666998" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665957" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662714" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">143</label>
          <heading>Evidence in proceedings</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>In any proceedings for an offence against this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319377" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664303 - part  5 Sector specific provisions" atidlm:xmlId="DLM1666999" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800322" atidlm:sourceXmlId="DLM1666999" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1666999" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662714" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, a certificate or document (including an electronic copy) of any of the following kinds is admissible in evidence and, in the absence of proof to the contrary, is sufficient evidence of the matter stated in the certificate or the document, as the case may require:</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>a certificate purporting to be signed by the chief executive, or by a delegate of the chief executive, to the effect that, at any specified date or period,—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>a named person is or was, or is not or was not, an enforcement officer or a person or organisation recognised under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319378" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664304 - prov 92" atidlm:xmlId="DLM1667000" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 92</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800323" atidlm:sourceXmlId="DLM1667000" atidlm:targetKey="14063869" atidlm:targetXmlId="DLM1662657" atidlm:targetName="prov 92" atidlm:sourceName="DLM1667000" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662714" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>a person was, or was not, registered as a participant in relation to an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319379" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664305 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1667001" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800324" atidlm:sourceXmlId="DLM1667001" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1667001" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662714" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>a certificate purporting to be signed by any person authorised to delegate to any person, or to persons of any kind or description, the exercise of any power or the  performance of any function under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319380" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664306 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667002" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800325" atidlm:sourceXmlId="DLM1667002" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667002" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662714" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, stating that the person has delegated—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>the exercise of the power or the performance of the function specified in the certificate to the person specified in the certificate; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>the exercise of the power or the performance of the function specified in the certificate to persons of a kind or description specified in the certificate, and that a named person specified in the certificate is a person of that kind or description.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The production of a certificate or document purporting to be a certificate to which subsection (1) applies is prima facie evidence that it is such a certificate or document, without proof of—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the signature of the person purporting to have signed the document; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the document’s nature.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665958"><amended-provision>Section 143</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319381" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667003" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800326" atidlm:sourceXmlId="DLM1667003" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667003" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665958" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
      </subpart>
      <subpart id="DLM1662715" in.amend="false" irdnumbering="no" toc="yes" act.no="40" act.type="public" date.as.at="2008-09-26" date.assent="2002-11-18" date.first.valid="2007-09-18" stage="reprint" year="2002">
        <label auto.number="no" denominator="yes">5</label>
        <heading>Review and appeal provisions</heading>
        <notes>
          <history>
            <history-note id="DLM1665959"><amended-provision>Subpart 5</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319382" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667004" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800327" atidlm:sourceXmlId="DLM1667004" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667004" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665959" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
          </history>
        </notes>
        <prov id="DLM1662716" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">144</label>
          <heading>Request for review of decisions</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>A person affected by a decision of the chief executive under a provision in this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319383" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664307 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667005" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800328" atidlm:sourceXmlId="DLM1667005" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667005" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662716" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> who is dissatisfied with the decision may, by notice to the chief executive within the period of 30 days after receiving notice of the decision, or within any further period that the chief executive allows, request the chief executive to review the decision.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The request must set out the grounds on which it is believed that the original decision should be reviewed.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>For the purposes of a review, the chief executive may require the person making the request for review to supply information additional to that contained in the request.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>Following a review, the chief executive may confirm, revoke, or vary the decision in the manner that the chief executive thinks fit.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>The decision requested to be reviewed remains valid unless and until altered by the chief executive.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">6</label>
              <para>
                <text>The chief executive must, as soon as practicable, give notice to the person who requested the review of the decision on the review, and of the reasons for it.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">7</label>
              <para>
                <text>A decision by the chief executive under this section is final, unless determined otherwise by a court under an appeal under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319384" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664308 - prov 145" atidlm:xmlId="DLM1667006" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 145</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800329" atidlm:sourceXmlId="DLM1667006" atidlm:targetKey="14064214" atidlm:targetXmlId="DLM1662717" atidlm:targetName="prov 145" atidlm:sourceName="DLM1667006" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662716" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319385" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664309 - prov 146" atidlm:xmlId="DLM1667007" atidlm:state="UNCHANGED"><atidlm:linkcontent>146</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800330" atidlm:sourceXmlId="DLM1667007" atidlm:targetKey="14064218" atidlm:targetXmlId="DLM1662718" atidlm:targetName="prov 146" atidlm:sourceName="DLM1667007" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662716" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">8</label>
              <para>
                <text>This section does not apply to any decision that the chief executive makes under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319386" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664310 - prov 90" atidlm:xmlId="DLM1667008" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 90</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800331" atidlm:sourceXmlId="DLM1667008" atidlm:targetKey="14063847" atidlm:targetXmlId="DLM1662655" atidlm:targetName="prov 90" atidlm:sourceName="DLM1667008" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662716" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> or of the chief executive in relation to emissions rulings (including a decision to decline making a ruling) under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319387" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664311 - prov 107" atidlm:xmlId="DLM1667009" atidlm:state="UNCHANGED"><atidlm:linkcontent>sections 107</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800332" atidlm:sourceXmlId="DLM1667009" atidlm:targetKey="14063757" atidlm:targetXmlId="DLM1662674" atidlm:targetName="prov 107" atidlm:sourceName="DLM1667009" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662716" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> to <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319388" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664312 - prov 117" atidlm:xmlId="DLM1667010" atidlm:state="UNCHANGED"><atidlm:linkcontent>117</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800333" atidlm:sourceXmlId="DLM1667010" atidlm:targetKey="14063741" atidlm:targetXmlId="DLM1662684" atidlm:targetName="prov 117" atidlm:sourceName="DLM1667010" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662716" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665960"><amended-provision>Section 144</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319389" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667011" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800334" atidlm:sourceXmlId="DLM1667011" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667011" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665960" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662717" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">145</label>
          <heading>Right of appeal to District Court</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>A person has a right of appeal to a District Court if affected by a decision of the chief executive under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319390" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664313 - prov 144" atidlm:xmlId="DLM1667012" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 144</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800335" atidlm:sourceXmlId="DLM1667012" atidlm:targetKey="14064210" atidlm:targetXmlId="DLM1662716" atidlm:targetName="prov 144" atidlm:sourceName="DLM1667012" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662717" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The court may confirm, reverse, or modify the decision appealed against.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>Every decision appealed against under this section continues in force pending the determination of the appeal, and no person is excused from complying with any of the provisions of this Act on the ground that any appeal is pending.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665961"><amended-provision>Section 145</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319391" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667013" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800336" atidlm:sourceXmlId="DLM1667013" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667013" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665961" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662718" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">146</label>
          <heading>Appeals to High Court on questions of law only</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>If a party to any proceedings before the District Court under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319392" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664314 - prov 145" atidlm:xmlId="DLM1667014" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 145</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800337" atidlm:sourceXmlId="DLM1667014" atidlm:targetKey="14064214" atidlm:targetXmlId="DLM1662717" atidlm:targetName="prov 145" atidlm:sourceName="DLM1667014" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662718" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is dissatisfied with any determination of the court as being erroneous in point of law, the party may appeal to the High Court by way of case stated for the opinion of the court on a question of law only.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665962"><amended-provision>Section 146</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319393" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667015" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800338" atidlm:sourceXmlId="DLM1667015" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667015" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665962" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
      </subpart>
      <subpart id="DLM1662719" in.amend="false" irdnumbering="no" toc="yes" act.no="40" act.type="public" date.as.at="2008-09-26" date.assent="2002-11-18" date.first.valid="2007-09-18" stage="reprint" year="2002">
        <label auto.number="no" denominator="yes">6</label>
        <heading>Miscellaneous provisions</heading>
        <notes>
          <history>
            <history-note id="DLM1665963"><amended-provision>Subpart 6</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319394" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667016" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800339" atidlm:sourceXmlId="DLM1667016" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667016" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665963" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
          </history>
        </notes>
        <prov id="DLM1662720" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">147</label>
          <heading>Giving of notices by chief executive</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>This section applies if this Act requires the chief executive to give a notice to a person.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If this section applies, the chief executive—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>must give the notice in writing to—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>the person; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>a representative authorised to act on behalf of the person; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>may give notice by—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>personal delivery to a person that is not a body corporate:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>personal delivery to a person that is a body corporate, if the personal delivery is made to the person’s office during working hours:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>an electronic means of communication to the person, if the chief executive complies with the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319395" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664315 - Electronic Transactions Act 2002" atidlm:xmlId="DLM1667017" atidlm:state="UNCHANGED"><atidlm:linkcontent>Electronic Transactions Act 2002</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800340" atidlm:sourceXmlId="DLM1667017" atidlm:targetKey="857807" atidlm:targetXmlId="DLM154184" atidlm:targetName="Electronic Transactions Act 2002" atidlm:sourceName="DLM1667017" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM154184.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662720" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iv</label>
                      <para>
                        <text>post to—</text>
                        <label-para skeleton="no">
                          <label auto.number="no" denominator="yes">A</label>
                          <para>
                            <text>the street address of the person’s usual or last known place of residence; or</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no">
                          <label auto.number="no" denominator="yes">B</label>
                          <para>
                            <text>the street address of any of the person’s usual or last known places of business; or</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no">
                          <label auto.number="no" denominator="yes">C</label>
                          <para>
                            <text>any other address, if the person has notified the chief executive that the person accepts notices at the address.</text>
                          </para>
                        </label-para>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>A notice given by post under subsection (2)(b)(iv) is to be treated as having been given at the time the notice would have been delivered in the ordinary course of the post.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665964"><amended-provision>Section 147</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319396" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667018" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800341" atidlm:sourceXmlId="DLM1667018" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667018" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665964" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662721" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">148</label>
          <heading>Giving of notices to chief executive</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>This section applies if this Act requires a person to give a notice to the chief executive.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If this section applies, the person must—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>give the notice in writing; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>may—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>give the notice to the office of the chief executive’s department designated by the chief executive by notice in the <emphasis style="italic">Gazette</emphasis>:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>give the notice by—</text>
                        <label-para skeleton="no">
                          <label auto.number="no" denominator="yes">A</label>
                          <para>
                            <text>personal delivery, if the personal delivery is made during working hours:</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no">
                          <label auto.number="no" denominator="yes">B</label>
                          <para>
                            <text>an electronic means of communication, if the person complies with the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319397" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664316 - Electronic Transactions Act 2002" atidlm:xmlId="DLM1667019" atidlm:state="UNCHANGED"><atidlm:linkcontent>Electronic Transactions Act 2002</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800342" atidlm:sourceXmlId="DLM1667019" atidlm:targetKey="857807" atidlm:targetXmlId="DLM154184" atidlm:targetName="Electronic Transactions Act 2002" atidlm:sourceName="DLM1667019" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM154184.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662721" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no">
                          <label auto.number="no" denominator="yes">C</label>
                          <para>
                            <text>post to the post office box number for the office.</text>
                          </para>
                        </label-para>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>A notice given by post under subsection (2)(b)(ii)(C) is treated as having been given at the time the notice would have been delivered in the ordinary course of the post.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665965"><amended-provision>Section 148</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319398" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667020" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800343" atidlm:sourceXmlId="DLM1667020" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667020" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665965" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662722" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">149</label>
          <heading>Sharing information</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The purpose of this section is to facilitate the exchange of information between any person with functions or powers under this Act, the Registrar, and the inventory agency.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>A person referred to in <citation jurisdiction="nz">subsection (1)</citation> (<def-term id="DLM1662723">person A</def-term>) must provide information to another person referred to in that subsection (<def-term id="DLM1662724">person B</def-term>) if the information—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>is requested by person B; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>is required by person B to assist person B to carry out his or her functions under this Act.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665966"><amended-provision>Section 149</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319400" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667021" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800344" atidlm:sourceXmlId="DLM1667021" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667021" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665966" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662725" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">150</label>
          <heading>Formation of consolidated group</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>Any 2 or more participants who are members of a group may, in respect of any activity or activities listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319401" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664317 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1667022" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800345" atidlm:sourceXmlId="DLM1667022" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1667022" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662725" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319402" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664318 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1667023" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800346" atidlm:sourceXmlId="DLM1667023" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1667023" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662725" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, elect to form and be treated as a consolidated group for the purposes of this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319403" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664319 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667024" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800347" atidlm:sourceXmlId="DLM1667024" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667024" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662725" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>A consolidated group may, in addition to participants who are members of the group, include a member of the group that is not a participant, if that entity is to act as the nominated entity.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>An election under subsection (1) must be made by giving notice to the chief executive in the prescribed form.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>A notice given under subsection (3) must—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>include—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>the names of each of the entities that are to be members of the consolidated group (and contact details of any member that is not registered as a participant); and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>the activities in respect of which the members elect to be treated as a consolidated group; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>nominate one of the entities listed in the notice (the <emphasis style="bold">nominated entity</emphasis>) as the agent of the consolidated group in respect of the activities specified in the notice and this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319404" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664320 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667025" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800348" atidlm:sourceXmlId="DLM1667025" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667025" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662725" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>contain an agreement by each entity listed in the notice as a member of the consolidated group—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>to be jointly and severally liable with the other members of the consolidated group for any obligations under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319405" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664321 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667026" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800349" atidlm:sourceXmlId="DLM1667026" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667026" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662725" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in respect of emissions and removals resulting from the activities specified in the notice; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>to the transfer to the consolidated group's holding account on behalf of the group of any units to which any member of the consolidated group may become entitled in respect of any removal activity listed in the notice.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>The chief executive must acknowledge the formation of a consolidated group by notice to all members of the group given within 1 month after the chief executive’s receipt of a notice under subsection (3).</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">6</label>
              <para>
                <text>If 2 or more participants have elected under subsection (1) to form a consolidated group, those participants must be treated for the purposes of this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319406" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664322 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667027" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800350" atidlm:sourceXmlId="DLM1667027" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667027" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662725" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> as being members of a consolidated group,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>if notice of the formation of the group is received by the chief executive by 30 September in any year, from the beginning of the following year:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>if notice of the formation of the group is received by the chief executive after 30 September in any year, from the beginning of the year following the next year.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">7</label>
              <para>
                <text>To avoid doubt, a participant may be a member of more than 1 consolidated group in relation to different activities.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665967"><amended-provision>Section 150</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319407" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667028" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800351" atidlm:sourceXmlId="DLM1667028" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667028" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665967" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662726" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">151</label>
          <heading>Changes to consolidated groups</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>If at any time 2 or more participants who are members of a group have formed a consolidated group, and at least 1 participant remains a member of the consolidated group, any other participant (or, in the circumstances specified in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319408" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664323 - prov 150" atidlm:xmlId="DLM1667029" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 150(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800352" atidlm:sourceXmlId="DLM1667029" atidlm:targetKey="14064652" atidlm:targetXmlId="DLM1662725" atidlm:targetName="prov 150" atidlm:sourceName="DLM1667029" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662726" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, any other entity that is a member of the group) may elect to join and be treated as a member of the consolidated group by giving notice to the chief executive in a form that the chief executive approves.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>A notice given under subsection (1) must—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>include—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>the name of the entity that elects to join the consolidated group (and the entity’s contact details if it is not registered as a participant) and sufficient information for the chief executive to identify the consolidated group that is to be joined; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>if the entity is a participant, the activity or activities in respect of which the entity elects to be treated as a member of that consolidated group; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>contain the agreement of the entity—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>to be jointly and severally liable with the other members of the consolidated group for any obligations under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319409" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664324 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667030" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800353" atidlm:sourceXmlId="DLM1667030" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667030" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662726" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in respect of emissions and removals resulting from the activities of the members of the group; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>if the entity is a participant, to the transfer to the consolidated group’s holding account on behalf of the group of any units to which the entity may become entitled in relation to any removal activities specified in the notice; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>contain the agreement of every existing member of the consolidated group—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>to be jointly and severally liable with the other members of the group for any obligations under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319410" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664325 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667031" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800354" atidlm:sourceXmlId="DLM1667031" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667031" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662726" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in respect of emissions and removals resulting from the activities of the joining entity; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>to the transfer to the consolidated group’s holding account, on behalf of the group, of any units to which the joining entity may become entitled in respect of the activity or activities of that entity specified in the notice.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The chief executive must acknowledge the joining of a member to a consolidated group by notice to all members of the group given within 1 month after the chief executive’s receipt of a notice under subsection (1).</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>If a participant has elected under subsection (1) to join a consolidated group, that participant must be treated for the purposes of this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319411" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664326 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667032" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800355" atidlm:sourceXmlId="DLM1667032" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667032" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662726" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> as being a member of that consolidated group,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>if notice of the election to join the group is received by the chief executive by 30 September in any year, from the beginning of the following year:</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>if notice of the election to join the group is received by the chief executive after 30 September in any year, from the beginning of the year following the next year.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>If an entity referred to in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319412" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664327 - prov 150" atidlm:xmlId="DLM1667033" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 150(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800356" atidlm:sourceXmlId="DLM1667033" atidlm:targetKey="14064652" atidlm:targetXmlId="DLM1662725" atidlm:targetName="prov 150" atidlm:sourceName="DLM1667033" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662726" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> has elected by notice under subsection (1) to join a consolidated group, that entity must be treated for the purposes of this Part as being a member of that consolidated group from the date of receipt by the chief executive of the notice, or from any later date that may be specified in the notice.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665968"><amended-provision>Section 151</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319413" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667034" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800357" atidlm:sourceXmlId="DLM1667034" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667034" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665968" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662727" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">152</label>
          <heading>Nominated entities</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The nominated entity for a consolidated group at any time is to be treated for the purposes of this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319414" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664328 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667035" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800358" atidlm:sourceXmlId="DLM1667035" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667035" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662727" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> as the agent at that time of the consolidated group, and of each entity that is at that time a member of the consolidated group, except where this Act otherwise expressly provides or the context otherwise requires.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>No entity is at any time a nominated entity for a consolidated group unless, at the time, the entity is a member of the consolidated group.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>An entity that is a nominated entity for a consolidated group may give notice to the chief executive, in a form that the chief executive approves, that—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the entity is to cease to be the agent for the consolidated group; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>another member entity is to become the agent for the consolidated group.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>If an entity gives notice under subsection (3), then, from the date of receipt by the chief executive of the notice, or from a later date that may be specified in the notice,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the notifying entity ceases to be the agent for the consolidated group; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the other entity becomes the agent (nominated entity) for the consolidated group.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665969"><amended-provision>Section 152</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319415" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667036" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800360" atidlm:sourceXmlId="DLM1667036" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667036" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665969" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662728" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">153</label>
          <heading>Effect of being member of consolidated group</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The nominated entity of a consolidated group must—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>have a holding account in the name of the consolidated group for the purposes of meeting the members’ obligations under this Part and <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319416" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664329 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667037" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800361" atidlm:sourceXmlId="DLM1667037" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667037" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662728" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>record in that holding account the names of all the members of the consolidated group; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>submit a single annual emissions return for the consolidated group in respect of a year, which must—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>meet the requirements of <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319417" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664330 - prov 65" atidlm:xmlId="DLM1667038" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 65(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800362" atidlm:sourceXmlId="DLM1667038" atidlm:targetKey="14063267" atidlm:targetXmlId="DLM1662495" atidlm:targetName="prov 65" atidlm:sourceName="DLM1667038" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662728" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in respect of the activities listed in the notice under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319418" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664331 - prov 150" atidlm:xmlId="DLM1667039" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 150(4)(a)(ii)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800363" atidlm:sourceXmlId="DLM1667039" atidlm:targetKey="14064652" atidlm:targetXmlId="DLM1662725" atidlm:targetName="prov 150" atidlm:sourceName="DLM1667039" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662728" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319419" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664332 - prov 151" atidlm:xmlId="DLM1667040" atidlm:state="UNCHANGED"><atidlm:linkcontent>151(2)(a)(ii)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800364" atidlm:sourceXmlId="DLM1667040" atidlm:targetKey="14064629" atidlm:targetXmlId="DLM1662726" atidlm:targetName="prov 151" atidlm:sourceName="DLM1667040" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662728" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> carried out by each member of the consolidated group:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>be signed by the nominated entity in accordance with <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319420" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664333 - prov 65" atidlm:xmlId="DLM1667041" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 65(2)(f)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800365" atidlm:sourceXmlId="DLM1667041" atidlm:targetKey="14063267" atidlm:targetXmlId="DLM1662495" atidlm:targetName="prov 65" atidlm:sourceName="DLM1667041" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662728" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> on behalf of the consolidated group.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>Each member of a consolidated group is jointly and severally liable to surrender the amount of units assessed in relation to the consolidated group in any year, and that joint and several liability is in substitution for any liability of those members under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319421" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664334 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667042" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800366" atidlm:sourceXmlId="DLM1667042" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667042" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662728" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> individually in respect of units to be surrendered for that year (to the extent that the surrender obligation relates to a period when the entity is a member of the consolidated group).</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The liability of every member of the consolidated group to surrender units in respect of any year is met by the transfer of the units assessed in relation to the consolidated group from the consolidated group’s holding account to a surrender account designated by the chief executive.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>Each member of a consolidated group is jointly entitled to any New Zealand units assessed in relation to the removal activities of the consolidated group in any year, and that joint entitlement is in substitution for any entitlement of those members under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319422" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664335 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667043" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800367" atidlm:sourceXmlId="DLM1667043" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667043" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662728" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> individually in respect of units to be transferred for that year (to the extent that the entitlement relates to a period when the entity is a member of the consolidated group). </text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>The entitlement of every member of the consolidated group to be transferred units for removal activities in respect of any year must be met by the transfer of the number of units assessed in relation to the consolidated group to the consolidated group’s holding account.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">6</label>
              <para>
                <text>This section—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>does not prevent the nominated entity submitting—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>a quarterly emissions return under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319423" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664336 - prov 66" atidlm:xmlId="DLM1667044" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 66</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800368" atidlm:sourceXmlId="DLM1667044" atidlm:targetKey="14063275" atidlm:targetXmlId="DLM1662496" atidlm:targetName="prov 66" atidlm:sourceName="DLM1667044" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662728" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> for other removal activities of the consolidated group; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>submitting an emissions return under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319424" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664337 - prov 118" atidlm:xmlId="DLM1667045" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 118</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800369" atidlm:sourceXmlId="DLM1667045" atidlm:targetKey="14063678" atidlm:targetXmlId="DLM1662686" atidlm:targetName="prov 118" atidlm:sourceName="DLM1667045" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662728" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in respect of a participant who is a member of the consolidated group; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>applies with any necessary modifications to the period of an emissions return in either of those circumstances.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665970"><amended-provision>Section 153</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319425" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667046" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800370" atidlm:sourceXmlId="DLM1667046" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667046" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665970" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662729" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">154</label>
          <heading>Emissions returns by consolidated group in respect of activities in Part 1 of Schedule 4</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The nominated entity of a consolidated group that has been formed in respect of an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319426" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664338 - head1 Part 1 Forestry removal activities" atidlm:xmlId="DLM1667047" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800371" atidlm:sourceXmlId="DLM1667047" atidlm:targetKey="14066643" atidlm:targetXmlId="DLM1662865" atidlm:targetName="head1 Part 1 Forestry removal activities" atidlm:sourceName="DLM1667047" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662729" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of Schedule 4</citation>—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>may submit a single emissions return under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319427" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664341 - prov 189" atidlm:xmlId="DLM1667048" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 189(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800372" atidlm:sourceXmlId="DLM1667048" atidlm:targetKey="14065045" atidlm:targetXmlId="DLM1662782" atidlm:targetName="prov 189" atidlm:sourceName="DLM1667048" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662729" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in respect of any activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319428" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664339 - head1 Part 1 Forestry removal activities" atidlm:xmlId="DLM1667049" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800373" atidlm:sourceXmlId="DLM1667049" atidlm:targetKey="14066643" atidlm:targetXmlId="DLM1662865" atidlm:targetName="head1 Part 1 Forestry removal activities" atidlm:sourceName="DLM1667049" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662729" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of Schedule 4</citation> carried out by a member in a year; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>must submit a single emissions return in respect of any activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319429" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664340 - head1 Part 1 Forestry removal activities" atidlm:xmlId="DLM1667050" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800374" atidlm:sourceXmlId="DLM1667050" atidlm:targetKey="14066643" atidlm:targetXmlId="DLM1662865" atidlm:targetName="head1 Part 1 Forestry removal activities" atidlm:sourceName="DLM1667050" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662729" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of Schedule 4</citation> carried out by any members when required to do so by <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319430" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664342 - prov 189" atidlm:xmlId="DLM1667051" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 189(4)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800375" atidlm:sourceXmlId="DLM1667051" atidlm:targetKey="14065045" atidlm:targetXmlId="DLM1662782" atidlm:targetName="prov 189" atidlm:sourceName="DLM1667051" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662729" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>must submit any emissions return required by <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319431" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664343 - prov 191" atidlm:xmlId="DLM1667052" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 191</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800376" atidlm:sourceXmlId="DLM1667052" atidlm:targetKey="14065013" atidlm:targetXmlId="DLM1662790" atidlm:targetName="prov 191" atidlm:sourceName="DLM1667052" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662729" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319432" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664344 - prov 193" atidlm:xmlId="DLM1667053" atidlm:state="UNCHANGED"><atidlm:linkcontent>193</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800377" atidlm:sourceXmlId="DLM1667053" atidlm:targetKey="14065218" atidlm:targetXmlId="DLM1662795" atidlm:targetName="prov 193" atidlm:sourceName="DLM1667053" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662761.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662729" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> on behalf of any member when a member is required to do so; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>must sign any emissions return referred to above in accordance with <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319433" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664345 - prov 65" atidlm:xmlId="DLM1667054" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 65(2)(f)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800378" atidlm:sourceXmlId="DLM1667054" atidlm:targetKey="14063267" atidlm:targetXmlId="DLM1662495" atidlm:targetName="prov 65" atidlm:sourceName="DLM1667054" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662729" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> on behalf of the consolidated group.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319434" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664346 - prov 153" atidlm:xmlId="DLM1667055" atidlm:state="UNCHANGED"><atidlm:linkcontent>Section 153(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800379" atidlm:sourceXmlId="DLM1667055" atidlm:targetKey="14064616" atidlm:targetXmlId="DLM1662728" atidlm:targetName="prov 153" atidlm:sourceName="DLM1667055" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662729" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> to (5)</citation> apply to the liability to surrender units or entitlement to be transferred units in relation to an emissions return referred to in this section as if the references to a year were a reference to the period covered by the emissions return.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665971"><amended-provision>Section 154</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319435" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667056" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800380" atidlm:sourceXmlId="DLM1667056" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667056" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665971" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662730" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">155</label>
          <heading>Ceasing to be member of consolidated group</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>An entity that is a member of a consolidated group ceases to be a member of the consolidated group if—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the entity so elects, by notice to the chief executive in a form that the chief executive approves; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the entity ceases to be a member of the group in respect of which it is eligible to be a member of the consolidated group; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>the entity ceases to be a participant, unless the entity is the nominated entity; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>the entity ceases to be the nominated entity and is not a participant; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">e</label>
                  <para>
                    <text>the entity is a member of a consolidated group that has ceased to have a nominated entity.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>An entity is treated as having ceased to be a member of a consolidated group,—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>if subsection (1)(a) applies, with effect from the date of receipt by the chief executive of the notice of election to cease to be a member of the consolidated group; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>if subsection (1)(b) applies, with effect from the date on which the entity ceased to be a member of the group in respect of which it is eligible to be a member of the consolidated group; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>if subsection (1)(c) applies, with effect from the date the participant’s name is removed from the register of participants under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319436" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664347 - prov 58" atidlm:xmlId="DLM1667057" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 58</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800381" atidlm:sourceXmlId="DLM1667057" atidlm:targetKey="14063343" atidlm:targetXmlId="DLM1662488" atidlm:targetName="prov 58" atidlm:sourceName="DLM1667057" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662730" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319437" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664348 - prov 59" atidlm:xmlId="DLM1667058" atidlm:state="UNCHANGED"><atidlm:linkcontent>59</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800382" atidlm:sourceXmlId="DLM1667058" atidlm:targetKey="14063290" atidlm:targetXmlId="DLM1662489" atidlm:targetName="prov 59" atidlm:sourceName="DLM1667058" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662730" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>if subsection (1)(d) applies, with effect from the date of receipt by the chief executive of the notice under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319438" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664349 - prov 152" atidlm:xmlId="DLM1667059" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 152(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800383" atidlm:sourceXmlId="DLM1667059" atidlm:targetKey="14064587" atidlm:targetXmlId="DLM1662727" atidlm:targetName="prov 152" atidlm:sourceName="DLM1667059" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662730" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> notifying that the entity has ceased to be the nominated entity for the group; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">e</label>
                  <para>
                    <text>if subsection (1)(e) applies, with effect from the date on which the consolidated group ceased to have a nominated entity.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>Subsection (1)(e) does not apply if—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the nominated entity ceases to be the nominated entity by reason of being liquidated; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>within 20 working days after that liquidation, or within such further period as the chief executive may allow, the other entities in the consolidated group have selected another nominated entity and notified the chief executive accordingly (in which case the selected entity is treated as the nominated entity with effect from the time of the liquidation).</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>An entity that ceases to be a member of a group in respect of which it is eligible to be a member of the consolidated group, or is a member of a consolidated group that ceases to have a nominated entity, must as soon as practicable give notice to the chief executive of this change of circumstances.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>The chief executive must acknowledge the cessation of membership of a member of a consolidated group by notice to that member and the other members of the consolidated group given within 1 month of—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the chief executive receiving a notice under— </text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>subsection (1)(a); or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319439" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664350 - prov 152" atidlm:xmlId="DLM1667060" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 152(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800384" atidlm:sourceXmlId="DLM1667060" atidlm:targetKey="14064587" atidlm:targetXmlId="DLM1662727" atidlm:targetName="prov 152" atidlm:sourceName="DLM1667060" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662730" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text> the chief executive becoming aware that subsection (1)(b) or (e) applies; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>the member being removed from the register of participants under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319440" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664351 - prov 58" atidlm:xmlId="DLM1667061" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 58</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800385" atidlm:sourceXmlId="DLM1667061" atidlm:targetKey="14063343" atidlm:targetXmlId="DLM1662488" atidlm:targetName="prov 58" atidlm:sourceName="DLM1667061" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662730" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319441" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664352 - prov 59" atidlm:xmlId="DLM1667062" atidlm:state="UNCHANGED"><atidlm:linkcontent>59</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800386" atidlm:sourceXmlId="DLM1667062" atidlm:targetKey="14063290" atidlm:targetXmlId="DLM1662489" atidlm:targetName="prov 59" atidlm:sourceName="DLM1667062" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662730" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665972"><amended-provision>Section 155</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319442" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667063" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800387" atidlm:sourceXmlId="DLM1667063" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667063" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665972" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662731" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">156</label>
          <heading>Effect of ceasing to be member of consolidated group</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="no"/>
              <para>
                <text>If an entity ceases to be a member of a consolidated group, the entity—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>continues to be jointly and severally liable with other members of the consolidated group for any obligations under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319443" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664353 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667064" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800388" atidlm:sourceXmlId="DLM1667064" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667064" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662731" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in respect of emissions and removals from the activities of the members of the consolidated group, and jointly entitled to any units transferred for the removal activities of the consolidated group, during the period in which the entity was a member of the consolidated group; but</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>is not liable for any obligations under this Part or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319444" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664354 - part  5 Sector specific provisions" atidlm:xmlId="DLM1667065" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800389" atidlm:sourceXmlId="DLM1667065" atidlm:targetKey="14064802" atidlm:targetXmlId="DLM1662760" atidlm:targetName="part  5 Sector specific provisions" atidlm:sourceName="DLM1667065" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662760.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662731" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in respect of emissions and removals from the activities of other members of the group, or entitled to the benefit of any units transferred for the removal activities of other members of the group, for any period during which the entity is not a member of the consolidated group.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665973"><amended-provision>Section 156</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319445" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667066" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800390" atidlm:sourceXmlId="DLM1667066" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667066" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665973" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662732" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">157</label>
          <heading>Joint activities</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>This section applies where and to the extent that any 2 or more persons jointly carry out an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319446" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664355 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1667067" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800391" atidlm:sourceXmlId="DLM1667067" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1667067" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662732" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319447" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664356 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1667068" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800392" atidlm:sourceXmlId="DLM1667068" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1667068" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662732" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, including (but not limited to) in partnership, under an unincorporated joint venture, as trustees of a trust, or through joint ownership of land.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If this section applies, the persons who carry out the activity—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>are together the participant in respect of an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319448" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664357 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1667069" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800393" atidlm:sourceXmlId="DLM1667069" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1667069" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662732" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> or may together register as the participant in relation to an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319449" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664358 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1667070" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800394" atidlm:sourceXmlId="DLM1667070" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1667070" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662732" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>are jointly and severally liable for the obligations of a participant in respect of the activity and are jointly entitled to any benefits of a participant resulting from the activity; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>may, if there are 25 or more persons (other than trustees) who jointly carry out an activity that relates to owning land, be described on the register of participants under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319450" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664359 - prov 56" atidlm:xmlId="DLM1667071" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 56</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800395" atidlm:sourceXmlId="DLM1667071" atidlm:targetKey="14063312" atidlm:targetXmlId="DLM1662486" atidlm:targetName="prov 56" atidlm:sourceName="DLM1667071" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662732" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319451" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664360 - prov 57" atidlm:xmlId="DLM1667072" atidlm:state="UNCHANGED"><atidlm:linkcontent>57</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800396" atidlm:sourceXmlId="DLM1667072" atidlm:targetKey="14063337" atidlm:targetXmlId="DLM1662487" atidlm:targetName="prov 57" atidlm:sourceName="DLM1667072" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662732" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in a manner prescribed in regulations.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>To avoid doubt, this section does not apply to persons who jointly carry out an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319452" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664361 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1667073" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800397" atidlm:sourceXmlId="DLM1667073" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1667073" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662732" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> but would not if they carried out that activity individually be a participant under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319453" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664362 - prov 54" atidlm:xmlId="DLM1667074" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 54(1)(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800398" atidlm:sourceXmlId="DLM1667074" atidlm:targetKey="14063224" atidlm:targetXmlId="DLM1662483" atidlm:targetName="prov 54" atidlm:sourceName="DLM1667074" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662732" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665974"><amended-provision>Section 157</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319454" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667075" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800400" atidlm:sourceXmlId="DLM1667075" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667075" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665974" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662733" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">158</label>
          <heading>Compensation for participants where public works result in liability to surrender units</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>This section applies if a person becomes a participant in respect of an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319455" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664363 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1667076" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800401" atidlm:sourceXmlId="DLM1667076" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1667076" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662733" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> fter being required to carry out the activity as a result of the exercise of a power that relates to a public work.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If this section applies, the person who exercised the power must, to the extent that the participant is not compensated under any other Act, compensate the participant for any liability to surrender units that the participant incurs as a result of the exercise of the power.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>All claims for compensation under subsection (2) must, unless settled by agreement, be determined in the manner provided by the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319456" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664364 - Public Works Act 1981" atidlm:xmlId="DLM1667077" atidlm:state="UNCHANGED"><atidlm:linkcontent>Public Works Act 1981</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800402" atidlm:sourceXmlId="DLM1667077" atidlm:targetKey="254400" atidlm:targetXmlId="DLM45426" atidlm:targetName="Public Works Act 1981" atidlm:sourceName="DLM1667077" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM45426.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662733" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, and the provisions of that Act relating to compensation apply accordingly.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>For the purposes of this section, <def-term id="DLM1662734">public work</def-term> has the same meaning as in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319457" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664365 - prov 2" atidlm:xmlId="DLM1667078" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800403" atidlm:sourceXmlId="DLM1667078" atidlm:targetKey="254407" atidlm:targetXmlId="DLM45433" atidlm:targetName="prov 2" atidlm:sourceName="DLM1667078" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM45426.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662733" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> of the Public Works Act 1981.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665975"><amended-provision>Section 158</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319458" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667079" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800404" atidlm:sourceXmlId="DLM1667079" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667079" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665975" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662735" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">159</label>
          <heading>Chief executive must surrender or repay units for person who is in default or insolvent</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>This section applies if a person—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>is required to surrender or repay units and does not do so, or does not surrender or repay the total number of units required to be surrendered or repaid, within 1 year of the date of a penalty notice given under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319459" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664366 - prov 134" atidlm:xmlId="DLM1667080" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 134</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800405" atidlm:sourceXmlId="DLM1667080" atidlm:targetKey="14064044" atidlm:targetXmlId="DLM1662703" atidlm:targetName="prov 134" atidlm:sourceName="DLM1667080" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662735" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319460" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664367 - prov 136" atidlm:xmlId="DLM1667081" atidlm:state="UNCHANGED"><atidlm:linkcontent>136</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800406" atidlm:sourceXmlId="DLM1667081" atidlm:targetKey="14064073" atidlm:targetXmlId="DLM1662705" atidlm:targetName="prov 136" atidlm:sourceName="DLM1667081" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662735" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in relation to the units; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>is a participant and enters into an insolvency process.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>If this section applies the chief executive must purchase and surrender on the person’s behalf, or transfer to the designated Crown holding account on behalf of the person required to repay units, any units that—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the person has failed to surrender or repay after 1 year; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the insolvent participant would be required to surrender or repay under any other provision of this Act.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The cost of purchasing units, and any administrative costs incurred in their surrender or repayment under subsection (2), constitutes an unsecured debt to the Crown and is recoverable by the chief executive in a court of competent jurisdiction.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>For the purposes of this section, <def-term id="DLM1662736">insolvency process</def-term> means receivership under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319461" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664368 - Receiverships Act 1993" atidlm:xmlId="DLM1667082" atidlm:state="UNCHANGED"><atidlm:linkcontent>Receiverships Act 1993</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800407" atidlm:sourceXmlId="DLM1667082" atidlm:targetKey="1689233" atidlm:targetXmlId="DLM327640" atidlm:targetName="Receiverships Act 1993" atidlm:sourceName="DLM1667082" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM327640.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662735" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, liquidation under the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319462" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664369 - Companies Act 1993" atidlm:xmlId="DLM1667083" atidlm:state="UNCHANGED"><atidlm:linkcontent>Companies Act 1993</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800408" atidlm:sourceXmlId="DLM1667083" atidlm:targetKey="1637803" atidlm:targetXmlId="DLM319569" atidlm:targetName="Companies Act 1993" atidlm:sourceName="DLM1667083" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM319569.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662735" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or bankruptcy under the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319463" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664370 - Insolvency Act 2006" atidlm:xmlId="DLM1667084" atidlm:state="UNCHANGED"><atidlm:linkcontent>Insolvency Act 2006</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800409" atidlm:sourceXmlId="DLM1667084" atidlm:targetKey="2357407" atidlm:targetXmlId="DLM385298" atidlm:targetName="Insolvency Act 2006" atidlm:sourceName="DLM1667084" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM385298.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662735" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665976"><amended-provision>Section 159</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319464" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667085" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800410" atidlm:sourceXmlId="DLM1667085" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667085" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665976" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662737" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">160</label>
          <heading>Reviews of operation of emissions trading scheme</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The Minister responsible for the administration of this Act must initiate a review of the operation and effectiveness of the emissions trading scheme established by this Act to be completed no later than 12 months before the end of—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>the first commitment period; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>if the first commitment period has expired, the subsequent commitment period in which the review is initiated; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>if there is no subsequent commitment period,—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>the 5-year period commencing on 1 January 2013:</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>each subsequent 5-year period after the period specified in subparagraph (i).</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>Without limiting the scope of the review, a review under subsection (1) must consider—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>whether an amendment to this Act in relation to the emissions trading scheme is necessary or desirable; and </text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>whether New Zealand has undertaken, or is expected to undertake, any international obligations with respect to its emissions and removals that are different from or additional to any international obligations that New Zealand had undertaken when this section came into force, or since the last review under this section; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>the stringency of any of the international obligations specified in paragraph (b); and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>the contribution of the emissions trading scheme established under this Act to progress toward any targets that are in effect in accordance with <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319465" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664371 - prov 224" atidlm:xmlId="DLM1667086" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 224</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800411" atidlm:sourceXmlId="DLM1667086" atidlm:targetKey="14066210" atidlm:targetXmlId="DLM1662840" atidlm:targetName="prov 224" atidlm:sourceName="DLM1667086" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662837.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> at the time the review is initiated; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">e</label>
                  <para>
                    <text>the types of Kyoto units and overseas units that may be surrendered for compliance with the emissions trading scheme established by this Act; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">f</label>
                  <para>
                    <text>the operation of the commitment period reserve (if any); and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">g</label>
                  <para>
                    <text>potential for linkage of the emissions trading scheme established under this Act to other greenhouse gas emissions trading schemes; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">h</label>
                  <para>
                    <text>the appropriateness of any methodologies that are prescribed for calculating emissions and removals; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">i</label>
                  <para>
                    <text>whether it is necessary or desirable to—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>omit any of the activities from <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319466" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664372 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1667087" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800412" atidlm:sourceXmlId="DLM1667087" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1667087" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319467" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664373 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1667088" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800413" atidlm:sourceXmlId="DLM1667088" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1667088" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>add any additional removal activities to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319468" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664374 - head1 Part 2 Other removal activities" atidlm:xmlId="DLM1667089" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800414" atidlm:sourceXmlId="DLM1667089" atidlm:targetKey="14066624" atidlm:targetXmlId="DLM1662867" atidlm:targetName="head1 Part 2 Other removal activities" atidlm:sourceName="DLM1667089" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of Schedule 4</citation>; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>amend the level of participant opt-in thresholds in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319469" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664375 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1667090" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800415" atidlm:sourceXmlId="DLM1667090" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1667090" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">j</label>
                  <para>
                    <text>what consequential changes to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319470" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664376 - subpart 2 Issuance and allocation of New Zealand units" atidlm:xmlId="DLM1667091" atidlm:state="UNCHANGED"><atidlm:linkcontent>subpart 2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800416" atidlm:sourceXmlId="DLM1667091" atidlm:targetKey="14063580" atidlm:targetXmlId="DLM1662498" atidlm:targetName="subpart 2 Issuance and allocation of New Zealand units" atidlm:sourceName="DLM1667091" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662498.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> of this Part in respect of allocation plans are necessary or desirable having regard to—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>whether New Zealand has undertaken, or is expected to undertake, any international obligations with respect to its emissions and removals that are different from, or additional to, any international obligations that New Zealand had undertaken when this section came into force, or since the last review under this section; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>the stringency of any of the international obligations specified in subparagraph (i); and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>whether it is necessary or desirable to omit any activities from <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319471" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664377 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1667092" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedules 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800417" atidlm:sourceXmlId="DLM1667092" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1667092" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319472" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664378 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1667093" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800418" atidlm:sourceXmlId="DLM1667093" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1667093" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iv</label>
                      <para>
                        <text>the relative climate change obligations and emissions policies of New Zealand’s trade competitors and trading partners; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">v</label>
                      <para>
                        <text>any significant changes in emissions mitigation technology; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">vi</label>
                      <para>
                        <text>the cost to the tax payer and the economy of providing free allocation under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319473" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664376 - subpart 2 Issuance and allocation of New Zealand units" atidlm:xmlId="DLM1667094" atidlm:state="UNCHANGED"><atidlm:linkcontent>subpart 2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800419" atidlm:sourceXmlId="DLM1667094" atidlm:targetKey="14063580" atidlm:targetXmlId="DLM1662498" atidlm:targetName="subpart 2 Issuance and allocation of New Zealand units" atidlm:sourceName="DLM1667094" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662498.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> of this Part; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">k</label>
                  <para>
                    <text>the appropriateness of the penalties in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319474" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664380 - subpart 4 Offences and penalties" atidlm:xmlId="DLM1667095" atidlm:state="UNCHANGED"><atidlm:linkcontent>subpart 4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800420" atidlm:sourceXmlId="DLM1667095" atidlm:targetKey="14064002" atidlm:targetXmlId="DLM1662697" atidlm:targetName="subpart 4 Offences and penalties" atidlm:sourceName="DLM1667095" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662697.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> of this Part; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">l</label>
                  <para>
                    <text>the implications (if any) of the following matters for the notification of intention under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319475" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664383 - prov 70" atidlm:xmlId="DLM1667096" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 70</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800421" atidlm:sourceXmlId="DLM1667096" atidlm:targetKey="14063475" atidlm:targetXmlId="DLM1662617" atidlm:targetName="prov 70" atidlm:sourceName="DLM1667096" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662498.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>New Zealand’s annual emissions for the 5 years before notification; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>the average price of units for the 2 years before notification; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">m</label>
                  <para>
                    <text>the operation of the Innovation Fund established under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319476" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664381 - prov 74" atidlm:xmlId="DLM1667097" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 74</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800422" atidlm:sourceXmlId="DLM1667097" atidlm:targetKey="14063525" atidlm:targetXmlId="DLM1662627" atidlm:targetName="prov 74" atidlm:sourceName="DLM1667097" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662498.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">n</label>
                  <para>
                    <text>the impacts of the forestry sector elements of the emissions trading scheme established under this Act on biodiversity within New Zealand; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">o</label>
                  <para>
                    <text>the costs and benefits of establishing an independent or quasi-independent government body to carry out the allocation process, or part of the allocation process, contained in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319477" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664382 - subpart 2 Issuance and allocation of New Zealand units" atidlm:xmlId="DLM1667098" atidlm:state="UNCHANGED"><atidlm:linkcontent>subpart 2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800423" atidlm:sourceXmlId="DLM1667098" atidlm:targetKey="14063580" atidlm:targetXmlId="DLM1662498" atidlm:targetName="subpart 2 Issuance and allocation of New Zealand units" atidlm:sourceName="DLM1667098" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662498.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662737" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of Part 4</citation> of this Act; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">p</label>
                  <para>
                    <text>social, economic, and environmental effects of the emissions trading scheme established by this Act (other than those considered under paragraphs (a) to (l)); and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">q</label>
                  <para>
                    <text>any other matter that the Minister responsible for the administration of this Act considers relevant.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The Minister responsible for the administration of the Act must appoint a panel to conduct any review under subsection (1) and report in accordance with terms of reference set by the Minister and the matters set out in this section.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>Following the completion of each review under subsection (1), the Minister responsible for the administration of this Act must—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>publish the report of the panel on the review; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>present a copy of the report to the House of Representatives.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>The Minister responsible for the administration of this Act may initiate reviews of the operation and effectiveness of the emissions trading scheme established by this Act at any time and may use any method of review (including, but not limited to, the method specified in this section).</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665977"><amended-provision>Section 160</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319478" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667099" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800424" atidlm:sourceXmlId="DLM1667099" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667099" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665977" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662738" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">161</label>
          <heading>Appointment and conduct of review panel</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>When appointing members to a review panel under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319479" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664384 - prov 160" atidlm:xmlId="DLM1667100" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 160</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800425" atidlm:sourceXmlId="DLM1667100" atidlm:targetKey="14064517" atidlm:targetXmlId="DLM1662737" atidlm:targetName="prov 160" atidlm:sourceName="DLM1667100" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662738" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the Minister responsible for the administration of the Act must—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>ensure there are a minimum of 3 and maximum of 7 members; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>ensure the majority of members are not employees of a Department as defined in the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319480" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664385 - State Sector Act 1988" atidlm:xmlId="DLM1667101" atidlm:state="UNCHANGED"><atidlm:linkcontent>State Sector Act 1998</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800426" atidlm:sourceXmlId="DLM1667101" atidlm:targetKey="717800" atidlm:targetXmlId="DLM129109" atidlm:targetName="State Sector Act 1988" atidlm:sourceName="DLM1667101" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM129109.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662738" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                  <notes>
                    <editorial-note>
                      <para>
                        <text>Should be 1988 — added to queries database (SH)</text>
                      </para>
                    </editorial-note>
                  </notes>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>consider whether the members have, in the Minister's opinion, the appropriate  knowledge, skill, and experience, including with respect to—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>this Act; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>New Zealand’s international obligations under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319481" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664386 - schedule 2 Kyoto Protocol to the United Nations Framework Convention on Climate Change" atidlm:xmlId="DLM1667102" atidlm:state="UNCHANGED"><atidlm:linkcontent>Protocol</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800427" atidlm:sourceXmlId="DLM1667102" atidlm:targetKey="890614" atidlm:targetXmlId="DLM160087" atidlm:targetName="schedule 2 Kyoto Protocol to the United Nations Framework Convention on Climate Change" atidlm:sourceName="DLM1667102" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM160087.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662738" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319482" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664387 - schedule 1 United Nations Framework Convention on Climate Change" atidlm:xmlId="DLM1667103" atidlm:state="UNCHANGED"><atidlm:linkcontent>Convention</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800428" atidlm:sourceXmlId="DLM1667103" atidlm:targetKey="888403" atidlm:targetXmlId="DLM160014" atidlm:targetName="schedule 1 United Nations Framework Convention on Climate Change" atidlm:sourceName="DLM1667103" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM160014.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662738" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and any other relevant international agreement; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>the operation of the emissions trading scheme established under this Act, including its environmental, social, and economic effects; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>appoint 1 member as the chairperson of the panel.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>The Minister must, by written notice to the panel, specify the terms of reference for the review to be conducted by the panel.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>A review panel must complete a draft report on the review and provide the report to the Minister responsible for the administration of the Act at least 1 month prior to completion of the final report.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>In conducting a review, the panel—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>must establish a procedure that is appropriate, fair in the circumstances, and in accordance with the terms of reference for the review; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>may call for submissions.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665978"><amended-provision>Section 161</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319483" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667104" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800429" atidlm:sourceXmlId="DLM1667104" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667104" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665978" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662739" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">162</label>
          <heading>Regulations adding further activity to Part 2 of Schedule 4</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The Governor-General may, by Order in Council, in accordance with a recommendation of the Minister, amend <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319484" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664388 - head1 Part 2 Other removal activities" atidlm:xmlId="DLM1667105" atidlm:state="UNCHANGED"><atidlm:linkcontent>Part 2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800430" atidlm:sourceXmlId="DLM1667105" atidlm:targetKey="14066624" atidlm:targetXmlId="DLM1662867" atidlm:targetName="head1 Part 2 Other removal activities" atidlm:sourceName="DLM1667105" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662739" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of Schedule 4</citation> by adding a further activity to that Part.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>Before making a recommendation under subsection (1), the Minister must consult, or be satisfied that the chief executive has consulted, the persons (or representatives of those persons) that appear to the Minister or the chief executive likely to be substantially affected by any Order in Council made in accordance with the recommendation. </text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>The process for consultation should, to the extent practicable in the circumstances, include—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>giving adequate and appropriate notice of—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>the proposed terms of the recommendation; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>the reasons for it; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>the provision of a reasonable opportunity for interested persons to consider the recommendation and make submissions; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>adequate and appropriate consideration of submissions.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>An Order in Council made under subsection (1)—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>takes effect for the removal activity or activities concerned on and from—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>1 January of the next year, if made on or before 30 June in any year; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>1 July of the next year, if made on or after 1 July in any year; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>expires,—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>if made on or before 30 June in any year, on the close of 31 December of that year, except so far as it is expressly confirmed by Act of Parliament passed during that year; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>if made on or after 1 July in any year, on the close of 30 June in the following year, except so far as it is expressly confirmed by Act of Parliament passed before the latter date.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665979"><amended-provision>Section 162</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319485" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667106" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800431" atidlm:sourceXmlId="DLM1667106" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667106" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665979" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
            </history>
          </notes>
        </prov>
        <prov id="DLM1662740" irdnumbering="no" skeleton="no" toc="yes">
          <label auto.number="no" denominator="yes">163</label>
          <heading>Regulations relating to methodologies and verifiers</heading>
          <prov.body>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">1</label>
              <para>
                <text>The Governor-General may, by Order in Council made on the recommendation of the Minister, make regulations for 1 or more of the following purposes:</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>prescribing the data or other information that must be collected under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319486" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664389 - prov 62" atidlm:xmlId="DLM1667107" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 62(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800432" atidlm:sourceXmlId="DLM1667107" atidlm:targetKey="14063246" atidlm:targetXmlId="DLM1662492" atidlm:targetName="prov 62" atidlm:sourceName="DLM1667107" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662740" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> in respect of an activity, and, if relevant, the mechanism or method by which the data or information must be collected; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>prescribing a methodology or methodologies for calculating emissions or removals from an activity for the purposes of <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319487" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664390 - prov 62" atidlm:xmlId="DLM1667108" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 62(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800433" atidlm:sourceXmlId="DLM1667108" atidlm:targetKey="14063246" atidlm:targetXmlId="DLM1662492" atidlm:targetName="prov 62" atidlm:sourceName="DLM1667108" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662740" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>prescribing the data or other information, or the calculations of emissions or removals, that must be verified by a person or organisation recognised by the chief executive under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319488" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664391 - prov 92" atidlm:xmlId="DLM1667109" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 92</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800434" atidlm:sourceXmlId="DLM1667109" atidlm:targetKey="14063869" atidlm:targetXmlId="DLM1662657" atidlm:targetName="prov 92" atidlm:sourceName="DLM1667109" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662740" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">d</label>
                  <para>
                    <text>authorising the chief executive to issue guidelines or standards by notice in the <emphasis style="italic">Gazette</emphasis> in relation to the data or information prescribed under paragraph (a), and providing that compliance with the guidelines or standards is to be treated as compliance with the relevant requirements in the regulations; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">e</label>
                  <para>
                    <text>prescribing, for the purposes of <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319489" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664392 - prov 92" atidlm:xmlId="DLM1667110" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 92</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800435" atidlm:sourceXmlId="DLM1667110" atidlm:targetKey="14063869" atidlm:targetXmlId="DLM1662657" atidlm:targetName="prov 92" atidlm:sourceName="DLM1667110" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662650.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662740" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>,—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>the process by which a person or organisation may be recognised as being able to verify information or calculations for the purposes of <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319490" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664393 - prov 62" atidlm:xmlId="DLM1667111" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 62(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800436" atidlm:sourceXmlId="DLM1667111" atidlm:targetKey="14063246" atidlm:targetXmlId="DLM1662492" atidlm:targetName="prov 62" atidlm:sourceName="DLM1667111" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662482.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662740" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or (c) </citation>or unique emissions factors for the purposes of regulations made under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319491" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664394 - prov 164" atidlm:xmlId="DLM1667112" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 164</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800437" atidlm:sourceXmlId="DLM1667112" atidlm:targetKey="14064485" atidlm:targetXmlId="DLM1662741" atidlm:targetName="prov 164" atidlm:sourceName="DLM1667112" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662740" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>the expertise, technical competence, or qualifications required for recognition as a person or organisation able to verify unique emissions factors or information relating to 1 or more types of data or information, the calculations of certain types of emissions or removals, or 1 or more activities; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para>
                        <text>any additional—</text>
                        <label-para skeleton="no">
                          <label auto.number="no" denominator="yes">A</label>
                          <para>
                            <text>requirements for recognition of an organisation; and</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no">
                          <label auto.number="no" denominator="yes">B</label>
                          <para>
                            <text>restrictions on the employees of the organisation who may carry out the duties of the organisation in respect of the recognition; and</text>
                          </para>
                        </label-para>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">iv</label>
                      <para>
                        <text>the period for which a person or organisation may be recognised, and the process for the renewal of recognition; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">v</label>
                      <para>
                        <text>conditions of recognition, which may include (but are not limited to) ongoing competency and professional standard requirements, membership of a professional body, and the provision of reports to the chief executive; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">vi</label>
                      <para>
                        <text>the procedure for, and circumstances in which, recognition may be suspended or revoked; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">vii</label>
                      <para>
                        <text>fees for recognition of a person or organisation, which may vary depending on the class of persons or organisations, or the type of verification in respect of which recognition is sought.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">2</label>
              <para>
                <text>A regulation made under subsection (1) may apply—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>generally or with respect to different classes of activity, persons, parts of New Zealand, or other things; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>in respect of the same classes of activity, persons, parts of New Zealand, or other things, in different circumstances; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>generally or at any specified time of each year.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text>A regulation made under subsection (1)(a) to (d) may have retrospective effect if the regulation is expressed to apply from the commencement of the year in which it is made, or in respect of a period after any particular date within the year in which it is made.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">4</label>
              <para>
                <text>A regulation made under subsection (1)(b), and any associated regulations made under other paragraphs of subsection (1),—</text>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">a</label>
                  <para>
                    <text>may, without limiting subsection (1), relate to emissions or removals that—</text>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">i</label>
                      <para>
                        <text>stem directly from an activity; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para>
                        <text>are associated with a product or other thing that is the subject of the activity; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">b</label>
                  <para>
                    <text>may require the use of a computer programme available via the Internet site of the department of the chief executive; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no">
                  <label auto.number="no" denominator="yes">c</label>
                  <para>
                    <text>must not cover any emissions in respect of which another person is required to surrender units or any removals of greenhouse gases in respect of which another person is entitled to a transfer of New Zealand units under this Act. </text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text>In making a recommendation in relation to a regulation under subsection (1)(a) or (b), the Minister must have regard to New Zealand’s international obligations (if any) in respect of the collection of data and information relating to, and the measurement of, emissions and removals from the activity.</text>
              </para>
            </subprov>
            <subprov skeleton="no">
              <label auto.number="no" denominator="yes">6</label>
              <para>
                <text>Any guidelines or standards issued by the chief executive under regulations made under subsection (1)(d) are regulations for the purposes of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319492" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664395 - Regulations (Disallowance) Act 1989" atidlm:xmlId="DLM1667113" atidlm:state="UNCHANGED"><atidlm:linkcontent>Regulations (Disallowance) Act 1989</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800438" atidlm:sourceXmlId="DLM1667113" atidlm:targetKey="1055024" atidlm:targetXmlId="DLM195534" atidlm:targetName="Regulations (Disallowance) Act 1989" atidlm:sourceName="DLM1667113" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM195534.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662740" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, but not for the purposes of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319493" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1664396 - Acts and Regulations Publication Act 1989" atidlm:xmlId="DLM1667114" atidlm:state="UNCHANGED"><atidlm:linkcontent>Acts and Regulations Publication Act 1989</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="800440" atidlm:sourceXmlId="DLM1667114" atidlm:targetKey="1045212" atidlm:targetXmlId="DLM195097" atidlm:targetName="Acts and Regulations Publication Act 1989" atidlm:sourceName="DLM1667114" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM195097.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1662740" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
            <history>
              <history-note id="DLM1665980"><amended-provision>Section 163</amended-provision>: <amending-operation>added</amending-operation>, on <amendment-date>26 September 2008</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1319494" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1663285 - prov 50" atidlm:xmlId="DLM1667115" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 50</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="800441" atidlm:sourceXmlId="DLM1667115" atidlm:targetKey="11037941" atidlm:targetXmlId="DLM1131123" atidlm:targetName="prov 50" atidlm:sourceName="DLM1667115" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1130900.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662719.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1665980" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Climate Change Response (Emissions Trading) Amendment Act 2008</amending-leg> (2008 No 85).</history-note>
