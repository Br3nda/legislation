      <prov id="DLM1661309" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">2A</label>
        <heading>Application of Schedules 3 and 4</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text>Any provision in this Act that imposes an obligation on, or provides an entitlement to, a person in respect of an activity listed in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1318237" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1662983 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1666063" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="799362" atidlm:sourceXmlId="DLM1666063" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1666063" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM158588.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1661309" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1318238" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1662984 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1666064" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="799363" atidlm:sourceXmlId="DLM1666064" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1666064" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM158588.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1661309" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>—</text>
              <label-para skeleton="no">
                <label auto.number="no" denominator="yes">a</label>
                <para>
                  <text>does not apply to that person unless the Part or subpart in <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1318239" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1662983 - schedule 3 Activities with respect to which persons must be participants" atidlm:xmlId="DLM1666065" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="799364" atidlm:sourceXmlId="DLM1666065" atidlm:targetKey="14066402" atidlm:targetXmlId="DLM1662841" atidlm:targetName="schedule 3 Activities with respect to which persons must be participants" atidlm:sourceName="DLM1666065" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662841.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM158588.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1661309" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1318240" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1662984 - schedule 4 Activities with respect to which persons may be participants" atidlm:xmlId="DLM1666066" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="799365" atidlm:sourceXmlId="DLM1666066" atidlm:targetKey="14066600" atidlm:targetXmlId="DLM1662864" atidlm:targetName="schedule 4 Activities with respect to which persons may be participants" atidlm:sourceName="DLM1666066" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1662864.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM158588.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1661309" atidlm:locale="xx_XX" a