      <label auto.number="no" denominator="yes"/>
      <empowering-prov>
        <citation jurisdiction="nz">
          <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1266155" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1624470 - prov 28" atidlm:parentFolderId="/NZ/NZAct/C/Commerce Amendment Act 2008" atidlm:xmlId="DLM1625255" atidlm:state="UNCHANGED">
            <atidlm:linkcontent>s 28</atidlm:linkcontent>
            <atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="779798" atidlm:sourceXmlId="DLM1625255" atidlm:targetKey="11431416" atidlm:targetXmlId="DLM1194777" atidlm:targetName="prov 28" atidlm:sourceName="DLM1625255" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1194517.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1194785.xml" atidlm:state="UNCHANGED">
