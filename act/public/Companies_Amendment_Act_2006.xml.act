    <part id="DLM389218" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-18" act.no="56" act.type="public" date.as.at="2007-09-03" date.assent="2006-11-07" stage="in-force" year="2006">
