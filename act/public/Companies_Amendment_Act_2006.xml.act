      <prov id="DLM389230" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">19</label>
        <heading xml:lang="en-NZ">Other duties of liquidator</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
