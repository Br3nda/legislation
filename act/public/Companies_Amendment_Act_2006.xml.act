  <schedule.group xml:lang="en-NZ">
    <schedule id="DLM389299" in.amend="false" irdnumbering="no" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" xml:lang="en-NZ" act.no="56" act.type="public" date.as.at="2007-09-03" date.assent="2006-11-07" date.first.valid="2007-09-18" stage="in-force" year="2006">
