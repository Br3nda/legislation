            <amend>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">ca</label>
                <para xml:lang="en-NZ">
                  <text>prescribing countries, States, or territories outside New Zealand for the purposes of <citation jurisdiction="nz">section 151(2)(eb)</citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
