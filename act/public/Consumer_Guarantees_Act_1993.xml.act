      <prov id="DLM312883" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">51</label>
        <heading xml:lang="en-NZ">Jurisdiction of Disputes Tribunal</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
