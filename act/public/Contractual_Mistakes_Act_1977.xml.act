<act id="DLM443633" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="54" act.type="public" date.as.at="2007-09-03" date.assent="1977-11-21" date.first.valid="2007-09-18" stage="reprint" year="1977">
