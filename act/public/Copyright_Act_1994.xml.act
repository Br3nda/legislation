      <prov id="DLM346895" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">224</label>
        <heading xml:lang="en-NZ">Appeal on question of law</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Where any party to any proceedings before the Tribunal under this Act is dissatisfied with any determination of the Tribunal as being erroneous in point of law, that party may appeal to the High Court on that question of law.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Every appeal under this section shall be dealt with in accordance with rules of Court.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="436047" atidlm:branchLabel="_NOVERSION_" atidlm:name="Films, Videos, and Publications Classification Act 1993" atidlm:xmlId="DLM880107" atidlm:state="UNCHANGED"><atidlm:linkcontent>1993 No 94</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="443533" atidlm:sourceXmlId="DLM880107" atidlm:targetKey="1612005" atidlm:targetXmlId="DLM312894" atidlm:targetName="Films, Videos, and Publications Classification Act 1993" atidlm:sourceName="DLM880107" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM312894.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM346871.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[10]/prov[20]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Appeal on question of law" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="224" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM345633" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="143" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Copyright Act 1994" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1994" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atid