      <prov id="DLM426053" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">9</label>
        <heading xml:lang="en-NZ">No compensation for diminution in rights attaching to copyright</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>No person is entitled to compensation from the Crown in respect of any diminution in the rights attaching to copyright that may arise by virtue of the enactment of this Part of this Act.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
