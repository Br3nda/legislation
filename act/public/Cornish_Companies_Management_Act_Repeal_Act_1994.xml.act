<?xml version="1.0" encoding="UTF-8"?>
<act id="DLM331031" in.amend="false" instructing.office="Economic Development" irdnumbering="no" xml:lang="en-NZ" year.imprint="1994" act.no="25" act.type="public" date.as.at="1994-07-01" date.assent="1994-07-01" date.first.valid="1994-07-01" stage="in-force" year="1994">
  <cover id="DLM331032">
    <title xml:lang="en-NZ">Cornish Companies Management Act Repeal Act 1994</title>
    <assent>1994-07-01</assent>
    <commencement>1 July 1994</commencement>
  </cover>
  <contents>
    <heading xml:lang="en-NZ">Contents</heading>
    <toc/>
  </contents>
  <front id="DLM331033" xml:lang="en-NZ">
    <long-title id="DLM331034" toc="yes" xml:lang="en-NZ">
      <para xml:lang="en-NZ">
        <text>An Act to repeal the <citation jurisdiction="nz">Cornish Companies Management Act 1974</citation></text>
      </para>
    </long-title>
    <enactment>BE IT ENACTED by the Parliament of New Zealand as follows:</enactment>
  </front>
  <body id="DLM331035" prov-type="section" xml:lang="en-NZ">
    <prov id="DLM331036" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">1</label>
      <heading xml:lang="en-NZ">Short Title</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text>This Act may be cited as the Cornish Companies Management Act Repeal Act 1994.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM331037" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">2</label>
      <heading xml:lang="en-NZ">Repeal of Cornish Companies Management Act 1974 and amendments</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text>The following enactments are hereby repealed:</text>
            <label-para skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">a</label>
              <para xml:lang="en-NZ">
                <text>the Cornish Companies Management Act 1974:</text>
              </para>
            </label-para>
            <label-para skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">b</label>
              <para xml:lang="en-NZ">
                <text>the Cornish Companies Management Amendment Act 1978:</text>
              </para>
            </label-para>
            <label-para skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">c</label>
              <para xml:lang="en-NZ">
                <text>the Cornish Companies Management Amendment Act 1980.</text>
              </para>
            </label-para>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <para xml:lang="en-NZ">
            <text>The Cornish Companies Management Order 1990 is hereby consequentially revoked.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
  </body>
  <end>
    <admin-office>This Act is administered by the <ministry>Ministry of Commerce</ministry></admin-office>
  </end>
</act>
