    <prov id="DLM968179" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">4</label>
      <heading xml:lang="en-NZ">Application of certain provisions of Companies Act 1993</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text><citation jurisdiction="nz">Section 55(1)</citation> is amended by inserting <amend.in quote="1">275,</amend.in> before <quote.in quote="1">292</quote.in>.</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <para xml:lang="en-NZ">
            <text><citation jurisdiction="nz">Section 55</citation> is amended by adding the following subsection:</text>
            <amend quote="1">
              <subprov skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes" quote="1">3</label>
                <para xml:lang="en-NZ">
                  <text>To avoid doubt, the reference in <citation jurisdiction="nz">section 275(4) of the Companies Act 1993</citation> to <citation jurisdiction="nz">clause 1(a) of Schedule 7 of the Companies Act 1993</citation> must be read as a reference to <citation jurisdiction="nz">section 65(1) of this Act</citation>.</text>
                </para>
              </subprov>
            </amend>
          </para>
        </subprov>
      </prov.body>
    </prov>
