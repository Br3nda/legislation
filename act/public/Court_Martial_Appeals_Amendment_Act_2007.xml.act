              <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes" quote="1">m</label>
                <para xml:lang="en-NZ">
                  <text>any Judge of the Court Martial Appeal Court:</text>
                </para>
              </label-para>
            </amend>
            <follow-text space-before="no">.</follow-text>
          </para>
          <para xml:lang="en-NZ">
            <text><citation jurisdiction="nz">Section 3(5)(h)</citation>: repeal and substitute:</text>
            <amend quote="1">
              <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes" quote="1">h</label>
                <para xml:lang="en-NZ">
                  <text>the Court Martial Appeal Court:</text>
                </para>
              </label-para>
            </amend>
            <follow-text space-before="no">.</follow-text>
          </para>
