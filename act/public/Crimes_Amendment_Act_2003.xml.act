      <prov id="DLM199795" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">12</label>
        <heading xml:lang="en-NZ">Prohibition on dealing, etc, with listening devices</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
