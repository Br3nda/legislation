        <label auto.number="no" denominator="yes">4</label>
        <heading xml:lang="en-NZ">New heading substituted</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The heading above <citation jurisdiction="nz">section 80</citation> is repealed and the following heading substituted: <amend.in><emphasis style="italic">Offence of oath to commit offence</emphasis></amend.in>.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM981009" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
