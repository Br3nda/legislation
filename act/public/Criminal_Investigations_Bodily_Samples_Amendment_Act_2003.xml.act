                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>compels any lawyer or person chosen under <citation jurisdiction="nz">section 50(1)(a) or (3)</citation>, or any person referred to in <citation jurisdiction="nz">section 50(1)(b)</citation>, to be present during the taking of a bodily sample; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>compels any parent to take a buccal sample from his or her child under <citation jurisdiction="nz">section 49A(4)(b) or (6)(a)</citation>.</text>
                  </para>
                </label-para>
              </amend>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM222150" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">30</label>
