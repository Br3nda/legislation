              <def-para id="DLM223861" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM223862" xml:lang="en-NZ">medical practitioner</def-term> means a medical practitioner registered under the <citation jurisdiction="nz">Medical Practitioners Act 1995</citation></text>
                </para>
              </def-para>
              <def-para id="DLM223863" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM223864" xml:lang="en-NZ">psychologist</def-term> means a psychologist registered under the <citation jurisdiction="nz">Psychologists Act 1981</citation></text>
                </para>
              </def-para>
            </para>
          </subprov>
        </prov.body>
