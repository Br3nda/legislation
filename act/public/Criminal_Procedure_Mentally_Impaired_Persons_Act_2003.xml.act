              <def-para id="DLM223836" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM223837" xml:lang="en-NZ">counsel</def-term>, in relation to a person, means a barrister and solicitor of the High Court of New Zealand who is representing the person in proceedings</text>
                </para>
              </def-para>
              <def-para id="DLM223838" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM223839" xml:lang="en-NZ">court</def-term> means any court exercising jurisdiction in criminal proceedings</text>
                </para>
              </def-para>
              <def-para id="DLM223840" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
