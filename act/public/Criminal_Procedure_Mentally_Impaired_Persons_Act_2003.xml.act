          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>This Act applies only to criminal proceedings in which a defendant is charged with an imprisonable offence.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
