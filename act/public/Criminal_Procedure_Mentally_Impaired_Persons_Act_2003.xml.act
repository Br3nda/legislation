              <def-para id="DLM223845" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM223846" xml:lang="en-NZ">health assessor</def-term> means—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>a practising psychiatrist who is registered as a medical practitioner; or</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
