              <def-para id="DLM223857" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM223858" xml:lang="en-NZ">psychologist</def-term> means a health practitioner who is, or is deemed to be, registered with the Psychologists Board continued by <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="137667" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 114" atidlm:xmlId="DLM583897" atidlm:state="UNCHANGED"><atidlm:linkcontent>114(1)(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="139795" atidlm:sourceXmlId="DLM583897" atidlm:targetKey="1207431" atidlm:targetXmlId="DLM204329" atidlm:targetName="prov 114" atidlm:sourceName="DLM583897" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM204327.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM223822.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[3]/prov.body[1]/subprov[1]/para[1]/def-para[16]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para psychologist" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM223817" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="115" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Criminal Procedure (Mentally Impaired Persons)