              <def-para id="DLM246079" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM246080" xml:lang="en-NZ">Silver</def-term> includes any substance containing silver, or having silver mixed in it, other than a substance that also contains gold or has gold mixed in it</text>
                </para>
              </def-para>
              <def-para id="DLM246081" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM246082" xml:lang="en-NZ">Submission</def-term> means a written submission</text>
                </para>
              </def-para>
