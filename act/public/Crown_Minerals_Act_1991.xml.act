      <prov deletion-status="repealed" id="DLM246754" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">82</label>
        <heading xml:lang="en-NZ">Lodging of certificates of extension</heading>
        <prov.body/>
        <notes>
          <history>
