      <prov id="DLM427010" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">20</label>
        <heading xml:lang="en-NZ">Boundary disputes</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Every dispute between the holders of adjacent pastoral land as to the boundary between them must be determined by the Commissioner or a person appointed by the Commissioner for the purpose.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
