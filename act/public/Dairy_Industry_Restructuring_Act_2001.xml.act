                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>means a processor of milk or milksolids or dairy products who is not an associated person of new co-op; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>includes New Zealand Dairy Foods Limited and any associated person of that company other than new co-op</text>
                    </para>
                  </label-para>
                </para>
              </def-para>
              <def-para id="DLM108904" xml:lang="en-NZ">
