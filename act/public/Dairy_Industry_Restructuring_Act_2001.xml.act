dlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2001" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></text>
                </para>
              </def-para>
              <def-para id="DLM108912" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM108913" xml:lang="en-NZ">June price</def-term> means the price of a co-operative share determined as at 1 June by new co-op's board under its constitution</text>
                </para>
              </def-para>
              <def-para id="DLM108914" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM108915" xml:lang="en-NZ">LIC</def-term> means Livestock Improvement Corporation Limited</text>
                </para>
              </def-para>
              <def-para id="DLM108916" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM108917" xml:lang="en-NZ">LIC board</def-term> means the board of directors of LIC</text>
                </para>
              </def-para>
              <def-para id="DLM108918" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM108919" xml:lang="en-NZ">Minister</def-term> means the Minister of the Crown who, under the authority of any warrant or with the authority of the Prime Minister, is for the time being responsible for the administration of this Act</text>
                </para>
              </def-para>
              <def-para id="DLM108920" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM108921" xml:lang="en-NZ">new co-op</def-term> means the amalgamated company under the new co-op amalgamation; and includes any company resulting from a further amalgamation involving new co-op</text>
                </para>
              </def-para>
              <def-para id="DLM108922" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM108923" xml:lang="en-NZ">new co-op amalgamation</def-term> means the amalgamation that occurs if The New Zealand Co-operative Dairy Company Limited, Kiwi Co-operative Dairies Limited, and Fonterra Co-operative Group Limited amalgamate under <citation jurisdiction="nz">Part <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="72125" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 13 Amalgamations" atidlm:xmlId="DLM518799" atidlm:state="UNCHANGED"><atidlm:linkcontent>13</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="73365" atidlm:sourceXmlId="DLM518799" atidlm:targetKey="1649415" atidlm:targetXmlId="DLM321142" atidlm:targetName="part 13 Amalgamations" atidlm:sourceName="DLM518799" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM321142.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM106755.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[4]/prov.body[1]/subprov[1]/para[1]/def-para[32]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para new co-op amalgamation" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM106750" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="51" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata at