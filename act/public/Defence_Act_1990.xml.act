            <history-note id="DLM206452">Sections <amended-provision>69 to 71</amended-provision> were <amending-operation>substituted</amending-operation>, as from <amendment-date>2 October 2000</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="297611" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 240" atidlm:xmlId="DLM742334" atidlm:state="UNCHANGED"><atidlm:linkcontent>240</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="302337" atidlm:sourceXmlId="DLM742334" atidlm:targetKey="325245" atidlm:targetXmlId="DLM61487" atidlm:targetName="prov 240" atidlm:sourceName="DLM742334" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM61442.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM206420.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[14]/notes[1]/history[1]/history-note[3]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM204972" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="28" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Defence Act 1990" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metad