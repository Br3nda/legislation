      <crosshead id="DLM1404076" toc="yes">Amendment to Child Support Rules 1992</crosshead>
      <prov id="DLM1404077" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">24</label>
        <heading>Amendment to Child Support Rules 1992</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text>This section amends the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1233407" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1599598 - Child Support Rules 1992" atidlm:xmlId="DLM1599706" atidlm:state="UNCHANGED"><atidlm:linkcontent>Child Support Rules 1992</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="768371" atidlm:sourceXmlId="DLM1599706" atidlm:targetKey="4724804" atidlm:targetXmlId="DLM152692" atidlm:targetName="Child Support Rules 1992" atidlm:sourceName="DLM1599706" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZReg/fragxyxz_DLM152692.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1404075.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1404077" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
              <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1233408" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1599600 - prov 37" atidlm:xmlId="DLM1599707" atidlm:state="UNCHANGED"><atidlm:linkcontent>Rule 37</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="768372" atidlm:sourceXmlId="DLM1599707" atidlm:targetKey="4726807" atidlm:targetXmlId="DLM153264" atidlm:targetName="prov 37" atidlm:sourceName="DLM1599707" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZReg/fragxyxz_DLM153256.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1404075.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1404077" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is amended by—</text>
              <label-para skeleton="no">
                <label auto.number="no" denominator="yes">a</label>
                <para>
                  <text>omitting <amend.in quote="1">is mentally disordered within the meaning of the Mental Health (Compulsory Assessment and Treatment) Act 1992</amend.in> and substituting <amend.in quote="1">poses a serious danger to the health or safety of others</amend.in>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no">
                <label auto.number="no" denominator="yes">b</label>
                <para>
                  <text>omitting <amend.in quote="1">the mentally disordered person</amend.in> and substituting <amend.in quote="1">the respondent</amend.in>.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <notes>
          <amends-note>
            <amends-effective>
              <date>2008-09-10</date>
            </amends-effective>
            <amends-affect>
              <citation jurisdiction="nz">
                <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1233409" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1599598 - Child Support Rules 1992" atidlm:xmlId="DLM1599708" atidlm:state="UNCHANGED">
                  <atidlm:linkcontent>Child Support Rules 1992</atidlm:linkcontent>
                  <atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="768373" atidlm:sourceXmlId="DLM1599708" atidlm:targetKey="4724804" atidlm:targetXmlId="DLM152692" atidlm:targetName="Child Support Rules 1992" atidlm:sourceName="DLM1599708" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZReg/fragxyxz_DLM152692.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1404075.xml" atidlm:state="UNCHANGED">
                    <atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1404077" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/>
                  </atidlm:resourcepair>
                </atidlm:link>
              </citation>
            </amends-affect>
          </amends-note>
        </notes>
