      <crosshead id="DLM1404078" toc="yes">Amendments to Freshwater Fish Farming Regulations 1983</crosshead>
      <prov id="DLM1404079" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">25</label>
        <heading>Amendments to Freshwater Fish Farming Regulations 1983</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text>This section amends the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1233410" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1599601 - Freshwater Fish Farming Regulations 1983" atidlm:xmlId="DLM1599709" atidlm:state="UNCHANGED"><atidlm:linkcontent>Freshwater Fish Farming Regulations 1983</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="768374" atidlm:sourceXmlId="DLM1599709" atidlm:targetKey="3576216" atidlm:targetXmlId="DLM93755" atidlm:targetName="Freshwater Fish Farming Regulations 1983" atidlm:sourceName="DLM1599709" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZReg/fragxyxz_DLM93755.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1404075.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1404079" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
