    <prov id="DLM169830" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">6</label>
      <heading xml:lang="en-NZ">Power of Tribunal to award interest</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
