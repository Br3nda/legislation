          <heading xml:lang="en-NZ">
            <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="98984" atidlm:branchLabel="_NOVERSION_" atidlm:name="Protection of Personal and Property Rights Act 1988" atidlm:xmlId="DLM545572" atidlm:state="UNCHANGED"><atidlm:linkcontent>Protection of Personal and Property Rights Act 1988</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="100778" atidlm:sourceXmlId="DLM545572" atidlm:targetKey="698400" atidlm:targetXmlId="DLM126527" atidlm:targetName="Protection of Personal and Property Rights Act 1988" atidlm:sourceName="DLM545572" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM126527.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM168715.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="head5" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[1]/schedule.misc[1]/head5[22]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="head5" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Protection of Personal and Property Rights Act 1988 (1988 No 4)" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM167890" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="63" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="District Courts Amendment Act 2002" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atid