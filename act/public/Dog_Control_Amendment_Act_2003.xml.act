      <prov id="DLM230407" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">43</label>
        <heading xml:lang="en-NZ">Retention of dog threatening public safety</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
