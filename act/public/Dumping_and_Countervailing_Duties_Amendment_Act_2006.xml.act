<act id="DLM392184" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="63" act.type="public" date.as.at="2007-09-03" date.assent="2006-11-21" date.first.valid="2007-09-18" stage="in-force" year="2006">
