    <schedule id="DLM307128" in.amend="false" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" xml:lang="en-NZ" irdnumbering="no" act.no="84" act.type="public" date.as.at="2007-09-03" date.assent="1993-08-10" date.first.valid="2007-09-03" stage="reprint" year="1993">
