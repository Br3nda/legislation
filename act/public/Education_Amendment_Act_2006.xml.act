        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The <citation jurisdiction="nz">School Trustees Act 1989</citation> is repealed.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Part <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="335624" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 1 School sites" atidlm:xmlId="DLM780358" atidlm:state="UNCHANGED"><atidlm:linkcontent>1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="341206" atidlm:sourceXmlId="DLM780358" atidlm:targetKey="1746618" atidlm:targetXmlId="DLM257499" atidlm:targetName="part 1 School sites" atidlm:sourceName="DLM780358" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM257499.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM373076.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM373083" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="335625" atidlm:branchLabel="_NOVERSION_" atidlm:name="Education Lands Act 1949" atidlm:xmlId="DLM780359" atidlm:state="UNCHANGED"><atidlm:linkcontent>Education Lands Act 1949</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="341207" atidlm:sourceXmlId="DLM780359" atidlm:targetKey="1745811" atidlm:targetXmlId="DLM257454" atidlm:targetName="Education Lands Act 1949" atidlm:sourceName="DLM780359" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM257454.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM373076.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM373083" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is repealed.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>The following provisions of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="335626" atidlm:branchLabel="_NOVERSION_" atidlm:name="Education Act 1964" atidlm:xmlId="DLM780360" atidlm:state="UNCHANGED"><atidlm:linkcontent>Education Act 1964</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="341208" atidlm:sourceXmlId="DLM780360" atidlm:targetKey="2305431" atidlm:targetXmlId="DLM356731" atidlm:targetName="Education Act 1964" atidlm:sourceName="DLM780360" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM356731.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM373076.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM373083" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> are repealed:</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text><citation jurisdiction="nz">Parts <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="335627" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 1 Central administration" atidlm:xmlId="DLM780361" atidlm:state="UNCHANGED"><atidlm:linkcontent>1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="341209" atidlm:sourceXmlId="DLM780361" atidlm:targetKey="2306001" atidlm:targetXmlId="DLM357224" atidlm:targetName="part 1 Central administration" atidlm:sourceName="DLM780361" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM357224.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM373076.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM373083" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="335628" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 2 Local administration" atidlm:xmlId="DLM780362" atidlm:state="UNCHANGED"><atidlm:linkcontent>2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="341210" atidlm:sourceXmlId="DLM780362" atidlm:targetKey="2307764" atidlm:targetXmlId="DLM357260" atidlm:targetName="part 2 Local administration" atidlm:sourceName="DLM780362" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM357260.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM373076.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM373083" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
