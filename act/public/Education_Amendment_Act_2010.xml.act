<act date.instructed="2009-01-21" id="DLM1956400" in.amend="false" instructing.office="Education" irdnumbering="no" xml:lang="en-NZ" year.imprint="2010" act.no="25" act.type="public" date.as.at="2010-05-19" date.assent="2010-05-19" date.first.valid="2010-05-20" stage="in-force" year="2010" prospective.consolidation="F">
