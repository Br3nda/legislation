              <def-para id="DLM282160" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM282161" xml:lang="en-NZ">Fittings</def-term> means everything used, or designed or intended for use, in or in connection with the generation, conversion, transformation, conveyance, or use of electricity</text>
                </para>
              </def-para>
              <def-para id="DLM282162" xml:lang="en-NZ">
