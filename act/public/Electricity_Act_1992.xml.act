              <def-para id="DLM282152" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM282153" xml:lang="en-NZ">Electricity retailer</def-term> means a person who supplies electricity to another person or other persons for any purpose other than for resupply by the other person or persons</text>
                </para>
              </def-para>
              <def-para id="DLM282154" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM282155" xml:lang="en-NZ">Employer licence</def-term> means an employer licence issued under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="382861" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 101" atidlm:xmlId="DLM826955" atidlm:state="UNCHANGED"><atidlm:linkcontent>101</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="389077" atidlm:sourceXmlId="DLM826955" atidlm:targetKey="1468035" atidlm:targetXmlId="DLM282898" atidlm:targetName="prov 101" atidlm:sourceName="DLM826955" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM282865.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM281865.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[33]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Employer licence" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM281857" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="122" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Electricity Act 1992" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1992" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of this Act</citation></text>
                </para>
              </def-para>
              <def-para id="DLM282156" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM282157" xml:lang="en-NZ">Existing works</def-term>,â€”</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>In relation to works owned by the Corporation, means any works constructed before the 1st day of January 1988; and includes any works that were wholly or partly in existence, or work on the construction of which commenced, before the 1st day of January 1988:</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>In relation to works owned by any other person, means any works constructed before the 1st day of January 1993; and includes any works that were wholly or partly in existence, or work on the construction of which commenced, before the 1st day of January 1993:</text>
                    </para>
                  </label-para>
                </para>
              </def-para>
              <def-para id="DLM282158" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM282159" xml:lang="en-NZ">Fire brigade</def-term> has the same meaning as in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="382862" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 2" atidlm:xmlId="DLM826956" atidlm:state="UNCHANGED"><atidlm:linkcontent>2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="389078" atidlm:sourceXmlId="DLM826956" atidlm:targetKey="2778200" atidlm:targetXmlId="DLM432653" atidlm:targetName="prov 2" atidlm:sourceName="DLM826956" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM432647.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM281865.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[35]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Fire brigade" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM281857" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="122" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Electricity Act 1992" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1992" atidlm:locale="xx_