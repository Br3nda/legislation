    <part id="DLM283152" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-18" act.no="122" act.type="public" date.as.at="2008-07-29" date.assent="1992-12-17" stage="reprint" year="1992">
