      <notes/>
      <prov id="DLM283153" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">140</label>
        <heading xml:lang="en-NZ">Appeals</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Any person who is dissatisfied with the whole or any part ofâ€”</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>Any decision of the Board relating to an application by that person for registration or for a provisional licence; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
