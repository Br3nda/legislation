        <prov id="DLM283782" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">172KK</label>
          <heading xml:lang="en-NZ">Right of appeal against suspension or termination orders</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>An industry participant in respect of which a suspension order or termination order is made may appeal to the High Court against the order.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The appeal must be made by giving notice of appeal within 20 working days after the date of the order appealed against or within any further time that the Court allows.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>SR 2003/374 <citation jurisdiction="nz">r <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="385324" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 187" atidlm:xmlId="DLM829713" atidlm:state="UNCHANGED"><atidlm:linkcontent>187</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="391803" atidlm:sourceXmlId="DLM829713" atidlm:targetKey="3881607" atidlm:targetXmlId="DLM233144" atidlm:targetName="prov 187" atidlm:sourceName="DLM829713" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZReg/fragxyxz_DLM233136.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM283396.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[14]/subpart[2]/prov[23]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Right of appeal against suspension or termination orders" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="172KK" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM281857" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="122" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Electricity Act 1992" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1992" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number