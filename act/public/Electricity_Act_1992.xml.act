              <def-para id="DLM282373" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM282374" xml:lang="en-NZ">serious harm</def-term> meansâ€”</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>death; or</text>
