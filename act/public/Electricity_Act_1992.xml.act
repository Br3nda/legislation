        <crosshead id="DLM284023" toc="yes" xml:lang="en-NZ">Duties of members</crosshead>
        <prov id="DLM284024" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">172S</label>
          <heading xml:lang="en-NZ">Duties of members</heading>
          <prov.body>
            <subprov deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>A member, when acting as a member, must not act as a representative of, or promote the interests or views of, any organisation to which the member belongs, a particular industry participant, or a particular group of industry participants.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
