              <def-para id="DLM282335" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM282336" xml:lang="en-NZ">Recognised certificate</def-term> means any certificate, or any other evidence of registration or recognition,â€”</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>That was granted outside New Zealand; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>That is evidence of proficiency in the work of an electrical service technician, an electrician, a line mechanic, or an electrical inspector; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">c</label>
                    <para xml:lang="en-NZ">
                      <text>That is prescribed as a recognised certificate:</text>
                    </para>
                  </label-para>
                </para>
              </def-para>
              <def-para id="DLM282337" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM282338" xml:lang="en-NZ">Register</def-term> means any register kept pursuant to <citation jurisdiction="nz">Part <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="382910" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 9 Registration and licensing of electrical workers" atidlm:xmlId="DLM827104" atidlm:state="UNCHANGED"><atidlm:linkcontent>9</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="389128" atidlm:sourceXmlId="DLM827104" atidlm:targetKey="1468004" atidlm:targetXmlId="DLM282865" atidlm:targetName="part 9 Registration and licensing of electrical workers" atidlm:sourceName="DLM827104" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM282865.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM281865.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[67]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Register" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM281857" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="122" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Electricity Act 1992" atidlm:locale="xx_XX" atidlm:state="UN