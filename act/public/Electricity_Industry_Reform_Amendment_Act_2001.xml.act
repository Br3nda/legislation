    <title xml:lang="en-NZ">Electricity Industry Reform Amendment Act 2001</title>
    <assent>2001-08-07</assent>
    <cover.reprint-note>
      <para xml:lang="en-NZ"/>
      <admin-office>This Act is administered in the <ministry>Ministry Economic Development</ministry>.</admin-office>
    </cover.reprint-note>
