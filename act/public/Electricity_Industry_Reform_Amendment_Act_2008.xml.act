<act date.instructed="2007-03-01" id="DLM1100196" in.amend="false" instructing.office="Economic Development" irdnumbering="no" xml:lang="en-NZ" year.imprint="2008" act.no="71" act.type="public" date.as.at="2008-09-16" date.assent="2008-09-16" date.first.valid="2008-10-14" stage="not-in-force" year="2008">
