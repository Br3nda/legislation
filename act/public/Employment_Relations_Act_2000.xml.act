resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36975" atidlm:sourceXmlId="DLM483469" atidlm:targetKey="317817" atidlm:targetXmlId="DLM59674" atidlm:targetName="prov 69OE" atidlm:sourceName="DLM483469" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM59657.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60398" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
