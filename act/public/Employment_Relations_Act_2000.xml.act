                  <text>the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36772" atidlm:branchLabel="_NOVERSION_" atidlm:name="Illegal Contracts Act 1970" atidlm:xmlId="DLM483916" atidlm:state="UNCHANGED"><atidlm:linkcontent>Illegal Contracts Act 1970</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="37486" atidlm:sourceXmlId="DLM483916" atidlm:targetKey="2548009" atidlm:targetXmlId="DLM396434" atidlm:targetName="Illegal Contracts Act 1970" atidlm:sourceName="DLM483916" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM396434.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60945" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">g</label>
                <para xml:lang="en-NZ">
                  <text>the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36773" atidlm:branchLabel="_NOVERSION_" atidlm:name="Minors' Contracts Act 1969" atidlm:xmlId="DLM483917" atidlm:state="UNCHANGED"><atidlm:linkcontent>Minors' Contracts Act 1969</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="37487" atidlm:sourceXmlId="DLM483917" atidlm:targetKey="2523818" atidlm:targetXmlId="DLM392349" atidlm:targetName="Minors' Contracts Act 1969" atidlm:sourceName="DLM483917" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM392349.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60945" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                </para>
