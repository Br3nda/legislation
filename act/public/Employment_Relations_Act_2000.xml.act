              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36769" atidlm:branchLabel="_NOVERSION_" atidlm:name="Contractual Remedies Act 1979" atidlm:xmlId="DLM483913" atidlm:state="UNCHANGED"><atidlm:linkcontent>Contractual Remedies Act 1979</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="37483" atidlm:sourceXmlId="DLM483913" atidlm:targetKey="146400" atidlm:targetXmlId="DLM31565" atidlm:targetName="Contractual Remedies Act 1979" atidlm:sourceName="DLM483913" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM31565.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60945" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36770" atidlm:branchLabel="_NOVERSION_" atidlm:name="Fair Trading Act 1986" atidlm:xmlId="DLM483914" atidlm:state="UNCHANGED"><atidlm:linkcontent>Fair Trading Act 1986</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="37484" atidlm:sourceXmlId="DLM483914" atidlm:targetKey="535000" atidlm:targetXmlId="DLM96438" atidlm:targetName="Fair Trading Act 1986" atidlm:sourceName="DLM483914" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM96438.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60945" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">e</label>
                <para xml:lang="en-NZ">
                  <text>the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36771" atidlm:branchLabel="_NOVERSION_" atidlm:name="Frustrated Contracts Act 1944" atidlm:xmlId="DLM483915" atidlm:state="UNCHANGED"><atidlm:linkcontent>Frustrated Contracts Act 1944</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="37485" atidlm:sourceXmlId="DLM483915" atidlm:targetKey="1633011" atidlm:targetXmlId="DLM236924" atidlm:targetName="Frustrated Contracts Act 1944" atidlm:sourceName="DLM483915" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM236924.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60945" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">f</label>
