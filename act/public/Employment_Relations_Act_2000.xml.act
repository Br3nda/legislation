markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="37465" atidlm:sourceXmlId="DLM483896" atidlm:targetKey="324235" atidlm:targetXmlId="DLM60939" atidlm:targetName="prov 161" atidlm:sourceName="DLM483896" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60939" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36753" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 161" atidlm:xmlId="DLM483897" atidlm:state="UNCHANGED"><atidlm:linkcontent>(da)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="37466" atidlm:sourceXmlId="DLM483897" atidlm:targetKey="324235" atidlm:targetXmlId="DLM60939" atidlm:targetName="prov 161" atidlm:sourceName="DLM483897" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60939" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36754" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 161" atidlm:xmlId="DLM483898" atidlm:state="UNCHANGED"><atidlm:linkcontent>(f)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="37467" atidlm:sourceXmlId="DLM483898" atidlm:targetKey="324235" atidlm:targetXmlId="DLM60939" atidlm:targetName="prov 161" atidlm:sourceName="DLM483898" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60939" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></insertwords>, the Authority does not have jurisdiction to make a determination about any matter relating to—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>bargaining; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the fixing of new terms and conditions of employment.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>Except as provided in this Act, no court has jurisdiction in relation to any matter that, under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36755" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 161" atidlm:xmlId="DLM483899" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="37468" atidlm:sourceXmlId="DLM483899" atidlm:targetKey="324235" atidlm:targetXmlId="DLM60939" atidlm:targetName="prov 161" atidlm:sourceName="DLM483899" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60939" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, is within the exclusive jurisdiction of the Authority.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 79(1)(b)-(g), (j)</citation>
        </cf>
