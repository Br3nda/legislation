      <prov id="DLM60344" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">111</label>
        <heading xml:lang="en-NZ">Definitions relating to personal grievances</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Each of the terms <def-term id="DLM60345" xml:lang="en-NZ">personal grievance</def-term>, <def-term id="DLM60346" xml:lang="en-NZ">discrimination</def-term>, <def-term id="DLM60347" xml:lang="en-NZ">sexual harassment</def-term>, <def-term id="DLM60348" xml:lang="en-NZ">racial harassment</def-term>, and <def-term id="DLM60349" xml:lang="en-NZ">duress</def-term> have in any employment agreement the meanings given to those terms by <citation jurisdiction="nz">sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36269" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 103" atidlm:xmlId="DLM483214" atidlm:state="UNCHANGED"><atidlm:linkcontent>103</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36736" atidlm:sourceXmlId="DLM483214" atidlm:targetKey="322406" atidlm:targetXmlId="DLM60322" atidlm:targetName="prov 103" atidlm:sourceName="DLM483214" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60344" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36270" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 104" atidlm:xmlId="DLM483215" atidlm:state="UNCHANGED"><atidlm:linkcontent>104</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36737" atidlm:sourceXmlId="DLM483215" atidlm:targetKey="322413" atidlm:targetXmlId="DLM60329" atidlm:targetName="prov 104" atidlm:sourceName="DLM483215" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60344" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36271" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 105" atidlm:xmlId="DLM483216" atidlm:state="UNCHANGED"><atidlm:linkcontent>105</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36738" atidlm:sourceXmlId="DLM483216" atidlm:targetKey="322416" atidlm:targetXmlId="DLM60332" atidlm:targetName="prov 105" atidlm:sourceName="DLM483216" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60344" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36272" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 106" atidlm:xmlId="DLM483217" atidlm:state="UNCHANGED"><atidlm:linkcontent>106</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36739" atidlm:sourceXmlId="DLM483217" atidlm:targetKey="322417" atidlm:targetXmlId="DLM60333" atidlm:targetName="prov 106" atidlm:sourceName="DLM483217" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60344" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36273" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 107" atidlm:xmlId="DLM483218" atidlm:state="UNCHANGED"><atidlm:linkcontent>107</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36740" atidlm:sourceXmlId="DLM483218" atidlm:targetKey="322421" atidlm:targetXmlId="DLM60337" atidlm:targetName="prov 107" atidlm:sourceName="DLM483218" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60344" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36274" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 108" atidlm:xmlId="DLM483219" atidlm:state="UNCHANGED"><atidlm:linkcontent>108</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36741" atidlm:sourceXmlId="DLM483219" atidlm:targetKey="322424" atidlm:targetXmlId="DLM60340" atidlm:targetName="prov 108" atidlm:sourceName="DLM483219" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60344" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36275" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 109" atidlm:xmlId="DLM483220" atidlm:state="UNCHANGED"><atidlm:linkcontent>109</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36742" atidlm:sourceXmlId="DLM483220" atidlm:targetKey="322425" atidlm:targetXmlId="DLM60341" atidlm:targetName="prov 109" atidlm:sourceName="DLM483220" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60344" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36276" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 110" atidlm:xmlId="DLM483221" atidlm:state="UNCHANGED"><atidlm:linkcontent>110</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36743" atidlm:sourceXmlId="DLM483221" atidlm:targetKey="322426" atidlm:targetXmlId="DLM60342" atidlm:targetName="prov 110" atidlm:sourceName="DLM483221" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60344" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> unless the employment agreement gives an extended meaning to the term.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 31</citation>
        </cf>
      </prov>
      <prov id="DLM60350" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">112</label>
        <heading xml:lang="en-NZ">Choice of procedures</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Where the circumstances giving rise to a personal grievance by an employee are also such that that employee would be entitled to make a complaint under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36277" atidlm:branchLabel="_NOVERSION_" atidlm:name="Human Rights Act 1993" atidlm:xmlId="DLM483222" atidlm:state="UNCHANGED"><atidlm:linkcontent>Human Rights Act 1993</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="36744" atidlm:sourceXmlId="DLM483222" atidlm:targetKey="1573056" atidlm:targetXmlId="DLM304211" atidlm:targetName="Human Rights Act 1993" atidlm:sourceName="DLM483222" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM304211.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60350" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the employee may take 1, but not both, of the following steps:</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the employee may, if the grievance is not otherwise resolved, apply to the Authority for the resolution of the grievance:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the employee may make, in relation to those circumstances, a complaint under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36278" atidlm:branchLabel="_NOVERSION_" atidlm:name="Human Rights Act 1993" atidlm:xmlId="DLM483223" atidlm:state="UNCHANGED"><atidlm:linkcontent>Human Rights Act 1993</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="36745" atidlm:sourceXmlId="DLM483223" atidlm:targetKey="1573056" atidlm:targetXmlId="DLM304211" atidlm:targetName="Human Rights Act 1993" atidlm:sourceName="DLM483223" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM304211.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60350" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>For the purposes of <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36279" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 112" atidlm:xmlId="DLM483224" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36746" atidlm:sourceXmlId="DLM483224" atidlm:targetKey="322434" atidlm:targetXmlId="DLM60350" atidlm:targetName="prov 112" atidlm:sourceName="DLM483224" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60350" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, an employee makes a complaint when proceedings in relation to that complaint are commenced by the complainant or the Commission.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>If an employee applies to the Authority for a resolution of the grievance under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36280" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 112" atidlm:xmlId="DLM483225" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36747" atidlm:sourceXmlId="DLM483225" atidlm:targetKey="322434" atidlm:targetXmlId="DLM60350" atidlm:targetName="prov 112" atidlm:sourceName="DLM483225" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60350" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the employee may not exercise or continue to exercise any rights in relation to the subject matter of the grievance that the employee may have under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36281" atidlm:branchLabel="_NOVERSION_" atidlm:name="Human Rights Act 1993" atidlm:xmlId="DLM483226" atidlm:state="UNCHANGED"><atidlm:linkcontent>Human Rights Act 1993</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="36748" atidlm:sourceXmlId="DLM483226" atidlm:targetKey="1573056" atidlm:targetXmlId="DLM304211" atidlm:targetName="Human Rights Act 1993" atidlm:sourceName="DLM483226" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM304211.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60350" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>If an employee makes a complaint under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36282" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 112" atidlm:xmlId="DLM483227" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36749" atidlm:sourceXmlId="DLM483227" atidlm:targetKey="322434" atidlm:targetXmlId="DLM60350" atidlm:targetName="prov 112" atidlm:sourceName="DLM483227" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60350" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the employee may not exercise or continue to exercise any rights in relation to the subject matter of the complaint that the employee may have under this Act.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 39</citation>
        </cf>
        <notes>
          <history>
            <history-note id="DLM60351">Subsection <amended-provision>(2)</amended-provision> was <amending-operation>substituted</amending-operation>, as from <amendment-date>1 January 2002</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36283" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 71" atidlm:xmlId="DLM483228" atidlm:state="UNCHANGED"><atidlm:linkcontent>71</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="36750" atidlm:sourceXmlId="DLM483228" atidlm:targetKey="667862" atidlm:targetXmlId="DLM122581" atidlm:targetName="prov 71" atidlm:sourceName="DLM483228" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM122519.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60351" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Human Rights Amendment Act 2001</amending-leg> (2001 No 96).</history-note>
            <history-note id="DLM60352">Subsections <amended-provision>(3)</amended-provision> and <amended-provision>(4)</amended-provision> were <amending-operation>inserted</amending-operation>, as from <amendment-date>1 December 2004</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36284" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 41" atidlm:xmlId="DLM483229" atidlm:state="UNCHANGED"><atidlm:linkcontent>41</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="36751" atidlm:sourceXmlId="DLM483229" atidlm:targetKey="1849258" atidlm:targetXmlId="DLM316438" atidlm:targetName="prov 41" atidlm:sourceName="DLM483229" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315908.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60352" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Employment Relations Amendment Act (No 2) 2004</amending-leg> (2004 No 86). <emphasis style="italic">See</emphasis> <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36285" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 73" atidlm:xmlId="DLM483230" atidlm:state="UNCHANGED"><atidlm:linkcontent>73</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36752" atidlm:sourceXmlId="DLM483230" atidlm:targetKey="1849229" atidlm:targetXmlId="DLM316478" atidlm:targetName="prov 73" atidlm:sourceName="DLM483230" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315908.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60352" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of that Act</citation> for the transitional provisions.</history-note>
          </history>
        </notes>
      </prov>
      <prov id="DLM60353" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">113</label>
        <heading xml:lang="en-NZ">Personal grievance provisions only way to challenge dismissal</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>If an employee who has been dismissed wishes to challenge that dismissal or any aspect of it, for any reason, in any court, that challenge may be brought only in the Authority under this Part as a personal grievance.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Nothing in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36286" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 113" atidlm:xmlId="DLM483231" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36753" atidlm:sourceXmlId="DLM483231" atidlm:targetKey="322437" atidlm:targetXmlId="DLM60353" atidlm:targetName="prov 113" atidlm:sourceName="DLM483231" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60353" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> prevents an action under this Part to recover—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>wages relating to a period of notice or alleged period of notice; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>wages or other money relating to the employment prior to the dismissal; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>other money payable on dismissal.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM60354" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">114</label>
        <heading xml:lang="en-NZ">Raising personal grievance</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Every employee who wishes to raise a personal grievance must, subject to <citation jurisdiction="nz">subsections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36287" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 114" atidlm:xmlId="DLM483232" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36754" atidlm:sourceXmlId="DLM483232" atidlm:targetKey="322438" atidlm:targetXmlId="DLM60354" atidlm:targetName="prov 114" atidlm:sourceName="DLM483232" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60354" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36288" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 114" atidlm:xmlId="DLM483233" atidlm:state="UNCHANGED"><atidlm:linkcontent>(4)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36755" atidlm:sourceXmlId="DLM483233" atidlm:targetKey="322438" atidlm:targetXmlId="DLM60354" atidlm:targetName="prov 114" atidlm:sourceName="DLM483233" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60354" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, raise the grievance with his or her employer within the period of 90 days beginning with the date on which the action alleged to amount to a personal grievance occurred or came to the notice of the employee, whichever is the later, unless the employer consents to the personal grievance being raised after the expiration of that period.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>For the purposes of <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36289" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 114" atidlm:xmlId="DLM483234" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36756" atidlm:sourceXmlId="DLM483234" atidlm:targetKey="322438" atidlm:targetXmlId="DLM60354" atidlm:targetName="prov 114" atidlm:sourceName="DLM483234" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60354" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, a grievance is raised with an employer as soon as the employee has made, or has taken reasonable steps to make, the employer or a representative of the employer aware that the employee alleges a personal grievance that the employee wants the employer to address.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>Where the employer does not consent to the personal grievance being raised after the expiration of the 90-day period, the employee may apply to the Authority for leave to raise the personal grievance after the expiration of that period.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>On an application under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36290" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 114" atidlm:xmlId="DLM483235" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36757" atidlm:sourceXmlId="DLM483235" atidlm:targetKey="322438" atidlm:targetXmlId="DLM60354" atidlm:targetName="prov 114" atidlm:sourceName="DLM483235" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60354" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the Authority, after giving the employer an opportunity to be heard, may grant leave accordingly, subject to such conditions (if any) as it thinks fit, if the Authority—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>is satisfied that the delay in raising the personal grievance was occasioned by exceptional circumstances (which may include any 1 or more of the circumstances set out in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36291" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 115" atidlm:xmlId="DLM483236" atidlm:state="UNCHANGED"><atidlm:linkcontent>115</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36758" atidlm:sourceXmlId="DLM483236" atidlm:targetKey="322439" atidlm:targetXmlId="DLM60355" atidlm:targetName="prov 115" atidlm:sourceName="DLM483236" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60354" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>); and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>considers it just to do so.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">5</label>
            <para xml:lang="en-NZ">
              <text>In any case where the Authority grants leave under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36292" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 114" atidlm:xmlId="DLM483237" atidlm:state="UNCHANGED"><atidlm:linkcontent>(4)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36800" atidlm:sourceXmlId="DLM483237" atidlm:targetKey="322438" atidlm:targetXmlId="DLM60354" atidlm:targetName="prov 114" atidlm:sourceName="DLM483237" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60354" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the Authority must direct the employer and employee to use mediation to seek to mutually resolve the grievance.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">6</label>
            <para xml:lang="en-NZ">
              <text>No action may be commenced in the Authority or the Court in relation to a personal grievance more than 3 years after the date on which the personal grievance was raised in accordance with this section.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 33</citation>
        </cf>
      </prov>
      <prov id="DLM60355" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">115</label>
        <heading xml:lang="en-NZ">Further provision regarding exceptional circumstances under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36293" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 114" atidlm:xmlId="DLM483238" atidlm:state="UNCHANGED"><atidlm:linkcontent>114</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36801" atidlm:sourceXmlId="DLM483238" atidlm:targetKey="322438" atidlm:targetXmlId="DLM60354" atidlm:targetName="prov 114" atidlm:sourceName="DLM483238" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60355" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>For the purposes of <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36294" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 114" atidlm:xmlId="DLM483239" atidlm:state="UNCHANGED"><atidlm:linkcontent>114(4)(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36802" atidlm:sourceXmlId="DLM483239" atidlm:targetKey="322438" atidlm:targetXmlId="DLM60354" atidlm:targetName="prov 114" atidlm:sourceName="DLM483239" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60355" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, exceptional circumstances include—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>where the employee has been so affected or traumatised by the matter giving rise to the grievance that he or she was unable to properly consider raising the grievance within the period specified in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36295" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 114" atidlm:xmlId="DLM483240" atidlm:state="UNCHANGED"><atidlm:linkcontent>114(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36803" atidlm:sourceXmlId="DLM483240" atidlm:targetKey="322438" atidlm:targetXmlId="DLM60354" atidlm:targetName="prov 114" atidlm:sourceName="DLM483240" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60355" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>where the employee made reasonable arrangements to have the grievance raised on his or her behalf by an agent of the employee, and the agent unreasonably failed to ensure that the grievance was raised within the required time; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>where the employee's employment agreement does not contain the explanation concerning the resolution of employment relationship problems that is required by <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36296" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 54" atidlm:xmlId="DLM483241" atidlm:state="UNCHANGED"><atidlm:linkcontent>54</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36804" atidlm:sourceXmlId="DLM483241" atidlm:targetKey="314657" atidlm:targetXmlId="DLM59115" atidlm:targetName="prov 54" atidlm:sourceName="DLM483241" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM58658.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60355" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36297" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 65" atidlm:xmlId="DLM483242" atidlm:state="UNCHANGED"><atidlm:linkcontent>65</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36805" atidlm:sourceXmlId="DLM483242" atidlm:targetKey="315822" atidlm:targetXmlId="DLM59157" atidlm:targetName="prov 65" atidlm:sourceName="DLM483242" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM59135.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60355" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, as the case may be; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>where the employer has failed to comply with the obligation under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36298" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 120" atidlm:xmlId="DLM483243" atidlm:state="UNCHANGED"><atidlm:linkcontent>120(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36806" atidlm:sourceXmlId="DLM483243" atidlm:targetKey="322444" atidlm:targetXmlId="DLM60360" atidlm:targetName="prov 120" atidlm:sourceName="DLM483243" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60355" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> to provide a statement of reasons for dismissal.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM60356" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">116</label>
        <heading xml:lang="en-NZ">Special provision where sexual harassment alleged</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Where a personal grievance involves allegations of sexual harassment, no account may be taken of any evidence of the complainant's sexual experience or reputation.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 35</citation>
        </cf>
      </prov>
      <prov id="DLM60357" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">117</label>
        <heading xml:lang="en-NZ">Sexual or racial harassment by person other than employer</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>This section applies where—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>a request of the kind described in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36299" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 108" atidlm:xmlId="DLM483244" atidlm:state="UNCHANGED"><atidlm:linkcontent>108(1)(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36807" atidlm:sourceXmlId="DLM483244" atidlm:targetKey="322424" atidlm:targetXmlId="DLM60340" atidlm:targetName="prov 108" atidlm:sourceName="DLM483244" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60357" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is made to an employee by a person (not being a representative of the employer) who is in the employ of the employee's employer or who is a customer or client of the employer; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>an employee is subjected to behaviour of the kind described in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36300" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 108" atidlm:xmlId="DLM483245" atidlm:state="UNCHANGED"><atidlm:linkcontent>108(1)(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36808" atidlm:sourceXmlId="DLM483245" atidlm:targetKey="322424" atidlm:targetXmlId="DLM60340" atidlm:targetName="prov 108" atidlm:sourceName="DLM483245" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60357" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> by a person (not being a representative of the employer) who is in the employ of the employee's employer or who is a customer or client of the employer; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>an employee is subjected to behaviour of the kind described in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36301" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 109" atidlm:xmlId="DLM483246" atidlm:state="UNCHANGED"><atidlm:linkcontent>109</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36809" atidlm:sourceXmlId="DLM483246" atidlm:targetKey="322425" atidlm:targetXmlId="DLM60341" atidlm:targetName="prov 109" atidlm:sourceName="DLM483246" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60357" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> by a person (not being a representative of the employer) who is in the employ of the employee's employer or who is a customer or client of the employer.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>If this section applies, the employee may make a complaint about that request or behaviour to the employee's employer or to a representative of the employer.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>The employer or representative, on receiving a complaint under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36302" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 117" atidlm:xmlId="DLM483247" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36810" atidlm:sourceXmlId="DLM483247" atidlm:targetKey="322441" atidlm:targetXmlId="DLM60357" atidlm:targetName="prov 117" atidlm:sourceName="DLM483247" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60357" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, must inquire into the facts.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>If the employer or representative is satisfied that the request was made or that the behaviour took place, the employer or representative must take whatever steps are practicable to prevent any repetition of such a request or of such behaviour.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 36(1), <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36303" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 36" atidlm:xmlId="DLM483248" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36811" atidlm:sourceXmlId="DLM483248" atidlm:targetKey="314612" atidlm:targetXmlId="DLM58670" atidlm:targetName="prov 36" atidlm:sourceName="DLM483248" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM58658.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60357" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
        </cf>
      </prov>
      <prov id="DLM60358" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">118</label>
        <heading xml:lang="en-NZ">Sexual or racial harassment after steps not taken to prevent repetition</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>This section applies if—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>a person in relation to whom an employee has made a complaint under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36304" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 117" atidlm:xmlId="DLM483249" atidlm:state="UNCHANGED"><atidlm:linkcontent>117(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36812" atidlm:sourceXmlId="DLM483249" atidlm:targetKey="322441" atidlm:targetXmlId="DLM60357" atidlm:targetName="prov 117" atidlm:sourceName="DLM483249" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60358" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> either—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
                    <para xml:lang="en-NZ">
                      <text>makes to that employee after the complaint a request of the kind described in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36305" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 108" atidlm:xmlId="DLM483250" atidlm:state="UNCHANGED"><atidlm:linkcontent>108(1)(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36813" atidlm:sourceXmlId="DLM483250" atidlm:targetKey="322424" atidlm:targetXmlId="DLM60340" atidlm:targetName="prov 108" atidlm:sourceName="DLM483250" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60358" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">ii</label>
                    <para xml:lang="en-NZ">
                      <text>subjects that employee after the complaint to behaviour of the kind described in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36306" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 108" atidlm:xmlId="DLM483251" atidlm:state="UNCHANGED"><atidlm:linkcontent>108(1)(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36814" atidlm:sourceXmlId="DLM483251" atidlm:targetKey="322424" atidlm:targetXmlId="DLM60340" atidlm:targetName="prov 108" atidlm:sourceName="DLM483251" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60358" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36307" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 109" atidlm:xmlId="DLM483252" atidlm:state="UNCHANGED"><atidlm:linkcontent>109</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36815" atidlm:sourceXmlId="DLM483252" atidlm:targetKey="322425" atidlm:targetXmlId="DLM60341" atidlm:targetName="prov 109" atidlm:sourceName="DLM483252" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60358" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                    </para>
                  </label-para>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the employer of that employee, or a representative of that employer, has not taken whatever steps are practicable to prevent the repetition of such a request or such behaviour.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>If this section applies, the employee is deemed for the purposes of this Act and for the purposes of any employment agreement to have a personal grievance by virtue of having been sexually harassed or racially harassed, as the case may be, in the course of the employee's employment as if the request or behaviour were that of the employee's employer.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 36(3)</citation>
        </cf>
      </prov>
      <prov id="DLM60359" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">119</label>
        <heading xml:lang="en-NZ">Presumption in discrimination cases</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36308" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 119" atidlm:xmlId="DLM483253" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36816" atidlm:sourceXmlId="DLM483253" atidlm:targetKey="322443" atidlm:targetXmlId="DLM60359" atidlm:targetName="prov 119" atidlm:sourceName="DLM483253" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60359" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> applies if, in any matter before the Authority or the Court,—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the employee establishes that the employer or the employer's representative took any action or omitted any action as described in any of <citation jurisdiction="nz">paragraphs <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36309" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 104" atidlm:xmlId="DLM483254" atidlm:state="UNCHANGED"><atidlm:linkcontent>(a) to (c)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36817" atidlm:sourceXmlId="DLM483254" atidlm:targetKey="322413" atidlm:targetXmlId="DLM60329" atidlm:targetName="prov 104" atidlm:sourceName="DLM483254" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60359" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of section 104(1)</citation> in relation to that employee; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>if it is a case where the employee alleges that the discrimination was by reason directly or indirectly of the employee's involvement in the activities of a union, the employee establishes that he or she was a person described in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36310" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 107" atidlm:xmlId="DLM483255" atidlm:state="UNCHANGED"><atidlm:linkcontent>107</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36818" atidlm:sourceXmlId="DLM483255" atidlm:targetKey="322421" atidlm:targetXmlId="DLM60337" atidlm:targetName="prov 107" atidlm:sourceName="DLM483255" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60359" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>If this subsection applies, there is a rebuttable presumption that the employer or representative of the employer discriminated against the employee on the grounds, or for the reason, specified in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36311" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 104" atidlm:xmlId="DLM483256" atidlm:state="UNCHANGED"><atidlm:linkcontent>104(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36819" atidlm:sourceXmlId="DLM483256" atidlm:targetKey="322413" atidlm:targetXmlId="DLM60329" atidlm:targetName="prov 104" atidlm:sourceName="DLM483256" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60359" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and alleged by the employee.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM60360" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">120</label>
        <heading xml:lang="en-NZ">Statement of reasons for dismissal</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Where an employee is dismissed, that employee may, within 60 days after the dismissal or within 60 days after the employee has become aware of the dismissal, whichever is the later, request the employer to provide a statement in writing of the reasons for the dismissal.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Every employer to whom a request is made under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36312" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 120" atidlm:xmlId="DLM483257" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36820" atidlm:sourceXmlId="DLM483257" atidlm:targetKey="322444" atidlm:targetXmlId="DLM60360" atidlm:targetName="prov 120" atidlm:sourceName="DLM483257" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60360" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must, within 14 days after the day on which the request is received, provide the statement to the person who made the request.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 38</citation>
        </cf>
      </prov>
      <prov id="DLM60361" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">121</label>
        <heading xml:lang="en-NZ">Statements privileged</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Any statements made or information given in the course of raising a personal grievance or in the course of attempting to resolve the grievance or in the course of any matter relating to a personal grievance are absolutely privileged.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 37</citation>
        </cf>
      </prov>
      <prov id="DLM60362" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">122</label>
        <heading xml:lang="en-NZ">Nature of personal grievance may be found to be of different type from that alleged</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Nothing in this Part or in any employment agreement prevents a finding that a personal grievance is of a type other than that alleged.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 34</citation>
        </cf>
      </prov>
      <crosshead id="DLM60363" toc="yes" xml:lang="en-NZ">Remedies in relation to personal grievances</crosshead>
      <prov id="DLM60364" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">123</label>
        <heading xml:lang="en-NZ">Remedies</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Where the Authority or the Court determines that an employee has a personal grievance, it may, in settling the grievance, provide for any 1 or more of the following remedies:</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>reinstatement of the employee in the employee's former position or the placement of the employee in a position no less advantageous to the employee:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the reimbursement to the employee of a sum equal to the whole or any part of the wages or other money lost by the employee as a result of the grievance:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>the payment to the employee of compensation by the employee's employer, including compensation for—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
                    <para xml:lang="en-NZ">
                      <text>humiliation, loss of dignity, and injury to the feelings of the employee; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">ii</label>
                    <para xml:lang="en-NZ">
                      <text>loss of any benefit, whether or not of a monetary kind, which the employee might reasonably have been expected to obtain if the personal grievance had not arisen:</text>
                    </para>
                  </label-para>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">ca</label>
                <para xml:lang="en-NZ">
                  <text>if the Authority or the Court finds that any workplace conduct or practices are a significant factor in the personal grievance, recommendations to the employer concerning the action the employer should take to prevent similar employment relationship problems occurring:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>if the Authority or the Court finds an employee to have been sexually or racially harassed in his or her employment, recommendations to the employer—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
                    <para xml:lang="en-NZ">
                      <text>concerning the action the employer should take in respect of the person who made the request or was guilty of the harassing behaviour, which action may include the transfer of that person, the taking of disciplinary action against that person, or the taking of rehabilitative action in respect of that person:</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">ii</label>
                    <para xml:lang="en-NZ">
                      <text>about any other action that it is necessary for the employer to take to prevent further harassment of the employee concerned or any other employee.</text>
                    </para>
                  </label-para>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>When making an order under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36313" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 123" atidlm:xmlId="DLM483258" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36821" atidlm:sourceXmlId="DLM483258" atidlm:targetKey="322448" atidlm:targetXmlId="DLM60364" atidlm:targetName="prov 123" atidlm:sourceName="DLM483258" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60364" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36314" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 123" atidlm:xmlId="DLM483259" atidlm:state="UNCHANGED"><atidlm:linkcontent>(c)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36822" atidlm:sourceXmlId="DLM483259" atidlm:targetKey="322448" atidlm:targetXmlId="DLM60364" atidlm:targetName="prov 123" atidlm:sourceName="DLM483259" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60364" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the Authority or the Court may order payment to the employee by instalments, but only if the financial position of the employer requires it.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 40</citation>
        </cf>
        <notes>
          <history>
            <history-note id="DLM60365">Paragraph <amended-provision>(ca)</amended-provision> was <amending-operation>inserted</amending-operation>, as from <amendment-date>1 December 2004</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36315" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 42" atidlm:xmlId="DLM483260" atidlm:state="UNCHANGED"><atidlm:linkcontent>42(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="36823" atidlm:sourceXmlId="DLM483260" atidlm:targetKey="1849259" atidlm:targetXmlId="DLM316439" atidlm:targetName="prov 42" atidlm:sourceName="DLM483260" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315908.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60365" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Employment Relations Amendment Act (No 2) 2004</amending-leg> (2004 No 86). <emphasis style="italic">See</emphasis> <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36316" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 73" atidlm:xmlId="DLM483261" atidlm:state="UNCHANGED"><atidlm:linkcontent>73</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36824" atidlm:sourceXmlId="DLM483261" atidlm:targetKey="1849229" atidlm:targetXmlId="DLM316478" atidlm:targetName="prov 73" atidlm:sourceName="DLM483261" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315908.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60365" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of that Act</citation> for the transitional provisions.</history-note>
            <history-note id="DLM60366">Subsection <amended-provision>(2)</amended-provision> was <amending-operation>inserted</amending-operation>, as from <amendment-date>1 December 2004</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36317" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 42" atidlm:xmlId="DLM483262" atidlm:state="UNCHANGED"><atidlm:linkcontent>42(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="36825" atidlm:sourceXmlId="DLM483262" atidlm:targetKey="1849259" atidlm:targetXmlId="DLM316439" atidlm:targetName="prov 42" atidlm:sourceName="DLM483262" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315908.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60366" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Employment Relations Amendment Act (No 2) 2004</amending-leg> (2004 No 86). <emphasis style="italic">See</emphasis> <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36318" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 73" atidlm:xmlId="DLM483263" atidlm:state="UNCHANGED"><atidlm:linkcontent>73</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36826" atidlm:sourceXmlId="DLM483263" atidlm:targetKey="1849229" atidlm:targetXmlId="DLM316478" atidlm:targetName="prov 73" atidlm:sourceName="DLM483263" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315908.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60366" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of that Act</citation> for the transitional provisions.</history-note>
          </history>
        </notes>
      </prov>
      <prov id="DLM60367" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">124</label>
        <heading xml:lang="en-NZ">Remedy reduced if contributing behaviour by employee</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Where the Authority or the Court determines that an employee has a personal grievance, the Authority or the Court must, in deciding both the nature and the extent of the remedies to be provided in respect of that personal grievance,—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>consider the extent to which the actions of the employee contributed towards the situation that gave rise to the personal grievance; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>if those actions so require, reduce the remedies that would otherwise have been awarded accordingly.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 ss 40(2), 41(3)</citation>
        </cf>
      </prov>
      <prov id="DLM60368" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">125</label>
        <heading xml:lang="en-NZ">Reinstatement to be primary remedy</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>This section applies where—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the remedies sought by or on behalf of an employee in respect of a personal grievance include reinstatement (as described in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36319" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 123" atidlm:xmlId="DLM483264" atidlm:state="UNCHANGED"><atidlm:linkcontent>123(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36827" atidlm:sourceXmlId="DLM483264" atidlm:targetKey="322448" atidlm:targetXmlId="DLM60364" atidlm:targetName="prov 123" atidlm:sourceName="DLM483264" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60368" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>); and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>it is determined that the employee did have a personal grievance.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>If this section applies the Authority must, whether or not it provides for any of the other remedies provided for in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36320" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 123" atidlm:xmlId="DLM483265" atidlm:state="UNCHANGED"><atidlm:linkcontent>123</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36828" atidlm:sourceXmlId="DLM483265" atidlm:targetKey="322448" atidlm:targetXmlId="DLM60364" atidlm:targetName="prov 123" atidlm:sourceName="DLM483265" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60368" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, provide, wherever practicable, for reinstatement as described in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36321" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 123" atidlm:xmlId="DLM483266" atidlm:state="UNCHANGED"><atidlm:linkcontent>123(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36829" atidlm:sourceXmlId="DLM483266" atidlm:targetKey="322448" atidlm:targetXmlId="DLM60364" atidlm:targetName="prov 123" atidlm:sourceName="DLM483266" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60368" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1987 No 77 s 228(1)</citation>
        </cf>
      </prov>
      <prov id="DLM60369" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">126</label>
        <heading xml:lang="en-NZ">Provisions applying if reinstatement ordered</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Where the remedy of reinstatement is provided by the Authority or the Court, the employee must be reinstated immediately or on such date as is specified by the Authority or the Court and, despite any challenge to or appeal against the determination of the Authority or the Court, the provisions for reinstatement remain in full force pending the outcome of those proceedings unless the Authority or the Court otherwise orders.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 42</citation>
        </cf>
      </prov>
      <prov id="DLM60370" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">127</label>
        <heading xml:lang="en-NZ">Authority may order interim reinstatement</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The Authority may if it thinks fit, on the application of an employee who has raised a personal grievance with his or her employer, make an order for the interim reinstatement of the employee pending the hearing of the personal grievance.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The employee must, at the time of filing the application for an order under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36322" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 127" atidlm:xmlId="DLM483267" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36830" atidlm:sourceXmlId="DLM483267" atidlm:targetKey="322454" atidlm:targetXmlId="DLM60370" atidlm:targetName="prov 127" atidlm:sourceName="DLM483267" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60370" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, file a signed undertaking that the employee will abide by any order that the Authority may make in respect of damages—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>that are sustained by the other party through the granting of the order for interim reinstatement; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>that the Authority decides that the employee ought to pay.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>The undertaking must be referred to in the order for interim reinstatement and is part of it.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>When determining whether to make an order for interim reinstatement, the Authority must apply the law relating to interim injunctions having regard to the object of this Act.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">5</label>
            <para xml:lang="en-NZ">
              <text>The order for interim reinstatement may be subject to any conditions that the Authority thinks fit.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">6</label>
            <para xml:lang="en-NZ">
              <text>The Authority may at any time rescind or vary an order made under this section.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">7</label>
            <para xml:lang="en-NZ">
              <text>Nothing in this section prevents the Court from granting an interim injunction reinstating an employee if the Court is seized of the proceedings dealing with the personal grievance.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM60371" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">128</label>
        <heading xml:lang="en-NZ">Reimbursement</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>This section applies where the Authority or the Court determines, in respect of any employee,—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>that the employee has a personal grievance; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>that the employee has lost remuneration as a result of the personal grievance.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>If this section applies then, subject to <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36323" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 128" atidlm:xmlId="DLM483268" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36831" atidlm:sourceXmlId="DLM483268" atidlm:targetKey="322455" atidlm:targetXmlId="DLM60371" atidlm:targetName="prov 128" atidlm:sourceName="DLM483268" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60371" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36324" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 124" atidlm:xmlId="DLM483269" atidlm:state="UNCHANGED"><atidlm:linkcontent>124</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36832" atidlm:sourceXmlId="DLM483269" atidlm:targetKey="322451" atidlm:targetXmlId="DLM60367" atidlm:targetName="prov 124" atidlm:sourceName="DLM483269" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60371" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the Authority must, whether or not it provides for any of the other remedies provided for in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36325" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 123" atidlm:xmlId="DLM483270" atidlm:state="UNCHANGED"><atidlm:linkcontent>123</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36833" atidlm:sourceXmlId="DLM483270" atidlm:targetKey="322448" atidlm:targetXmlId="DLM60364" atidlm:targetName="prov 123" atidlm:sourceName="DLM483270" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60371" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, order the employer to pay to the employee the lesser of a sum equal to that lost remuneration or to 3 months' ordinary time remuneration.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>Despite <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36326" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 128" atidlm:xmlId="DLM483271" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36834" atidlm:sourceXmlId="DLM483271" atidlm:targetKey="322455" atidlm:targetXmlId="DLM60371" atidlm:targetName="prov 128" atidlm:sourceName="DLM483271" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60371" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the Authority may, in its discretion, order an employer to pay to an employee by way of compensation for remuneration lost by that employee as a result of the personal grievance, a sum greater than that to which an order under that subsection may relate.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 41(1), (2)</citation>
        </cf>
      </prov>
      <crosshead id="DLM60372" toc="yes" xml:lang="en-NZ">Disputes</crosshead>
      <prov id="DLM60373" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">129</label>
        <heading xml:lang="en-NZ">Person bound by, or party to, employment agreement may pursue dispute under this Act</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Where there is a dispute about the interpretation, application, or operation of an employment agreement, any person bound by the agreement or any party to the agreement may pursue that dispute in accordance with <citation jurisdiction="nz">Part <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36327" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 10 Institutions" atidlm:xmlId="DLM483272" atidlm:state="UNCHANGED"><atidlm:linkcontent>10</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36835" atidlm:sourceXmlId="DLM483272" atidlm:targetKey="324200" atidlm:targetXmlId="DLM60904" atidlm:targetName="part 10 Institutions" atidlm:sourceName="DLM483272" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60373" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>If the dispute relates to a collective agreement, the person or party pursuing the dispute must ensure that all union and employer parties to the agreement have notice of the existence of the dispute.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 44</citation>
        </cf>
      </prov>
      <crosshead id="DLM60374" toc="yes" xml:lang="en-NZ">Recovery of wages</crosshead>
      <prov id="DLM60375" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">130</label>
        <heading xml:lang="en-NZ">Wages and time record</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Every employer must at all times keep a record (called the wages and time record) showing, in the case of each employee employed by that employer,—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the name of the employee:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the employee's age, if under 20 years of age:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>the employee's postal address:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>the kind of work on which the employee is usually employed:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">e</label>
                <para xml:lang="en-NZ">
                  <text>whether the employee is employed under an individual employment agreement or a collective agreement:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">f</label>
                <para xml:lang="en-NZ">
                  <text>in the case of an employee employed under a collective agreement, the title and expiry date of the agreement, and the employee's classification under it:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">g</label>
                <para xml:lang="en-NZ">
                  <text>where necessary for the purpose of calculating the employee's pay, the hours between which the employee is employed on each day, and the days of the employee's employment during each pay period:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">h</label>
                <para xml:lang="en-NZ">
                  <text>the wages paid to the employee each pay period and the method of calculation:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">i</label>
                <para xml:lang="en-NZ">
                  <text>details of any employment relations education leave taken under <citation jurisdiction="nz">Part <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36328" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 7 Employment relations education leave" atidlm:xmlId="DLM483273" atidlm:state="UNCHANGED"><atidlm:linkcontent>7</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36836" atidlm:sourceXmlId="DLM483273" atidlm:targetKey="319600" atidlm:targetXmlId="DLM59935" atidlm:targetName="part 7 Employment relations education leave" atidlm:sourceName="DLM483273" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM59935.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60375" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">j</label>
                <para xml:lang="en-NZ">
                  <text>such other particulars as may be prescribed.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Every employer must, upon request by an employee or by a person authorised under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36329" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 236" atidlm:xmlId="DLM483274" atidlm:state="UNCHANGED"><atidlm:linkcontent>236</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36837" atidlm:sourceXmlId="DLM483274" atidlm:targetKey="325235" atidlm:targetXmlId="DLM61477" atidlm:targetName="prov 236" atidlm:sourceName="DLM483274" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM61442.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60375" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> to represent an employee, provide that employee or person immediately with access to or a copy of or an extract from any part or all of the wages and time record relating to the employment of the employee by the employer at any time in the preceding 6 years at which the employer was obliged to keep such a record.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>Where an employer keeps a wages and time record in accordance with any other Act, that employer is not required to keep a wages and time record under this Act in respect of the same matters.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>Every employer who fails to comply with any requirement of this section is liable to a penalty imposed by the Authority.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 47</citation>
        </cf>
      </prov>
      <prov id="DLM60376" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">131</label>
        <heading xml:lang="en-NZ">Arrears</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Where—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>there has been default in payment to an employee of any wages or other money payable by an employer to an employee under an employment agreement or a contract of apprenticeship; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>any payments of any such wages or other money has been made at a rate lower than that legally payable,—</text>
                </para>
              </label-para>
              <text>the whole or any part, as the case may require, of any such wages or other money may be recovered by the employee by action commenced in the prescribed manner in the Authority.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1A</label>
            <para xml:lang="en-NZ">
              <text>The Authority may order payment of the wages or other money to the employee by instalments, but only if the financial position of the employer requires it.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36330" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 131" atidlm:xmlId="DLM483275" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36838" atidlm:sourceXmlId="DLM483275" atidlm:targetKey="322460" atidlm:targetXmlId="DLM60376" atidlm:targetName="prov 131" atidlm:sourceName="DLM483275" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60376" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> applies despite the acceptance by the employee of any payment at a lower rate or any express or implied agreement to the contrary.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36331" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 131" atidlm:xmlId="DLM483276" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36840" atidlm:sourceXmlId="DLM483276" atidlm:targetKey="322460" atidlm:targetXmlId="DLM60376" atidlm:targetName="prov 131" atidlm:sourceName="DLM483276" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60376" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> does not affect any other remedies for the recovery of wages or other money payable by an employer to any employee under an employment agreement or a contract of apprenticeship.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 48(1)</citation>
        </cf>
        <notes>
          <history>
            <history-note id="DLM60377">Subsection <amended-provision>(1A)</amended-provision> was <amending-operation>inserted</amending-operation>, as from <amendment-date>1 December 2004</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36332" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 43" atidlm:xmlId="DLM483277" atidlm:state="UNCHANGED"><atidlm:linkcontent>43</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="36841" atidlm:sourceXmlId="DLM483277" atidlm:targetKey="1849260" atidlm:targetXmlId="DLM316440" atidlm:targetName="prov 43" atidlm:sourceName="DLM483277" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315908.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60377" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Employment Relations Amendment Act (No 2) 2004</amending-leg> (2004 No 86). <emphasis style="italic">See</emphasis> <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36333" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 73" atidlm:xmlId="DLM483278" atidlm:state="UNCHANGED"><atidlm:linkcontent>73</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36842" atidlm:sourceXmlId="DLM483278" atidlm:targetKey="1849229" atidlm:targetXmlId="DLM316478" atidlm:targetName="prov 73" atidlm:sourceName="DLM483278" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315908.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60377" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of that Act</citation> for the transitional provisions.</history-note>
          </history>
        </notes>
      </prov>
      <prov id="DLM60378" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">132</label>
        <heading xml:lang="en-NZ">Failure to keep or produce records</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Where any claim is brought before the Authority under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36334" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 131" atidlm:xmlId="DLM483279" atidlm:state="UNCHANGED"><atidlm:linkcontent>131</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36843" atidlm:sourceXmlId="DLM483279" atidlm:targetKey="322460" atidlm:targetXmlId="DLM60376" atidlm:targetName="prov 131" atidlm:sourceName="DLM483279" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60378" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> to recover wages or other money payable to an employee, the employee may call evidence to show that—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the defendant employer failed to keep or produce a wages and time record in respect of that employee as required by this Act; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>that failure prejudiced the employee's ability to bring an accurate claim under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36335" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 131" atidlm:xmlId="DLM483280" atidlm:state="UNCHANGED"><atidlm:linkcontent>131</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36844" atidlm:sourceXmlId="DLM483280" atidlm:targetKey="322460" atidlm:targetXmlId="DLM60376" atidlm:targetName="prov 131" atidlm:sourceName="DLM483280" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60378" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Where evidence of the type referred to in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36336" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 132" atidlm:xmlId="DLM483281" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36845" atidlm:sourceXmlId="DLM483281" atidlm:targetKey="322462" atidlm:targetXmlId="DLM60378" atidlm:targetName="prov 132" atidlm:sourceName="DLM483281" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60378" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is given, the Authority may, unless the defendant proves that those claims are incorrect, accept as proved all claims made by the employee in respect of—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the wages actually paid to the employee:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the hours, days, and time worked by the employee.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>A defendant may not use as evidence any wages and time record that would be inadmissible under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36337" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 232" atidlm:xmlId="DLM483282" atidlm:state="UNCHANGED"><atidlm:linkcontent>232(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36846" atidlm:sourceXmlId="DLM483282" atidlm:targetKey="325221" atidlm:targetXmlId="DLM61463" atidlm:targetName="prov 232" atidlm:sourceName="DLM483282" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM61442.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60378" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 50</citation>
        </cf>
      </prov>
      <crosshead id="DLM60379" toc="yes" xml:lang="en-NZ">Penalties</crosshead>
      <prov id="DLM60380" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">133</label>
        <heading xml:lang="en-NZ">Jurisdiction concerning penalties</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The Authority has full and exclusive jurisdiction to deal with all actions for the recovery of penalties under this Act—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>for any breach of an employment agreement; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>for a breach of any provision of this Act for which a penalty in the Authority is provided in the particular provision.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36338" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 133" atidlm:xmlId="DLM483283" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36847" atidlm:sourceXmlId="DLM483283" atidlm:targetKey="322464" atidlm:targetXmlId="DLM60380" atidlm:targetName="prov 133" atidlm:sourceName="DLM483283" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60380" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is subject to—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text><citation jurisdiction="nz">sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36339" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 177" atidlm:xmlId="DLM483284" atidlm:state="UNCHANGED"><atidlm:linkcontent>177</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36848" atidlm:sourceXmlId="DLM483284" atidlm:targetKey="324259" atidlm:targetXmlId="DLM60963" atidlm:targetName="prov 177" atidlm:sourceName="DLM483284" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60380" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36340" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 178" atidlm:xmlId="DLM483285" atidlm:state="UNCHANGED"><atidlm:linkcontent>178</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36849" atidlm:sourceXmlId="DLM483285" atidlm:targetKey="324261" atidlm:targetXmlId="DLM60965" atidlm:targetName="prov 178" atidlm:sourceName="DLM483285" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60380" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> (which allow for the referral or removal of certain matters to the Employment Court); and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>any right to have the matter heard by the Court under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36341" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 179" atidlm:xmlId="DLM483286" atidlm:state="UNCHANGED"><atidlm:linkcontent>179</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36850" atidlm:sourceXmlId="DLM483286" atidlm:targetKey="324263" atidlm:targetXmlId="DLM60967" atidlm:targetName="prov 179" atidlm:sourceName="DLM483286" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60904.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60380" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>Subject to any rights of appeal under this Act, the Court has full and exclusive jurisdiction to deal with all actions for the recovery of penalties under this Act for a breach of any other provision of this Act for which a penalty in the Court is provided in the particular provision.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 51</citation>
        </cf>
      </prov>
      <prov id="DLM60381" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">134</label>
        <heading xml:lang="en-NZ">Penalties for breach of employment agreement</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Every party to an employment agreement who breaches that agreement is liable to a penalty under this Act.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Every person who incites, instigates, aids, or abets any breach of an employment agreement is liable to a penalty imposed by the Authority.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 52</citation>
        </cf>
      </prov>
      <prov id="DLM60382" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">135</label>
        <heading xml:lang="en-NZ">Recovery of penalties</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Any action for the recovery of a penalty may be brought,—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>in the case of a breach of an employment agreement, at the suit of any party to the employment agreement who is affected by the breach; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>in the case of a breach of this Act, at the suit of any person in relation to whom the breach is alleged to have taken place; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>if permitted in the particular penalty provision, by a Labour Inspector.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Every person who is liable to a penalty under this Act is liable,—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>in the case of an individual, to a penalty not exceeding $5,000:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>in the case of a company or other corporation, to a penalty not exceeding $10,000.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>A claim for 2 or more penalties against the same person may be joined in the same action.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>In any claim for a penalty the Authority or the Court may give judgment for the total amount claimed, or any amount, not exceeding the maximum specified in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36342" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 135" atidlm:xmlId="DLM483287" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36851" atidlm:sourceXmlId="DLM483287" atidlm:targetKey="322466" atidlm:targetXmlId="DLM60382" atidlm:targetName="prov 135" atidlm:sourceName="DLM483287" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60382" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, or the Authority or the Court may dismiss the action.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4A</label>
            <para xml:lang="en-NZ">
              <text>The Authority or the Court may order payment of a penalty by instalments, but only if the financial position of the person paying the penalty requires it.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">5</label>
            <para xml:lang="en-NZ">
              <text>An action for the recovery of a penalty under this Act must be commenced within 12 months after the earlier of—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the date when the cause of action first became known to the person bringing the action; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the date when the cause of action should reasonably have become known to the person bringing the action.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 53</citation>
        </cf>
        <notes>
          <history>
            <history-note id="DLM60383">Subsection <amended-provision>(4A)</amended-provision> was <amending-operation>inserted</amending-operation>, as from <amendment-date>1 December 2004</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36343" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 44" atidlm:xmlId="DLM483288" atidlm:state="UNCHANGED"><atidlm:linkcontent>44(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="36852" atidlm:sourceXmlId="DLM483288" atidlm:targetKey="1849261" atidlm:targetXmlId="DLM316441" atidlm:targetName="prov 44" atidlm:sourceName="DLM483288" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315908.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60383" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Employment Relations Amendment Act (No 2) 2004</amending-leg> (2004 No 86). <emphasis style="italic">See</emphasis> <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36344" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 73" atidlm:xmlId="DLM483289" atidlm:state="UNCHANGED"><atidlm:linkcontent>73</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36853" atidlm:sourceXmlId="DLM483289" atidlm:targetKey="1849229" atidlm:targetXmlId="DLM316478" atidlm:targetName="prov 73" atidlm:sourceName="DLM483289" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315908.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60383" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of that Act</citation> for the transitional provisions.</history-note>
            <history-note id="DLM60384">Subsection <amended-provision>(5)</amended-provision> was <amending-operation>substituted</amending-operation>, as from <amendment-date>1 December 2004</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36345" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 44" atidlm:xmlId="DLM483290" atidlm:state="UNCHANGED"><atidlm:linkcontent>44(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="36854" atidlm:sourceXmlId="DLM483290" atidlm:targetKey="1849261" atidlm:targetXmlId="DLM316441" atidlm:targetName="prov 44" atidlm:sourceName="DLM483290" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315908.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60384" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Employment Relations Amendment Act (No 2) 2004</amending-leg> (2004 No 86). <emphasis style="italic">See</emphasis> <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36346" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 73" atidlm:xmlId="DLM483291" atidlm:state="UNCHANGED"><atidlm:linkcontent>73</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36855" atidlm:sourceXmlId="DLM483291" atidlm:targetKey="1849229" atidlm:targetXmlId="DLM316478" atidlm:targetName="prov 73" atidlm:sourceName="DLM483291" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315908.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60384" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of that Act</citation> for the transitional provisions.</history-note>
          </history>
        </notes>
      </prov>
      <prov id="DLM60385" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">136</label>
        <heading xml:lang="en-NZ">Application of penalties recovered</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Subject to any order made under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36347" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 136" atidlm:xmlId="DLM483292" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36856" atidlm:sourceXmlId="DLM483292" atidlm:targetKey="322469" atidlm:targetXmlId="DLM60385" atidlm:targetName="prov 136" atidlm:sourceName="DLM483292" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60385" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, every penalty recovered in any penalty action, whether before the Authority or the Court, must be paid into the Authority or the Court, as the case requires, and not to the plaintiff, and must then be paid by the Authority or the Court into the Crown Bank Account.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The Authority or the Court may order that the whole or any part of any penalty recovered must be paid to any person.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1991 No 22 s 54</citation>
        </cf>
      </prov>
      <crosshead id="DLM60386" toc="yes" xml:lang="en-NZ">Compliance orders</crosshead>
      <prov id="DLM60387" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">137</label>
        <heading xml:lang="en-NZ">Power of Authority to order compliance</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>This section applies where any person has not observed or complied with—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>any provision of—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
                    <para xml:lang="en-NZ">
                      <text>any employment agreement; or</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">ii</label>
