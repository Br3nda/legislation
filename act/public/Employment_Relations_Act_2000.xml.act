        <head5 id="DLM62775" toc="yes" xml:lang="en-NZ">
          <heading xml:lang="en-NZ">
            <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="38414" atidlm:branchLabel="_NOVERSION_" atidlm:name="Southland Electricity Act 1993" atidlm:xmlId="DLM485454" atidlm:state="UNCHANGED"><atidlm:linkcontent>Southland Electricity Act 1993</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="39100" atidlm:sourceXmlId="DLM485454" atidlm:targetKey="1694000" atidlm:targetXmlId="DLM328343" atidlm:targetName="Southland Electricity Act 1993" atidlm:sourceName="DLM485454" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM328343.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM62251.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM62775" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> (1993 No 147)</citation>
          </heading>
        </head5>
        <para xml:lang="en-NZ">
          <text>Repeal <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="38415" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 8" atidlm:parentFolderId="/NZ/NZAct/E/Employment Relations Act 2000" atidlm:xmlId="DLM485455" atidlm:state="UNCHANGED"><atidlm:linkcontent>8</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="39101" atidlm:sourceXmlId="DLM485455" atidlm:targetKey="1694606" atidlm:targetXmlId="DLM328392" atidlm:targetName="prov 8" atidlm:sourceName="DLM485455" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM328386.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM62251.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM62251" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substitute:</text>
          <amend quote="1">
            <prov id="DLM62776" irdnumbering="no" quote="1" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">8</label>
              <heading xml:lang="en-NZ">Collective agreements</heading>
              <prov.body>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no"/>
                  <para xml:lang="en-NZ">
                    <text>Before entering into any collective agreement under the Employment Relations Act 2000, the company must consult with the State Services Commissioner over the conditions of employment to be included in the collective agreement.</text>
