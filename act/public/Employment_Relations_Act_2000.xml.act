                  <label auto.number="no" denominator="yes" quote="1">i</label>
                  <para xml:lang="en-NZ">
                    <text>any institution that is subject to <citation jurisdiction="nz">Part IX of the Education Act 1989</citation>; or</text>
                  </para>
                </label-para>
                <label-para quote="1" skeleton="no" xml:lang="en-NZ">
