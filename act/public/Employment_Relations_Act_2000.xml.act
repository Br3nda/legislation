      <heading xml:lang="en-NZ">Essential services</heading>
      <schedule.misc>
        <head1 id="DLM61703" toc="yes" type="part" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">Part A</label>
          <prov id="DLM61704" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
