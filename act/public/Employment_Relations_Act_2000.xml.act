                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>non-observance of or non-compliance with <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36626" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 69OC" atidlm:xmlId="DLM483470" atidlm:state="UNCHANGED"><atidlm:linkcontent>69OC</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36976" atidlm:sourceXmlId="DLM483470" atidlm:targetKey="317811" atidlm:targetXmlId="DLM59668" atidlm:targetName="prov 69OC" atidlm:sourceName="DLM483470" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM59657.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60398" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36627" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 69OD" atidlm:xmlId="DLM483471" atidlm:state="UNCHANGED"><atidlm:linkcontent>69OD</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="36977" atidlm:sourceXmlId="DLM483471" atidlm:targetKey="317814" atidlm:targetXmlId="DLM59671" atidlm:targetName="prov 69OD" atidlm:sourceName="DLM483471" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM59657.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM60398" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="36628" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 69OE" atidlm:xmlId="DLM483472" atidlm:state="UNCHANGED"><atidlm:linkcontent>69OE</atidlm:linkcontent><atidlm:resourc