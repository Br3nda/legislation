              <def-para id="DLM267157" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM267158" xml:lang="en-NZ">Establishing authority</def-term> means—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
