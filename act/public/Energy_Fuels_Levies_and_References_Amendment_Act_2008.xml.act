              <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1232496" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1598761 - Energy (Fuels, Levies, and References) Act 1989" atidlm:xmlId="DLM1599103" atidlm:state="UNCHANGED">
                <atidlm:linkcontent>Energy (Fuels, Levies, and References) Act 1989</atidlm:linkcontent>
                <atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="767978" atidlm:sourceXmlId="DLM1599103" atidlm:targetKey="1040205" atidlm:targetXmlId="DLM194753" atidlm:targetName="Energy (Fuels, Levies, and References) Act 1989" atidlm:sourceName="DLM1599103" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM194753.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1382700.xml" atidlm:state="UNCHANGED">
