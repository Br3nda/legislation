            <atidlm:linkcontent>s 17</atidlm:linkcontent>
            <atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="768010" atidlm:sourceXmlId="DLM1599134" atidlm:targetKey="12472019" atidlm:targetXmlId="DLM1382846" atidlm:targetName="prov 17" atidlm:sourceName="DLM1599134" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1382700.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1382900.xml" atidlm:state="UNCHANGED">
