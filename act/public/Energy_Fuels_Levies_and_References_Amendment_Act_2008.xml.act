                  <atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1382846" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/>
                </atidlm:resourcepair>
              </atidlm:link>
            </citation>
            <citation jurisdiction="nz">
