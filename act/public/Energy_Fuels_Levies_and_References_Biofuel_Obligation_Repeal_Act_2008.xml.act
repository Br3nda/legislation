                <amends-affect>
                  <citation jurisdiction="nz">
                    <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1476407" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1781902 - Biofuel Obligation (Exempt Persons and Energy Content Values) Order 2008" atidlm:xmlId="DLM1781907" atidlm:state="UNCHANGED">
