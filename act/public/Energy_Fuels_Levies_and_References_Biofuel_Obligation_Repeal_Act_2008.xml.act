      <prov id="DLM1763920" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">4</label>
        <heading>Part 3A repealed</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="yes" denominator="yes"/>
            <para>
              <text>Part 3A is repealed.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
