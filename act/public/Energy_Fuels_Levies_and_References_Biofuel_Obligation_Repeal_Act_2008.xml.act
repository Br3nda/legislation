    <part date.instructed="2008-11-27" id="DLM1763919" in.amend="false" instructing.office="Economic Development" irdnumbering="no" toc="yes" date.first.valid="2008-12-23" act.no="110" act.type="public" date.as.at="2008-12-23" date.assent="2008-12-22" stage="not-in-force" year="2008">
