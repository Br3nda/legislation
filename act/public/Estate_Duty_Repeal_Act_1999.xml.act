            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Parts <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="11253" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 1 Estate duty" atidlm:xmlId="DLM458038" atidlm:state="UNCHANGED"><atidlm:linkcontent>1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="10995" atidlm:sourceXmlId="DLM458038" atidlm:targetKey="2488628" atidlm:targetXmlId="DLM386332" atidlm:targetName="part 1 Estate duty" atidlm:sourceName="DLM458038" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM386332.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM30077.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM30080" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="11254" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 2 Exemptions and reliefs from estate duty" atidlm:xmlId="DLM458039" atidlm:state="UNCHANGED"><atidlm:linkcontent>2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="10996" atidlm:sourceXmlId="DLM458039" atidlm:targetKey="2489211" atidlm:targetXmlId="DLM386625" atidlm:targetName="part 2 Exemptions and reliefs from estate duty" atidlm:sourceName="DLM458039" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM386625.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM30077.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM30080" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="11255" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 3 Assessment and collection of estate duty" atidlm:xmlId="DLM458040" atidlm:state="UNCHANGED"><atidlm:linkcontent>3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="10997" atidlm:sourceXmlId="DLM458040" atidlm:targetKey="2489628" atidlm:targetXmlId="DLM386705" atidlm:targetName="part 3 Assessment and collection of estate duty" atidlm:sourceName="DLM458040" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM386705.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM30077.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM30080" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> are repealed.</text>
