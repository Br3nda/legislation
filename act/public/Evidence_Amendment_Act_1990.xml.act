    <prov id="DLM210953" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">5</label>
      <heading xml:lang="en-NZ">Facsimiles</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text>Subject to any <insertwords>rules made under the authority of any other Act</insertwords>, and to any contrary direction by the Court—</text>
            <label-para skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">a</label>
              <para xml:lang="en-NZ">
                <text>A facsimile of a document or thing that is admissible in evidence under this Act is admissible evidence of that document or thing:</text>
              </para>
            </label-para>
            <label-para skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">b</label>
              <para xml:lang="en-NZ">
                <text>Judicial notice shall be taken of a facsimile of a document or thing of which judicial notice is required to be taken under this Act.</text>
              </para>
            </label-para>
          </para>
        </subprov>
      </prov.body>
      <notes>
