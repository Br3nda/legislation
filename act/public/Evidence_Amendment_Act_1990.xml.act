    <prov id="DLM210942" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">2</label>
      <heading xml:lang="en-NZ">Interpretation</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text>In this Act, unless the context otherwise requires,—</text>
            <def-para id="DLM210943" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM210944" xml:lang="en-NZ">Australian Act</def-term> means—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>An Act of the Commonwealth of Australia:</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>An Act of a State of the Commonwealth of Australia:</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>An Act or an Ordinance of a Territory of the Commonwealth of Australia:</text>
                  </para>
                </label-para>
              </para>
            </def-para>
            <def-para id="DLM210945" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
