                                <entry colname="col2">2.</entry>
                                <entry nameend="col4" namest="col3">has been signed by<leader fill="dot"/></entry>
                              </row>
                              <row>
                                <entry colname="col1"/>
                                <entry colname="col2">3.</entry>
                                <entry nameend="col4" namest="col3">acting in the capacity of<leader fill="dot"/></entry>
                              </row>
                              <row>
                                <entry colname="col1"/>
                                <entry colname="col2">4.</entry>
                                <entry nameend="col4" namest="col3">bears the seal/stamp of<leader fill="dot"/></entry>
                              </row>
                              <row>
                                <entry align="center" nameend="col4" namest="col1">Certified</entry>
                              </row>
