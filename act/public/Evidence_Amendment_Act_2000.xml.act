                            <colspec colname="col4" colnum="4" colwidth="2.53*"/>
                            <thead>
                              <row>
                                <entry align="center" nameend="col4" namest="col1">APOSTILLE</entry>
                              </row>
                            </thead>
                            <tbody>
                              <row>
                                <entry align="center" nameend="col4" namest="col1">(Convention de La Haye du 5 octobre 1961)</entry>
                              </row>
                              <row>
                                <entry colname="col1"/>
                                <entry colname="col2">1.</entry>
                                <entry nameend="col4" namest="col3">Country:<leader fill="dot"/></entry>
                              </row>
