<act date.instructed="2010-03-01" id="DLM2924400" in.amend="false" instructing.office="Customs Service" irdnumbering="no" xml:lang="en-NZ" year.imprint="2010" act.no="23" act.type="public" date.as.at="2010-04-29" date.assent="2010-04-29" date.first.valid="2010-04-29" stage="in-force" year="2010" prospective.consolidation="F">
