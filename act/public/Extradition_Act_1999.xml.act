      <prov id="DLM26755" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">70</label>
        <heading xml:lang="en-NZ">Custody pending determination of appeal</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>If—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>A District Court makes a determination under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="8348" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 24" atidlm:xmlId="DLM455438" atidlm:state="UNCHANGED"><atidlm:linkcontent>24</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="8432" atidlm:sourceXmlId="DLM455438" atidlm:targetKey="85026" atidlm:targetXmlId="DLM26227" atidlm:targetName="prov 24" atidlm:sourceName="DLM455438" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26201.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM26746.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM26755" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="8349" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 45" atidlm:xmlId="DLM455439" atidlm:state="UNCHANGED"><atidlm:linkcontent>45</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="8433" atidlm:sourceXmlId="DLM455439" atidlm:targetKey="86621" atidlm:targetXmlId="DLM26286" atidlm:targetName="prov 45" atidlm:sourceName="DLM455439" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26265.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM26746.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM26755" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>Immediately after the court makes the determination, either party informs the court that the party intends to appeal against the determination,—</text>
                </para>
              </label-para>
              <text>the court may order that the person who is the subject of the determination continue to be detained or, as the case may be, issue a warrant for the arrest and detention of the person, pending the determination of the appeal.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>If—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>A District Court makes a determination under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="8350" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 24" atidlm:xmlId="DLM455440" atidlm:state="UNCHANGED"><atidlm:linkcontent>24</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="8434" atidlm:sourceXmlId="DLM455440" atidlm:targetKey="85026" atidlm:targetXmlId="DLM26227" atidlm:targetName="prov 24" atidlm:sourceName="DLM455440" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26201.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM26746.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM26755" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="8351" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 45" atidlm:xmlId="DLM455441" atidlm:state="UNCHANGED"><atidlm:linkcontent>45</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="8435" atidlm:sourceXmlId="DLM455441" atidlm:targetKey="86621" atidlm:targetXmlId="DLM26286" atidlm:targetName="prov 45" atidlm:sourceName="DLM455441" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26265.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM26746.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM26755" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>Either party files a notice of appeal against the determination,—</text>
                </para>
              </label-para>
              <text>any District Court or the High Court may order that the person who is the subject of the determination continue to be detained or, as the case may be, issue a warrant for the arrest and detention of the person, pending the determination of the appeal.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>If a person is detained under an order made under this section or arrested and detained under a warrant issued under this section,—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>If the proceedings are under <citation jurisdiction="nz">Part <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="8352" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 3 Extradition from New Zealand to certain treaty countries and certain Commonwealth and other countries" atidlm:xmlId="DLM455442" atidlm:state="UNCHANGED"><atidlm:linkcontent>3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="8436" atidlm:sourceXmlId="DLM455442" atidlm:targetKey="85000" atidlm:targetXmlId="DLM26201" atidlm:targetName="part 3 Extradition from New Zealand to certain treaty countries and certain Commonwealth and other countries" atidlm:sourceName="DLM455442" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26201.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM26746.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM26755" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="8353" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 22" atidlm:xmlId="DLM455443" atidlm:state="UNCHANGED"><atidlm:linkcontent>22</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="8437" atidlm:sourceXmlId="DLM455443" atidlm:targetKey="85018" atidlm:targetXmlId="DLM26219" atidlm:targetName="prov 22" atidlm:sourceName="DLM455443" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26201.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM26746.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM26755" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="