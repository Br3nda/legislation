                <para xml:lang="en-NZ">
                  <text>If the proceedings are under <citation jurisdiction="nz">Part <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="8359" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 4 Extradition from New Zealand to Australia and designated countries" atidlm:xmlId="DLM455449" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="8444" atidlm:sourceXmlId="DLM455449" atidlm:targetKey="86600" atidlm:targetXmlId="DLM26265" atidlm:targetName="part 4 Extradition from New Zealand to Australia and designated countries" atidlm:sourceName="DLM455449" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26265.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM26746.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM26755" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="8360" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 43" atidlm:xmlId="DLM455450" atidlm:state="UNCHANGED"><atidlm:linkcontent>43</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="8445" atidlm:sourceXmlId="DLM455450" atidlm:targetKey="86612" atidlm:targetXmlId="DLM26277" atidlm:targetName="prov 43" atidlm:sourceName="DLM455450" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26265.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM26746.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM26755" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="8361" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 44" atidlm:xm