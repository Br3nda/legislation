      <prov id="DLM236371" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">61</label>
        <heading xml:lang="en-NZ">Other enactments, functions, and powers not affected</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Nothing in this Act affects functions or powers conferred on any person by any other enactment.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
    </part>
