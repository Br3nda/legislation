                <row>
                  <entry colsep="1">
                    <citation jurisdiction="nz">1982, No 118—The <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="419882" atidlm:branchLabel="_NOVERSION_" atidlm:name="Friendly Societies And Credit Unions Act 1982" atidlm:xmlId="DLM863982" atidlm:state="UNCHANGED"><atidlm:linkcontent>Friendly Societies and Credit Unions Act 1982</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="427085" atidlm:sourceXmlId="DLM863982" atidlm:targetKey="345400" atidlm:targetXmlId="DLM59731" atidlm:targetName="Friendly Societies And Credit Unions Act 1982" atidlm:sourceName="DLM863982" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM59731.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM325197.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[2]/schedule.misc[1]/para[1]/table[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM323597" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="106" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Financial Reporting Act 1993" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1993" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
                  </entry>
                  <entry>
                    <para xml:lang="en-NZ">
                      <text>By adding to <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="419883" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 59" atidlm:xmlId="DLM863983" atidlm:state="UNCHANGED"><atidlm:linkcontent>59</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="427086" atidlm:sourceXmlId="DLM863983" atidlm:targetKey="347675" atidlm:targetXmlId="DLM60202" atidlm:targetName="prov 59" atidlm:sourceName="DLM863983" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM59827.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM325197.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[2]/schedule.misc[1]/para[1]/table[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM323597" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="106" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Financial Reporting Act 1993" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1993" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> the following subsection:</text>
                      <amend>
                        <subprov skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">3</label>
                          <para xml:lang="en-NZ">
                            <text>Nothing in this section limits or affects the requirements imposed by the Financial Reporting Act 1993 on a Registered society or branch which is an issuer within the meaning of <citation jurisdiction="nz">section 4 of that Act</citation>.</text>
                          </para>
                        </subprov>
                      </amend>
                    </para>
                    <para xml:lang="en-NZ">
                      <text>By repealing <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="419884" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 60" atidlm:xmlId="DLM863984" atidlm:state="UNCHANGED"><atidlm:linkcontent>60(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="427087" atidlm:sourceXmlId="DLM863984" atidlm:targetKey="347677" atidlm:targetXmlId="DLM60204" atidlm:targetName="prov 60" atidlm:sourceName="DLM863984" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM59827.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM325197.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[2]/schedule.misc[1]/para[1]/table[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM323597" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="106" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Financial Reporting Act 1993" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1993" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, and substituting the following subsection:</text>
                      <amend>
                        <subprov skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">2</label>
                          <para xml:lang="en-NZ">
                            <text>For the purposes of <citation jurisdiction="nz">subsection (1)(a) of this section</citation>, the accounting records must—</text>
                            <label-para skeleton="no" xml:lang="en-NZ">
                              <label auto.number="no" denominator="yes">a</label>
                              <para xml:lang="en-NZ">
                                <text>Correctly record and explain the transactions of the society or branch; and</text>
                              </para>
                            </label-para>
                            <label-para skeleton="no" xml:lang="en-NZ">
                              <label auto.number="no" denominator="yes">b</label>
                              <para xml:lang="en-NZ">
                                <text>Enable the financial position of the society or branch to be determined with reasonable accuracy at any time; and</text>
                              </para>
                            </label-para>
                            <label-para skeleton="no" xml:lang="en-NZ">
                              <label auto.number="no" denominator="yes">c</label>
                              <para xml:lang="en-NZ">
                                <text>In the case of a society or branch that is not an issuer within the meaning of <citation jurisdiction="nz">section 4 of the Financial Reporting Act 1993</citation>, be such as are necessary to give a true and fair view of the state of affairs of the society or branch; and</text>
                              </para>
                            </label-para>
                            <label-para skeleton="no" xml:lang="en-NZ">
                              <label auto.number="no" denominator="yes">d</label>
                              <para xml:lang="en-NZ">
                                <text>In the case of a society or branch that is an issuer within the meaning of <citation jurisdiction="nz">section 4 of the Financial Reporting Act 1993</citation>, be such as are necessary to ensure that the financial statements of the society or branch within the meaning of that Act comply with that Act; and</text>
                              </para>
                            </label-para>
                            <label-para skeleton="no" xml:lang="en-NZ">
                              <label auto.number="no" denominator="yes">e</label>
                              <para xml:lang="en-NZ">
                                <text>Enable the accounts of the society or branch to be readily and properly audited.</text>
                              </para>
                            </label-para>
                          </para>
                        </subprov>
                      </amend>
                    </para>
                    <para xml:lang="en-NZ">
                      <text>By adding to <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="419885" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 61" atidlm:xmlId="DLM863985" atidlm:state="UNCHANGED"><atidlm:linkcontent>61</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="427088" atidlm:sourceXmlId="DLM863985" atidlm:targetKey="347679" atidlm:targetXmlId="DLM60206" atidlm:targetName="prov 61" atidlm:sourceName="DLM863985" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM59827.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM325197.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[2]/schedule.misc[1]/para[1]/table[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM323597" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="106" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Financial Reporting Act 1993" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1993" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNC