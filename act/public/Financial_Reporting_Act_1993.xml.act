        <label auto.number="no" denominator="yes">35B</label>
        <heading xml:lang="en-NZ">Registrar of Companies may grant exemptions to directors of overseas companies</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
