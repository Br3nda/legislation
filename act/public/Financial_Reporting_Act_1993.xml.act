      <prov id="DLM324898" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">4B</label>
        <heading xml:lang="en-NZ">Exemption from being issuer under section 4(1)(ba)</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The Securities Commission may, by notice in the <emphasis style="italic">Gazette</emphasis>, exempt—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
