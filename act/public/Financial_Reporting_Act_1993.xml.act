      <prov deletion-status="not-in-force" id="DLM325117" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">29C</label>
        <heading xml:lang="en-NZ">Failure to comply with applicable financial reporting standard must be disregarded if directors of reporting entity are exempt</heading>
        <prov.body/>
