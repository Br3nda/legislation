<act date.instructed="2007-07-11" id="DLM1109400" in.amend="false" instructing.office="Economic Development" irdnumbering="no" xml:lang="en-NZ" year.imprint="2008" act.no="97" act.type="public" date.as.at="2008-09-29" date.assent="2008-09-29" date.first.valid="2008-09-30" stage="in-force" year="2008">
