      <schedule.amendments>
        <schedule.amendments.group2 id="DLM1109608" toc="yes">
          <heading><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1298329" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1651429 - Privacy Act 1993" atidlm:xmlId="DLM1651432" atidlm:state="UNCHANGED"><atidlm:linkcontent>Privacy Act 1993</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="792416" atidlm:sourceXmlId="DLM1651432" atidlm:targetKey="1539005" atidlm:targetXmlId="DLM296638" atidlm:targetName="Privacy Act 1993" atidlm:sourceName="DLM1651432" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM296638.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1109607.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1109608" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> (1993 No 28)</heading>
          <para>
            <text>Part 1 of <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1289739" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1641819 - schedule 2 Public registers" atidlm:xmlId="DLM1643539" atidlm:state="UNCHANGED"><atidlm:linkcontent>Schedule 2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="788742" atidlm:sourceXmlId="DLM1643539" atidlm:targetKey="1548605" atidlm:targetXmlId="DLM298476" atidlm:targetName="schedule 2 Public registers" atidlm:sourceName="DLM1643539" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM298476.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1109607.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1109608" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>: insert the following item in its appropriate alphabetical order:</text>
            <amend quote="1">
              <para quote="1">
                <table align="left" fontsize="9" frame="none" id="DLM1109609" leading="9" pgwide="1">
                  <tgroup cols="3">
