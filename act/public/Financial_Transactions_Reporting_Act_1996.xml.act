                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>Prescribing the forms of applications, warrants, reports, and other documents required under this Act:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
