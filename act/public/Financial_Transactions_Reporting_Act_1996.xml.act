              <def-para id="DLM1502106" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM1502107" xml:lang="en-NZ">incorporated law firm</def-term> has the meaning given to it by <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1151820" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1502109 - prov 6" atidlm:xmlId="DLM1502139" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 6</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="720540" atidlm:sourceXmlId="DLM1502139" atidlm:targetKey="2207400" atidlm:targetXmlId="DLM364948" atidlm:targetName="prov 6" atidlm:sourceName="DLM1502139" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM364944.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM373809.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[13]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para incorporated law firm" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM373803" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="9" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Financial Transactions Reporting Act 1996" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:met