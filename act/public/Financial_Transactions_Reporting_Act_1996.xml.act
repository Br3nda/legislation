      <prov id="DLM374136" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">31</label>
        <heading xml:lang="en-NZ">Obligation to keep prescribed records</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Every financial institution shall keep such records as may be prescribed by regulations made under <citation jurisdiction="nz">section <intref href="DLM374191">56</intref> of this Act</citation>, and shall retain them for such period as may be prescribed by such regulations.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM374137" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
