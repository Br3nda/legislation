              <def-para id="DLM373849" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM373850" xml:lang="en-NZ">Occasional transaction</def-term>, subject to any regulations made under this Act, means any transaction—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>That involves the deposit, withdrawal, exchange, or transfer of cash; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>That is conducted by any person otherwise than through a facility in respect of which that person is a facility holder:</text>
                    </para>
                  </label-para>
                </para>
              </def-para>
              <def-para deletion-status="repealed" id="DLM373851" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
