      <prov id="DLM374175" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">46A</label>
        <heading xml:lang="en-NZ">Power to stop vehicles</heading>
        <prov.body>
