    <part id="DLM373809" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-18" act.no="9" act.type="public" date.as.at="2008-08-01" date.assent="1996-04-01" stage="reprint" year="1996">
      <label auto.number="no" denominator="yes">1</label>
      <heading xml:lang="en-NZ">Preliminary provisions</heading>
      <prov id="DLM373810" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">2</label>
        <heading xml:lang="en-NZ">Interpretation</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>In this Act, unless the context otherwise requires,—</text>
              <def-para id="DLM373811" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM373812" xml:lang="en-NZ">Cash</def-term>—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>Means any coin or paper money that is designated as legal tender in the country of issue; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>Except in <citation jurisdiction="nz">Part <intref href="DLM374142">5</intref> of this Act</citation>, includes—</text>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">i</label>
                        <para xml:lang="en-NZ">
                          <text>Bearer bonds:</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">ii</label>
                        <para xml:lang="en-NZ">
                          <text>Travellers cheques:</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">iii</label>
                        <para xml:lang="en-NZ">
                          <text>Postal notes:</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">iv</label>
                        <para xml:lang="en-NZ">
                          <text>Money orders:</text>
                        </para>
                      </label-para>
                    </para>
                  </label-para>
                </para>
              </def-para>
              <def-para id="DLM373813" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM373814" xml:lang="en-NZ">Cash report</def-term> means a report made pursuant to <citation jurisdiction="nz">section <intref href="DLM374143">37</intref> of this Act</citation></text>
                </para>
              </def-para>
              <def-para deletion-status="repealed" id="DLM373815" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text>
                    <def-term id="DLM373816" xml:lang="en-NZ">Collector</def-term>
                  </text>
                  <notes>
