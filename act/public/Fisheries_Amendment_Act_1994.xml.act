    <prov id="DLM333557" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">9</label>
      <heading xml:lang="en-NZ">Crown not liable for effects of Act</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text>Nothing authorised or effected by this Act shall be regarded as making the Crown or any other person liable to pay compensation to any person or as otherwise making the Crown or any person guilty of a civil wrong.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM333558" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
