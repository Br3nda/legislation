<act id="DLM305692" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="71" act.type="public" date.as.at="2007-09-03" date.assent="2004-08-12" date.first.valid="2007-09-18" stage="in-force" year="2004">
