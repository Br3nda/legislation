      <prov id="DLM92730" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">7</label>
        <heading xml:lang="en-NZ">Alteration of quota management areas without agreement of quota owners</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
