      <prov id="DLM92739" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">14</label>
        <heading xml:lang="en-NZ">Transfers of individual transferable quota</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
