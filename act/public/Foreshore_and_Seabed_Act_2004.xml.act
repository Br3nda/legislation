              <def-para id="DLM319862" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM319863" xml:lang="en-NZ">enactment</def-term>—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
