            <amend>
              <subprov skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">4</label>
                <para xml:lang="en-NZ">
                  <text>A Fire Authority must keep and maintain a current fire plan for its district.</text>
                </para>
              </subprov>
              <subprov skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">4A</label>
                <para xml:lang="en-NZ">
                  <text>A fire plan must contain the prescribed information.</text>
                </para>
              </subprov>
              <subprov skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">4B</label>
                <para xml:lang="en-NZ">
                  <text>A fire plan must be available for public inspection, free of charge, at the office of the Fire Authority during ordinary office hours.</text>
                </para>
              </subprov>
              <subprov skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">4C</label>
                <para xml:lang="en-NZ">
