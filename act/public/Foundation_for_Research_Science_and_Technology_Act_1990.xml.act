        <prov id="DLM213967" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">13</label>
          <heading xml:lang="en-NZ">Superannuation or retiring allowances</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>For the purpose of providing a superannuation fund or retiring allowance for any of the officers or employees of the Foundation, sums by way of subsidy may from time to time be paid into any scheme under the <citation jurisdiction="nz">National Provident Fund Act 1950</citation> containing provision for employer subsidy or into any other employer-subsidised scheme approved by the Minister of Finance for the purposes of this clause.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
