    <prov id="DLM213929" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">9</label>
      <heading xml:lang="en-NZ">Membership of Foundation</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text>The Foundation shall consist of not fewer than 5 nor more than 9 members<struckoutwords/>.</text>
          </para>
        </subprov>
        <subprov deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
        </subprov>
        <subprov deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">3</label>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
