      <subpart id="DLM285905" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" act.no="124" act.type="public" date.as.at="2007-09-20" date.assent="1992-12-17" date.first.valid="2007-09-18" stage="reprint" year="1992">
        <label auto.number="no" denominator="yes">1</label>
        <heading xml:lang="en-NZ">General and regulation-making powers</heading>
        <notes>
          <history>
            <history-note id="DLM285906">Subparts <amended-provision>1</amended-provision>, <amended-provision>2</amended-provision>, and <amended-provision>4</amended-provision> of Part 4A (comprising sections 43A to 43ZZF, and section 43ZZR) were <amending-operation>inserted</amending-operation>, as from <amendment-date>18 October 2004</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="386760" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 5" atidlm:xmlId="DLM831346" atidlm:state="UNCHANGED"><atidlm:linkcontent>5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="393532" atidlm:sourceXmlId="DLM831346" atidlm:targetKey="1834800" atidlm:targetXmlId="DLM315529" atidlm:targetName="prov 5" atidlm:sourceName="DLM831346" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM315527.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM285905.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/subpart[1]/notes[1]/history[1]/history-note[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM285411" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="124" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Gas Act 1992" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1992" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Gas Amendment Act 2004</amending-leg> (2004 No 83).</history-note>
