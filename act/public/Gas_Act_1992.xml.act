      <subpart id="DLM286226" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" act.no="124" act.type="public" date.as.at="2007-09-20" date.assent="1992-12-17" date.first.valid="2007-09-18" stage="reprint" year="1992">
        <label auto.number="no" denominator="yes">2</label>
        <heading xml:lang="en-NZ">Co-regulation of gas industry</heading>
