              <def-para id="DLM390788" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM390789" xml:lang="en-NZ">person</def-term> includes the government of a country, a corporation sole, an incorporated or unincorporated body or person, and any association or combination of individual persons or incorporated or unincorporated persons</text>
                </para>
              </def-para>
              <def-para id="DLM390790" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM390791" xml:lang="en-NZ">prescribed</def-term> means prescribed by regulations made under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="362582" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 57" atidlm:xmlId="DLM807588" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 57</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="369563" atidlm:sourceXmlId="DLM807588" atidlm:targetKey="2413608" atidlm:targetXmlId="DLM390887" atidlm:targetName="prov 57" atidlm:sourceName="DLM807588" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM390813.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM390761.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[14]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para prescribed" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM390755" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="60" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Geographical Indications (Wine and Spirits) Registration Act 2006" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:nam