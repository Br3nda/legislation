  <front id="DLM207527" xml:lang="en-NZ">
    <long-title id="DLM207528" toc="yes" xml:lang="en-NZ">
      <para xml:lang="en-NZ">
        <text>An Act to make miscellaneous amendments in relation to government superannuation</text>
      </para>
    </long-title>
