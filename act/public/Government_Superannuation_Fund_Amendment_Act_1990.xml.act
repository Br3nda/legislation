      <prov id="DLM207546" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">9</label>
        <heading xml:lang="en-NZ">Part not to apply to pensioners and annuitants who are resident overseas as at 31 March 1990</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>This Part of this Act does not apply to any retiring allowance, annual allowance, or annuity that is, as at the 31st day of March 1990, being paid by the Board to an address outside New Zealand in respect of a person whom the Board is satisfied is not resident in New Zealand within the meaning of the <citation jurisdiction="nz">Income Tax Act 1976</citation>.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM207547" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
