          <para>
            <text><citation jurisdiction="nz">This Act</citation> amends the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="966808" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1333100 - Hazardous Substances and New Organisms Act 1996" atidlm:xmlId="DLM1333108" atidlm:state="UNCHANGED"><atidlm:linkcontent>Hazardous Substances and New Organisms Act 1996</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="667528" atidlm:sourceXmlId="DLM1333108" atidlm:targetKey="2038205" atidlm:targetXmlId="DLM381221" atidlm:targetName="Hazardous Substances and New Organisms Act 1996" atidlm:sourceName="DLM1333108" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM381221.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1229902.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/prov[3]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Principal Act amended" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="3" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM1229902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="22" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Hazardous Substances and New Organisms Amendment Act 2008" atidlm:locale="xx_XX" atidlm:state="UNCHANGE