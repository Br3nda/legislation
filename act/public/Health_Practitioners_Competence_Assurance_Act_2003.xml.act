          <amend>
            <def-para id="DLM205264" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM205265" xml:lang="en-NZ">doctor</def-term> means a health practitioner who is, or is deemed to be, registered with the Medical Council of New Zealand continued by <citation jurisdiction="nz">section 114(1)(a) of the Health Practitioners Competence Assurance Act 2003</citation> as a practitioner of the profession of medicine.</text>
              </para>
            </def-para>
          </amend>
        </para>
        <head5 id="DLM205266" toc="yes" xml:lang="en-NZ">
          <heading xml:lang="en-NZ"><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="128513" atidlm:branchLabel="_NOVERSION_" atidlm:name="Alcoholism and Drug Addiction Act 1966" atidlm:xmlId="DLM574565" atidlm:state="UNCHANGED"><atidlm:linkcontent>Alcoholism and Drug Addiction Act 1966</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="130472" atidlm:sourceXmlId="DLM574565" atidlm:targetKey="2442801" atidlm:targetXmlId="DLM380084" atidlm:targetName="Alcoholism and Drug Addiction Act 1966" atidlm:sourceName="DLM574565" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM380084.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM205262.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="head5" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[4]/schedule.misc[1]/head5[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="head5" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Alcoholism and Drug Addiction Act 1966 (1966 No 97)" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM203311" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="48" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Health Practitioners Competence Assurance Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadat