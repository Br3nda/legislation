              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>Unless sooner extended or cancelled, the registration of such a person under this subpart expires on the expiry of the period for which the certificate of temporary registration had been granted.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
