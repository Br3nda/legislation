          <amend>
            <def-para id="DLM205778" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM205779" xml:lang="en-NZ">medical practitioner</def-term> means a health practitioner who is, or is deemed to be, registered with the Medical Council of New Zealand continued by <citation jurisdiction="nz">section 114(1)(a) of the Health Practitioners Competence Assurance Act 2003</citation> as a practitioner of the profession of medicine.</text>
              </para>
            </def-para>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Omit from <citation jurisdiction="nz">sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="128828" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 88" atidlm:parentFolderId="/NZ/NZAct/H/Health Practitioners Competence Assurance Act 2003" atidlm:xmlId="DLM574879" atidlm:state="UNCHANGED"><atidlm:linkcontent>88(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="130629" atidlm:sourceXmlId="DLM574879" atidlm:targetKey="2014473" atidlm:targetXmlId="DLM307283" atidlm:targetName="prov 88" atidlm:sourceName="DLM574879" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM307082.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM205262.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[4]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Acts amended" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="4" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM203311" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="48" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Health Practitioners Competence Assurance Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx