        <head5 id="DLM206692" toc="yes" xml:lang="en-NZ">
          <heading xml:lang="en-NZ"><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="129129" atidlm:branchLabel="_NOVERSION_" atidlm:name="Venereal Diseases Regulations 1982" atidlm:xmlId="DLM575479" atidlm:state="UNCHANGED"><atidlm:linkcontent>Venereal Diseases Regulations 1982</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="131177" atidlm:sourceXmlId="DLM575479" atidlm:targetKey="3519409" atidlm:targetXmlId="DLM82354" atidlm:targetName="Venereal Diseases Regulations 1982" atidlm:sourceName="DLM575479" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZReg/fragxyxz_DLM82354.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM206208.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="head5" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[6]/schedule.misc[1]/head5[39]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="head5" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Venereal Diseases Regulations 1982 (SR 1982/215)" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM203311" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="48" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Health Practitioners Competence Assurance Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="L