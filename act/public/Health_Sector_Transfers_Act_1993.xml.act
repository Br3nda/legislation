    <part id="DLM295107" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-03" act.no="23" act.type="public" date.as.at="2007-09-03" date.assent="1993-05-10" stage="reprint" year="1993">
