                    <amend>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">e</label>
                        <para xml:lang="en-NZ">
                          <text>Any home or premises in which 5 or more disabled persons who are under the age of 17 years are receiving care:</text>
                        </para>
                      </label-para>
                    </amend>
                    <follow-text space-before="no">.</follow-text>
