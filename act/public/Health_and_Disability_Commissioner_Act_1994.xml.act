            <def-para id="DLM333836" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM333837" xml:lang="en-NZ">Health</def-term> means human health</text>
              </para>
            </def-para>
            <def-para id="DLM333838" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM333839" xml:lang="en-NZ">health care institution</def-term> means—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>premises used to provide health care services within the meaning of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="426839" atidlm:branchLabel="_NOVERSION_" atidlm:name="Health and Disability Services (Safety) Act 2001" atidlm:xmlId="DLM870922" atidlm:state="UNCHANGED"><atidlm:linkcontent>Health and Disability Services (Safety) Act 2001</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="434030" atidlm:sourceXmlId="DLM870922" atidlm:targetKey="653600" atidlm:targetXmlId="DLM119974" atidlm:targetName="Health and Disability Services (Safety) Act 2001" atidlm:sourceName="DLM870922" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM119974.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM333583.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[20]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para health care institution" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM333583" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="88" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Health and Disability Commissioner Act 1994" ati