      <prov id="DLM334182" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">79</label>
        <heading xml:lang="en-NZ">Amendment to <citation jurisdiction="nz">Accident Rehabilitation and Compensation Insurance Act 1992</citation></heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Section 14 of the Accident Rehabilitation and Compensation Insurance Act 1992</citation> is hereby amended by inserting, after <citation jurisdiction="nz">subsection (3)</citation>, the following subsection:</text>
              <amend>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">3A</label>
                  <para xml:lang="en-NZ">
                    <text>Nothing in this section shall apply to any proceedings under <citation jurisdiction="nz">section 50 or section 51 of the Health and Disability Commissioner Act 1994</citation> or under <citation jurisdiction="nz">section 83 or section 90 or section 122 or section 123 or section 124 of the Human Rights Act 1993</citation>.</text>
                  </para>
                </subprov>
              </amend>
            </para>
          </subprov>
        </prov.body>
      </prov>
