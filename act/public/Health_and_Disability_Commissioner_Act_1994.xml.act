    <part id="DLM334131" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-18" act.no="88" act.type="public" date.as.at="2007-09-03" date.assent="1994-10-20" stage="reprint" year="1994">
      <label auto.number="no" denominator="yes">5</label>
