      <prov id="DLM120921" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">61</label>
        <heading xml:lang="en-NZ">Statutory references to psychiatric hospitals</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>After 30 September 2002, a reference to a psychiatric hospital in an enactment must be read as a reference to a hospital within the meaning of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="76240" atidlm:branchLabel="_NOVERSION_" atidlm:name="Mental Health (Compulsory Assessment and Treatment) Act 1992" atidlm:xmlId="DLM522799" atidlm:state="UNCHANGED"><atidlm:linkcontent>Mental Health (Compulsory Assessment and Treatment) Act 1992</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="77459" atidlm:sourceXmlId="DLM522799" atidlm:targetKey="1366251" atidlm:targetXmlId="DLM262175" atidlm:targetName="Mental Health (Compulsory Assessment and Treatment) Act 1992" atidlm:sourceName="DLM522799" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM262175.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM120591.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/prov[11]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Statutory references to psychiatric hospitals" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="61" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM119974" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="93" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Health and Disability Services (Safety) Act 2001" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2001" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>,</citation> unless—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text><emphasis style="bold">psychiatric hospital</emphasis> is defined in or for the purposes of the enactment; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the reference is part of a provision referring to <citation jurisdiction="nz">Part 5 of the Hospitals Act 1957</citation>.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
      </prov>
    </part>
  </body>
  <schedule.group xml:lang="en-NZ">
    <schedule id="DLM120922" in.amend="false" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" xml:lang="en-NZ" irdnumbering="no" act.no="93" act.type="public" date.as.at="2007-09-03" date.assent="2001-11-30" date.first.valid="2007-09-03" stage="reprint" year="2001">
      <label auto.number="no" denominator="yes">1</label>
      <empowering-prov>s 44(3)</empowering-prov>
      <heading xml:lang="en-NZ">Form of search warrant</heading>
      <schedule.misc>
        <para xml:lang="en-NZ">
          <text>Warrant under <citation jurisdiction="nz">Section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="76241" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 44" atidlm:parentFolderId="/NZ/NZAct/H/Health and Disability Services (Safety) Act 2001" atidlm:xmlId="DLM522900" atidlm:state="UNCHANGED"><atidlm:linkcontent>44</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="77460" atidlm:sourceXmlId="DLM522900" atidlm:targetKey="656606" atidlm:targetXmlId="DLM120581" atidlm:targetName="prov 44" atidlm:sourceName="DLM522900" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM120575.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM120922.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Form of search warrant" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="1" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM119974" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="93" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Health and Disability Services (Safety) Act 2001" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2001" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of Health and Disability Services (Safety) Act 2001</citation> to enter dwellinghouse</text>
          <table align="center" fontsize="11.5" frame="none" id="DLM120923" leading="13.5" xml:lang="en-NZ">
            <tgroup cols="2">
              <colspec align="left" colname="col1" colnum="1" colwidth="1*"/>
              <colspec colname="col2" colnum="2" colwidth="9*"/>
              <tbody>
                <row rowsep="0">
                  <entry nameend="col2" namest="col1">To:</entry>
                </row>
                <row rowsep="0">
                  <entry nameend="col2" namest="col1">authorised person</entry>
                </row>
                <row rowsep="0">
                  <entry nameend="col2" namest="col1">Being satisfied on written application made on oath by an authorised person that there are reasonable grounds to believe that health care services are being provided in <insertwords><emphasis style="italic">here describe dwellinghouse</emphasis></insertwords> (or in another place that cannot reasonably be entered except through that dwellinghouse), and that—</entry>
                </row>
                <row rowsep="0">
                  <entry colname="col1" colsep="0">(a)</entry>
                  <entry colname="col2">they are being provided otherwise than in compliance with <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="76242" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 9" atidlm:xmlId="DLM522901" atidlm:state="UNCHANGED"><atidlm:linkcontent>9</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="77461" atidlm:sourceXmlId="DLM522901" atidlm:targetKey="654659" atidlm:targetXmlId="DLM120538" atidlm:targetName="prov 9" atidlm:sourceName="DLM522901" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM119979.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM120922.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[1]/schedule.misc[1]/para[1]/table[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="table" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM119974" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="93" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Health and Disability Services (Safety) Act 2001" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2001" atidlm:locale="xx_XX" atidlm: