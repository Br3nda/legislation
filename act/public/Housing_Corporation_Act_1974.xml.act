      <prov id="DLM412068" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">19</label>
        <heading xml:lang="en-NZ">Powers of Corporation</heading>
        <prov.body>
          <subprov deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1A</label>
            <para xml:lang="en-NZ">
              <text>The Corporation may take any action in the performance of its functions or achievement of its objectives jointly, or in conjunction, with—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>a local authority; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>any other person or organisation that provides housing without having profit or gain as its principal motive; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>any other person or organisation.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1B</label>
