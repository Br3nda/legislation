          <amend>
            <label-para skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">v</label>
              <para xml:lang="en-NZ">
                <text>a disability allowance payable under <citation jurisdiction="nz">section 69C</citation>; but.</text>
              </para>
            </label-para>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Omit from paragraph (d) of the definition in section <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="33619" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 3" atidlm:parentFolderId="/NZ/NZAct/H/Housing Restructuring (Income-Related Rents) Amendment Act 2000" atidlm:xmlId="DLM480268" atidlm:state="UNCHANGED"><atidlm:linkcontent>3(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="33988" atidlm:sourceXmlId="DLM480268" atidlm:targetKey="2344575" atidlm:targetXmlId="DLM359124" atidlm:targetName="prov 3" atidlm:sourceName="DLM480268" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM359116.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM58308.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Enactments amended" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="2" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM57286" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="22" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Housing Restructuring (Income-Related Rents) Amendment Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:na