        <heading xml:lang="en-NZ">Disclosure of tenancy information for benefit purposes</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
