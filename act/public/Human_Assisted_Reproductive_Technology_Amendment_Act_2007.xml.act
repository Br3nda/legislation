              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">ja</label>
                <para xml:lang="en-NZ">
                  <text>the donor's reasons for donating:</text>
                </para>
              </label-para>
            </amend>
            <follow-text space-before="no">.</follow-text>
          </para>
        </subprov>
      </prov.body>
