    <prov id="DLM968343" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">4</label>
      <heading xml:lang="en-NZ">Status of surrogacy arrangements and prohibition of commercial surrogacy arrangements</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text><citation jurisdiction="nz">Section 14(2)</citation> is amended by omitting <amend.in><citation jurisdiction="nz">Status of Children Amendment Act 1987</citation></amend.in> and substituting <amend.in><citation jurisdiction="nz">Part 2 of the Status of Children Act 1969</citation></amend.in>.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM968344" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">5</label>
      <heading xml:lang="en-NZ">Voluntary register to be maintained by Registrar-General</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text><citation jurisdiction="nz">Section 63(2)</citation> is amended by inserting the following paragraph after <citation jurisdiction="nz">paragraph (j)</citation>:</text>
