        <prov id="DLM305916" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <heading xml:lang="en-NZ">Staff</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The Director may, in accordance with this clause, appoint any employees (including acting or temporary or casual employees) that he or she considers necessary for the efficient carrying out of the functions of the Director.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
