      <prov id="DLM305726" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">148D</label>
        <heading xml:lang="en-NZ">Proceedings</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Any proceedings to which the Race Relations Conciliator was a party or that he or she was considering bringing, before the commencement of this section, may be brought, continued, completed, and enforced by or against the Commission.</text>
            </para>
          </subprov>
        </prov.body>
