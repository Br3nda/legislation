      <prov id="DLM305492" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">138</label>
        <heading xml:lang="en-NZ">No adverse statement</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text><insertwords>The Commission must not</insertwords>, in any report or statement made pursuant to this Act, make any comment that is adverse to any person unless that person has been given an opportunity to be heard.</text>
            </para>
          </subprov>
        </prov.body>
