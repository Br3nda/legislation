    <part id="DLM304268" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-18" act.no="82" act.type="public" date.as.at="2007-09-20" date.assent="1993-08-10" stage="reprint" year="1993">
      <label auto.number="no" denominator="yes">1</label>
      <heading xml:lang="en-NZ">Human Rights Commission</heading>
      <prov id="DLM304269" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">4</label>
        <heading xml:lang="en-NZ">Continuation of Human Rights Commission</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>There shall continue to be a Human Rights Commission, which shall be the same body as the Human Rights Commission established under <citation jurisdiction="nz">section 4 of the Human Rights Commission Act 1977</citation>.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
