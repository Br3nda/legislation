      <prov id="DLM304668" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">71</label>
        <heading xml:lang="en-NZ">Reports on superannuation schemes</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The Commission shall from time to time, after consultation with the Government Actuary, report to the Minister on whether discrimination on the prohibited grounds has been eliminated from superannuation schemes.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1977 No 49 s 89</citation>
        </cf>
      </prov>
      <prov id="DLM304669" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
