          <para xml:lang="en-NZ">
            <text>Omit from <citation jurisdiction="nz">Schedules <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="76982" atidlm:branchLabel="_NOVERSION_" atidlm:name="schedule 4 Organisations for the purposes of subpart 2 of Part 5" atidlm:xmlId="DLM523639" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="78253" atidlm:sourceXmlId="DLM523639" atidlm:targetKey="895907" atidlm:targetXmlId="DLM163544" atidlm:targetName="schedule 4 Organisations for the purposes of subpart 2 of Part 5" atidlm:sourceName="DLM523639" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM163544.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM122903.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM122904" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="76983" atidlm:branchLabel="_NOVERSION_" atidlm:name="schedule 5 Crown entities required to include statements of service performance in their annual financial statements" atidlm:xmlId="DLM523640" atidlm:state="UNCHANGED"><atidlm:linkcontent>5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="78254" atidlm:sourceXmlId="DLM523640" atidlm:targetKey="896644" atidlm:targetXmlId="DLM163652" atidlm:targetName="schedule 5 Crown entities required to include statements of service performance in their annual financial statements" atidlm:sourceName="DLM523640" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM163652.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM122903.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM122904" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atid