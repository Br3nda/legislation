      <prov id="DLM22062" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">42</label>
        <heading xml:lang="en-NZ">Delegation of powers of Minister</heading>
