        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text>This Act is the Industrial and Provident Societies Amendment Act 2007.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
