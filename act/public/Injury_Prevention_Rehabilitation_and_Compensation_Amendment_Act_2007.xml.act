      <heading xml:lang="en-NZ">Consequential amendments</heading>
      <schedule.misc>
        <head1 id="DLM405610" toc="yes" type="part" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <heading xml:lang="en-NZ">Consequential amendments to principal Act</heading>
          <head5 id="DLM405611" toc="yes" xml:lang="en-NZ">
            <heading xml:lang="en-NZ">
