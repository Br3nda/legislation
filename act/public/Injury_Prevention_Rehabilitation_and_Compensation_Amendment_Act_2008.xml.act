              <label-para skeleton="no">
                <label auto.number="no" denominator="yes">a</label>
                <para>
                  <text>omitting <amend.in quote="1">6 months after the due date</amend.in> and substituting <amend.in quote="1">210 days after the due date</amend.in>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no">
                <label auto.number="no" denominator="yes">b</label>
                <para>
                  <text>omitting <amend.in quote="1">every 6 months</amend.in> and substituting <amend.in quote="1">every 180 days</amend.in>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no">
                <label auto.number="no" denominator="yes">c</label>
                <para>
                  <text>omitting <amend.in quote="1">first 6-month period</amend.in> and substituting <amend.in quote="1">210-day period</amend.in>.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
