      <prov.body>
        <subprov skeleton="no">
          <label auto.number="no" denominator="no"/>
          <para>
            <text>This Act amends the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="963605" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1330900 - Injury Prevention, Rehabilitation, and Compensation Act 2001" atidlm:xmlId="DLM1330905" atidlm:state="UNCHANGED"><atidlm:linkcontent>Injury Prevention, Rehabilitation, and Compensation Act 2001</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="666045" atidlm:sourceXmlId="DLM1330905" atidlm:targetKey="585802" atidlm:targetXmlId="DLM99493" atidlm:targetName="Injury Prevention, Rehabilitation, and Compensation Act 2001" atidlm:sourceName="DLM1330905" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM99493.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1210503.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/prov[3]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Principal Act amended" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="3" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM1210503" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="26" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Injury Prevention, Rehabilitation, and Compensation (Social Assistance) Amendment Act 2008" atidlm:locale=