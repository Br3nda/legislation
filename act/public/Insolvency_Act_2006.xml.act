tidlm:resourcepair></atidlm:link></citation>
          </cf>
        </prov>
        <prov id="DLM386919" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">175</label>
          <heading xml:lang="en-NZ">Time for holding examination</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The Court must hold the public examination of the bankrupt as soon as practicable, but not before 5 working days have elapsed after the Assignee has sent the bankrupt a notice under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="355337" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 174" atidlm:xmlId="DLM800050" atidlm:state="UNCHANGED"><atidlm:linkcontent>174</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="361408" atidlm:sourceXmlId="DLM800050" atidlm:targetKey="2365613" atidlm:targetXmlId="DLM386918" atidlm:targetName="prov 174" atidlm:sourceName="DLM800050" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM386904.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM386904.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[3]/subpart[5]/prov[11]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Time for holding examination" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="175" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM385298" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="55" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Insolvency Act 2006" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:me