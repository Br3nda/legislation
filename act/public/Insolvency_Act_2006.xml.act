        <heading xml:lang="en-NZ">Proposals</heading>
        <prov id="DLM387563" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">325</label>
          <heading xml:lang="en-NZ">Meaning of debt, etc</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>In this subpart, unless the context otherwise requires,—</text>
                <def-para id="DLM387564" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM387565" xml:lang="en-NZ">debt</def-term> means a debt that would be provable in the insolvent's bankruptcy</text>
                  </para>
                </def-para>
                <def-para id="DLM387566" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM387567" xml:lang="en-NZ">insolvent</def-term> means a person who is not a bankrupt, but who is unable to pay his or her debts as they become due.</text>
                  </para>
                </def-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The debt of an insolvent is provable under this subpart.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1967 No 54 s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="356797" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 139" atidlm:xmlId="DLM802308" atidlm:state="UNCHANGED"><atidlm:linkcontent>139</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="363357" atidlm:sourceXmlId="DLM802308" atidlm:targetKey="2474202" atidlm:targetXmlId="DLM383746" atidlm:targetName="prov 139" atidlm:sourceName="DLM802308" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM383745.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM387562.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/subpart[2]/prov[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Meaning of debt, etc" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="325" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM385298" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="55" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Insolvency Act 2006" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2006" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
          </cf>
