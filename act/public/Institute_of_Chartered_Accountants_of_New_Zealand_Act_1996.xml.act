              </para>
            </def-para>
            <def-para id="DLM391380" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM391381" xml:lang="en-NZ">Disciplinary body</def-term> or <def-term id="DLM391382" xml:lang="en-NZ">body</def-term> means the Disciplinary Tribunal or the Appeals Council</text>
              </para>
            </def-para>
            <def-para id="DLM391383" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM391384" xml:lang="en-NZ">Disciplinary Tribunal</def-term> means the Disciplinary Tribunal referred to in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="478510" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 6" atidlm:xmlId="DLM922510" atidlm:state="UNCHANGED"><atidlm:linkcontent>6(1)(g)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="486472" atidlm:sourceXmlId="DLM922510" atidlm:targetKey="2079002" atidlm:targetXmlId="DLM391405" atidlm:targetName="prov 6" atidlm:sourceName="DLM922510" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM391402.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM391363.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[7]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Disciplinary Tribunal" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM391363" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="39" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Institute of Chartered Accountants of New Zealand Act 1996" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/>