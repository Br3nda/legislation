<?xml version="1.0" encoding="UTF-8"?>
<act id="DLM391363" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="39" act.type="public" date.as.at="2007-09-03" date.assent="1996-06-24" date.first.valid="2007-09-18" stage="reprint" year="1996">
  <cover id="DLM391364">
    <title xml:lang="en-NZ">Institute of Chartered Accountants of New Zealand Act 1996</title>
    <assent>1996-06-24</assent>
    <cover.reprint-note>
      <para xml:lang="en-NZ"/>
      <admin-office>This Act is administered in the <ministry>Treasury</ministry></admin-office>
    </cover.reprint-note>
  </cover>
  <contents>
    <heading xml:lang="en-NZ">Contents</heading>
    <toc/>
  </contents>
  <front id="DLM391365" xml:lang="en-NZ">
    <long-title id="DLM391366" toc="yes" xml:lang="en-NZ">
      <para xml:lang="en-NZ">
        <text>An Act to—</text>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">a</label>
          <para xml:lang="en-NZ">
            <text>Continue the New Zealand Society of Accountants under the name <emphasis style="bold">Institute of Chartered Accountants of New Zealand</emphasis>; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">b</label>
          <para xml:lang="en-NZ">
            <text>Require the Institute to have rules governing membership, discipline, and other matters and a code of ethics governing the professional conduct of its members; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">c</label>
          <para xml:lang="en-NZ">
            <text>Prohibit the use of terms implying membership of the Institute by persons who are not members; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">d</label>
          <para xml:lang="en-NZ">
            <text>Provide for related matters; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">e</label>
          <para xml:lang="en-NZ">
            <text>Repeal the <citation jurisdiction="nz">New Zealand Society of Accountants Act 1958</citation></text>
          </para>
        </label-para>
      </para>
    </long-title>
    <enactment>BE IT ENACTED by the Parliament of New Zealand as follows:</enactment>
  </front>
  <body id="DLM391367" prov-type="section" xml:lang="en-NZ">
    <prov id="DLM391368" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
