        <prov id="DLM225947" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">108</label>
          <heading xml:lang="en-NZ">Other remedies still available</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>Nothing in this subpart prevents the exercise of any other remedy or proceeding available by or on behalf of any person who is, or is alleged to be, unlawfully detained, confined, or imprisoned.</text>
              </para>
            </subprov>
          </prov.body>
