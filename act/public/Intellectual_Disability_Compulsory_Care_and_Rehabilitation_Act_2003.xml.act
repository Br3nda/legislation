        <prov id="DLM225470" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">55</label>
          <heading xml:lang="en-NZ">Right to company</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>Every care recipient is entitled to the company of others.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
