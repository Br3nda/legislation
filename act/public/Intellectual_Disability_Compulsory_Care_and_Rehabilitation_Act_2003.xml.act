        <prov id="DLM225466" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">51</label>
          <heading xml:lang="en-NZ">Medical treatment</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>Every care recipient is entitled to medical treatment and other health care appropriate to his or her condition.</text>
              </para>
            </subprov>
          </prov.body>
