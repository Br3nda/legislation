          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text><def-term id="DLM225175" xml:lang="en-NZ">Care recipient no longer subject to the criminal justice system</def-term> means a person who—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
