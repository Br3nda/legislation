      </prov>
      <prov id="DLM64998" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">168</label>
        <heading xml:lang="en-NZ">ICC's powers while sitting in New Zealand</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>While the ICC is sitting in New Zealand, it may exercise its functions and powers as provided under the Statute and under the Rules.</text>
            </para>
          </subprov>
        </prov.body>
        <cf><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="42171" atidlm:branchLabel="_NOVERSION_" atidlm:name="International War Crimes Tribunals Act 1995" atidlm:xmlId="DLM489005" atidlm:state="UNCHANGED"><atidlm:linkcontent>1995 No 27</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="42846" atidlm:sourceXmlId="DLM489005" atidlm:targetKey="1940603" atidlm:targetXmlId="DLM365015" atidlm:targetName="International War Crimes Tribunals Act 1995" atidlm:sourceName="DLM489005" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM365015.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64995.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64998" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="42172" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 37" atidlm:xmlId="DLM489006" atidlm:state="UNCHANGED"><atidlm:linkcontent>37</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="42847" atidlm:sourceXmlId="DLM489006" atidlm:targetKey="1943601" atidlm:targetXmlId="DLM365915" atidlm:targetName="prov 37" atidlm:sourceName="DLM489006" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM365913.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64995.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64998" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; <citation jurisdiction="nz">Statute, articles <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="42173" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 4" atidlm:xmlId="DLM489007" atidlm:state="UNCHANGED"><atidlm:linkcontent>4(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="42848" atidlm:sourceXmlId="DLM489007" atidlm:targetKey="347023" atidlm:targetXmlId="DLM65559" atidlm:targetName="prov Article 4" atidlm:sourceName="DLM489007" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64995.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64998" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="42174" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 64" atidlm:xmlId="DLM489008" atidlm:state="UNCHANGED"><atidlm:linkcontent>64</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="42849" atidlm:sourceXmlId="DLM489008" atidlm:targetKey="347088" atidlm:targetXmlId="DLM66024" atidlm:targetName="prov Article 64" atidlm:sourceName="DLM489008" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64995.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64998" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></cf>
      </prov>
      <prov id="DLM64999" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">169</label>
        <heading xml:lang="en-NZ">ICC may administer oaths in New Zealand</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The ICC may, at any sitting of the ICC in New Zealand, administer an oath or affirmation giving an undertaking as to truthfulness in accordance with the practice and procedure of the ICC.</text>
            </para>
          </subprov>
        </prov.body>
        <cf><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="42175" atidlm:branchLabel="_NOVERSION_" atidlm:name="International War Crimes Tribunals Act 1995" atidlm:xmlId="DLM489009" atidlm:state="UNCHANGED"><atidlm:linkcontent>1995 No 27</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="42850" atidlm:sourceXmlId="DLM489009" atidlm:targetKey="1940603" atidlm:targetXmlId="DLM365015" atidlm:targetName="International War Crimes Tribunals Act 1995" atidlm:sourceName="DLM489009" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM365015.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64995.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64999" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="42176" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 38" atidlm:xmlId="DLM489010" atidlm:state="UNCHANGED"><atidlm:linkcontent>38(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="42851" atidlm:sourceXmlId="DLM489010" atidlm:targetKey="1943602" atidlm:targetXmlId="DLM365916" atidlm:targetName="prov 38" atidlm:sourceName="DLM489010" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM365913.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64995.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64999" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; <citation jurisdiction="nz">Statute, article <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="42177" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 69" atidlm:xmlId="DLM489011" atidlm:state="UNCHANGED"><atidlm:linkcontent>69(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="42852" atidlm:sourceXmlId="DLM489011" atidlm:targetKey="347093" atidlm:targetXmlId="DLM66029" atidlm:targetName="prov Article 69" atidlm:sourceName="DLM489011" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64995.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64999" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></cf>
      </prov>
      <prov id="DLM65500" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">170</label>
        <heading xml:lang="en-NZ">Orders made by ICC not subject to review</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>No application for review under <citation jurisdiction="nz">Part <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="42178" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 1 Single procedure for the judicial review of the exercise of or failure to exercise a statutory power" atidlm:xmlId="DLM489012" atidlm:state="UNCHANGED"><atidlm:linkcontent>1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="42853" atidlm:sourceXmlId="DLM489012" atidlm:targetKey="2621018" atidlm:targetXmlId="DLM408335" atidlm:targetName="part 1 Single procedure for the judicial review of the exercise of or failure to exercise a statutory power" atidlm:sourceName="DLM489012" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM408335.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64995.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM65500" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="42179" atidlm:branchLabel="_NOVERSION_" atidlm:name="Judicature Amendment Act 1972" atidlm:xmlId="DLM489013" atidlm:state="UNCHANGED"><atidlm:linkcontent>Judicature Amendment Act 1972</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="42854" atidlm:sourceXmlId="DLM489013" atidlm:targetKey="2620402" atidlm:targetXmlId="DLM408329" atidlm:targetName="Judicature Amendment Act 1972" atidlm:sourceName="DLM489013" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM408329.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64995.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM65500" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and no application for an order of mandamus or prohibition or certiorari or for a declaration or injunction may be brought in respect of any judgment or order or determination of the ICC that is made or given at a sitting of the ICC in New Zealand.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="42180" atidlm:branchLabel="_NOVERSION_" atidlm:name="International War Crimes Tribunals Act 1995" atidlm:xmlId="DLM489014" atidlm:state="UNCHANGED"><atidlm:linkcontent>1995 No 27</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="42855" atidlm:sourceXmlId="DLM489014" atidlm:targetKey="1940603" atidlm:targetXmlId="DLM365015" atidlm:targetName="International War Crimes Tribunals Act 1995" atidlm:sourceName="DLM489014" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM365015.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64995.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM65500" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="42181" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 39" atidlm:xmlId="DLM489015" atidlm:state="UNCHANGED"><atidlm:linkcontent>39</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="42856" atidlm:sourceXmlId="DLM489015" atidlm:targetKey="1943603" atidlm:targetXmlId="DLM365917" atidlm:targetName="prov 39" atidlm:sourceName="DLM489015" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM365913.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64995.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM65500" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
