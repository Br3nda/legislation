tidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="42603" atidlm:sourceXmlId="DLM488685" atidlm:targetKey="347138" atidlm:targetXmlId="DLM66074" atidlm:targetName="prov Article 110" atidlm:sourceName="DLM488685" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64950.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64966" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
