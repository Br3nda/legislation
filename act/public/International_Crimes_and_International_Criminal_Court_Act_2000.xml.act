      <prov id="DLM64184" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">27</label>
        <heading xml:lang="en-NZ">Execution of requests</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>If the ICC makes a request for assistance, the request must be dealt with in accordance with the relevant procedure under the law of New Zealand (as provided in this Act).</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>If the request for assistance specifies that it should be executed in a particular manner that is not prohibited by New Zealand law or by using a particular procedure that is not prohibited by New Zealand law, the Attorney-General or the Minister, as the case may be, must use his or her best endeavours to ensure that the request is executed in that manner or using that procedure, as the case may be.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">Statute, articles <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39545" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 88" atidlm:xmlId="DLM486484" atidlm:state="UNCHANGED"><atidlm:linkcontent>88</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40038" atidlm:sourceXmlId="DLM486484" atidlm:targetKey="347115" atidlm:targetXmlId="DLM66051" atidlm:targetName="prov Article 88" atidlm:sourceName="DLM486484" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64184" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39546" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 99" atidlm:xmlId="DLM486485" atidlm:state="UNCHANGED"><atidlm:linkcontent>99(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40520" atidlm:sourceXmlId="DLM486485" atidlm:targetKey="347126" atidlm:targetXmlId="DLM66062" atidlm:targetName="prov Article 99" atidlm:sourceName="DLM486485" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64184" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
        </cf>
      </prov>
      <prov id="DLM64185" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">28</label>
        <heading xml:lang="en-NZ">Consultation</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The Attorney-General or the Minister, as the case may be, must consult with the ICC, without delay, if—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>a request for assistance is received from the ICC that does not contain or is not accompanied by the appropriate information or the appropriate documents specified in <citation jurisdiction="nz">articles <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39547" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 87" atidlm:xmlId="DLM486486" atidlm:state="UNCHANGED"><atidlm:linkcontent>87</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40521" atidlm:sourceXmlId="DLM486486" atidlm:targetKey="347114" atidlm:targetXmlId="DLM66050" atidlm:targetName="prov Article 87" atidlm:sourceName="DLM486486" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64185" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39548" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 91" atidlm:xmlId="DLM486487" atidlm:state="UNCHANGED"><atidlm:linkcontent>91</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40522" atidlm:sourceXmlId="DLM486487" atidlm:targetKey="347118" atidlm:targetXmlId="DLM66054" atidlm:targetName="prov Article 91" atidlm:sourceName="DLM486487" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64185" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39549" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 92" atidlm:xmlId="DLM486488" atidlm:state="UNCHANGED"><atidlm:linkcontent>92</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40523" atidlm:sourceXmlId="DLM486488" atidlm:targetKey="347119" atidlm:targetXmlId="DLM66055" atidlm:targetName="prov Article 92" atidlm:sourceName="DLM486488" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64185" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39550" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 93" atidlm:xmlId="DLM486489" atidlm:state="UNCHANGED"><atidlm:linkcontent>93</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40524" atidlm:sourceXmlId="DLM486489" atidlm:targetKey="347120" atidlm:targetXmlId="DLM66056" atidlm:targetName="prov Article 93" atidlm:sourceName="DLM486489" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64185" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39551" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 96" atidlm:xmlId="DLM486490" atidlm:state="UNCHANGED"><atidlm:linkcontent>96</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40525" atidlm:sourceXmlId="DLM486490" atidlm:targetKey="347123" atidlm:targetXmlId="DLM66059" atidlm:targetName="prov Article 96" atidlm:sourceName="DLM486490" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64185" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the Statute</citation>; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the ICC has not provided sufficient information for a request for assistance to be executed; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>in the case of a request for surrender,—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
                    <para xml:lang="en-NZ">
                      <text>the person sought cannot be located in New Zealand; or</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">ii</label>
                    <para xml:lang="en-NZ">
                      <text>it appears that the person in New Zealand is clearly not the person named in the warrant or judgment, as the case may be; or</text>
                    </para>
                  </label-para>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>execution of a request for assistance in its current form would require the breach of an existing treaty obligation to another State; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">e</label>
                <para xml:lang="en-NZ">
                  <text>for any other reason there are or may be difficulties with the execution of a request for assistance received from the ICC.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Before refusing any request for assistance, the Attorney-General or the Minister, as the case may be, must consult with the ICC to ascertain whether the assistance sought could be provided—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>subject to conditions; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>at a later date or in an alternative manner.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>Without limiting the types of conditions under which assistance may be provided, the Attorney-General may agree to the transmission of documents or information to the Prosecutor on a confidential basis, on the condition that the Prosecutor will use them solely for the purpose of generating new evidence.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>If the Attorney-General transmits documents or information subject to the condition specified in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39552" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 28" atidlm:xmlId="DLM486491" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40526" atidlm:sourceXmlId="DLM486491" atidlm:targetKey="337606" atidlm:targetXmlId="DLM64185" atidlm:targetName="prov 28" atidlm:sourceName="DLM486491" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64185" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the Attorney-General may subsequently consent to the disclosure of such documents or information for use as evidence under the provisions of <citation jurisdiction="nz">Parts <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39553" atidlm:branchLabel="_NOVERSION_" atidlm:name="head1 5 Investigation and Prosecution" atidlm:xmlId="DLM486492" atidlm:state="UNCHANGED"><atidlm:linkcontent>5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40527" atidlm:sourceXmlId="DLM486492" atidlm:targetKey="347075" atidlm:targetXmlId="DLM66011" atidlm:targetName="head1 5 Investigation and Prosecution" atidlm:sourceName="DLM486492" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64185" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39554" atidlm:branchLabel="_NOVERSION_" atidlm:name="head1 6 The Trial" atidlm:xmlId="DLM486493" atidlm:state="UNCHANGED"><atidlm:linkcontent>6</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40528" atidlm:sourceXmlId="DLM486493" atidlm:targetKey="347085" atidlm:targetXmlId="DLM66021" atidlm:targetName="head1 6 The Trial" atidlm:sourceName="DLM486493" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64185" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the Statute</citation> and in accordance with the Rules.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">Statute, articles <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39555" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 93" atidlm:xmlId="DLM486494" atidlm:state="UNCHANGED"><atidlm:linkcontent>93(5)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40529" atidlm:sourceXmlId="DLM486494" atidlm:targetKey="347120" atidlm:targetXmlId="DLM66056" atidlm:targetName="prov Article 93" atidlm:sourceName="DLM486494" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64185" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39556" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 93" atidlm:xmlId="DLM486495" atidlm:state="UNCHANGED"><atidlm:linkcontent>93(8)(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40530" atidlm:sourceXmlId="DLM486495" atidlm:targetKey="347120" atidlm:targetXmlId="DLM66056" atidlm:targetName="prov Article 93" atidlm:sourceName="DLM486495" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64185" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39557" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 93" atidlm:xmlId="DLM486496" atidlm:state="UNCHANGED"><atidlm:linkcontent>(c)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40531" atidlm:sourceXmlId="DLM486496" atidlm:targetKey="347120" atidlm:targetXmlId="DLM66056" atidlm:targetName="prov Article 93" atidlm:sourceName="DLM486496" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64185" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39558" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 97" atidlm:xmlId="DLM486497" atidlm:state="UNCHANGED"><atidlm:linkcontent>97</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40532" atidlm:sourceXmlId="DLM486497" atidlm:targetKey="347124" atidlm:targetXmlId="DLM66060" atidlm:targetName="prov Article 97" atidlm:sourceName="DLM486497" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64185" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
