              <amend>
                <def-para id="DLM65534" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM65535" xml:lang="en-NZ">international crime</def-term> has the same meaning as in <citation jurisdiction="nz">section 4(1) of the International Crimes and International Criminal Court Act 2000</citation>.</text>
                  </para>
                </def-para>
              </amend>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
