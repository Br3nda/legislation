te="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43134" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 39" atidlm:xmlId="DLM489866" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43616" atidlm:sourceXmlId="DLM489866" atidlm:targetKey="347061" atidlm:targetXmlId="DLM65597" atidlm:targetName="prov Article 39" atidlm:sourceName="DLM489866" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66089" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, article 42, paragraphs <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43135" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 42" atidlm:xmlId="DLM489867" atidlm:state="UNCHANGED"><atidlm:linkcontent>4 to 9</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43617" atidlm:sourceXmlId="DLM489867" atidlm:targetKey="347064" atidlm:targetXmlId="DLM66000" atidlm:targetName="prov Article 42" atidlm:sourceName="DLM489867" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66089" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, article 43, paragraphs <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43136" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 43" atidlm:xmlId="DLM489868" atidlm:state="UNCHANGED"><atidlm:linkcontent>2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43618" atidlm:sourceXmlId="DLM489868" atidlm:targetKey="347065" atidlm:targetXmlId="DLM66001" atidlm:targetName="prov Article 43" atidlm:sourceName="DLM489868" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66089" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43137" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 43" atidlm:xmlId="DLM489869" atidlm:state="UNCHANGED"><atidlm:linkcontent>3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43619" atidlm:sourceXmlId="DLM489869" atidlm:targetKey="347065" atidlm:targetXmlId="DLM66001" atidlm:targetName="prov Article 43" atidlm:sourceName="DLM489869" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66089" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, and articles <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43138" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 44" atidlm:xmlId="DLM489870" atidlm:state="UNCHANGED"><atidlm:linkcontent>44</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43620" atidlm:sourceXmlId="DLM489870" atidlm:targetKey="347066" atidlm:targetXmlId="DLM66002" atidlm:targetName="prov Article 44" atidlm:sourceName="DLM489870" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66089" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43139" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 46" atidlm:xmlId="DLM489871" atidlm:state="UNCHANGED"><atidlm:linkcontent>46</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43621" atidlm:sourceXmlId="DLM489871" atidlm:targetKey="347068" atidlm:targetXmlId="DLM66004" atidlm:targetName="prov Article 46" atidlm:sourceName="DLM489871" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66089" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43140" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 47" atidlm:xmlId="DLM489872" atidlm:state="UNCHANGED"><atidlm:linkcontent>47</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43622" atidlm:sourceXmlId="DLM489872" atidlm:targetKey="347069" atidlm:targetXmlId="DLM66005" atidlm:targetName="prov Article 47" atidlm:sourceName="DLM489872" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66089" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43141" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 49" atidlm:xmlId="DLM489873" atidlm:state="UNCHANGED"><atidlm:linkcontent>49</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43623" atidlm:sourceXmlId="DLM489873" atidlm:targetKey="347071" atidlm:targetXmlId="DLM66007" atidlm:targetName="prov Article 49" atidlm:sourceName="DLM489873" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66089" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, may be proposed at any time, notwithstanding <citation jurisdiction="nz">article 121, paragraph <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43142" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 121" atidlm:xmlId="DLM489874" atidlm:state="UNCHANGED"><atidlm:linkcontent>1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43624" atidlm:sourceXmlId="DLM489874" atidlm:targetKey="347152" atidlm:targetXmlId="DLM66088" atidlm:targetName="prov Article 121" atidlm:sourceName="DLM489874" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66089" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, by any State Party. The text of any proposed amendment shall be submitted to the Secretary-General of the United Nations or such other person designated by the Assembly of States Parties who shall promptly circulate it to all States Parties and to others participating in the Assembly.</text>
                  </para>
                </subprov>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no">2.</label>
                  <para xml:lang="en-NZ">
                    <text>Amendments under this article on which consensus cannot be reached shall be adopted by the Assembly of States Parties or by a Review Conference, by a two-thirds majority of States Parties. Such amendments shall enter into force for all States Parties six months after their adoption by the Assembly or, as the case may be, by the Conference.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
            <prov id="DLM66090" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">
                <emphasis style="underline">Article 123</emphasis>
              </label>
              <heading xml:lang="en-NZ">
                <emphasis style="underline">Review of the Statute</emphasis>
              </heading>
              <prov.body>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no">1.</label>
                  <para xml:lang="en-NZ">
                    <text>Seven years after the entry into force of this Statute the Secretary-General of the United Nations shall convene a Review Conference to consider any amendments to this Statute. Such review may include, but is not limited to, the list of crimes contained in <citation jurisdiction="nz">article <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43143" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 5" atidlm:xmlId="DLM489875" atidlm:state="UNCHANGED"><atidlm:linkcontent>5</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43625" atidlm:sourceXmlId="DLM489875" atidlm:targetKey="347025" atidlm:targetXmlId="DLM65561" atidlm:targetName="prov Article 5" atidlm:sourceName="DLM489875" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66090" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>. The Conference shall be open to those participating in the Assembly of States Parties and on the same conditions.</text>
                  </para>
                </subprov>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no">2.</label>
                  <para xml:lang="en-NZ">
                    <text>At any time thereafter, at the request of a State Party and for the purposes set out in <citation jurisdiction="nz">paragraph <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43144" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 123" atidlm:xmlId="DLM489876" atidlm:state="UNCHANGED"><atidlm:linkcontent>1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43626" atidlm:sourceXmlId="DLM489876" atidlm:targetKey="347154" atidlm:targetXmlId="DLM66090" atidlm:targetName="prov Article 123" atidlm:sourceName="DLM489876" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66090" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the Secretary-General of the United Nations shall, upon approval by a majority of States Parties, convene a Review Conference.</text>
                  </para>
                </subprov>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no">3.</label>
                  <para xml:lang="en-NZ">
                    <text>The provisions of <citation jurisdiction="nz">article 121, paragraphs <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43145" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 121" atidlm:xmlId="DLM489877" atidlm:state="UNCHANGED"><atidlm:linkcontent>3 to 7</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43627" atidlm:sourceXmlId="DLM489877" atidlm:targetKey="347152" atidlm:targetXmlId="DLM66088" atidlm:targetName="prov Article 121" atidlm:sourceName="DLM489877" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66090" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, shall apply to the adoption and entry into force of any amendment to the Statute considered at a Review Conference.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
            <prov id="DLM66091" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">
                <emphasis style="underline">Article 124</emphasis>
              </label>
              <heading xml:lang="en-NZ">
                <emphasis style="underline">Transitional Provision</emphasis>
              </heading>
              <prov.body>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no"/>
                  <para xml:lang="en-NZ">
                    <text>Notwithstanding <citation jurisdiction="nz">article 12, paragraphs <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43146" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 12" atidlm:xmlId="DLM489878" atidlm:state="UNCHANGED"><atidlm:linkcontent>1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43628" atidlm:sourceXmlId="DLM489878" atidlm:targetKey="347032" atidlm:targetXmlId="DLM65568" atidlm:targetName="prov Article 12" atidlm:sourceName="DLM489878" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66091" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43147" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 12" atidlm:xmlId="DLM489879" atidlm:state="UNCHANGED"><atidlm:linkcontent>2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43629" atidlm:sourceXmlId="DLM489879" atidlm:targetKey="347032" atidlm:targetXmlId="DLM65568" atidlm:targetName="prov Article 12" atidlm:sourceName="DLM489879" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66091" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, a State, on becoming a party to this Statute, may declare that, for a period of seven years after the entry into force of this Statute for the State concerned, it does not accept the jurisdiction of the Court with respect to the category of crimes referred to in <citation jurisdiction="nz">article <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43148" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 8" atidlm:xmlId="DLM489880" atidlm:state="UNCHANGED"><atidlm:linkcontent>8</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43630" atidlm:sourceXmlId="DLM489880" atidlm:targetKey="347028" atidlm:targetXmlId="DLM65564" atidlm:targetName="prov Article 8" atidlm:sourceName="DLM489880" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66091" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> when a crime is alleged to have been committed by its nationals or on its territory. A declaration under this article may be withdrawn at any time. The provisions of this article shall be reviewed at the Review Conference convened in accordance with <citation jurisdiction="nz">article 123, paragraph <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="43149" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 123" atidlm:xmlId="DLM489881" atidlm:state="UNCHANGED"><atidlm:linkcontent>1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="43631" atidlm:sourceXmlId="DLM489881" atidlm:targetKey="347154" atidlm:targetXmlId="DLM66090" atidlm:targetName="prov Article 123" atidlm:sourceName="DLM489881" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM66091" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
            <prov id="DLM66092" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">
                <emphasis style="underline">Article 125</emphasis>
              </label>
              <heading xml:lang="en-NZ">
                <emphasis style="underline">Signature, ratification, acceptance, approval or accession</emphasis>
              </heading>
              <prov.body>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no">1.</label>
                  <para xml:lang="en-NZ">
                    <text>This Statute shall be open for signature by all States in Rome, at the headquarters of the Food and Agriculture Organization of the United Nations, on 17 July 1998. Thereafter, it shall remain open for signature in Rome at the Ministry of Foreign Affairs of Italy until 17 October 1998. After that date, the Statute shall remain open for signature in New York, at United Nations Headquarters, until 31 December 2000.</text>
                  </para>
                </subprov>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no">2.</label>
                  <para xml:lang="en-NZ">
                    <text>This Statute is subject to ratification, acceptance or approval by signatory States. Instruments of ratification, acceptance or approval shall be deposited with the Secretary-General of the United Nations.</text>
                  </para>
                </subprov>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no">3.</label>
                  <para xml:lang="en-NZ">
                    <text>This Statute shall be open to accession by all States. Instruments of accession shall be deposited with the Secretary-General of the United Nations.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
            <prov id="DLM66093" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">
                <emphasis style="underline">Article 126</emphasis>
              </label>
              <heading xml:lang="en-NZ">
                <emphasis style="underline">Entry into force</emphasis>
              </heading>
              <prov.body>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no">1.</label>
                  <para xml:lang="en-NZ">
                    <text>This Statute shall enter into force on the first day of the month after the 60th day following the date of the deposit of the 60th instrument of ratification, acceptance, approval or accession with the Secretary-General of the United Nations.</text>
                  </para>
                </subprov>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no">2.</label>
                  <para xml:lang="en-NZ">
                    <text>For each State ratifying, accepting, approving or acceding to this Statute after the deposit of the 60th instrument of ratification, acceptance, approval or accession, the Statute shall enter into force on the first day of the month after the 60th day following the deposit by such State of its instrument of ratification, acceptance, approval or accession.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
            <prov id="DLM66094" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">
                <emphasis style="underline">Article 127</emphasis>
              </label>
              <heading xml:lang="en-NZ">
                <emphasis style="underline">Withdrawal</emphasis>
              </heading>
              <prov.body>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no">1.</label>
                  <para xml:lang="en-NZ">
                    <text>A State Party may, by written notification addressed to the Secretary-General of the United Nations, withdraw from this Statute. The withdrawal shall take effect one year after the date of receipt of the notification, unless the notification specifies a later date.</text>
                  </para>
                </subprov>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no">2.</label>
                  <para xml:lang="en-NZ">
                    <text>A State shall not be discharged, by reason of its withdrawal, from the obligations arising from this Statute while it was a Party to the Statute, including any financial obligations which may have accrued. Its withdrawal shall not affect any cooperation with the Court in connection with criminal investigations and proceedings in relation to which the withdrawing State had a duty to cooperate and which were commenced prior to the date on which the withdrawal became effective, nor shall it prejudice in any way the continued consideration of any matter which was already under consideration by the Court prior to the date on which the withdrawal became effective.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
            <prov id="DLM66095" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">
                <emphasis style="underline">Article 128</emphasis>
              </label>
              <heading xml:lang="en-NZ">
                <emphasis style="underline">Authentic texts</emphasis>
              </heading>
              <prov.body>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no"/>
                  <para xml:lang="en-NZ">
                    <text>The original of this Statute, of which the Arabic, Chinese, English, French, Russian and Spanish texts are equally authentic, shall be deposited with the Secretary-General of the United Nations, who shall send certified copies thereof to all States.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
          </head1>
        </conv.body>
        <conv.end xml:lang="en-NZ">
          <signature-block>
            <para xml:lang="en-NZ">
              <text>IN WITNESS WHEREOF, the undersigned, being duly authorized thereto by their respective Governments, have signed this Statute.</text>
            </para>
            <para xml:lang="en-NZ">
              <text>DONE at Rome, this 17th day of July 1998.</text>
            </para>
          </signature-block>
        </conv.end>
      </conv>
    </schedule>
  </schedule.group>
