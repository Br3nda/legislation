:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="42174" atidlm:sourceXmlId="DLM488401" atidlm:targetKey="347137" atidlm:targetXmlId="DLM66073" atidlm:targetName="prov Article 109" atidlm:sourceName="DLM488401" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64931.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64933" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
