806" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM365943.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64188" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; <citation jurisdiction="nz">Statute, articles <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39568" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 86" atidlm:xmlId="DLM486807" atidlm:state="UNCHANGED"><atidlm:linkcontent>86</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40542" atidlm:sourceXmlId="DLM486807" atidlm:targetKey="347113" atidlm:targetXmlId="DLM66049" atidlm:targetName="prov Article 86" atidlm:sourceName="DLM486807" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64188" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39569" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 90" atidlm:xmlId="DLM486808" atidlm:state="UNCHANGED"><atidlm:linkcontent>90(8)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40543" atidlm:sourceXmlId="DLM486808" atidlm:targetKey="347117" atidlm:targetXmlId="DLM66053" atidlm:targetName="prov Article 90" atidlm:sourceName="DLM486808" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64188" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="39570" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 93" atidlm:xmlId="DLM486809" atidlm:state="UNCHANGED"><atidlm:linkcontent>93(6)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="40544" atidlm:sourceXmlId="DLM486809" atidlm:targetKey="347120" atidlm:targetXmlId="DLM66056" atidlm:targetName="prov Article 93" atidlm:sourceName="DLM486809" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64179.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64188" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></cf>
