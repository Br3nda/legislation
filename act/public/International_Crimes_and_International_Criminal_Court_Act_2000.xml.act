pe="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41535" atidlm:sourceXmlId="DLM487846" atidlm:targetKey="347088" atidlm:targetXmlId="DLM66024" atidlm:targetName="prov Article 64" atidlm:sourceName="DLM487846" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64900" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41111" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 93" atidlm:xmlId="DLM487847" atidlm:state="UNCHANGED"><atidlm:linkcontent>93(1)(g)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41536" atidlm:sourceXmlId="DLM487847" atidlm:targetKey="347120" atidlm:targetXmlId="DLM66056" atidlm:targetName="prov Article 93" atidlm:sourceName="DLM487847" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64900" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
        </cf>
      </prov>
      <crosshead id="DLM64901" toc="yes" xml:lang="en-NZ">Search and seizure</crosshead>
      <prov id="DLM64902" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">101</label>
        <heading xml:lang="en-NZ">Request for search and seizure</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>This section applies if the ICC makes a request under any of <citation jurisdiction="nz">articles <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41112" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 19" atidlm:xmlId="DLM487848" atidlm:state="UNCHANGED"><atidlm:linkcontent>19(8)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41537" atidlm:sourceXmlId="DLM487848" atidlm:targetKey="347039" atidlm:targetXmlId="DLM65575" atidlm:targetName="prov Article 19" atidlm:sourceName="DLM487848" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41113" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 56" atidlm:xmlId="DLM487849" atidlm:state="UNCHANGED"><atidlm:linkcontent>56</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41538" atidlm:sourceXmlId="DLM487849" atidlm:targetKey="347079" atidlm:targetXmlId="DLM66015" atidlm:targetName="prov Article 56" atidlm:sourceName="DLM487849" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41114" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 64" atidlm:xmlId="DLM487850" atidlm:state="UNCHANGED"><atidlm:linkcontent>64</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41539" atidlm:sourceXmlId="DLM487850" atidlm:targetKey="347088" atidlm:targetXmlId="DLM66024" atidlm:targetName="prov Article 64" atidlm:sourceName="DLM487850" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41115" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 93" atidlm:xmlId="DLM487851" atidlm:state="UNCHANGED"><atidlm:linkcontent>93(1)(h)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41540" atidlm:sourceXmlId="DLM487851" atidlm:targetKey="347120" atidlm:targetXmlId="DLM66056" atidlm:targetName="prov Article 93" atidlm:sourceName="DLM487851" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the Statute</citation> for search and seizure.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The Attorney-General may give authority for the request to proceed if he or she is satisfied that—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the request relates to an investigation being conducted by the Prosecutor or a proceeding before the ICC; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>any thing relevant to the investigation or proceedings is or may be located in New Zealand.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>If the Attorney-General gives authority for the request to proceed, he or she may authorise a member of the police, in writing, to apply to a District Court Judge for a search warrant under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41116" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 102" atidlm:xmlId="DLM487852" atidlm:state="UNCHANGED"><atidlm:linkcontent>102</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41541" atidlm:sourceXmlId="DLM487852" atidlm:targetKey="340639" atidlm:targetXmlId="DLM64903" atidlm:targetName="prov 102" atidlm:sourceName="DLM487852" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
        </prov.body>
        <cf><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41117" atidlm:branchLabel="_NOVERSION_" atidlm:name="Mutual Assistance in Criminal Matters Act 1992" atidlm:xmlId="DLM487853" atidlm:state="UNCHANGED"><atidlm:linkcontent>1992 No 86</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="41542" atidlm:sourceXmlId="DLM487853" atidlm:targetKey="1420005" atidlm:targetXmlId="DLM273056" atidlm:targetName="Mutual Assistance in Criminal Matters Act 1992" atidlm:sourceName="DLM487853" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM273056.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41118" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 43" atidlm:xmlId="DLM487854" atidlm:state="UNCHANGED"><atidlm:linkcontent>43</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41543" atidlm:sourceXmlId="DLM487854" atidlm:targetKey="1422814" atidlm:targetXmlId="DLM274456" atidlm:targetName="prov 43" atidlm:sourceName="DLM487854" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM274075.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; <citation jurisdiction="nz">Statute, articles <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41119" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 19" atidlm:xmlId="DLM487855" atidlm:state="UNCHANGED"><atidlm:linkcontent>19(8)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41544" atidlm:sourceXmlId="DLM487855" atidlm:targetKey="347039" atidlm:targetXmlId="DLM65575" atidlm:targetName="prov Article 19" atidlm:sourceName="DLM487855" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41120" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 56" atidlm:xmlId="DLM487856" atidlm:state="UNCHANGED"><atidlm:linkcontent>56</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41545" atidlm:sourceXmlId="DLM487856" atidlm:targetKey="347079" atidlm:targetXmlId="DLM66015" atidlm:targetName="prov Article 56" atidlm:sourceName="DLM487856" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41121" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 64" atidlm:xmlId="DLM487857" atidlm:state="UNCHANGED"><atidlm:linkcontent>64</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41546" atidlm:sourceXmlId="DLM487857" atidlm:targetKey="347088" atidlm:targetXmlId="DLM66024" atidlm:targetName="prov Article 64" atidlm:sourceName="DLM487857" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41122" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 93" atidlm:xmlId="DLM487858" atidlm:state="UNCHANGED"><atidlm:linkcontent>93(1)(h)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41547" atidlm:sourceXmlId="DLM487858" atidlm:targetKey="347120" atidlm:targetXmlId="DLM66056" atidlm:targetName="prov Article 93" atidlm:sourceName="DLM487858" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></cf>
      </prov>
      <prov id="DLM64903" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">102</label>
        <heading xml:lang="en-NZ">Issue of search warrant</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>This section applies if a District Court Judge, on an application in writing made on oath or affirmation by a member of the police, is satisfied that there are reasonable grounds for believing that there is in or on any place or thing—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>any thing on or in respect of which an international crime has been, or is suspected of having been, committed; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>any thing that may be evidence as to the commission of any such crime.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>If this section applies, the District Court Judge may issue a warrant in respect of any thing referred to in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41123" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 102" atidlm:xmlId="DLM487859" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41548" atidlm:sourceXmlId="DLM487859" atidlm:targetKey="340639" atidlm:targetXmlId="DLM64903" atidlm:targetName="prov 102" atidlm:sourceName="DLM487859" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64903" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>A Judge must not issue a warrant under this section unless the application contains, or the applicant otherwise supplies to the Judge, such information as the Judge requires concerning the grounds on which the warrant is sought.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>A Judge may issue a warrant under this section subject to such conditions as the Judge thinks fit.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41124" atidlm:branchLabel="_NOVERSION_" atidlm:name="International War Crimes Tribunals Act 1995" atidlm:xmlId="DLM487860" atidlm:state="UNCHANGED"><atidlm:linkcontent>1995 No 27</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="41549" atidlm:sourceXmlId="DLM487860" atidlm:targetKey="1940603" atidlm:targetXmlId="DLM365015" atidlm:targetName="International War Crimes Tribunals Act 1995" atidlm:sourceName="DLM487860" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM365015.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64903" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41125" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 48" atidlm:xmlId="DLM487861" atidlm:state="UNCHANGED"><atidlm:linkcontent>48</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41550" atidlm:sourceXmlId="DLM487861" atidlm:targetKey="1945402" atidlm:targetXmlId="DLM365929" atidlm:targetName="prov 48" atidlm:sourceName="DLM487861" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM365927.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64903" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
        </cf>
      </prov>
      <prov id="DLM64904" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">103</label>
        <heading xml:lang="en-NZ">Form and content of search warrant</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>A warrant issued under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41126" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 102" atidlm:xmlId="DLM487862" atidlm:state="UNCHANGED"><atidlm:linkcontent>102</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41551" atidlm:sourceXmlId="DLM487862" atidlm:targetKey="340639" atidlm:targetXmlId="DLM64903" atidlm:targetName="prov 102" atidlm:sourceName="DLM487862" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64904" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>be in the prescribed form; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>be directed to a member of the police by name, or to any class of members of the police specified in the warrant, or generally to every member of the police; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>specify any conditions that the District Court Judge has imposed under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41127" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 102" atidlm:xmlId="DLM487863" atidlm:state="UNCHANGED"><atidlm:linkcontent>102(4)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41552" atidlm:sourceXmlId="DLM487863" atidlm:targetKey="340639" atidlm:targetXmlId="DLM64903" atidlm:targetName="prov 102" atidlm:sourceName="DLM487863" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64904" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41128" atidlm:branchLabel="_NOVERSION_" atidlm:name="Extradition Act 1999" atidlm:xmlId="DLM487864" atidlm:state="UNCHANGED"><atidlm:linkcontent>1999 No 55</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="41553" atidlm:sourceXmlId="DLM487864" atidlm:targetKey="82400" atidlm:targetXmlId="DLM25627" atidlm:targetName="Extradition Act 1999" atidlm:sourceName="DLM487864" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM25627.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64904" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41129" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 84" atidlm:xmlId="DLM487865" atidlm:state="UNCHANGED"><atidlm:linkcontent>84</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41554" atidlm:sourceXmlId="DLM487865" atidlm:targetKey="91003" atidlm:targetXmlId="DLM26771" atidlm:targetName="prov 84" atidlm:sourceName="DLM487865" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26768.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64904" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
        </cf>
      </prov>
      <prov id="DLM64905" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">104</label>
        <heading xml:lang="en-NZ">Powers conferred by warrant</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Subject to any conditions specified in the warrant under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41130" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 103" atidlm:xmlId="DLM487866" atidlm:state="UNCHANGED"><atidlm:linkcontent>103</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41555" atidlm:sourceXmlId="DLM487866" atidlm:targetKey="340640" atidlm:targetXmlId="DLM64904" atidlm:targetName="prov 103" atidlm:sourceName="DLM487866" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64905" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, a warrant issued under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41131" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 102" atidlm:xmlId="DLM487867" atidlm:state="UNCHANGED"><atidlm:linkcontent>102</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41556" atidlm:sourceXmlId="DLM487867" atidlm:targetKey="340639" atidlm:targetXmlId="DLM64903" atidlm:targetName="prov 102" atidlm:sourceName="DLM487867" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64905" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> authorises the member of the police executing the warrant—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>to enter and search the place or thing specified in the warrant at any time by day or night during the currency of the warrant; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>to use such assistants as may be reasonable in the circumstances for the purpose of the entry and search; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>to use such force as is reasonable in the circumstances for the purposes of effecting entry, and for breaking open any thing in or on the place searched; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>to search for and seize any thing referred to in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41132" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 102" atidlm:xmlId="DLM487868" atidlm:state="UNCHANGED"><atidlm:linkcontent>102(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41557" atidlm:sourceXmlId="DLM487868" atidlm:targetKey="340639" atidlm:targetXmlId="DLM64903" atidlm:targetName="prov 102" atidlm:sourceName="DLM487868" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64905" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>A person called on to assist any member of the police executing a warrant issued under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41133" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 102" atidlm:xmlId="DLM487869" atidlm:state="UNCHANGED"><atidlm:linkcontent>102</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41558" atidlm:sourceXmlId="DLM487869" atidlm:targetKey="340639" atidlm:targetXmlId="DLM64903" atidlm:targetName="prov 102" atidlm:sourceName="DLM487869" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64905" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> has the powers described in <citation jurisdiction="nz">paragraphs <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41134" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 104" atidlm:xmlId="DLM487870" atidlm:state="UNCHANGED"><atidlm:linkcontent>(c)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41560" atidlm:sourceXmlId="DLM487870" atidlm:targetKey="340641" atidlm:targetXmlId="DLM64905" atidlm:targetName="prov 104" atidlm:sourceName="DLM487870" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64905" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41135" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 104" atidlm:xmlId="DLM487871" atidlm:state="UNCHANGED"><atidlm:linkcontent>(d)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41561" atidlm:sourceXmlId="DLM487871" atidlm:targetKey="340641" atidlm:targetXmlId="DLM64905" atidlm:targetName="prov 104" atidlm:sourceName="DLM487871" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64905" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of subsection (1)</citation>.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41136" atidlm:branchLabel="_NOVERSION_" atidlm:name="Extradition Act 1999" atidlm:xmlId="DLM487872" atidlm:state="UNCHANGED"><atidlm:linkcontent>1999 No 55</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="41562" atidlm:sourceXmlId="DLM487872" atidlm:targetKey="82400" atidlm:targetXmlId="DLM25627" atidlm:targetName="Extradition Act 1999" atidlm:sourceName="DLM487872" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM25627.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64905" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41137" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 85" atidlm:xmlId="DLM487873" atidlm:state="UNCHANGED"><atidlm:linkcontent>85</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41563" atidlm:sourceXmlId="DLM487873" atidlm:targetKey="91004" atidlm:targetXmlId="DLM26772" atidlm:targetName="prov 85" atidlm:sourceName="DLM487873" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26768.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64905" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
        </cf>
      </prov>
      <prov id="DLM64906" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">105</label>
        <heading xml:lang="en-NZ">Power to stop vehicles</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>If it is necessary for a member of the police to stop a vehicle for the purpose of exercising a search power conferred by a warrant issued under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41138" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 102" atidlm:xmlId="DLM487874" atidlm:state="UNCHANGED"><atidlm:linkcontent>102</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41564" atidlm:sourceXmlId="DLM487874" atidlm:targetKey="340639" atidlm:targetXmlId="DLM64903" atidlm:targetName="prov 102" atidlm:sourceName="DLM487874" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64906" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of this Act</citation>, <citation jurisdiction="nz">sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41139" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 314B" atidlm:xmlId="DLM487875" atidlm:state="UNCHANGED"><atidlm:linkcontent>314B to 314D</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41565" atidlm:sourceXmlId="DLM487875" atidlm:targetKey="2150257" atidlm:targetXmlId="DLM331223" atidlm:targetName="prov 314B" atidlm:sourceName="DLM487875" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM331214.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64906" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41140" atidlm:branchLabel="_NOVERSION_" atidlm:name="Crimes Act 1961" atidlm:xmlId="DLM487876" atidlm:state="UNCHANGED"><atidlm:linkcontent>Crimes Act 1961</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="41566" atidlm:sourceXmlId="DLM487876" atidlm:targetKey="2134539" atidlm:targetXmlId="DLM327381" atidlm:targetName="Crimes Act 1961" atidlm:sourceName="DLM487876" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM327381.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64906" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> apply with any necessary modifications as if references in those sections to a statutory search power were references to <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41141" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 104" atidlm:xmlId="DLM487877" atidlm:state="UNCHANGED"><atidlm:linkcontent>104(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41567" atidlm:sourceXmlId="DLM487877" atidlm:targetKey="340641" atidlm:targetXmlId="DLM64905" atidlm:targetName="prov 104" atidlm:sourceName="DLM487877" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64906" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of this Act</citation>.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41142" atidlm:branchLabel="_NOVERSION_" atidlm:name="Extradition Act 1999" atidlm:xmlId="DLM487878" atidlm:state="UNCHANGED"><atidlm:linkcontent>1999 No 55</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="41568" atidlm:sourceXmlId="DLM487878" atidlm:targetKey="82400" atidlm:targetXmlId="DLM25627" atidlm:targetName="Extradition Act 1999" atidlm:sourceName="DLM487878" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM25627.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64906" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41143" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 86" atidlm:xmlId="DLM487879" atidlm:state="UNCHANGED"><atidlm:linkcontent>86</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41569" atidlm:sourceXmlId="DLM487879" atidlm:targetKey="91005" atidlm:targetXmlId="DLM26773" atidlm:targetName="prov 86" atidlm:sourceName="DLM487879" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26768.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64906" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
        </cf>
      </prov>
      <prov id="DLM64907" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">106</label>
        <heading xml:lang="en-NZ">Person executing warrant to produce evidence of authority</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Every member of the police executing any warrant issued under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41144" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 102" atidlm:xmlId="DLM487880" atidlm:state="UNCHANGED"><atidlm:linkcontent>102</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41570" atidlm:sourceXmlId="DLM487880" atidlm:targetKey="340639" atidlm:targetXmlId="DLM64903" atidlm:targetName="prov 102" atidlm:sourceName="DLM487880" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64907" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>must have that warrant with him or her; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>must produce it on initial entry and, if requested, at any subsequent time; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>must, if requested at the time of the execution of the warrant or at any subsequent time, provide a copy of the warrant within 7 days after the request is made.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41145" atidlm:branchLabel="_NOVERSION_" atidlm:name="Extradition Act 1999" atidlm:xmlId="DLM487881" atidlm:state="UNCHANGED"><atidlm:linkcontent>1999 No 55</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="41571" atidlm:sourceXmlId="DLM487881" atidlm:targetKey="82400" atidlm:targetXmlId="DLM25627" atidlm:targetName="Extradition Act 1999" atidlm:sourceName="DLM487881" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM25627.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64907" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41146" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 87" atidlm:xmlId="DLM487882" atidlm:state="UNCHANGED"><atidlm:linkcontent>87</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41572" atidlm:sourceXmlId="DLM487882" atidlm:targetKey="91006" atidlm:targetXmlId="DLM26774" atidlm:targetName="prov 87" atidlm:sourceName="DLM487882" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26768.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64907" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
        </cf>
      </prov>
      <prov id="DLM64908" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">107</label>
        <heading xml:lang="en-NZ">Notice of execution of warrant</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Every member of the police who executes a warrant issued under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41147" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 102" atidlm:xmlId="DLM487883" atidlm:state="UNCHANGED"><atidlm:linkcontent>102</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41573" atidlm:sourceXmlId="DLM487883" atidlm:targetKey="340639" atidlm:targetXmlId="DLM64903" atidlm:targetName="prov 102" atidlm:sourceName="DLM487883" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64908" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must, not later than 7 days after the seizure of any thing under that warrant, give to the owner or occupier of the place or thing searched, and to every other person whom the member of the police has reason to believe may have an interest in the thing seized, a written notice specifying—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the date and time of the execution of the warrant; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the identity of the person who executed the warrant; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>the thing seized under the warrant.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>If the warrant is executed, a report on the execution of the warrant, together with a copy of any notice given under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41148" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 107" atidlm:xmlId="DLM487884" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41574" atidlm:sourceXmlId="DLM487884" atidlm:targetKey="340644" atidlm:targetXmlId="DLM64908" atidlm:targetName="prov 107" atidlm:sourceName="DLM487884" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64908" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must be sent to the Attorney-General, without delay.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>If the warrant is not able to be executed, a report explaining the reasons for this must be sent to the Attorney-General, without delay.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41149" atidlm:branchLabel="_NOVERSION_" atidlm:name="Extradition Act 1999" atidlm:xmlId="DLM487885" atidlm:state="UNCHANGED"><atidlm:linkcontent>1999 No 55</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="41575" atidlm:sourceXmlId="DLM487885" atidlm:targetKey="82400" atidlm:targetXmlId="DLM25627" atidlm:targetName="Extradition Act 1999" atidlm:sourceName="DLM487885" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM25627.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64908" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41150" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 88" atidlm:xmlId="DLM487886" atidlm:state="UNCHANGED"><atidlm:linkcontent>88</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41576" atidlm:sourceXmlId="DLM487886" atidlm:targetKey="91007" atidlm:targetXmlId="DLM26775" atidlm:targetName="prov 88" atidlm:sourceName="DLM487886" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26768.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64908" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
        </cf>
      </prov>
      <prov id="DLM64909" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">108</label>
        <heading xml:lang="en-NZ">Disposal of things seized</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>If a member of the police seizes a thing under a warrant issued under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41151" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 102" atidlm:xmlId="DLM487887" atidlm:state="UNCHANGED"><atidlm:linkcontent>102</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41577" atidlm:sourceXmlId="DLM487887" atidlm:targetKey="340639" atidlm:targetXmlId="DLM64903" atidlm:targetName="prov 102" atidlm:sourceName="DLM487887" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64909" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, it must be delivered into the custody and control of—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the Commissioner of Police; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>a commissioned officer of police designated by the Commissioner to receive things seized under this Act.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The Commissioner of Police or designated officer must—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>inform the Attorney-General, without delay, that the thing has been so delivered; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>retain the thing for a period not exceeding 3 months from the day on which the thing was seized, pending the Attorney-General's direction under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41152" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 108" atidlm:xmlId="DLM487888" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41578" atidlm:sourceXmlId="DLM487888" atidlm:targetKey="340645" atidlm:targetXmlId="DLM64909" atidlm:targetName="prov 108" atidlm:sourceName="DLM487888" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64909" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> about how to deal with the thing; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>comply with any direction that the Attorney-General gives.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>The Attorney-General may, by written notice, give the Commissioner of Police or designated officer a direction—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>requiring the Commissioner of Police or designated officer to send the thing to the ICC; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>requiring the Commissioner of Police or designated officer to deal with the thing in some other way.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>The Attorney-General must direct the Commissioner of Police or designated officer to return the thing seized to the person from whose possession it was seized as soon as practicable, if—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the ICC advises that the thing is not required for the Prosecutor's investigation or its proceeding; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>no other direction is given by the Attorney-General before the expiry of 3 months from the day on which the thing was seized.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">5</label>
            <para xml:lang="en-NZ">
              <text>Despite <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41153" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 108" atidlm:xmlId="DLM487889" atidlm:state="UNCHANGED"><atidlm:linkcontent>(4)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41579" atidlm:sourceXmlId="DLM487889" atidlm:targetKey="340645" atidlm:targetXmlId="DLM64909" atidlm:targetName="prov 108" atidlm:sourceName="DLM487889" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64909" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the Attorney-General may refuse to return the thing to the person from whom it was seized if—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the thing is the subject of a dispute as to who is entitled to it; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the thing is required for the investigation of an offence within the jurisdiction of New Zealand; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>possession of the thing by the person would be unlawful in New Zealand.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <cf><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41154" atidlm:branchLabel="_NOVERSION_" atidlm:name="International War Crimes Tribunals Act 1995" atidlm:xmlId="DLM487890" atidlm:state="UNCHANGED"><atidlm:linkcontent>1995 No 27</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="41580" atidlm:sourceXmlId="DLM487890" atidlm:targetKey="1940603" atidlm:targetXmlId="DLM365015" atidlm:targetName="International War Crimes Tribunals Act 1995" atidlm:sourceName="DLM487890" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM365015.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64909" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41155" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 55" atidlm:xmlId="DLM487891" atidlm:state="UNCHANGED"><atidlm:linkcontent>55</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41581" atidlm:sourceXmlId="DLM487891" atidlm:targetKey="1945414" atidlm:targetXmlId="DLM365942" atidlm:targetName="prov 55" atidlm:sourceName="DLM487891" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM365927.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64909" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41156" atidlm:branchLabel="_NOVERSION_" atidlm:name="Extradition Act 1999" atidlm:xmlId="DLM487892" atidlm:state="UNCHANGED"><atidlm:linkcontent>1999 No 55</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="41582" atidlm:sourceXmlId="DLM487892" atidlm:targetKey="82400" atidlm:targetXmlId="DLM25627" atidlm:targetName="Extradition Act 1999" atidlm:sourceName="DLM487892" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM25627.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64909" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41157" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 89" atidlm:xmlId="DLM487893" atidlm:state="UNCHANGED"><atidlm:linkcontent>89(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41583" atidlm:sourceXmlId="DLM487893" atidlm:targetKey="91009" atidlm:targetXmlId="DLM26777" atidlm:targetName="prov 89" atidlm:sourceName="DLM487893" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26768.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64909" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></cf>
      </prov>
      <crosshead id="DLM64910" toc="yes" xml:lang="en-NZ">Provision of records and documents</crosshead>
      <prov id="DLM64911" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">109</label>
        <heading xml:lang="en-NZ">Facilitating provision of records and documents</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>This section applies if the ICC makes a request under <citation jurisdiction="nz">article <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41158" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 93" atidlm:xmlId="DLM487894" atidlm:state="UNCHANGED"><atidlm:linkcontent>93(1)(i)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41584" atidlm:sourceXmlId="DLM487894" atidlm:targetKey="347120" atidlm:targetXmlId="DLM66056" atidlm:targetName="prov Article 93" atidlm:sourceName="DLM487894" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64911" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the Statute</citation> for the provision of records and documents, including official records and documents.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The Attorney-General may give authority for the request to proceed if the Attorney-General is satisfied that—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the request relates to an investigation being conducted by the Prosecutor or a proceeding before the ICC; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the document or record sought is or may be in New Zealand.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>If the Attorney-General gives authority for the request to proceed,—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the Attorney-General must forward the request to the appropriate New Zealand agency; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>that agency must, without delay,—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
                    <para xml:lang="en-NZ">
                      <text>use its best endeavours to locate and make available the document or record sought; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">ii</label>
                    <para xml:lang="en-NZ">
                      <text>make such report on its endeavours as it considers to be appropriate in the circumstances; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">iii</label>
                    <para xml:lang="en-NZ">
                      <text>deliver the document or record, if located, to the Attorney-General.</text>
                    </para>
                  </label-para>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>This section does not give any person power to require the production of a document or record.</text>
            </para>
          </subprov>
        </prov.body>
        <cf><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41159" atidlm:branchLabel="_NOVERSION_" atidlm:name="Mutual Assistance in Criminal Matters Act 1992" atidlm:xmlId="DLM487895" atidlm:state="UNCHANGED"><atidlm:linkcontent>1992 No 86</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="41585" atidlm:sourceXmlId="DLM487895" atidlm:targetKey="1420005" atidlm:targetXmlId="DLM273056" atidlm:targetName="Mutual Assistance in Criminal Matters Act 1992" atidlm:sourceName="DLM487895" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM273056.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64911" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41160" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 32" atidlm:xmlId="DLM487896" atidlm:state="UNCHANGED"><atidlm:linkcontent>32(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41586" atidlm:sourceXmlId="DLM487896" atidlm:targetKey="1422846" atidlm:targetXmlId="DLM274417" atidlm:targetName="prov 32" atidlm:sourceName="DLM487896" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM274075.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64911" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; <citation jurisdiction="nz">Statute, article <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41161" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 93" atidlm:xmlId="DLM487897" atidlm:state="UNCHANGED"><atidlm:linkcontent>93(1)(i)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41587" atidlm:sourceXmlId="DLM487897" atidlm:targetKey="347120" atidlm:targetXmlId="DLM66056" atidlm:targetName="prov Article 93" atidlm:sourceName="DLM487897" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64911" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></cf>
      </prov>
      <crosshead id="DLM64912" toc="yes" xml:lang="en-NZ">Protecting victims and witnesses and preserving evidence</crosshead>
      <prov id="DLM64913" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">110</label>
        <heading xml:lang="en-NZ">Protecting victims and witnesses and preserving evidence</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>This section applies if the ICC requests—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>assistance under <citation jurisdiction="nz">article <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41162" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 93" atidlm:xmlId="DLM487898" atidlm:state="UNCHANGED"><atidlm:linkcontent>93(1)(j)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41588" atidlm:sourceXmlId="DLM487898" atidlm:targetKey="347120" atidlm:targetXmlId="DLM66056" atidlm:targetName="prov Article 93" atidlm:sourceName="DLM487898" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64913" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the Statute</citation> in protecting victims and witnesses or preserving evidence:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>assistance under <citation jurisdiction="nz">article <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41163" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 19" atidlm:xmlId="DLM487899" atidlm:state="UNCHANGED"><atidlm:linkcontent>19(8)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41589" atidlm:sourceXmlId="DLM487899" atidlm:targetKey="347039" atidlm:targetXmlId="DLM65575" atidlm:targetName="prov Article 19" atidlm:sourceName="DLM487899" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64913" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or paragraphs <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41164" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 56" atidlm:xmlId="DLM487900" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41590" atidlm:sourceXmlId="DLM487900" atidlm:targetKey="347079" atidlm:targetXmlId="DLM66015" atidlm:targetName="prov Article 56" atidlm:sourceName="DLM487900" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64913" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41165" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 56" atidlm:xmlId="DLM487901" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41591" atidlm:sourceXmlId="DLM487901" atidlm:targetKey="347079" atidlm:targetXmlId="DLM66015" atidlm:targetName="prov Article 56" atidlm:sourceName="DLM487901" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64913" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of article 56</citation>, in preserving evidence.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The Attorney-General may give authority for the request to proceed if the Attorney-General is satisfied that—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the request relates to an investigation being conducted by the Prosecutor or a proceeding before the ICC; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the assistance sought is not prohibited by New Zealand law.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>If the Attorney-General gives authority for the request to proceed,—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the Attorney-General must—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
                    <para xml:lang="en-NZ">
                      <text>take such steps as the Attorney-General thinks appropriate in the particular case; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">ii</label>
                    <para xml:lang="en-NZ">
                      <text>forward the request to the appropriate New Zealand agency; and</text>
                    </para>
                  </label-para>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>that agency must, without delay,—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
                    <para xml:lang="en-NZ">
                      <text>use its best endeavours to give effect to the request; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">ii</label>
                    <para xml:lang="en-NZ">
                      <text>make such report on its endeavours as it considers to be appropriate in the circumstances; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">iii</label>
                    <para xml:lang="en-NZ">
                      <text>deliver the report to the Attorney-General.</text>
                    </para>
                  </label-para>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">Statute, articles <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41166" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 19" atidlm:xmlId="DLM487902" atidlm:state="UNCHANGED"><atidlm:linkcontent>19(8)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41592" atidlm:sourceXmlId="DLM487902" atidlm:targetKey="347039" atidlm:targetXmlId="DLM65575" atidlm:targetName="prov Article 19" atidlm:sourceName="DLM487902" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64913" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41167" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 56" atidlm:xmlId="DLM487903" atidlm:state="UNCHANGED"><atidlm:linkcontent>56(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41593" atidlm:sourceXmlId="DLM487903" atidlm:targetKey="347079" atidlm:targetXmlId="DLM66015" atidlm:targetName="prov Article 56" atidlm:sourceName="DLM487903" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64913" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41168" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 56" atidlm:xmlId="DLM487904" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41594" atidlm:sourceXmlId="DLM487904" atidlm:targetKey="347079" atidlm:targetXmlId="DLM66015" atidlm:targetName="prov Article 56" atidlm:sourceName="DLM487904" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64913" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41169" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 68" atidlm:xmlId="DLM487905" atidlm:state="UNCHANGED"><atidlm:linkcontent>68</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41595" atidlm:sourceXmlId="DLM487905" atidlm:targetKey="347092" atidlm:targetXmlId="DLM66028" atidlm:targetName="prov Article 68" atidlm:sourceName="DLM487905" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64913" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="41170" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov Article 93" atidlm:xmlId="DLM487906" atidlm:state="UNCHANGED"><atidlm:linkcontent>93(1)(j)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="41596" atidlm:sourceXmlId="DLM487906" atidlm:targetKey="347120" atidlm:targetXmlId="DLM66056" atidlm:targetName="prov Article 93" atidlm:sourceName="DLM487906" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM65536.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM64913" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
