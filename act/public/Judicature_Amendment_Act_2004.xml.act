            <amend>
              <prov id="DLM294339" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">26Q</label>
                <heading xml:lang="en-NZ">Immunity of Associate Judges</heading>
                <prov.body>
                  <subprov skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="no"/>
                    <para xml:lang="en-NZ">
                      <text>Every Associate Judge has the same immunities as a Judge of the High Court.</text>
                    </para>
                  </subprov>
