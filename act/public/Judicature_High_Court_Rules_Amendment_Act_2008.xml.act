                  <atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="791008" atidlm:sourceXmlId="DLM1648708" atidlm:targetKey="304400" atidlm:targetXmlId="DLM53347" atidlm:targetName="Summary Offences Act 1981" atidlm:sourceName="DLM1648708" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM53347.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1463518.xml" atidlm:state="UNCHANGED">
                    <atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1463528" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/>
                  </atidlm:resourcepair>
