      <prov.body>
        <subprov skeleton="no">
          <label auto.number="no" denominator="yes">1</label>
          <para>
            <text>This Act (except <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1044016" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1398452 - prov 4" atidlm:xmlId="DLM1398615" atidlm:state="UNCHANGED"><atidlm:linkcontent>sections 4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="688620" atidlm:sourceXmlId="DLM1398615" atidlm:targetKey="12459650" atidlm:targetXmlId="DLM1379214" atidlm:targetName="prov 4" atidlm:sourceName="DLM1398615" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1379202.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1379202.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1379201" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1044017" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1398453 - prov 11" atidlm:xmlId="DLM1398616" atidlm:state="UNCHANGED"><atidlm:linkcontent>11</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="688621" atidlm:sourceXmlId="DLM1398616" atidlm:targetKey="12459664" atidlm:targetXmlId="DLM1379226" atidlm:targetName="prov 11" atidlm:sourceName="DLM1398616" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1379202.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1379202.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1379201" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1044018" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1398454 - prov 12" atidlm:xmlId="DLM1398617" atidlm:state="UNCHANGED"><atidlm:linkcontent>12</atidlm:link