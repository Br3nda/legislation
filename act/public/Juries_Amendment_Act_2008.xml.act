          <label auto.number="no" denominator="yes">2</label>
          <para>
            <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1044020" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1398452 - prov 4" atidlm:xmlId="DLM1398619" atidlm:state="UNCHANGED"><atidlm:linkcontent>Sections 4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="688624" atidlm:sourceXmlId="DLM1398619" atidlm:targetKey="12459650" atidlm:targetXmlId="DLM1379214" atidlm:targetName="prov 4" atidlm:sourceName="DLM1398619" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1379202.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1379202.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1379201" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1044021" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1398453 - prov 11" atidlm:xmlId="DLM1398620" atidlm:state="UNCHANGED"><atidlm:linkcontent>11</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="688625" atidlm:sourceXmlId="DLM1398620" atidlm:targetKey="12459664" atidlm:targetXmlId="DLM1379226" atidlm:targetName="prov 11" atidlm:sourceName="DLM1398620" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1379202.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1379202.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1379201" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1044022" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1398454 - prov 12" atidlm:xmlId="DLM1398621" atidlm:state="UNCHANGED"><atidlm:linkcontent>12</atidlm:linkcontent><atidlm:r