<act date.instructed="2008-03-18" id="DLM2095500" in.amend="false" instructing.office="Transport" irdnumbering="no" xml:lang="en-NZ" year.imprint="2009" act.no="36" act.type="public" date.as.at="2009-10-27" date.assent="2009-10-27" date.first.valid="2009-11-01" stage="not-in-force" year="2009">
