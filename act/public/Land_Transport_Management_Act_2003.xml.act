    <part id="DLM227127" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-10" act.no="118" act.type="public" date.as.at="2007-09-10" date.assent="2003-11-12" stage="reprint" year="2003">
      <label auto.number="no" denominator="yes">2</label>
      <heading xml:lang="en-NZ">Funding of land transport system</heading>
      <subpart id="DLM227128" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" act.no="118" act.type="public" date.as.at="2007-09-10" date.assent="2003-11-12" date.first.valid="2007-09-10" stage="reprint" year="2003">
        <label auto.number="no" denominator="yes">1</label>
        <heading xml:lang="en-NZ">Funding system</heading>
        <crosshead id="DLM227129" toc="yes" xml:lang="en-NZ">National land transport fund</crosshead>
        <prov id="DLM227130" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">8</label>
          <heading xml:lang="en-NZ">National land transport fund</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>All land transport revenue must, as soon as practicable after its receipt by the relevant collecting body, be paid into a Crown Bank Account and be identified as being part of the national land transport fund.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>All roading revenue held in the Crown Bank Account under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="140038" atidlm:branchLabel="_NOVERSION_" atidlm:name="Transit New Zealand Act 1989" atidlm:xmlId="DLM586161" atidlm:state="UNCHANGED"><atidlm:linkcontent>Transit New Zealand Act 1989</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="142074" atidlm:sourceXmlId="DLM586161" atidlm:targetKey="951201" atidlm:targetXmlId="DLM173368" atidlm:targetName="Transit New Zealand Act 1989" atidlm:sourceName="DLM586161" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM173368.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM227128.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[2]/subpart[1]/prov[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="National land transport fund" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="8" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM226229" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="118" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Land Transport Management Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:valu