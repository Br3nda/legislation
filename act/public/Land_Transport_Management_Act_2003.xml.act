          <amend>
            <def-para id="DLM228775" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM228776" xml:lang="en-NZ">toll</def-term> has the same meaning as in <citation jurisdiction="nz">section 5 of the Land Transport Management Act 2003</citation></text>
              </para>
            </def-para>
            <def-para id="DLM228777" xml:lang="en-NZ">
