              <def-para id="DLM226275" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM226276" xml:lang="en-NZ">board</def-term>, in relation to an entity, means the board members of the entity</text>
                </para>
              </def-para>
              <def-para id="DLM226277" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM226278" xml:lang="en-NZ">board member</def-term> means a member of an entity's board</text>
                </para>
              </def-para>
              <def-para id="DLM226279" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM226280" xml:lang="en-NZ">capital project</def-term> —</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>means an individual land transport-related activity of a capital nature; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>includes—</text>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">i</label>
                        <para xml:lang="en-NZ">
                          <text>planning, design, and supervision related to the particular capital project; and</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">ii</label>
                        <para xml:lang="en-NZ">
                          <text>construction and reconstruction; and</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">iii</label>
                        <para xml:lang="en-NZ">
                          <text>any activity of a capital nature the purpose of which is to improve public safety in relation to land transport; and</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">iv</label>
                        <para xml:lang="en-NZ">
                          <text>administration related to the particular capital project</text>
                        </para>
                      </label-para>
                    </para>
                  </label-para>
                </para>
              </def-para>
              <def-para id="DLM226281" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
