                  </history>
                </notes>
              </para>
            </def-para>
            <def-para id="DLM249227" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM249228" xml:lang="en-NZ">Court</def-term> means<struckoutwords/> the <insertwords>High Court</insertwords>, including the additional members</text>
                <notes>
                  <history>
                    <history-note id="DLM249229">Court: this definition was <amending-operation>substituted</amending-operation> by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="287694" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 4" atidlm:xmlId="DLM732298" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="292130" atidlm:sourceXmlId="DLM732298" atidlm:targetKey="2500601" atidlm:targetXmlId="DLM388234" atidlm:targetName="prov 4" atidlm:sourceName="DLM732298" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM388232.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM249212.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[3]/para[1]/notes[1]/history[1]/history-note[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM249212" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="50" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Land Valuation Proceedings Act 1948" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm