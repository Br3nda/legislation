        <prov id="DLM368132" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">17</label>
          <heading xml:lang="en-NZ">Certain matters not affected</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>Nothing in this Act prevents—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the lawful disposal of—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>the practice of the donor; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>the practice of the incorporated firm; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para xml:lang="en-NZ">
                        <text>the shares of the donor in the incorporated firm; or</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>the grant by a lawyer or conveyancing practitioner of a power of attorney otherwise than under this Act.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1982 No 123 <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="327007" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 70" atidlm:xmlId="DLM771551" atidlm:state="UNCHANGED"><atidlm:linkcontent>s 70(11)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="332107" atidlm:sourceXmlId="DLM771551" atidlm:targetKey="360237" atidlm:targetXmlId="DLM63225" atidlm:targetName="prov 70" atidlm:sourceName="DLM771551" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM62888.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM368106.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM368132" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
