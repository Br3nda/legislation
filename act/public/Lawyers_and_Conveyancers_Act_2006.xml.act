      <empowering-prov>
        <citation jurisdiction="nz">
          <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="325693" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 44" atidlm:parentFolderId="/NZ/NZAct/L/Lawyers and Conveyancers Act 2006" atidlm:xmlId="DLM770538" atidlm:state="UNCHANGED">
            <atidlm:linkcontent>s 44</atidlm:linkcontent>
            <atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="331071" atidlm:sourceXmlId="DLM770538" atidlm:targetKey="2208801" atidlm:targetXmlId="DLM365759" atidlm:targetName="prov 44" atidlm:sourceName="DLM770538" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM365724.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM368106.xml" atidlm:state="UNCHANGED">
