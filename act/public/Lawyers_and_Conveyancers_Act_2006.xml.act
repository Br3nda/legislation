          <citation jurisdiction="nz">1982 No 123 <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="320644" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 2" atidlm:xmlId="DLM765398" atidlm:state="UNCHANGED"><atidlm:linkcontent>ss 2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="325744" atidlm:sourceXmlId="DLM765398" atidlm:targetKey="356406" atidlm:targetXmlId="DLM62325" atidlm:targetName="prov 2" atidlm:sourceName="DLM765398" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM62319.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM364944.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM364948" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="320645" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 65" atidlm:xmlId="DLM765399" atidlm:state="UNCHANGED"><atidlm:linkcontent>65(4)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="325745" atidlm:sourceXmlId="DLM765399" atidlm:targetKey="360223" atidlm:targetXmlId="DLM63211" atidlm:targetName="prov 65" atidlm:sourceName="DLM765399" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM62888.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM364944.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM364948" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
