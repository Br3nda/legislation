    <schedule id="DLM368142" in.amend="false" irdnumbering="no" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" xml:lang="en-NZ" act.no="1" act.type="public" date.as.at="2009-07-01" date.assent="2006-03-20" date.first.valid="2008-08-01" stage="reprint" year="2006">
