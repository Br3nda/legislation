    <prov id="DLM303910" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">12</label>
      <heading xml:lang="en-NZ">Long-term council community plan</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
