      <prov id="DLM262158" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">150</label>
        <heading xml:lang="en-NZ">Revocations</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The Local Government (Nelson-Marlborough Region) Reorganisation Order 1989 is hereby amended by revoking—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>Clauses 4 and 5; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>Part II.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The Local Government (Nelson-Marlborough Region) Reorganisation Amendment Order 1990 is hereby amended by revoking clauses 2 to 5.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>The Agricultural Pests Destruction (Nelson-Marlborough Region) Pest Destruction District Order 1990 is hereby revoked.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>The Agricultural Pests Destruction (Pests of Local Importance in Nelson-Marlborough Region) Order 1990 is hereby revoked.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
