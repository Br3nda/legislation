          <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="82609" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 18" atidlm:parentFolderId="/NZ/NZAct/L/Local Government (Rating) Act 2002" atidlm:xmlId="DLM529146" atidlm:state="UNCHANGED">
