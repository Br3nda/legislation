      <prov id="DLM411608" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">25</label>
        <heading xml:lang="en-NZ">Amending the Auckland Harbour Board (Half Moon Bay) Vesting and Empowering Act 1968</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
