<act id="DLM324348" in.amend="false" instructing.office="Fisheries" irdnumbering="no" xml:lang="en-NZ" year.imprint="2009" act.no="107" act.type="public" date.as.at="2008-09-28" date.assent="2004-12-21" date.first.valid="2007-09-18" stage="reprint" year="2004">
