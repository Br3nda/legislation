<?xml version="1.0" encoding="UTF-8"?>
<act id="DLM328958" in.amend="false" instructing.office="Te Puni Kōkiri" irdnumbering="no" xml:lang="en-NZ" year.imprint="1994" act.no="3" act.type="public" date.as.at="1994-06-09" date.assent="1994-06-09" date.first.valid="1991-06-20" stage="in-force" year="1994">
  <cover id="DLM328959">
    <title xml:lang="en-NZ">Maori Purposes (Wi Pere Trust) Act 1994</title>
    <assent>1994-06-09</assent>
    <commencement>see section 1(2)</commencement>
  </cover>
  <contents>
    <heading xml:lang="en-NZ">Contents</heading>
    <toc/>
  </contents>
  <front id="DLM328960" xml:lang="en-NZ">
    <long-title id="DLM328961" toc="yes" xml:lang="en-NZ">
      <para xml:lang="en-NZ">
        <text>An Act to amend the Maori Purposes Act 1991</text>
      </para>
    </long-title>
    <enactment>BE IT ENACTED by the Parliament of New Zealand as follows:</enactment>
  </front>
  <body id="DLM328962" prov-type="section" xml:lang="en-NZ">
    <prov id="DLM328963" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">1</label>
      <heading xml:lang="en-NZ">Short Title and Commencement</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text>This Act may be cited as the Maori Purposes (Wi Pere Trust) Act 1994, and shall be read together with and deemed part of the <citation jurisdiction="nz"><leg-title href="DLM229597">Maori Purposes Act 1991</leg-title></citation> (hereinafter referred to as the principal Act).</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <para xml:lang="en-NZ">
            <text>This Act shall be deemed to have come into force on 20 June 1991 (being the date of commencement of the principal Act).</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM328964" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">2</label>
      <heading xml:lang="en-NZ">Wi Pere Trust</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text><citation jurisdiction="nz"><extref href="DLM229803">Section 2(12)</extref> of the principal Act</citation> is hereby amended by omitting the expression <amend.in><citation jurisdiction="nz">Part 3</citation></amend.in>, and substituting the expression <amend.in><citation jurisdiction="nz">Parts 2 and 3</citation></amend.in>.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
  </body>
  <end id="DLM2105601">
    <admin-office>This Act is administered by <ministry>Te Puni Kōkiri</ministry>.</admin-office>
  </end>
</act>
