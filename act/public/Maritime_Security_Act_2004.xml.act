      <prov id="DLM241887" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">80</label>
        <heading xml:lang="en-NZ">Instructions of chief executive under certain provisions that conflict with other instructions</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
