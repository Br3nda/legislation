<act id="DLM378359" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="39" act.type="public" date.as.at="2007-09-03" date.assent="2006-09-12" date.first.valid="2007-09-18" stage="in-force" year="2006">
  <cover id="DLM378360">
    <title xml:lang="en-NZ">Meat Board Amendment Act 2006</title>
    <assent>2006-09-12</assent>
    <cover.reprint-note>
      <para xml:lang="en-NZ"/>
      <admin-office>This Act is administered by the <ministry>Ministry of Agriculture</ministry> and Forestry.</admin-office>
    </cover.reprint-note>
  </cover>
