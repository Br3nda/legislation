      <prov id="DLM43992" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">16</label>
        <heading xml:lang="en-NZ">Amendments to other enactments</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The enactments specified in the Schedule are amended in the manner indicated in that schedule.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM43993" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">17</label>
        <heading xml:lang="en-NZ">Repeals</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Sections 20 to 26 of the Nurses Amendment Act 1990</citation> are consequentially repealed.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
    </part>
  </body>
