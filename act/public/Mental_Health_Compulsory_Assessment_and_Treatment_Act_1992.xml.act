            <def-para id="DLM262185" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM262186" xml:lang="en-NZ">Clinician</def-term> means a person who holds a professional qualification relevant to the assessment, treatment, and care of patients with mental disorder</text>
              </para>
            </def-para>
