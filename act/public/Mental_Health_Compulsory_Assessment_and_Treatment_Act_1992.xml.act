<act id="DLM262175" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="46" act.type="public" date.as.at="2007-09-03" date.assent="1992-06-15" date.first.valid="2007-09-18" stage="reprint" year="1992">
  <cover id="DLM262176">
    <title xml:lang="en-NZ">Mental Health (Compulsory Assessment and Treatment) Act 1992</title>
    <assent>1992-06-15</assent>
    <cover.reprint-note>
      <para xml:lang="en-NZ"/>
      <admin-office>This Act is administered in the <ministry>Ministry of Health</ministry>.</admin-office>
    </cover.reprint-note>
  </cover>
  <contents>
    <heading xml:lang="en-NZ">Contents</heading>
    <toc/>
  </contents>
  <front id="DLM262177" xml:lang="en-NZ">
    <long-title id="DLM262178" toc="yes" xml:lang="en-NZ">
      <para xml:lang="en-NZ">
        <text>An Act to redefine the circumstances in which and the conditions under which persons may be subjected to compulsory psychiatric assessment and treatment, to define the rights of such persons and to provide better protection for those rights, and generally to reform and consolidate the law relating to the assessment and treatment of persons suffering from mental disorder</text>
      </para>
    </long-title>
    <enactment>BE IT ENACTED by the Parliament of New Zealand as follows:</enactment>
  </front>
  <body id="DLM262179" prov-type="section" xml:lang="en-NZ">
    <prov id="DLM262180" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">1</label>
      <heading xml:lang="en-NZ">Short Title and commencement</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text>This Act may be cited as the Mental Health (Compulsory Assessment and Treatment) Act 1992.</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <para xml:lang="en-NZ">
            <text>This Act shall come into force on the 1st day of November 1992.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
