      <prov id="DLM263852" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">98A</label>
        <heading xml:lang="en-NZ">District inspectors to report monthly</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Every district inspector must, once a month,â€”</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>Prepare a written report on the exercise of his or her powers, duties, and functions under this Act during the preceding month; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>Give the report to the Director.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <notes>
          <history>
