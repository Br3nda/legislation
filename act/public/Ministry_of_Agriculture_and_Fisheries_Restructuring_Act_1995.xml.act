      <prov id="DLM366869" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">22</label>
        <heading xml:lang="en-NZ">Transfer of southern scallop quota</heading>
        <prov.body>
          <subprov deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
          </subprov>
          <subprov deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
