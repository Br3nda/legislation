<?xml version="1.0" encoding="UTF-8"?>
<act id="DLM366813" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="31" act.type="public" date.as.at="2007-09-03" date.assent="1995-06-23" date.first.valid="2007-09-18" stage="reprint" year="1995">
  <cover id="DLM366814">
    <title xml:lang="en-NZ">Ministry of Agriculture and Fisheries (Restructuring) Act 1995</title>
    <assent>1995-06-23</assent>
    <cover.reprint-note>
      <para xml:lang="en-NZ"/>
      <admin-office>This Act is administered in the <ministry>Ministry of Agriculture</ministry> and the Ministry of Fisheries.</admin-office>
    </cover.reprint-note>
  </cover>
  <contents>
    <heading xml:lang="en-NZ">Contents</heading>
    <toc/>
  </contents>
  <front id="DLM366815" xml:lang="en-NZ">
    <long-title id="DLM366816" toc="yes" xml:lang="en-NZ">
      <para xml:lang="en-NZ">
        <text>An Act—</text>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">a</label>
          <para xml:lang="en-NZ">
            <text>To provide for the restructuring of the Ministry of Agriculture and Fisheries; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">b</label>
          <para xml:lang="en-NZ">
            <text>To repeal the <citation jurisdiction="nz">Ministry of Agriculture and Fisheries Act 1953</citation>; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">c</label>
          <para xml:lang="en-NZ">
            <text>To amend certain related enactments; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">d</label>
          <para xml:lang="en-NZ">
            <text>To provide for related matters</text>
          </para>
        </label-para>
      </para>
    </long-title>
    <enactment>BE IT ENACTED by the Parliament of New Zealand as follows:</enactment>
  </front>
  <body id="DLM366817" prov-type="section" xml:lang="en-NZ">
    <prov id="DLM366818" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">1</label>
      <heading xml:lang="en-NZ">Short Title and commencement</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text>This Act may be cited as the Ministry of Agriculture and Fisheries (Restructuring) Act 1995.</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <para xml:lang="en-NZ">
            <text>This Act shall come into force on the 1st day of July 1995.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM366819" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">2</label>
      <heading xml:lang="en-NZ">Interpretation</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text>In this Act, unless the context otherwise requires,—</text>
            <def-para id="DLM366820" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM366821" xml:lang="en-NZ">Agency</def-term> means the Ministry of Agriculture, the Ministry of Fisheries, or any Crown Research Institute</text>
              </para>
            </def-para>
            <def-para id="DLM366822" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM366823" xml:lang="en-NZ">Intellectual property</def-term>—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>Includes—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>All property rights constituted by the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="450580" atidlm:branchLabel="_NOVERSION_" atidlm:name="Patents Act 1953" atidlm:xmlId="DLM894802" atidlm:state="UNCHANGED"><atidlm:linkcontent>Patents Act 1953</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="458505" atidlm:sourceXmlId="DLM894802" atidlm:targetKey="1858855" atidlm:targetXmlId="DLM280030" atidlm:targetName="Patents Act 1953" atidlm:sourceName="DLM894802" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM280030.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM366813.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Intellectual property" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM366813" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="31" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ministry of Agriculture and Fisheries (Restructuring) Act 1995" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1995" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="450581" atidlm:branchLabel="_NOVERSION_" atidlm:name="Designs Act 1953" atidlm:xmlId="DLM894803" atidlm:state="UNCHANGED"><atidlm:linkcontent>Designs Act 1953</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="458506" atidlm:sourceXmlId="DLM894803" atidlm:targetKey="1865837" atidlm:targetXmlId="DLM281070" atidlm:targetName="Designs Act 1953" atidlm:sourceName="DLM894803" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM281070.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM366813.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Intellectual property" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM366813" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="31" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ministry of Agriculture and Fisheries (Restructuring) Act 1995" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1995" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, or the <citation jurisdiction="nz">Trade Marks Act 1953</citation>; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>Any trade secret; but</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>Does not include any property rights constituted by the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="450582" atidlm:branchLabel="_NOVERSION_" atidlm:name="Copyright Act 1994" atidlm:xmlId="DLM894804" atidlm:state="UNCHANGED"><atidlm:linkcontent>Copyright Act 1994</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="458507" atidlm:sourceXmlId="DLM894804" atidlm:targetKey="1805385" atidlm:targetXmlId="DLM345633" atidlm:targetName="Copyright Act 1994" atidlm:sourceName="DLM894804" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM345633.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM366813.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Intellectual property" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM366813" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="31" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ministry of Agriculture and Fisheries (Restructuring) Act 1995" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1995" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                  </para>
                </label-para>
              </para>
            </def-para>
            <def-para id="DLM366824" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM366825" xml:lang="en-NZ">Responsible chief executive</def-term>, in relation to any function or matter, means, subject to any enactment, the chief executive for the time being of a responsible Ministry who has, with the authority of the Prime Minister, assumed responsibility for that function or matter</text>
              </para>
            </def-para>
            <def-para id="DLM366826" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM366827" xml:lang="en-NZ">Responsible Minister</def-term>, in relation to any function or matter, means, subject to any enactment, any Minister of the Crown who, under the authority of any warrant or with the authority of the Prime Minister, is for the time being responsible for that function or matter</text>
              </para>
            </def-para>
            <def-para id="DLM366828" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM366829" xml:lang="en-NZ">Responsible Ministry</def-term>, in relation to any function or matter, means, subject to any enactment, the Ministry that has, with the authority of the Prime Minister, for the time being assumed responsibility for that function or matter</text>
              </para>
            </def-para>
            <def-para id="DLM366830" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM366831" xml:lang="en-NZ">Working day</def-term> means any day except—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>A Saturday, a Sunday, Good Friday, Easter Monday, Anzac Day, Labour Day, the Sovereign's birthday, and Waitangi Day; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>A day in the period commencing with the 20th day of December in any year and ending with the 15th day of January in the following year.</text>
                  </para>
                </label-para>
              </para>
            </def-para>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <part id="DLM366832" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-03" act.no="31" act.type="public" date.as.at="2007-09-03" date.assent="1995-06-23" stage="reprint" year="1995">
      <label auto.number="no" denominator="yes">1</label>
      <heading xml:lang="en-NZ">Restructuring of Ministry of Agriculture and Fisheries</heading>
      <crosshead id="DLM366833" toc="yes" xml:lang="en-NZ">Repeal of Ministry of Agriculture and Fisheries Act 1953</crosshead>
      <prov id="DLM366834" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">3</label>
        <heading xml:lang="en-NZ">Abolition of Ministry of Agriculture and Fisheries</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The Ministry of Agriculture and Fisheries constituted under the <citation jurisdiction="nz">Ministry of Agriculture and Fisheries Act 1953</citation> is hereby abolished.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM366835" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">4</label>
        <heading xml:lang="en-NZ">Repeals</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The following enactments are hereby consequentially repealed:</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>The <citation jurisdiction="nz">Ministry of Agriculture and Fisheries Act 1953</citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>The <citation jurisdiction="nz">Ministry of Agriculture and Fisheries Amendment Act 1972</citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>So much of <citation jurisdiction="nz">Schedule <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="450583" atidlm:branchLabel="_NOVERSION_" atidlm:name="schedule 3 Acts amended" atidlm:xmlId="DLM894805" atidlm:state="UNCHANGED"><atidlm:linkcontent>3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="458508" atidlm:sourceXmlId="DLM894805" atidlm:targetKey="567000" atidlm:targetXmlId="DLM101358" atidlm:targetName="schedule 3 Acts amended" atidlm:sourceName="DLM894805" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM101358.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM366832.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Repeals" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="4" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM366813" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="31" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ministry of Agriculture and Fisheries (Restructuring) Act 1995" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1995" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> to the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="450584" atidlm:branchLabel="_NOVERSION_" atidlm:name="Official Information Amendment Act 1987" atidlm:xmlId="DLM894806" atidlm:state="UNCHANGED"><atidlm:linkcontent>Official Information Amendment Act 1987</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="458509" atidlm:sourceXmlId="DLM894806" atidlm:targetKey="566000" atidlm:targetXmlId="DLM101320" atidlm:targetName="Official Information Amendment Act 1987" atidlm:sourceName="DLM894806" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM101320.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM366832.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Repeals" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="4" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM366813" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="31" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ministry of Agriculture and Fisheries (Restructuring) Act 1995" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1995" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> as relates to the <citation jurisdiction="nz">Ministry of Agriculture and Fisheries Act 1953</citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>The <citation jurisdiction="nz">Ministry of Agriculture and Fisheries Amendment Act 1989</citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">e</label>
                <para xml:lang="en-NZ">
                  <text><citation jurisdiction="nz">Section 12(5) of the Meat Export Control Amendment Act 1989</citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
