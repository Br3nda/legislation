      </prov>
      <prov id="DLM356254" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">15</label>
        <heading xml:lang="en-NZ">Powers of Minister to prohibit prescribing, etc</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
