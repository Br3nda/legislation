                <def-para id="DLM188139" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM188140" xml:lang="en-NZ">Customs</def-term> means the New Zealand Customs Service</text>
                  </para>
                </def-para>
                <def-para id="DLM188141" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM188142" xml:lang="en-NZ">Customs information</def-term> —</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">a</label>
                      <para xml:lang="en-NZ">
                        <text>means information about any person whom the Comptroller of Customs believes has imported more than 3 motor vehicles during a specified period; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">b</label>
                      <para xml:lang="en-NZ">
                        <text>may include,—</text>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">i</label>
                          <para xml:lang="en-NZ">
                            <text>in the case of an individual, the person's full name, residential address, date of birth, and occupation:</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">ii</label>
                          <para xml:lang="en-NZ">
                            <text>in the case of a company, the company's name and the address of the company's registered office:</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">iii</label>
                          <para xml:lang="en-NZ">
                            <text>whether the person indicated the purpose for importing the motor vehicles:</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">iv</label>
                          <para xml:lang="en-NZ">
                            <text>any other particulars that the Comptroller of Customs considers relevant to the purpose set out in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118300" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 120" atidlm:xmlId="DLM564474" atidlm:state="UNCHANGED"><atidlm:linkcontent>120</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="119969" atidlm:sourceXmlId="DLM564474" atidlm:targetKey="1088801" atidlm:targetXmlId="DLM188912" atidlm:targetName="prov 120" atidlm:sourceName="DLM564474" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188114.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/subpart[2]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[13]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Customs information" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislati