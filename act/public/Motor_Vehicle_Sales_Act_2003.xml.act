    <part id="DLM188544" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-18" act.no="12" act.type="public" date.as.at="2007-12-03" date.assent="2003-04-09" stage="reprint" year="2003">
      <label auto.number="no" denominator="yes">2</label>
      <heading xml:lang="en-NZ">Motor vehicle trading</heading>
      <subpart id="DLM188545" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" act.no="12" act.type="public" date.as.at="2007-12-03" date.assent="2003-04-09" date.first.valid="2007-09-18" stage="reprint" year="2003">
        <label auto.number="no" denominator="yes">1</label>
        <heading xml:lang="en-NZ">Key provisions</heading>
        <crosshead id="DLM188546" toc="yes" xml:lang="en-NZ">Registration requirement</crosshead>
        <prov id="DLM188547" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">10</label>
          <heading xml:lang="en-NZ">Motor vehicle traders must be registered</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A person must not, unless the person is registered as a motor vehicle trader under this Act,—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>carry on the business of motor vehicle trading; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>hold out that the person is a motor vehicle trader.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>If 2 or more persons carry on the business of motor vehicle trading in partnership, or otherwise in conjunction with one another, each of those persons must be registered.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <crosshead id="DLM188548" toc="yes" xml:lang="en-NZ">Banned persons</crosshead>
        <prov id="DLM188549" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">11</label>
          <heading xml:lang="en-NZ">Persons banned from participating in business of motor vehicle trading</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A person must not participate in the business of motor vehicle trading if that person is a banned person.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188550" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">12</label>
          <heading xml:lang="en-NZ">Banned persons not to be employed or engaged in business of motor vehicle trading</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A person must not employ or engage a banned person in any capacity that involves participation in the business of motor vehicle trading.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <crosshead id="DLM188551" toc="yes" xml:lang="en-NZ">Tampering with odometers</crosshead>
        <prov id="DLM188552" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">13</label>
          <heading xml:lang="en-NZ">Tampering with odometers prohibited</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A person must not tamper with a motor vehicle's odometer.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <crosshead id="DLM188553" toc="yes" xml:lang="en-NZ">Disclosure requirements</crosshead>
        <prov id="DLM188554" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">14</label>
          <heading xml:lang="en-NZ">Particulars about used motor vehicle must be displayed</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A motor vehicle trader must not offer or display for sale, or cause or permit to be offered or displayed for sale, a used motor vehicle unless there is attached to that motor vehicle in a prominent position a notice containing the particulars set out in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118360" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 15" atidlm:xmlId="DLM564634" atidlm:state="UNCHANGED"><atidlm:linkcontent>15(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120070" atidlm:sourceXmlId="DLM564634" atidlm:targetKey="1082810" atidlm:targetXmlId="DLM188555" atidlm:targetName="prov 15" atidlm:sourceName="DLM564634" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188545.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188545.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[2]/subpart[1]/prov[5]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Particulars about used motor vehicle must be displayed" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="14" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>A motor vehicle trader who is a car market operator must take reasonable steps to ensure that a consumer selling a used motor vehicle through the motor vehicle trader attaches to the motor vehicle in a prominent position the notice referred to in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118361" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 14" atidlm:xmlId="DLM564635" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120071" atidlm:sourceXmlId="DLM564635" atidlm:targetKey="1082808" atidlm:targetXmlId="DLM188554" atidlm:targetName="prov 14" atidlm:sourceName="DLM564635" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188545.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188545.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[2]/subpart[1]/prov[5]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Particulars about used motor vehicle must be displayed" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="14" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188555" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">15</label>
          <heading xml:lang="en-NZ">Particulars (including statement about buyer taking motor vehicle free of security interests) that must be contained in notice attached to used motor vehicle</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>All of the following particulars must be contained in every notice that, in accordance with <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118362" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 14" atidlm:xmlId="DLM564636" atidlm:state="UNCHANGED"><atidlm:linkcontent>14(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120072" atidlm:sourceXmlId="DLM564636" atidlm:targetKey="1082808" atidlm:targetXmlId="DLM188554" atidlm:targetName="prov 14" atidlm:sourceName="DLM564636" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188545.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188545.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[2]/subpart[1]/prov[6]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Particulars (including statement about buyer taking motor vehicle free of security interests) that must be contained in notice attached to used motor vehicle" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="15" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, must be attached to a used motor vehicle:</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
