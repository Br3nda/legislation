        <prov id="DLM188909" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">119</label>
          <heading xml:lang="en-NZ">Power of District Court to impose additional penalty in certain cases</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>On the conviction of any person of an offence against <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118800" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 95" atidlm:xmlId="DLM564972" atidlm:state="UNCHANGED"><atidlm:linkcontent>95(1)(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120514" atidlm:sourceXmlId="DLM564972" atidlm:targetKey="1088408" atidlm:targetXmlId="DLM188875" atidlm:targetName="prov 95" atidlm:sourceName="DLM564972" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188873.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188873.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[3]/prov[25]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Power of District Court to impose additional penalty in certain cases" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="119" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118801" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 95" atidlm:xmlId="DLM564973" atidlm:state="UNCHANGED"><atidlm:linkcontent>(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120515" atidlm:sourceXmlId="DLM564973" atidlm:targetKey="1088408" atidlm:targetXmlId="DLM188875" atidlm:targetName="prov 95" atidlm:sourceName="DLM564973" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188873.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188873.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[3]/prov[25]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Power of District Court to impose additional penalty in certain cases" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="119" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the District Court in which the conviction is entered may, in addition to any penalty that may be imposed under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118802" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 118" atidlm:xmlId="DLM564974" atidlm:state="UNCHANGED"><atidlm:linkcontent>118</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120516" atidlm:sourceXmlId="DLM564974" atidlm:targetKey="1088402" atidlm:targetXmlId="DLM188908" atidlm:targetName="prov 118" atidlm:sourceName="DLM564974" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188873.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188873.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[3]/prov[25]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Power of District Court to impose additional penalty in certain cases" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="119" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> for the offence, order that any money received by that person as commission or profit for any services or work performed as an unregistered motor vehicle trader be forfeited to the Crown.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>All money forfeited to the Crown is recoverable by the Crown in any court of competent jurisdiction, or may be assessed by the District Court in which the defendant is convicted and be recoverable in like manner as a fine.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
      </subpart>
      <subpart id="DLM188910" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" act.no="12" act.type="public" date.as.at="2007-12-03" date.assent="2003-04-09" date.first.valid="2007-09-18" stage="reprint" year="2003">
        <label auto.number="no" denominator="yes">4</label>
        <heading xml:lang="en-NZ">Information-gathering powers</heading>
        <crosshead id="DLM188911" toc="yes" xml:lang="en-NZ">Information-matching provisions</crosshead>
        <prov id="DLM188912" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">120</label>
          <heading xml:lang="en-NZ">Purpose of disclosure of Customs' information</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The purpose of <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118803" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 121" atidlm:xmlId="DLM564975" atidlm:state="UNCHANGED"><atidlm:linkcontent>121</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120517" atidlm:sourceXmlId="DLM564975" atidlm:targetKey="1088802" atidlm:targetXmlId="DLM188913" atidlm:targetName="prov 121" atidlm:sourceName="DLM564975" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Purpose of disclosure of Customs' information" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="120" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is to facilitate the disclosure of information from Customs to the Registrar for the purposes of—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>identifying any person who imports more than 3 motor vehicles during a specified period; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>determining whether or not that person should be registered as a motor vehicle trader under this Act.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188913" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">121</label>
          <heading xml:lang="en-NZ">Customs must supply Customs' information to Registrar</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The Comptroller of Customs, or any authorised officer of Customs, must supply to the Registrar any Customs' information for the time being held by Customs at the time and at the intervals agreed to between the Comptroller and the Registrar.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>On receiving information from Customs under this section, the Registrar may cause a comparison of that information to be made with the information held in the register.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188914" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">122</label>
          <heading xml:lang="en-NZ">Purpose of disclosure of information by Ministry of Transport</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The purpose of <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118804" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 123" atidlm:xmlId="DLM564976" atidlm:state="UNCHANGED"><atidlm:linkcontent>123</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120518" atidlm:sourceXmlId="DLM564976" atidlm:targetKey="1088804" atidlm:targetXmlId="DLM188915" atidlm:targetName="prov 123" atidlm:sourceName="DLM564976" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[3]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Purpose of disclosure of information by Ministry of Transport" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="122" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is to facilitate the disclosure of information from the Ministry of Transport to the Registrar for the purposes of—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>identifying any person who sells more than 6 motor vehicles during a specified period; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>determining whether or not that person is, or should be, a banned person or registered as a motor vehicle trader under this Act.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188915" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">123</label>
          <heading xml:lang="en-NZ">Ministry of Transport must supply vehicle registration information to Registrar</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The Secretary for Transport, or any person authorised by the Secretary, must supply to the Registrar any vehicle registration information for the time being held under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118805" atidlm:branchLabel="_NOVERSION_" atidlm:name="Transport (Vehicle and Driver Registration and Licensing) Act 1986" atidlm:xmlId="DLM564977" atidlm:state="UNCHANGED"><atidlm:linkcontent>Transport (Vehicle and Driver Registration and Licensing) Act 1986</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="120520" atidlm:sourceXmlId="DLM564977" atidlm:targetKey="500000" atidlm:targetXmlId="DLM90414" atidlm:targetName="Transport (Vehicle and Driver Registration and Licensing) Act 1986" atidlm:sourceName="DLM564977" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM90414.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[4]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Ministry of Transport must supply vehicle registration information to Registrar" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="123" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> at the time and at the intervals agreed to between the Secretary and the Registrar.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>On receiving information from the Ministry of Transport under this section, the Registrar may cause a comparison of that information to be made with the information held in the register.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <crosshead id="DLM188916" toc="yes" xml:lang="en-NZ">Inspection</crosshead>
        <prov id="DLM188917" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">124</label>
          <heading xml:lang="en-NZ">Purpose of inspection by Registrar</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The powers in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118806" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 125" atidlm:xmlId="DLM564978" atidlm:state="UNCHANGED"><atidlm:linkcontent>125</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120521" atidlm:sourceXmlId="DLM564978" atidlm:targetKey="1088807" atidlm:targetXmlId="DLM188918" atidlm:targetName="prov 125" atidlm:sourceName="DLM564978" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[5]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Purpose of inspection by Registrar" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="124" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> may be used if, in the Registrar's opinion, it is necessary for the purpose of—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>determining whether a person is complying, or has complied, with this Act; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>detecting contraventions of this Act.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188918" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">125</label>
          <heading xml:lang="en-NZ">Inspection powers</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The Registrar, or any person authorised by the Registrar, may—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>require a person to produce for inspection relevant documents within that person's possession or control; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>inspect and take copies of relevant documents; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>take possession of relevant documents and remove them from the place where they are kept and retain them for a reasonable time, for the purpose of taking copies; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">d</label>
                  <para xml:lang="en-NZ">
                    <text>retain relevant documents for a period that is in all the circumstances reasonable if there are reasonable grounds for believing that they are evidence of the commission of an offence.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188919" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">126</label>
          <heading xml:lang="en-NZ">Conditions of authorisation</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The Registrar must not authorise a person to carry out an inspection unless the Registrar is satisfied that the person is suitably qualified or trained, or the person is a member of a class of persons who are suitably qualified or trained, to carry out an inspection.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>A person authorised by the Registrar to carry out an inspection must, on entering any premises, and when requested at any subsequent time, produce—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the authorisation referred to in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118807" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 126" atidlm:xmlId="DLM564979" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120522" atidlm:sourceXmlId="DLM564979" atidlm:targetKey="1088808" atidlm:targetXmlId="DLM188919" atidlm:targetName="prov 126" atidlm:sourceName="DLM564979" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[7]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Conditions of authorisation" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="126" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>evidence of that person's identity.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>An authorisation must be in writing and must contain—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>a reference to this section; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>the full name of the authorised person; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>a statement of the powers conferred on the authorised person by <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118808" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 125" atidlm:xmlId="DLM564980" atidlm:state="UNCHANGED"><atidlm:linkcontent>125</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120523" atidlm:sourceXmlId="DLM564980" atidlm:targetKey="1088807" atidlm:targetXmlId="DLM188918" atidlm:targetName="prov 125" atidlm:sourceName="DLM564980" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[7]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Conditions of authorisation" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="126" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">d</label>
                  <para xml:lang="en-NZ">
                    <text>the purpose for which those powers may be exercised.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188920" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">127</label>
          <heading xml:lang="en-NZ">Registrar's entry to premises restricted</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The Registrar, or any person authorised by the Registrar, may not enter any place except in the circumstances set out in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118809" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 127" atidlm:xmlId="DLM564981" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120524" atidlm:sourceXmlId="DLM564981" atidlm:targetKey="1088809" atidlm:targetXmlId="DLM188920" atidlm:targetName="prov 127" atidlm:sourceName="DLM564981" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[8]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Registrar's entry to premises restricted" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="127" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The circumstances are—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the occupier of the place consents to the entry; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>the place is a public place and the entry is made when it is open to the public; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>the place is a motor vehicle trader's place of business and it is—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>open for carrying on the business; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>otherwise open to the public; or</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">d</label>
                  <para xml:lang="en-NZ">
                    <text>the entry is authorised by a search warrant issued under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118810" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 130" atidlm:xmlId="DLM564982" atidlm:state="UNCHANGED"><atidlm:linkcontent>130</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120525" atidlm:sourceXmlId="DLM564982" atidlm:targetKey="1088813" atidlm:targetXmlId="DLM188924" atidlm:targetName="prov 130" atidlm:sourceName="DLM564982" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[8]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Registrar's entry to premises restricted" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="127" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188921" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">128</label>
          <heading xml:lang="en-NZ">Disclosure of information from inspection</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>On the direction of the Registrar, a person authorised to make an inspection under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118811" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 125" atidlm:xmlId="DLM564983" atidlm:state="UNCHANGED"><atidlm:linkcontent>125</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120526" atidlm:sourceXmlId="DLM564983" atidlm:targetKey="1088807" atidlm:targetXmlId="DLM188918" atidlm:targetName="prov 125" atidlm:sourceName="DLM564983" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[9]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Disclosure of information from inspection" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="128" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must give all records and disclose all information acquired in the course of the inspection to—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the Registrar; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>any person specified by the Registrar for the purposes of—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>exercising powers conferred by this Act; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>detecting and prosecuting offences against any other Act (but those records and information are not admissible in any criminal proceedings against the person from whom the records or information were acquired or any person to whom they relate); or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para xml:lang="en-NZ">
                        <text>a liquidator for the purposes of liquidation of a company; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iv</label>
                      <para xml:lang="en-NZ">
                        <text>the Official Assignee for the purposes of a bankruptcy.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188922" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">129</label>
          <heading xml:lang="en-NZ">Restriction on disclosure of information from inspection</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A person must not communicate to any person any information acquired in the course of an inspection except—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>in accordance with <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118812" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 128" atidlm:xmlId="DLM564984" atidlm:state="UNCHANGED"><atidlm:linkcontent>128</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120527" atidlm:sourceXmlId="DLM564984" atidlm:targetKey="1088810" atidlm:targetXmlId="DLM188921" atidlm:targetName="prov 128" atidlm:sourceName="DLM564984" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[10]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Restriction on disclosure of information from inspection" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="129" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>for the purposes of this Act; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>in accordance with the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118813" atidlm:branchLabel="_NOVERSION_" atidlm:name="Official Information Act 1982" atidlm:xmlId="DLM564985" atidlm:state="UNCHANGED"><atidlm:linkcontent>Official Information Act 1982</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="120528" atidlm:sourceXmlId="DLM564985" atidlm:targetKey="371800" atidlm:targetXmlId="DLM64784" atidlm:targetName="Official Information Act 1982" atidlm:sourceName="DLM564985" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64784.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[10]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Restriction on disclosure of information from inspection" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="129" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> or the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118814" atidlm:branchLabel="_NOVERSION_" atidlm:name="Privacy Act 1993" atidlm:xmlId="DLM564986" atidlm:state="UNCHANGED"><atidlm:linkcontent>Privacy Act 1993</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="120529" atidlm:sourceXmlId="DLM564986" atidlm:targetKey="1539005" atidlm:targetXmlId="DLM296638" atidlm:targetName="Privacy Act 1993" atidlm:sourceName="DLM564986" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM296638.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[10]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Restriction on disclosure of information from inspection" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="129" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">d</label>
                  <para xml:lang="en-NZ">
                    <text>for the purposes of any court proceedings; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">e</label>
                  <para xml:lang="en-NZ">
                    <text>for any purposes that may be specified in any other Act.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <crosshead id="DLM188923" toc="yes" xml:lang="en-NZ">Search warrants</crosshead>
        <prov id="DLM188924" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">130</label>
          <heading xml:lang="en-NZ">Issue of warrant</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>Any District Court Judge, Community Magistrate, Justice of the Peace, or Registrar of a District Court may issue a search warrant for any place if satisfied, on application in writing made on oath, that there are reasonable grounds for believing that there is at that place any thing—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>in respect of which an offence under this Act has been or is being committed; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>that has been, is being, or is intended to be used by any person for the commission of an offence under this Act; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>that is or may be evidence of the commission of an offence under this Act by any person.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The Judge, Community Magistrate, Justice, or Registrar may impose any reasonable conditions on the exercise of the warrant that he or she thinks fit.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188925" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">131</label>
          <heading xml:lang="en-NZ">Powers conferred by warrant</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A warrant authorises the person named in it—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>to enter and search the place specified in the warrant on 1 occasion within 20 working days of the date of issue of the warrant at a time that is reasonable in the circumstances:</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>to use any assistance that is reasonable in the circumstances:</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>to use any force for gaining entry and for breaking open any article or thing that is reasonable in the circumstances:</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">d</label>
                  <para xml:lang="en-NZ">
                    <text>to search for and remove documents or any article or thing that the person executing the warrant believes on reasonable grounds may be relevant:</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">e</label>
                  <para xml:lang="en-NZ">
                    <text>when necessary, to take copies of documents, or extracts from documents, that the person executing the warrant believes on reasonable grounds may be relevant:</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">f</label>
                  <para xml:lang="en-NZ">
                    <text>if necessary, to require a person to reproduce, or assist any person executing the warrant to reproduce in usable form, information recorded or stored in a document.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>A person assisting the person executing the warrant also has the powers referred to in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118815" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 131" atidlm:xmlId="DLM564987" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)(c) to (e)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120530" atidlm:sourceXmlId="DLM564987" atidlm:targetKey="1088814" atidlm:targetXmlId="DLM188925" atidlm:targetName="prov 131" atidlm:sourceName="DLM564987" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[12]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Powers conferred by warrant" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="131" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>The warrant must be executed in accordance with any reasonable conditions that may be specified in the warrant when it is issued.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188926" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">132</label>
          <heading xml:lang="en-NZ">Form and content of search warrant</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A search warrant must—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>be in the prescribed form; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>be directed to—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>a member of the police by name; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>any member of the police; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para xml:lang="en-NZ">
                        <text>the Registrar; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iv</label>
                      <para xml:lang="en-NZ">
                        <text>a person authorised by the Registrar under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118816" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 125" atidlm:xmlId="DLM564988" atidlm:state="UNCHANGED"><atidlm:linkcontent>125</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120531" atidlm:sourceXmlId="DLM564988" atidlm:targetKey="1088807" atidlm:targetXmlId="DLM188918" atidlm:targetName="prov 125" atidlm:sourceName="DLM564988" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[13]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Form and content of search warrant" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="132" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>contain all of the following particulars:</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>the place or thing that may be searched in accordance with the warrant; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>the offence for which the warrant is issued; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para xml:lang="en-NZ">
                        <text>a description of the kind of property that is authorised to be seized; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iv</label>
                      <para xml:lang="en-NZ">
                        <text>the period during which the warrant may be executed; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">v</label>
                      <para xml:lang="en-NZ">
                        <text>any conditions specified under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118817" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 130" atidlm:xmlId="DLM564989" atidlm:state="UNCHANGED"><atidlm:linkcontent>130(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120532" atidlm:sourceXmlId="DLM564989" atidlm:targetKey="1088813" atidlm:targetXmlId="DLM188924" atidlm:targetName="prov 130" atidlm:sourceName="DLM564989" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[13]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Form and content of search warrant" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="132" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188927" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">133</label>
          <heading xml:lang="en-NZ">Warrant must be produced</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A person executing a warrant must—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>have the warrant with him or her; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>produce it on initial entry and, if requested, at any subsequent time; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>identify himself or herself to the owner or occupier or person in charge of the place if that person is present; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">d</label>
                  <para xml:lang="en-NZ">
                    <text>produce evidence of his or her identity.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188928" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">134</label>
          <heading xml:lang="en-NZ">Other duties of person who executes warrant</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A person who executes a warrant must, before completing the search, leave in a prominent place at the place searched,—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>in the case of a search carried out at a time when the owner or occupier was not present, a written notice stating—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>the date and time when the warrant was executed; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>the name of the person who executed the warrant; and</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>in the case of a search where a document or article or thing was seized from the place being searched, a list of documents or articles or things that were seized during the search.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188929" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">135</label>
          <heading xml:lang="en-NZ">When alternative to list of seized documents may be provided</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>If it is not practicable to prepare a list under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118818" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 134" atidlm:xmlId="DLM564990" atidlm:state="UNCHANGED"><atidlm:linkcontent>134</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120533" atidlm:sourceXmlId="DLM564990" atidlm:targetKey="1088817" atidlm:targetXmlId="DLM188928" atidlm:targetName="prov 134" atidlm:sourceName="DLM564990" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[16]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="When alternative to list of seized documents may be provided" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="135" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> before completing the search, or if the owner or occupier of the place being searched consents, the person executing the warrant—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>may, instead of leaving a list, leave a notice stating that documents or articles or things have been seized during the search and that, within 5 working days of the search, a list will be delivered, left, or sent stating what documents, articles, or things have been seized; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>must, within 5 working days of the search,—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">i</label>
                      <para xml:lang="en-NZ">
                        <text>deliver a list to the owner or occupier; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">ii</label>
                      <para xml:lang="en-NZ">
                        <text>leave a list in a prominent position at the place searched; or</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">iii</label>
                      <para xml:lang="en-NZ">
                        <text>send a list by mail to the owner or occupier of the place searched.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188930" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">136</label>
          <heading xml:lang="en-NZ">Matters that must be stated in list of documents</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A list under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118819" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 134" atidlm:xmlId="DLM564991" atidlm:state="UNCHANGED"><atidlm:linkcontent>134</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120534" atidlm:sourceXmlId="DLM564991" atidlm:targetKey="1088817" atidlm:targetXmlId="DLM188928" atidlm:targetName="prov 134" atidlm:sourceName="DLM564991" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[17]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Matters that must be stated in list of documents" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="136" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must state—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>the documents, articles, and things that have been seized; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>the location from where they were seized; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>the location where they are being held.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188931" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">137</label>
          <heading xml:lang="en-NZ">Duty to assist</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The occupier or person in charge of the place that a person authorised by a warrant enters for the purpose of searching must provide that person with all reasonable facilities and assistance in executing the warrant.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188932" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">138</label>
          <heading xml:lang="en-NZ">Power to inspect and take copies of documents, etc, obtained under warrant</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The Registrar, or any person authorised by the Registrar for the purpose, may inspect and take copies of any documents or extracts from them obtained under a warrant.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188933" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">139</label>
          <heading xml:lang="en-NZ">Disposal of things seized</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>In any proceedings for an offence relating to any thing seized under warrant, the court may order, either at the trial or hearing or on an application, that the thing be delivered to the person appearing to the court to be entitled to it, or that it be otherwise disposed of in any manner that the court thinks fit.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>Any member of the police or the Registrar may, at any time, unless an order has been made under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118820" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 139" atidlm:xmlId="DLM564992" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120535" atidlm:sourceXmlId="DLM564992" atidlm:targetKey="1088822" atidlm:targetXmlId="DLM188933" atidlm:targetName="prov 139" atidlm:sourceName="DLM564992" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[20]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Disposal of things seized" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="139" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, return the thing to the person from whom it was seized, or apply to a District Court Judge for an order for its disposal.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>On any such application, the District Court Judge may make any order that a court may make under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118821" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 139" atidlm:xmlId="DLM564993" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120536" atidlm:sourceXmlId="DLM564993" atidlm:targetKey="1088822" atidlm:targetXmlId="DLM188933" atidlm:targetName="prov 139" atidlm:sourceName="DLM564993" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[20]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Disposal of things seized" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="139" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">4</label>
              <para xml:lang="en-NZ">
                <text>If proceedings for an offence relating to the thing are not brought within a period of 3 months of seizure, any person claiming to be entitled to the thing may, after the expiry of that period, apply to a District Court Judge for an order that it be delivered to him or her.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">5</label>
              <para xml:lang="en-NZ">
                <text>On any such application, the District Court Judge may—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>adjourn the application, on any terms that he or she thinks fit, for proceedings to be brought; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>make any order that a court may make under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118822" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 139" atidlm:xmlId="DLM564994" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120537" atidlm:sourceXmlId="DLM564994" atidlm:targetKey="1088822" atidlm:targetXmlId="DLM188933" atidlm:targetName="prov 139" atidlm:sourceName="DLM564994" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[20]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Disposal of things seized" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="139" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM188934" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">140</label>
          <heading xml:lang="en-NZ">Court order for disposal of things seized to be suspended on conviction</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>If any person is convicted in any proceedings for an offence relating to anything for which a warrant has been issued, and any order is made under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118823" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 139" atidlm:xmlId="DLM564995" atidlm:state="UNCHANGED"><atidlm:linkcontent>139</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120538" atidlm:sourceXmlId="DLM564995" atidlm:targetKey="1088822" atidlm:targetXmlId="DLM188933" atidlm:targetName="prov 139" atidlm:sourceName="DLM564995" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[21]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Court order for disposal of things seized to be suspended on conviction" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="140" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the operation of the order is suspended,—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>in any case, until the expiration of the time prescribed by the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118824" atidlm:branchLabel="_NOVERSION_" atidlm:name="Summary Proceedings Act 1957" atidlm:xmlId="DLM564996" atidlm:state="UNCHANGED"><atidlm:linkcontent>Summary Proceedings Act 1957</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="120539" atidlm:sourceXmlId="DLM564996" atidlm:targetKey="2034230" atidlm:targetXmlId="DLM310742" atidlm:targetName="Summary Proceedings Act 1957" atidlm:sourceName="DLM564996" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM310742.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188910.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[4]/prov[21]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Court order for disposal of things seized to be suspended on conviction" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="140" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> for the filing of a notice of appeal or an application for leave to appeal; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>if a notice of appeal is filed within the time so prescribed, until the determination of the appeal; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>if application for leave to appeal is filed within the time so prescribed, until the application is determined, and, if leave to appeal is granted, until the determination of the appeal.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>If the operation of any such order is suspended until the determination of the appeal, the Court determining the appeal may, by order, cancel or vary the order.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
      </subpart>
    </part>
    <part id="DLM188935" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-18" act.no="12" act.type="public" date.as.at="2007-12-03" date.assent="2003-04-09" stage="reprint" year="2003">
      <label auto.number="no" denominator="yes">5</label>
      <heading xml:lang="en-NZ">Miscellaneous provisions</heading>
      <crosshead id="DLM188936" toc="yes" xml:lang="en-NZ">Exemption for car auctioneers from licensing requirement under Auctioneers Act 1928</crosshead>
      <prov id="DLM188937" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">141</label>
        <heading xml:lang="en-NZ">Exemption for car auctioneers from licensing requirement under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118825" atidlm:branchLabel="_NOVERSION_" atidlm:name="Auctioneers Act 1928" atidlm:xmlId="DLM564997" atidlm:state="UNCHANGED"><atidlm:linkcontent>Auctioneers Act 1928</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="120540" atidlm:sourceXmlId="DLM564997" atidlm:targetKey="1451825" atidlm:targetXmlId="DLM205305" atidlm:targetName="Auctioneers Act 1928" atidlm:sourceName="DLM564997" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM205305.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Exemption for car auctioneers from licensing requirement under Auctioneers Act 1928" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="141" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>If a car auctioneer who is registered as a motor vehicle trader under this Act carries on a business that is limited to conducting sales by auction of motor vehicles only, the car auctioneer—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>is exempt from the requirement to be licensed under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118826" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 3" atidlm:xmlId="DLM564998" atidlm:state="UNCHANGED"><atidlm:linkcontent>3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120541" atidlm:sourceXmlId="DLM564998" atidlm:targetKey="1452884" atidlm:targetXmlId="DLM205332" atidlm:targetName="prov 3" atidlm:sourceName="DLM564998" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM205331.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Exemption for car auctioneers from licensing requirement under Auctioneers Act 1928" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="141" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118827" atidlm:branchLabel="_NOVERSION_" atidlm:name="Auctioneers Act 1928" atidlm:xmlId="DLM564999" atidlm:state="UNCHANGED"><atidlm:linkcontent>Auctioneers Act 1928</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="120542" atidlm:sourceXmlId="DLM564999" atidlm:targetKey="1451825" atidlm:targetXmlId="DLM205305" atidlm:targetName="Auctioneers Act 1928" atidlm:sourceName="DLM564999" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM205305.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Exemption for car auctioneers from licensing requirement under Auctioneers Act 1928" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="141" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>must be treated as if the car auctioneer is a person licensed to conduct sales by auction of motor vehicles for the purposes of that Act.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>However, a car auctioneer—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>must continue to comply with the duties imposed on auctioneers by the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118828" atidlm:branchLabel="_NOVERSION_" atidlm:name="Auctioneers Act 1928" atidlm:xmlId="DLM565100" atidlm:state="UNCHANGED"><atidlm:linkcontent>Auctioneers Act 1928</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="120543" atidlm:sourceXmlId="DLM565100" atidlm:targetKey="1451825" atidlm:targetXmlId="DLM205305" atidlm:targetName="Auctioneers Act 1928" atidlm:sourceName="DLM565100" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM205305.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Exemption for car auctioneers from licensing requirement under Auctioneers Act 1928" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="141" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>may apply for and hold a licence under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118829" atidlm:branchLabel="_NOVERSION_" atidlm:name="Auctioneers Act 1928" atidlm:xmlId="DLM565101" atidlm:state="UNCHANGED"><atidlm:linkcontent>Auctioneers Act 1928</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="120544" atidlm:sourceXmlId="DLM565101" atidlm:targetKey="1451825" atidlm:targetXmlId="DLM205305" atidlm:targetName="Auctioneers Act 1928" atidlm:sourceName="DLM565101" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM205305.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Exemption for car auctioneers from licensing requirement under Auctioneers Act 1928" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="141" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> if the car auctioneer wishes to do so.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <crosshead id="DLM188938" toc="yes" xml:lang="en-NZ">Service of notices</crosshead>
      <prov id="DLM188939" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">142</label>
        <heading xml:lang="en-NZ">Service of notices</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Any notice or any other document required to be served on, or given to, any person under this Act, or any regulation made under this Act, is sufficiently served if,—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>in the case of an applicant for registration or a motor vehicle trader registered under this Act, the notice or document is—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
                    <para xml:lang="en-NZ">
                      <text>delivered to the person at the person's address for service stated in accordance with <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118830" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 32" atidlm:xmlId="DLM565102" atidlm:state="UNCHANGED"><atidlm:linkcontent>32(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120545" atidlm:sourceXmlId="DLM565102" atidlm:targetKey="1085202" atidlm:targetXmlId="DLM188582" atidlm:targetName="prov 32" atidlm:sourceName="DLM565102" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188579.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Service of notices" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="142" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">ii</label>
                    <para xml:lang="en-NZ">
                      <text>sent by fax or email to the person's fax number or email address, as the case may be, stated in accordance with <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118831" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 32" atidlm:xmlId="DLM565103" atidlm:state="UNCHANGED"><atidlm:linkcontent>32(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120546" atidlm:sourceXmlId="DLM565103" atidlm:targetKey="1085202" atidlm:targetXmlId="DLM188582" atidlm:targetName="prov 32" atidlm:sourceName="DLM565103" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188579.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Service of notices" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="142" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">iii</label>
                    <para xml:lang="en-NZ">
                      <text>posted in a letter addressed to the person at the person's address for service stated in accordance with <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118832" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 32" atidlm:xmlId="DLM565104" atidlm:state="UNCHANGED"><atidlm:linkcontent>32(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120547" atidlm:sourceXmlId="DLM565104" atidlm:targetKey="1085202" atidlm:targetXmlId="DLM188582" atidlm:targetName="prov 32" atidlm:sourceName="DLM565104" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188579.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Service of notices" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="142" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                    </para>
                  </label-para>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>in any other case, the notice or document is—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
                    <para xml:lang="en-NZ">
                      <text>delivered to the person; or</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">ii</label>
                    <para xml:lang="en-NZ">
                      <text>left at the person's usual or last known place of residence or business in New Zealand; or</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">iii</label>
                    <para xml:lang="en-NZ">
                      <text>posted in a letter addressed to the person at the person's place of residence or business in New Zealand.</text>
                    </para>
                  </label-para>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>A notice or document that is sent to a person at a fax number or an email address must be treated as received by that person not later than 2 days after the date on which it is sent.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>A notice or document that is posted to a person must be treated as received by that person not later than 7 days after the date on which it is posted.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>However, a notice or document must not be treated as received if the person to whom it is posted or sent proves that it was not received, otherwise than through fault on the person's part.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">5</label>
            <para xml:lang="en-NZ">
              <text>For the purposes of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118833" atidlm:branchLabel="_NOVERSION_" atidlm:name="Summary Proceedings Act 1957" atidlm:xmlId="DLM565105" atidlm:state="UNCHANGED"><atidlm:linkcontent>Summary Proceedings Act 1957</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="120548" atidlm:sourceXmlId="DLM565105" atidlm:targetKey="2034230" atidlm:targetXmlId="DLM310742" atidlm:targetName="Summary Proceedings Act 1957" atidlm:sourceName="DLM565105" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM310742.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Service of notices" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="142" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, an infringement notice sent to a person by post under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118834" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 142" atidlm:xmlId="DLM565106" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120549" atidlm:sourceXmlId="DLM565106" atidlm:targetKey="1089413" atidlm:targetXmlId="DLM188939" atidlm:targetName="prov 142" atidlm:sourceName="DLM565106" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Service of notices" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="142" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must be treated as having been served on the person on the date that it was posted.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <crosshead id="DLM188940" toc="yes" xml:lang="en-NZ">Levy regulations</crosshead>
      <prov id="DLM188941" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">143</label>
        <heading xml:lang="en-NZ">Regulations may impose levy</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Every motor vehicle trader, or class of motor vehicle traders, specified in regulations made under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118835" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 143" atidlm:xmlId="DLM565107" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120550" atidlm:sourceXmlId="DLM565107" atidlm:targetKey="1089438" atidlm:targetXmlId="DLM188941" atidlm:targetName="prov 143" atidlm:sourceName="DLM565107" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[3]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Regulations may impose levy" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="143" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> must pay to the Minister, for each period of 12 months during which the motor vehicle trader is registered under this Act, a levy of an amount stated in, or calculated or set or reset in accordance with, those regulations for, or in connection with, the costs of the operation and administration of the Disputes Tribunals.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The Governor-General may, by Order in Council made on the recommendation of the Minister, make regulations—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>prescribing different levies for different classes of motor vehicle traders:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>specifying the amount of the levy:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>providing for the method by which the levy will be calculated:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>specifying the criteria and other requirements by and against which the levy will be set or reset:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">e</label>
                <para xml:lang="en-NZ">
                  <text>providing for the payment and collection of the levy:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">f</label>
                <para xml:lang="en-NZ">
                  <text>exempting any motor vehicle trader or class of motor vehicle traders from paying the levy:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">g</label>
                <para xml:lang="en-NZ">
                  <text>providing for waivers or refunds of the whole or any part of the levy:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">h</label>
                <para xml:lang="en-NZ">
                  <text>providing for any other matters necessary or desirable to set, calculate, administer, collect, and enforce the levy.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <crosshead id="DLM188942" toc="yes" xml:lang="en-NZ">Regulations</crosshead>
      <prov id="DLM188943" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">144</label>
        <heading xml:lang="en-NZ">Regulations</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The Governor-General may, by Order in Council, make regulations for all or any of the following purposes:</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>prescribing forms to be used for the purposes of this Act, and the matters to be specified in the forms:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>prescribing fees payable under this Act:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>prescribing procedures for the payment of prescribed fees:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>prescribing procedures, requirements, and other matters, not inconsistent with this Act, for any register or list that the Registrar is required to keep under this Act, including matters relating to—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
                    <para xml:lang="en-NZ">
                      <text>the operation of the register or list:</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">ii</label>
                    <para xml:lang="en-NZ">
                      <text>access to the register or list:</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">iii</label>
                    <para xml:lang="en-NZ">
                      <text>the location of, and hours of access to, the register or list:</text>
                    </para>
                  </label-para>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">e</label>
                <para xml:lang="en-NZ">
                  <text>providing for any other matters contemplated by this Act that are necessary for its administration or necessary for giving it full effect.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <crosshead id="DLM188944" toc="yes" xml:lang="en-NZ">Amendments and repeal</crosshead>
      <prov id="DLM188945" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">145</label>
        <heading xml:lang="en-NZ">Amendments to other enactments</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The enactments set out in <citation jurisdiction="nz">Schedule <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118836" atidlm:branchLabel="_NOVERSION_" atidlm:name="schedule 2 Enactments amended" atidlm:xmlId="DLM565108" atidlm:state="UNCHANGED"><atidlm:linkcontent>2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120551" atidlm:sourceXmlId="DLM565108" atidlm:targetKey="1090619" atidlm:targetXmlId="DLM189200" atidlm:targetName="schedule 2 Enactments amended" atidlm:sourceName="DLM565108" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM189200.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[5]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Amendments to other enactments" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="145" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> are amended in the manner set out in that schedule.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM188946" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">146</label>
        <heading xml:lang="en-NZ">Repeal</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The <citation jurisdiction="nz">Motor Vehicle Dealers Act 1975 (1975 No 127)</citation> is repealed.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <crosshead id="DLM188947" toc="yes" xml:lang="en-NZ">No compensation for loss of office</crosshead>
      <prov id="DLM188948" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">147</label>
        <heading xml:lang="en-NZ">No compensation for loss of office</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The Crown is not liable to make a payment to, or otherwise compensate, any person in respect of the person ceasing for any reason to hold any office established by or under the former Act.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <crosshead id="DLM188949" toc="yes" xml:lang="en-NZ">Transitional provisions for existing licence applications</crosshead>
      <prov id="DLM188950" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">148</label>
        <heading xml:lang="en-NZ">Existing licence applications must be treated as applications for registration under this Act in certain cases</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>An existing licence application must be treated as if it were an application for registration as a motor vehicle trader made under this Act; and the provisions relating to applications for registration under this Act apply with any necessary modifications.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118837" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 148" atidlm:xmlId="DLM565109" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120552" atidlm:sourceXmlId="DLM565109" atidlm:targetKey="1089420" atidlm:targetXmlId="DLM188950" atidlm:targetName="prov 148" atidlm:sourceName="DLM565109" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[8]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Existing licence applications must be treated as applications for registration under this Act in certain cases" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="148" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> applies only if, within 20 working days after receiving the notice referred to in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118838" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 150" atidlm:xmlId="DLM565110" atidlm:state="UNCHANGED"><atidlm:linkcontent>150</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120553" atidlm:sourceXmlId="DLM565110" atidlm:targetKey="1089422" atidlm:targetXmlId="DLM188952" atidlm:targetName="prov 150" atidlm:sourceName="DLM565110" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[8]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Existing licence applications must be treated as applications for registration under this Act in certain cases" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="148" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the applicant—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>gives written notice to the Registrar that the applicant wishes to have the application determined under this Act; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>provides the Registrar with the information or other matters that are required to ensure that the application complies with <citation jurisdiction="nz">sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118839" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 31" atidlm:xmlId="DLM565111" atidlm:state="UNCHANGED"><atidlm:linkcontent>31</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120554" atidlm:sourceXmlId="DLM565111" atidlm:targetKey="1085201" atidlm:targetXmlId="DLM188581" atidlm:targetName="prov 31" atidlm:sourceName="DLM565111" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188579.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[8]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Existing licence applications must be treated as applications for registration under this Act in certain cases" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="148" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="118840" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 32" atidlm:xmlId="DLM565112" atidlm:state="UNCHANGED"><atidlm:linkcontent>32</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="120555" atidlm:sourceXmlId="DLM565112" atidlm:targetKey="1085202" atidlm:targetXmlId="DLM188582" atidlm:targetName="prov 32" atidlm:sourceName="DLM565112" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188579.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM188935.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[5]/prov[8]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Existing licence applications must be treated as applications for registration under this Act in certain cases" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="148" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM188103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="12" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Motor Vehicle Sales Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
