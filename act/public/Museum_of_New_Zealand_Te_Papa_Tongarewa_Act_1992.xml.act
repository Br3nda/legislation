      <prov id="DLM260258" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">19</label>
        <heading xml:lang="en-NZ">Gifts</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Any person who is not expressly prohibited from doing so by any enactment, or by the terms of any trust or endowment, may give to the Board any real property, money, or other personal property.</text>
            </para>
          </subprov>
          <subprov deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
