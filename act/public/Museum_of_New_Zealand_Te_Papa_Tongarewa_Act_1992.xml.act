    <part id="DLM260266" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-03" act.no="19" act.type="public" date.as.at="2007-09-03" date.assent="1992-04-08" stage="reprint" year="1992">
      <label auto.number="no" denominator="yes">2</label>
      <heading xml:lang="en-NZ">Dissolution of Board of Trustees of the National Art Gallery, the National Museum, and the National War Memorial</heading>
      <prov id="DLM260267" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">24</label>
        <heading xml:lang="en-NZ">Dissolution of Board of Trustees of the National Art Gallery, the National Museum, and the National War Memorial</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The Board of Trustees of the National Art Gallery, the National Museum, and the National War Memorial constituted under <citation jurisdiction="nz">section 4 of the National Art Gallery, Museum, and War Memorial Act 1972</citation> is hereby dissolved.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>No member of the Board of Trustees of the National Art Gallery, the National Museum, and the National War Memorial shall be entitled to compensation for loss of office resulting from the dissolution of that Board.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM260268" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">25</label>
        <heading xml:lang="en-NZ">Assets and liabilities</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
