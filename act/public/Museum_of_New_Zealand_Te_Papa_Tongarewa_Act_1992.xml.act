<?xml version="1.0" encoding="UTF-8"?>
<act id="DLM260203" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="19" act.type="public" date.as.at="2007-09-03" date.assent="1992-04-08" date.first.valid="2007-09-18" stage="reprint" year="1992">
  <cover id="DLM260204">
    <title xml:lang="en-NZ">Museum of New Zealand Te Papa Tongarewa Act 1992</title>
    <assent>1992-04-08</assent>
    <cover.reprint-note>
      <para xml:lang="en-NZ"/>
      <admin-office>This Act is administered in the <ministry>Ministry of Cultural Affairs</ministry></admin-office>
    </cover.reprint-note>
  </cover>
  <contents>
    <heading xml:lang="en-NZ">Contents</heading>
    <toc/>
  </contents>
  <front id="DLM260205" xml:lang="en-NZ">
    <long-title id="DLM260206" toc="yes" xml:lang="en-NZ">
      <para xml:lang="en-NZ">
        <text>An Act—</text>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">a</label>
          <para xml:lang="en-NZ">
            <text>To establish the Museum of New Zealand Te Papa Tongarewa; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">b</label>
          <para xml:lang="en-NZ">
            <text>To constitute the Museum of New Zealand Te Papa Tongarewa Board, and to define the Board's functions and powers; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">c</label>
          <para xml:lang="en-NZ">
            <text>To dissolve the Board of Trustees of the National Art Gallery, the National Museum, and the National War Memorial; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">d</label>
          <para xml:lang="en-NZ">
            <text>To repeal the <citation jurisdiction="nz">National Art Gallery, Museum, and War Memorial Act 1972</citation></text>
          </para>
        </label-para>
      </para>
    </long-title>
    <enactment>BE IT ENACTED by the Parliament of New Zealand as follows:</enactment>
  </front>
  <body id="DLM260207" prov-type="section" xml:lang="en-NZ">
    <prov id="DLM260208" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">1</label>
      <heading xml:lang="en-NZ">Short Title and commencement</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text>This Act may be cited as the Museum of New Zealand Te Papa Tongarewa Act 1992.</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <para xml:lang="en-NZ">
            <text>This Act shall come into force on the 1st day of July 1992.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM260209" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
