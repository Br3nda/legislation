<act id="DLM273056" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="86" act.type="public" date.as.at="2007-09-03" date.assent="1992-09-25" date.first.valid="2007-09-18" stage="reprint" year="1992">
  <cover id="DLM273057">
    <title xml:lang="en-NZ">Mutual Assistance in Criminal Matters Act 1992</title>
    <assent>1992-09-25</assent>
    <cover.reprint-note>
      <para xml:lang="en-NZ"/>
      <admin-office>This Act is administered in the <ministry>Ministry of Justice</ministry>.</admin-office>
    </cover.reprint-note>
  </cover>
  <contents>
    <heading xml:lang="en-NZ">Contents</heading>
    <toc/>
  </contents>
  <front id="DLM273058" xml:lang="en-NZ">
    <long-title id="DLM273059" toc="yes" xml:lang="en-NZ">
      <para xml:lang="en-NZ">
        <text>An Act to facilitate the provision and obtaining of international assistance in criminal matters</text>
      </para>
    </long-title>
    <enactment>BE IT ENACTED by the Parliament of New Zealand as follows:</enactment>
  </front>
  <body id="DLM273060" prov-type="section" xml:lang="en-NZ">
    <prov id="DLM273061" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">1</label>
      <heading xml:lang="en-NZ">Short Title and commencement</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text>This Act may be cited as the Mutual Assistance in Criminal Matters Act 1992.</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <para xml:lang="en-NZ">
            <text>This Act shall come into force on the 1st day of April 1993.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <part id="DLM273062" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-03" act.no="86" act.type="public" date.as.at="2007-09-03" date.assent="1992-09-25" stage="reprint" year="1992">
      <label auto.number="no" denominator="yes">1</label>
      <heading xml:lang="en-NZ">Preliminary provisions</heading>
      <prov id="DLM273063" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">2</label>
        <heading xml:lang="en-NZ">Interpretation</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>In this Act, unless the context otherwise requires,â€”</text>
              <def-para id="DLM273064" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM273065" xml:lang="en-NZ">Central Authority</def-term>, in relation to any foreign country, means the person or authority for the time being designated by that country for the purposes of transmitting or receiving requests made under or pursuant to this Act</text>
                </para>
              </def-para>
              <def-para id="DLM273066" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
