              <def-para id="DLM274037" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM274038" xml:lang="en-NZ">Serious offence</def-term> means an offence against the law of New Zealand punishable by imprisonment for a term of 5 years or more</text>
                </para>
              </def-para>
              <def-para id="DLM274039" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM274040" xml:lang="en-NZ">Tainted property</def-term> has the same meaning as in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="373776" atidlm:branchLabel="_NOVERSION_" atidlm:name="def-para tainted property" atidlm:xmlId="DLM818183" atidlm:state="UNCHANGED"><atidlm:linkcontent>2(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="380942" atidlm:sourceXmlId="DLM818183" atidlm:targetKey="1316812" atidlm:targetXmlId="DLM251039" atidlm:targetName="def-para tainted property" atidlm:sourceName="DLM818183" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM250674.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM273062.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[35]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Tainted property" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM273056" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="86" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Mutual Assistance in Criminal Matters Act 1992" atidlm:locale="xx_XX" atidlm:state="UNCHANGE