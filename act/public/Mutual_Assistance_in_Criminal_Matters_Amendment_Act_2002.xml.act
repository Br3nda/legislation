              <amend>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">e</label>
                  <para xml:lang="en-NZ">
                    <text>subject to a community-based sentence (within the meaning of <citation jurisdiction="nz">section 4(1) of the Sentencing Act 2002</citation>).</text>
                  </para>
                </label-para>
              </amend>
            </para>
          </subprov>
