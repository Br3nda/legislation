            <def-para id="DLM225848" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM225849" xml:lang="en-NZ">Rights</def-term> means all rights, powers, privileges, and immunities, whether actual, contingent, or prospective</text>
              </para>
            </def-para>
            <def-para id="DLM225850" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM225851" xml:lang="en-NZ">Solicitor</def-term> means a person enrolled as a barrister and solicitor of the Court under or by virtue of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="317177" atidlm:branchLabel="_NOVERSION_" atidlm:name="Law Practitioners Act 1982" atidlm:xmlId="DLM761972" atidlm:state="UNCHANGED"><atidlm:linkcontent>Law Practitioners Act 1982</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="322582" atidlm:sourceXmlId="DLM761972" atidlm:targetKey="356400" atidlm:targetXmlId="DLM62319" atidlm:targetName="Law Practitioners Act 1982" atidlm:sourceName="DLM761972" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM62319.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM225563.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[38]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Solicitor" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM225563" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="126" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="National Provident Fund Restructuring Act 1990" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1990" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></text>
              </para>
            </def-para>
            <def-para id="DLM225852" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM225853" xml:lang="en-NZ">Superannuation scheme</def-term> has the same meaning as in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="317178" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 2" atidlm:xmlId="DLM761973" atidlm:state="UNCHANGED"><atidlm:linkcontent>2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="322583" atidlm:sourceXmlId="DLM761973" atidlm:targetKey="807214" atidlm:targetXmlId="DLM143297" atidlm:targetName="prov 2" atidlm:sourceName="DLM761973" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM143291.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM225563.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[39]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Superannuation scheme" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM225563" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="126" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="National Provident Fund Restructuring Act 1990" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1990" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="317179" atidlm:branchLabel="_NOVERSION_" atidlm:name="Superannuation Schemes Act 1989" atidlm:xmlId="DLM761974" atidlm:state="UNCHANGED"><atidlm:linkcontent>Superannuation Schemes Act 1989</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="322584" atidlm:sourceXmlId="DLM761974" atidlm:targetKey="807222" atidlm:targetXmlId="DLM143291" atidlm:targetName="Superannuation Schemes Act 1989" atidlm:sourceName="DLM761974" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM143291.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM225563.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[39]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Superannuation scheme" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM225563" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="126" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="National Provident Fund Restructuring Act 1990" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="1990" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, and includes each existing scheme</text>
              </para>
            </def-para>
            <def-para id="DLM225854" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM225855" xml:lang="en-NZ">Transfer day</def-term> means the 1st day of April 1991</text>
              </para>
            </def-para>
            <def-para id="DLM225856" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM225857" xml:lang="en-NZ">Trust deed</def-term> means any trust deed in respect of any scheme, and includes any amendment, replacement, or consolidation of any trust deed</text>
              </para>
            </def-para>
            <def-para id="DLM225858" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM225859" xml:lang="en-NZ">Trust fund</def-term> means a trust fund for an existing scheme that is established as a result of the proposal</text>
              </para>
            </def-para>
            <def-para id="DLM225860" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM225861" xml:lang="en-NZ">Unclaimed money</def-term> means any money—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>That is or was payable to any person out of the Fund or any trust fund; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>For which no claim or demand has been made by or on behalf of that person; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>That remains as part of the Fund or any trust fund in case any person establishes a lawful claim to the money.</text>
                  </para>
                </label-para>
              </para>
            </def-para>
          </para>
        </subprov>
      </prov.body>
