<?xml version="1.0" encoding="UTF-8"?>
<act id="DLM225563" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="126" act.type="public" date.as.at="2007-09-03" date.assent="1990-09-07" date.first.valid="2007-09-18" stage="reprint" year="1990">
  <cover id="DLM225564">
    <title xml:lang="en-NZ">National Provident Fund Restructuring Act 1990</title>
    <assent>1990-09-07</assent>
    <cover.reprint-note>
      <para xml:lang="en-NZ"/>
      <admin-office>This Act is administered in the <ministry>Treasury</ministry>.</admin-office>
    </cover.reprint-note>
  </cover>
  <contents>
    <heading xml:lang="en-NZ">Contents</heading>
    <toc/>
  </contents>
  <front id="DLM225565" xml:lang="en-NZ">
    <long-title id="DLM225566" toc="yes" xml:lang="en-NZ">
      <para xml:lang="en-NZ">
        <text>An Act to provide for—</text>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">a</label>
          <para xml:lang="en-NZ">
            <text>The continuation of the National Provident Fund Board as the Board of Trustees of the National Provident Fund; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">b</label>
          <para xml:lang="en-NZ">
            <text>The restructuring of the National Provident Fund and its superannuation schemes and investment arrangements; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">c</label>
          <para xml:lang="en-NZ">
            <text>Other incidental matters; and</text>
          </para>
        </label-para>
        <label-para skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">d</label>
          <para xml:lang="en-NZ">
            <text>The repeal of the <citation jurisdiction="nz">National Provident Fund Act 1950</citation></text>
          </para>
        </label-para>
      </para>
    </long-title>
    <enactment>BE IT ENACTED by the Parliament of New Zealand as follows:</enactment>
  </front>
  <body id="DLM225567" prov-type="section" xml:lang="en-NZ">
