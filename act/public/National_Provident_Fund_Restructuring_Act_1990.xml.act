      <prov deletion-status="repealed" id="DLM226605" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">74</label>
        <heading xml:lang="en-NZ">Amendment to <citation jurisdiction="nz">Income Tax Act 1976</citation> in respect of National Provident Fund Board</heading>
        <prov.body/>
