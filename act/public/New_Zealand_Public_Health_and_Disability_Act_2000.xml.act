        <prov id="DLM82011" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">15</label>
          <heading xml:lang="en-NZ">When appointed members come into office</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>Every appointed member comes into office on the date specified for that purpose in the notice appointing the member or, if no date is specified in the notice, from the date on which the notice is published in the <emphasis style="italic">Gazette</emphasis>.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
