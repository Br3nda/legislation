              <def-para id="DLM81605" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM81606" xml:lang="en-NZ">dissolved entity</def-term> means any of the following</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>the Health Funding Authority:</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>any hospital and health service:</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">c</label>
                    <para xml:lang="en-NZ">
                      <text>New Zealand Blood Service Limited:</text>
