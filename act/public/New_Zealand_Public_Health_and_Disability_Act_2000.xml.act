              <def-para id="DLM80429" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80430" xml:lang="en-NZ">New Zealand disability strategy</def-term> means a strategy referred to in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="55781" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 8" atidlm:xmlId="DLM502894" atidlm:state="UNCHANGED"><atidlm:linkcontent>8(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="57022" atidlm:sourceXmlId="DLM502894" atidlm:targetKey="433201" atidlm:targetXmlId="DLM80487" atidlm:targetName="prov 8" atidlm:sourceName="DLM502894" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM80486.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM80055.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[5]/prov.body[1]/subprov[1]/para[1]/def-para[28]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para New Zealand disability strategy" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM80050" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="91" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="New Zealand Public Health and Disability Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></text>
                </para>
              </def-para>
              <def-para id="DLM80431" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80432" xml:lang="en-NZ">New Zealand health strategy</def-term> means a strategy referred to in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="55782" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 8" atidlm:xmlId="DLM502895" atidlm:state="UNCHANGED"><atidlm:linkcontent>8(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="57023" atidlm:sourceXmlId="DLM502895" atidlm:targetKey="433201" atidlm:targetXmlId="DLM80487" atidlm:targetName="prov 8" atidlm:sourceName="DLM502895" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM80486.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM80055.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[5]/prov.body[1]/subprov[1]/para[1]/def-para[29]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para New Zealand health strategy" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM80050" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="91" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="New Zealand Public Health and Disability Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></text>
                </para>
              </def-para>
              <def-para id="DLM80433" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80434" xml:lang="en-NZ">partner</def-term>, in the phrase <quote.in>spouse or partner</quote.in> and in related contexts, means a civil union partner or a de facto partner</text>
                  <notes>
                    <history>
                      <history-note id="DLM80435">partner: this definition was <amending-operation>inserted</amending-operation>, as from <amendment-date>26 April 2005</amendment-date>, by section <amending-provision href="DLM333795">7</amending-provision> <amending-leg>Relationships (Statutory References) Act 2005</amending-leg> (2005 No 3).</history-note>
                    </history>
                  </notes>
                </para>
              </def-para>
              <def-para id="DLM80436" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80437" xml:lang="en-NZ">personal health</def-term> means the health of an individual</text>
                </para>
              </def-para>
              <def-para id="DLM80438" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80439" xml:lang="en-NZ">personal health services</def-term> means goods, services, and facilities provided to an individual for the purpose of improving or protecting the health of that individual, whether or not they are also provided for another purpose; and includes goods, services, and facilities provided for related or incidental purposes</text>
                </para>
              </def-para>
              <def-para id="DLM80440" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80441" xml:lang="en-NZ">Pharmac</def-term> means the Pharmaceutical Management Agency established by <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="55783" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 46" atidlm:xmlId="DLM502896" atidlm:state="UNCHANGED"><atidlm:linkcontent>46</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="57024" atidlm:sourceXmlId="DLM502896" atidlm:targetKey="435202" atidlm:targetXmlId="DLM80875" atidlm:targetName="prov 46" atidlm:sourceName="DLM502896" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM80873.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM80055.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[5]/prov.body[1]/subprov[1]/para[1]/def-para[33]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Pharmac" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM80050" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="91" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="New Zealand Public Health and Disability Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></text>
                </para>
              </def-para>
              <def-para id="DLM80442" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80443" xml:lang="en-NZ">pharmaceutical</def-term> means a medicine, therapeutic medical device, or related product or related thing</text>
                </para>
              </def-para>
              <def-para id="DLM80444" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80445" xml:lang="en-NZ">pharmaceutical schedule</def-term> means the list of pharmaceuticals for the time being in force that states, in respect of each pharmaceutical, the subsidy that the Crown intends to provide for the supply of that pharmaceutical to a person who is eligible for the subsidy</text>
                </para>
              </def-para>
              <def-para id="DLM80446" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80447" xml:lang="en-NZ">provider</def-term> means a person who provides, or arranges the provision of, services</text>
                </para>
              </def-para>
              <def-para id="DLM80448" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80449" xml:lang="en-NZ">public health</def-term> means the health of all of—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>the people of New Zealand; or</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>a community or section of such people</text>
                    </para>
                  </label-para>
                </para>
              </def-para>
              <def-para id="DLM80450" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80451" xml:lang="en-NZ">publicly-owned health and disability organisation</def-term> means a DHB, Pharmac, NZBS, and <insertwords>CHFA</insertwords></text>
                  <notes>
                    <history>
                      <history-note id="DLM80452">publicly-owned health and disability organisation: this definition was <amending-operation>amended</amending-operation>, as from <amendment-date>17 May 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="55784" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 5" atidlm:xmlId="DLM502897" atidlm:state="UNCHANGED"><atidlm:linkcontent>5(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="57025" atidlm:sourceXmlId="DLM502897" atidlm:targetKey="2089608" atidlm:targetXmlId="DLM348477" atidlm:targetName="prov 5" atidlm:sourceName="DLM502897" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM348467.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM80055.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[5]/prov.body[1]/subprov[1]/para[1]/def-para[38]/para[1]/notes[1]/history[1]/history-note[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM80050" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="91" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="New Zealand Public Health and Disability Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>New Zealand Public Health and Disability Amendment Act 2005</amending-leg> (2005 No 63) <amending-instruction>by substituting the expression <quote.in>CHFA</quote.in> for the expression <quote.in>RHMU</quote.in></amending-instruction>.</history-note>
                    </history>
                  </notes>
                </para>
              </def-para>
              <def-para id="DLM80453" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80454" xml:lang="en-NZ">public health services</def-term> means goods, services, and facilities provided for the purpose of improving, promoting, or protecting public health or preventing population-wide disease, disability, or injury; and includes—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>regulatory functions relating to health or disability matters; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>health protection and health promotion services; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">c</label>
                    <para xml:lang="en-NZ">
                      <text>goods, services, and facilities provided for related or incidental functions or purposes</text>
                    </para>
                  </label-para>
                </para>
              </def-para>
              <def-para id="DLM80455" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80456" xml:lang="en-NZ">publicly available</def-term>, in relation to a document, means that the document is promptly provided on request by any person on payment of a charge, if any, that is reasonable</text>
                </para>
              </def-para>
              <def-para id="DLM80457" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM80458" xml:lang="en-NZ">resident population</def-term>, in relation to a DHB, means the eligible people residing in the geographical area of the DHB (as specified in <citation jurisdiction="nz">Schedule <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="55785" atidlm:branchLabel="_NOVERSION_" atidlm:name="schedule 1 DHBs and their geographical areas" atidlm:xmlId="DLM502898" atidlm:state="UNCHANGED"><atidlm:linkcontent>1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="57026" atidlm:sourceXmlId="DLM502898" atidlm:targetKey="438200" atidlm:targetXmlId="DLM81650" atidlm:targetName="schedule 1 DHBs and their geographical areas" atidlm:sourceName="DLM502898" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM81650.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM80055.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[5]/prov.body[1]/subprov[1]/para[1]/def-para[41]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para resident population" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM80050" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="91" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="New Zealand Public Health and Disability Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>)</text>
                </para>
              </def-para>
              <def-para deletion-status="repealed" id="DLM80459" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text>
                    <def-term id="DLM80460" xml:lang="en-NZ">RHMU</def-term>
                  </text>
                  <notes>
                    <history>
                      <history-note id="DLM80461">RHMU: this definition was <amending-operation>repealed</amending-operation>, as from <amendment-date>17 May 2005</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="55786" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 5" atidlm:xmlId="DLM502899" atidlm:state="UNCHANGED"><atidlm:linkcontent>5(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="57027" atidlm:sourceXmlId="DLM502899" atidlm:targetKey="2089608" atidlm:targetXmlId="DLM348477" atidlm:targetName="prov 5" atidlm:sourceName="DLM502899" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM348467.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM80055.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[5]/prov.body[1]/subprov[1]/para[1]/def-para[42]/para[1]/notes[1]/history[1]/history-note[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM80050" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="91" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="New Zealand Public Health and Disability Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>New Zealand Public Health and Disability Amendment Act 2005</amending-leg> (2005 No 63).</history-note>
                    </history>
                  </notes>
                </para>
              </def-para>
              <def-para id="DLM80462" xml:lang="en-NZ">
