          <amend>
            <def-para id="DLM83163" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM83164" xml:lang="en-NZ">Crown funding agreement</def-term> has the same meaning as in <citation jurisdiction="nz">section 6(1) of the New Zealand Public Health and Disability Act 2000</citation></text>
              </para>
            </def-para>
            <def-para id="DLM83165" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM83166" xml:lang="en-NZ">disability services</def-term> has the same meaning as disability support services has in <citation jurisdiction="nz">section 6(1) of the New Zealand Public Health and Disability Act 2000</citation></text>
              </para>
            </def-para>
            <def-para id="DLM83167" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM83168" xml:lang="en-NZ">district health board</def-term> means a district health board established by or under <citation jurisdiction="nz">section 19 of the New Zealand Public Health and Disability Act 2000</citation></text>
              </para>
            </def-para>
            <def-para id="DLM83169" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM83170" xml:lang="en-NZ">service agreement</def-term> has the same meaning as in <citation jurisdiction="nz">section 6(1) of the New Zealand Public Health and Disability Act 2000</citation></text>
              </para>
            </def-para>
          </amend>
          <follow-text space-before="no">.</follow-text>
        </para>
        <para xml:lang="en-NZ">
          <text>Omit from <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="58590" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 4" atidlm:parentFolderId="/NZ/NZAct/N/New Zealand Public Health and Disability Act 2000" atidlm:xmlId="DLM505397" atidlm:state="UNCHANGED"><atidlm:linkcontent>4(e)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="59736" atidlm:sourceXmlId="DLM505397" atidlm:targetKey="2807283" atidlm:targetXmlId="DLM437001" atidlm:targetName="prov 4" atidlm:sourceName="DLM505397" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM436789.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM83147.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[9]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Consequential amendments to Acts" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="9" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM80050" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="91" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="New Zealand Public Health and Disability Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2000" atidlm: