        <prov id="DLM82086" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">29</label>
          <heading xml:lang="en-NZ">Voting</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>All questions arising at any meeting of a board must be decided by a majority of the votes cast by the members present.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>If a vote is tied, the member presiding at the meeting has no second or casting vote, and the question is negatived.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <prov id="DLM82087" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">30</label>
          <heading xml:lang="en-NZ">Supplementary procedure</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A board may regulate its procedure, at its meetings and otherwise, in any manner not inconsistent with this Act it thinks fit.</text>
              </para>
            </subprov>
          </prov.body>
        </prov>
        <head5 id="DLM82088" toc="yes" xml:lang="en-NZ">
          <heading xml:lang="en-NZ">Admission of public</heading>
        </head5>
