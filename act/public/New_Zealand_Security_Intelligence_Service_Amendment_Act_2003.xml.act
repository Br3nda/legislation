<act id="DLM219992" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="108" act.type="public" date.as.at="2007-09-03" date.assent="2003-10-30" date.first.valid="2007-09-18" stage="in-force" year="2003">
