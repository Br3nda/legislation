<act id="DLM32239" in.amend="false" irdnumbering="no" xml:lang="en-NZ" act.no="91" act.type="public" date.as.at="2007-09-03" date.assent="1999-08-31" date.first.valid="2007-09-18" stage="in-force" year="1999">
