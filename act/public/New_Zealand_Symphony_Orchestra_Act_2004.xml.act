<act id="DLM242831" in.amend="false" instructing.office="Culture and Heritage" irdnumbering="no" xml:lang="en-NZ" year.imprint="2009" act.no="20" act.type="public" date.as.at="2005-01-25" date.assent="2004-04-05" date.first.valid="2007-09-18" stage="reprint" year="2004">
