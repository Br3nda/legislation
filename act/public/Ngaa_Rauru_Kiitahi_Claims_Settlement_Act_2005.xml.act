        <prov id="DLM359656" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">58</label>
          <heading xml:lang="en-NZ">Interpretation</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>In this subpart, unless the context otherwise requires,—</text>
                <def-para id="DLM359657" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM359658" xml:lang="en-NZ">entitlement land</def-term> means a site over which an Uukaipoo entitlement is granted</text>
                  </para>
                </def-para>
                <def-para id="DLM359659" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM359660" xml:lang="en-NZ">land holding agent</def-term> means the Minister of the Crown responsible for the department of State that manages the entitlement land or the Commissioner of Crown Lands, as the case may be</text>
                  </para>
                </def-para>
                <def-para id="DLM359661" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM359662" xml:lang="en-NZ">Uukaipoo entitlement</def-term> means an entitlement granted to the governance entity—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">a</label>
                      <para xml:lang="en-NZ">
                        <text>under this subpart; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">b</label>
                      <para xml:lang="en-NZ">
