            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">6</label>
              <para xml:lang="en-NZ">
                <text>The fee simple estate in the Nukumaru site vests in the governance entity.</text>
              </para>
            </subprov>
