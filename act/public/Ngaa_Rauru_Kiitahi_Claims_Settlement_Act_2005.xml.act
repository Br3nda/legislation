      <subpart id="DLM359721" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" act.no="84" act.type="public" date.as.at="2007-09-03" date.assent="2005-06-27" date.first.valid="2007-09-03" stage="reprint" year="2005">
        <label auto.number="no" denominator="yes">6</label>
        <heading xml:lang="en-NZ">Shellfish quota</heading>
        <prov id="DLM359722" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">103</label>
          <heading xml:lang="en-NZ">Interpretation</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>In this subpart, unless the context otherwise requires,—</text>
                <def-para id="DLM359723" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM359724" xml:lang="en-NZ">excess shellfish quota</def-term> is the aggregate shellfish quota that—</text>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">a</label>
                      <para xml:lang="en-NZ">
                        <text>exceeds the quota permitted by <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="314406" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 59" atidlm:xmlId="DLM759366" atidlm:state="UNCHANGED"><atidlm:linkcontent>59</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="319721" atidlm:sourceXmlId="DLM759366" atidlm:targetKey="2120938" atidlm:targetXmlId="DLM396334" atidlm:targetName="prov 59" atidlm:sourceName="DLM759366" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM395527.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM359721.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[6]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para excess shellfish quota" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM359210" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="84" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ngaa Rauru Kiitahi Claims Settlement Act 2005" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2005" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="314407" atidlm:branchLabel="_NOVERSION_" atidlm:name="Fisheries Act 1996" atidlm:xmlId="DLM759367" atidlm:state="UNCHANGED"><atidlm:linkcontent>Fisheries Act 1996</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="319722" atidlm:sourceXmlId="DLM759367" atidlm:targetKey="2111006" atidlm:targetXmlId="DLM394191" atidlm:targetName="Fisheries Act 1996" atidlm:sourceName="DLM759367" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM394191.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM359721.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[6]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para excess shellfish quota" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM359210" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="84" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ngaa Rauru Kiitahi Claims Settlement Act 2005" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2005" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                      </para>
                    </label-para>
                    <label-para skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes">b</label>
                      <para xml:lang="en-NZ">
                        <text>is—</text>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">i</label>
                          <para xml:lang="en-NZ">
                            <text>purchased by the governance entity under the deed granting a right of first refusal over shellfish quota that is to be entered into by the Minister and the governance entity under clause 12.6 of the deed of settlement; and</text>
                          </para>
                        </label-para>
                        <label-para skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes">ii</label>
                          <para xml:lang="en-NZ">
                            <text>received by the governance entity from the Treaty of Waitangi Fisheries Commission</text>
                          </para>
                        </label-para>
                      </para>
                    </label-para>
                  </para>
                </def-para>
                <def-para id="DLM359725" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM359726" xml:lang="en-NZ">Minister</def-term> has the meaning given to it in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="314408" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 2" atidlm:xmlId="DLM759368" atidlm:state="UNCHANGED"><atidlm:linkcontent>2(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="319723" atidlm:sourceXmlId="DLM759368" atidlm:targetKey="2114117" atidlm:targetXmlId="DLM394199" atidlm:targetName="prov 2" atidlm:sourceName="DLM759368" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM394198.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM359721.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[6]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para Minister" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM359210" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="84" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ngaa Rauru Kiitahi Claims Settlement Act 2005" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legis