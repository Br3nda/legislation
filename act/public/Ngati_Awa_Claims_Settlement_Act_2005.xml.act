              <def-para id="DLM339327" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM339328" xml:lang="en-NZ">statutory area</def-term> means the areas and rivers defined in <citation jurisdiction="nz">Schedule <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="289224" atidlm:branchLabel="_NOVERSION_" atidlm:name="schedule 3 Statutory acknowledgements" atidlm:xmlId="DLM734215" atidlm:state="UNCHANGED"><atidlm:linkcontent>3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="293852" atidlm:sourceXmlId="DLM734215" atidlm:targetKey="2026603" atidlm:targetXmlId="DLM340404" atidlm:targetName="schedule 3 Statutory acknowledgements" atidlm:sourceName="DLM734215" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340404.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM338906.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[2]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[56]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para statutory area" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM338760" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="28" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ngati Awa Claims Settlement Act 2005" atidlm:locale="xx_XX" ati