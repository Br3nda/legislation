        </para>
        <para xml:lang="en-NZ">
          <text>Except as expressly provided in <citation jurisdiction="nz">subpart <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="292055" atidlm:branchLabel="_NOVERSION_" atidlm:name="subpart 3 Statutory acknowledgements and deeds of recognition" atidlm:parentFolderId="/NZ/NZAct/N/Ngati Awa Claims Settlement Act 2005" atidlm:xmlId="DLM737243" atidlm:state="UNCHANGED"><atidlm:linkcontent>3</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="297096" atidlm:sourceXmlId="DLM737243" atidlm:targetKey="2018224" atidlm:targetXmlId="DLM339711" atidlm:targetName="subpart 3 Statutory acknowledgements and deeds of recognition" atidlm:sourceName="DLM737243" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM339711.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340416.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[5]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Statutory acknowledgement for Mokorua Scenic Reserve" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="5" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM338760" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="28" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ngati Awa Claims Settlement Act 20