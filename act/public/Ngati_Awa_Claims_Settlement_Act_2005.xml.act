              </def-para>
              <def-para id="DLM338982" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM338983" xml:lang="en-NZ">offshore islands</def-term>â€”</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>means the following islands situated in the Bay of Plenty:</text>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">i</label>
                        <para xml:lang="en-NZ">
                          <text>Motiti Island:</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">ii</label>
                        <para xml:lang="en-NZ">
                          <text>Tokata Island:</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">iii</label>
                        <para xml:lang="en-NZ">
                          <text>Rurima Island:</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">iv</label>
                        <para xml:lang="en-NZ">
                          <text>Moutoki Island:</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">v</label>
                        <para xml:lang="en-NZ">
                          <text>Moutohora (Whale Island):</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">vi</label>
                        <para xml:lang="en-NZ">
                          <text>Whakari (White) Island:</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">vii</label>
                        <para xml:lang="en-NZ">
                          <text>Te Paepae o Aotea (formerly Volkner Rocks); and</text>
                        </para>
                      </label-para>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>includes any islands, islets, or rocks adjacent to those islands</text>
                    </para>
                  </label-para>
                </para>
              </def-para>
