        <prov id="DLM339717" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">44</label>
          <heading xml:lang="en-NZ">Historic Places Trust and Environment Court to have regard to statutory acknowledgements</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
