        <citation jurisdiction="nz">
          <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="292119" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 40" atidlm:parentFolderId="/NZ/NZAct/N/Ngati Awa Claims Settlement Act 2005" atidlm:xmlId="DLM737607" atidlm:state="UNCHANGED">
            <atidlm:linkcontent>s 40</atidlm:linkcontent>
