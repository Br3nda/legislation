            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>In this subpart, unless the context otherwise requires,â€”</text>
                <def-para id="DLM340012" xml:lang="en-NZ">
                  <para xml:lang="en-NZ">
                    <text><def-term id="DLM340013" xml:lang="en-NZ">entitlement land</def-term> means a site over which a Nohoanga entitlement is granted</text>
                  </para>
                </def-para>
