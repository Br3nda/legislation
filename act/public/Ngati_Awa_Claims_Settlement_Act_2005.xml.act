        <head5 id="DLM340442" toc="yes" xml:lang="en-NZ">
          <heading xml:lang="en-NZ">Limitations on effect of statutory acknowledgement</heading>
        </head5>
        <para xml:lang="en-NZ">
          <text>Except as expressly provided in <citation jurisdiction="nz">sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="292083" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 41" atidlm:parentFolderId="/NZ/NZAct/N/Ngati Awa Claims Settlement Act 2005" atidlm:xmlId="DLM737271" atidlm:state="UNCHANGED"><atidlm:linkcontent>41 to 44</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="297245" atidlm:sourceXmlId="DLM737271" atidlm:targetKey="2018220" atidlm:targetXmlId="DLM339714" atidlm:targetName="prov 41" atidlm:sourceName="DLM737271" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM339711.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340437.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[8]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Statutory acknowledgement for Part Ohiwa Harbour" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="8" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM338760" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="28" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ngati Awa Claims Settlement Act 2005" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atid