        </prov>
        <prov id="DLM339710" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">39</label>
          <heading xml:lang="en-NZ">Application of other enactments</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
