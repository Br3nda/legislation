                <text>The Crown acknowledges the statements made by Ngati Awa of the particular cultural, spiritual, historical, and traditional association of Ngati Awa with the statutory areas listed in <citation jurisdiction="nz">Parts <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="289379" atidlm:branchLabel="_NOVERSION_" atidlm:name="head1 1 Statutory areas for which statutory acknowledgement only provided" atidlm:xmlId="DLM734770" atidlm:state="UNCHANGED"><atidlm:linkcontent>1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="294451" atidlm:sourceXmlId="DLM734770" atidlm:targetKey="2026600" atidlm:targetXmlId="DLM340405" atidlm:targetName="head1 1 Statutory areas for which statutory acknowledgement only provided" atidlm:sourceName="DLM734770" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM340404.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM339711.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[3]/prov[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Statutory acknowledgements by the Crown" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="40" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM338760" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="28" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidl