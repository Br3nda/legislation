                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>for the purpose of public notice only; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
