              <def-para id="DLM349762" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM349763" xml:lang="en-NZ">business day</def-term> means the period of 9 am to 5 pm on any day of the week other thanâ€”</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>Saturday, Sunday, Good Friday, Easter Monday, Anzac Day, the Sovereign's Birthday, Labour Day, and Waitangi Day; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>a day in the period commencing with 25 December in a year and ending with the close of 15 January in the following year; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">c</label>
                    <para xml:lang="en-NZ">
                      <text>the days observed as the anniversaries of Wellington and Auckland</text>
                    </para>
                  </label-para>
                </para>
              </def-para>
              <def-para id="DLM349764" xml:lang="en-NZ">
