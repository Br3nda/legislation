                      <text>any other department of State authorised by the Prime Minister to exercise powers and perform functions and duties under <citation jurisdiction="nz">subpart <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="303067" atidlm:branchLabel="_NOVERSION_" atidlm:name="subpart 1 Protocols" atidlm:xmlId="DLM748041" atidlm:state="UNCHANGED"><atidlm:linkcontent>1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="308046" atidlm:sourceXmlId="DLM748041" atidlm:targetKey="2098016" atidlm:targetXmlId="DLM350076" atidlm:targetName="subpart 1 Protocols" atidlm:sourceName="DLM748041" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM350076.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM349751.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[2]/prov[2]/prov.body[1]/subprov[1]/para[1]/def-para[39]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para responsible Ministry" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM349701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="72" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ngati Tuwharetoa (Bay of Plenty) Claims Settlement Act 2005" atidlm:locale=