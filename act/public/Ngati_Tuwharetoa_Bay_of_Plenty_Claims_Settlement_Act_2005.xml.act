        <citation jurisdiction="nz">
          <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="304495" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 98" atidlm:parentFolderId="/NZ/NZAct/N/Ngati Tuwharetoa (Bay of Plenty) Claims Settlement Act 2005" atidlm:xmlId="DLM749167" atidlm:state="UNCHANGED">
