              <para xml:lang="en-NZ">
                <text>The fisheries protocol does not have the effect of granting, creating, or providing evidence of, any estate or interest in, or any rights relating to, assets or other property rights held, managed, or administered under fisheries legislation or under the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="303113" atidlm:branchLabel="_NOVERSION_" atidlm:name="Treaty of Waitangi (Fisheries Claims) Settlement Act 1992" atidlm:xmlId="DLM748087" atidlm:state="UNCHANGED"><atidlm:linkcontent>Treaty of Waitangi (Fisheries Claims) Settlement Act 1992</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="308133" atidlm:sourceXmlId="DLM748087" atidlm:targetKey="1458007" atidlm:targetXmlId="DLM281432" atidlm:targetName="Treaty of Waitangi (Fisheries Claims) Settlement Act 1992" atidlm:sourceName="DLM748087" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM281432.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM350076.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/subpart[1]/prov[4]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Limitation of rights" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="24" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM349701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number