          <label-para skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">a</label>
            <para xml:lang="en-NZ">
              <text>this statutory acknowledgement does not affect, and is not to be taken into account in, the exercise of any power, duty, or function by any person or entity under any statute, regulation, or bylaw; and</text>
            </para>
          </label-para>
          <label-para skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">b</label>
            <para xml:lang="en-NZ">
              <text>without limiting <citation jurisdiction="nz">paragraph <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="304482" atidlm:branchLabel="_NOVERSION_" atidlm:name="head5  Limitations on effect of statutory acknowledgement" atidlm:parentFolderId="/NZ/NZAct/N/Ngati Tuwharetoa (Bay of Plenty) Claims Settlement Act 2005" atidlm:xmlId="DLM749154" atidlm:state="UNCHANGED"><atidlm:linkcontent>(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="309269" atidlm:sourceXmlId="DLM749154" atidlm:targetKey="2105407" atidlm:targetXmlId="DLM350592" atidlm:targetName="head5  Limitations on effect of statutory acknowledgement" atidlm:sourceName="DLM749154" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM350586.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM350586.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[7]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Statutory acknowledgement for Rangitaiki River" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="7" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM349701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="72" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ngati Tuwharetoa (Bay of Plenty)