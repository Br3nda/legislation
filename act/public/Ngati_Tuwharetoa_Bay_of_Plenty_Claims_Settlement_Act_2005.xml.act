              <def-para id="DLM350028" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM350029" xml:lang="en-NZ">relevant consent authority</def-term> means a consent authority of the region or district that contains, or is adjacent to, a statutory area</text>
                </para>
              </def-para>
              <def-para id="DLM350030" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM350031" xml:lang="en-NZ">representative entity</def-term> meansâ€”</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>the governance entity; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>any person (including any trust or trustees) acting for, or on behalf ofâ€”</text>
                      <label-para skeleton="no" xml:lang="en-NZ">
