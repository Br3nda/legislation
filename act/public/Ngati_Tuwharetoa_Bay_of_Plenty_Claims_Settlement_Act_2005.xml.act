        <crosshead id="DLM350211" toc="yes" xml:lang="en-NZ">Provisions relating to vesting of cultural redress properties</crosshead>
        <prov id="DLM350212" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">33</label>
          <heading xml:lang="en-NZ">Vesting subject to encumbrances</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
