    <schedule id="DLM350565" in.amend="false" irdnumbering="no" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" act.no="72" act.type="public" date.as.at="2008-05-23" date.assent="2005-05-23" date.first.valid="2005-05-24" stage="reprint" year="2005">
