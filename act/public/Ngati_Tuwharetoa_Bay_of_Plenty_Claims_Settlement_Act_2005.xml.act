        </head5>
        <para xml:lang="en-NZ">
          <text>Except as expressly provided in <citation jurisdiction="nz">sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="304461" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 39" atidlm:parentFolderId="/NZ/NZAct/N/Ngati Tuwharetoa (Bay of Plenty) Claims Settlement Act 2005" atidlm:xmlId="DLM749133" atidlm:state="UNCHANGED"><atidlm:linkcontent>39 to 41</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="309248" atidlm:sourceXmlId="DLM749133" atidlm:targetKey="2099618" atidlm:targetXmlId="DLM350220" atidlm:targetName="prov 39" atidlm:sourceName="DLM749133" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM350216.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM350572.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[5]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="schedule" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Statutory acknowledgement for Lake Tamurenui Wildlife Management Reserve" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="5" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM349701" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="72" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Ngati Tuwharetoa (Bay of Plenty) Claims Settlement Act 2005" atidlm:locale="xx_XX" atidlm:s