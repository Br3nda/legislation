                  <row>
                    <entry align="justify" colsep="0" nameend="colspec0" namest="col1">
                      <para>
                        <text>land that adjoins the sea or a lake and exceeds 0.4 hectares and is 1 of the following types of land:</text>
                        <list>
                          <item>
                            <label auto.number="no" denominator="yes"/>
                            <para>
