      <prov id="DLM120612" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">19A</label>
        <heading xml:lang="en-NZ">Extension of partner's/paternity leave entitlements <insertwords>for spouses or partners of employees</insertwords> in certain circumstances</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>This section applies if—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>an employee (<def-term id="DLM120613" xml:lang="en-NZ">employee A</def-term>) is entitled to—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
