      <prov id="DLM121740" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">71S</label>
        <heading xml:lang="en-NZ">Backdating of payments</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The first payment of a parental leave payment includes an amount in respect of the period from the start of the parental leave if the application is received on or after the date of commencement of the parental leave.</text>
            </para>
          </subprov>
        </prov.body>
