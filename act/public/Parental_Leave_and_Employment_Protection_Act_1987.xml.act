      <prov id="DLM121099" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">71</label>
        <heading xml:lang="en-NZ">Procedures available to State employees</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Nothing in this Act shall limit the procedures by which State employees may enforce their conditions of employment and those procedures may be used, where appropriate, to enforce the rights conferred on State employees by this Act.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>1980, No 162, <citation jurisdiction="nz">s 47</citation></cf>
      </prov>
