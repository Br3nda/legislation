      <prov id="DLM121503" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">71A</label>
        <heading xml:lang="en-NZ">Purpose</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The purpose of this Part is to entitle certain employees and self-employed persons to up to 14 weeks of parental leave payments out of public money when they take parental leave.</text>
            </para>
          </subprov>
        </prov.body>
