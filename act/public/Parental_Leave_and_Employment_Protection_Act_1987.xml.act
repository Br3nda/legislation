      <prov id="DLM121577" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">71H</label>
        <heading xml:lang="en-NZ">Joint adoptions</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>If 2 spouses <insertwords>or partners</insertwords> assume the care of a child with a view to adoption by them both jointly,—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the spouses <insertwords>or partners</insertwords> must jointly nominate which 1 of them is to be primarily entitled to the parental leave payment; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
