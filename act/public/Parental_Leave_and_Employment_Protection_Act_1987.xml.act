              <def-para id="DLM120178" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM120179" xml:lang="en-NZ">partner's/paternity leave</def-term>—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>means partner's/paternity leave to which an employee is entitled in accordance with this Act; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>includes, for the purposes of <citation jurisdiction="nz">Parts <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="209086" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 6 Protection of employment" atidlm:xmlId="DLM654646" atidlm:state="UNCHANGED"><atidlm:linkcontent>VI to 7A</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="212212" atidlm:sourceXmlId="DLM654646" atidlm:targetKey="659600" atidlm:targetXmlId="DLM121019" atidlm:targetName="part 6 Protection of employment" atidlm:sourceName="DLM654646" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM121019.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM120115.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[19]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para partner's/paternity leave" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM120103" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="129" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Parental Leave and Employment Protection Act 1987" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Leg