      <prov id="DLM120484" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">16</label>
        <heading xml:lang="en-NZ">Ability to perform work</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Where, by reason of pregnancy, a female employee is unable to perform her work to the safety of herself or others or is incapable of performing her work adequately, her employer may temporarily transfer her from one job to another.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz">1980 No 162 s 24</citation>
        </cf>
      </prov>
    </part>
    <part id="DLM120485" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-03" act.no="129" act.type="public" date.as.at="2007-09-03" date.assent="1987-07-10" stage="reprint" year="1987">
