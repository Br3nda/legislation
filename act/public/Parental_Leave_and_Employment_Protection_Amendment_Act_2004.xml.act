      </prov>
      <prov id="DLM316668" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">5</label>
        <heading xml:lang="en-NZ">Long Title</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The Long Title of the principal Act is amended by omitting the words <amend.in>12 weeks</amend.in>, and substituting the words <amend.in>13 weeks</amend.in>.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
