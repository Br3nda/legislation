        </cf>
      </prov>
      <crosshead id="DLM55890" toc="yes" xml:lang="en-NZ">Further provisions relating to Parliamentary Service</crosshead>
      <prov id="DLM55891" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">12</label>
        <heading xml:lang="en-NZ">Further provisions relating to Parliamentary Service</heading>
        <prov.body>
