        <prov id="DLM56352" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">15</label>
          <heading xml:lang="en-NZ">Functions of State Services Commissioner in relation to Parliamentary Service</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
