      <empowering-prov>
        <citation jurisdiction="nz">s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86588" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 125" atidlm:parentFolderId="/NZ/NZAct/P/Parole Act 2002" atidlm:xmlId="DLM533211" atidlm:state="UNCHANGED"><atidlm:linkcontent>125</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88038" atidlm:sourceXmlId="DLM533211" atidlm:targetKey="784636" atidlm:targetXmlId="DLM140118" atidlm:targetName="prov 125" atidlm:sourceName="DLM533211" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM140119.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM140119" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
