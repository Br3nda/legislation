        <heading xml:lang="en-NZ">Rules about court practice and procedure</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The Governor-General may from time to time, by Order in Council, make rules regulating the practice and procedure of courts in proceedings under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86522" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 107I" atidlm:xmlId="DLM533045" atidlm:state="UNCHANGED"><atidlm:linkcontent>107I</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87891" atidlm:sourceXmlId="DLM533045" atidlm:targetKey="783629" atidlm:targetXmlId="DLM139630" atidlm:targetName="prov 107I" atidlm:sourceName="DLM533045" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139601.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139601.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139664" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86523" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 107M" atidlm:xmlId="DLM533046" atidlm:state="UNCHANGED"><atidlm:linkcontent>107M</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87892" atidlm:sourceXmlId="DLM533046" atidlm:targetKey="783643" atidlm:targetXmlId="DLM139644" atidlm:targetName="prov 107M" atidlm:sourceName="DLM533046" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139601.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139601.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139664" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, or section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86524" atidlm:branchLabel="_NOVERSION_" atidlm:nam