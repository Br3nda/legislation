      <prov id="DLM139663" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">107T</label>
        <heading xml:lang="en-NZ">Offence to breach extended supervision order</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>An offender who is subject to an extended supervision order and who breaches, without reasonable excuse, any conditions attaching to that order commits an offence and is liable on summary conviction to imprisonment for a term not exceeding 2 years.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
