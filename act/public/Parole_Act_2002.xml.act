        <prov id="DLM138808" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">32</label>
          <heading xml:lang="en-NZ">When parole ends</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>An offender who has been released on parole ceases to be on parole—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>when the offender reaches his or her statutory release date (as determined under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85512" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 17" atidlm:xmlId="DLM532039" atidlm:state="UNCHANGED"><atidlm:linkcontent>17(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="86904" atidlm:sourceXmlId="DLM532039" atidlm:targetKey="781043" atidlm:targetXmlId="DLM138478" atidlm:targetName="prov 17" atidlm:sourceName="DLM532039" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM138808" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>); or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>if the offender resumes detention in a <insertwords>prison</insertwords> under a final recall order.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85513" atidlm:branchLabel="_NOVERSION_" atidlm:name="Criminal Justice Act 1985" atidlm:xmlId="DLM532040" atidlm:state="UNCHANGED"><atidlm:linkcontent>1985 No 120</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="86905" atidlm:sourceXmlId="DLM532040" atidlm:targetKey="442200" atidlm:targetXmlId="DLM76615" atidlm:targetName="Criminal Justice Act 1985" atidlm:sourceName="DLM532040" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM76615.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM138808" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85514" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 107L" atidlm:xmlId="DLM532041" atidlm:state="UNCHANGED"><atidlm:linkcontent>107L(8)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="86906" atidlm:sourceXmlId="DLM532041" atidlm:targetKey="447950" atidlm:targetXmlId="DLM78502" atidlm:targetName="prov 107L" atidlm:sourceName="DLM532041" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM78252.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM138808" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
