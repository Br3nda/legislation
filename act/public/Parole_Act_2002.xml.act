                <text>In the case of an extradition offender (as defined in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85928" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 93" atidlm:xmlId="DLM532553" atidlm:state="UNCHANGED"><atidlm:linkcontent>93</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87449" atidlm:sourceXmlId="DLM532553" atidlm:targetKey="781249" atidlm:targetXmlId="DLM139367" atidlm:targetName="prov 93" atidlm:sourceName="DLM532553" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>), <def-term id="DLM139348" xml:lang="en-NZ">pre-sentence detention</def-term> includes detention in custody overseas in relation to a request for extradition, as recorded on a certificate obtained under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85929" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 62" atidlm:xmlId="DLM532554" atidlm:state="UNCHANGED"><atidlm:linkcontent>62</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87450" atidlm:sourceXmlId="DLM532554" atidlm:targetKey="89002" atidlm:targetXmlId="DLM26733" atidlm:targetName="prov 62" atidlm:sourceName="DLM532554" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM26731.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85930" atidlm:branchLabel="_NOVERSION_" atidlm:name="Extradition Act 1999" atidlm:xmlId="DLM532555" atidlm:state="UNCHANGED"><atidlm:linkcontent>Extradition Act 1999</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="87451" atidlm:sourceXmlId="DLM532555" atidlm:targetKey="82400" atidlm:targetXmlId="DLM25627" atidlm:targetName="Extradition Act 1999" atidlm:sourceName="DLM532555" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM25627.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> (including any substitute certificate obtained under that section).</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">4</label>
              <para xml:lang="en-NZ">
                <text>In the case of an ICC offender (as defined in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85931" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 93" atidlm:xmlId="DLM532556" atidlm:state="UNCHANGED"><atidlm:linkcontent>93</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87452" atidlm:sourceXmlId="DLM532556" atidlm:targetKey="781249" atidlm:targetXmlId="DLM139367" atidlm:targetName="prov 93" atidlm:sourceName="DLM532556" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>), <def-term id="DLM139349" xml:lang="en-NZ">pre-sentence detention</def-term> includes detention in custody overseas, as recorded on a certificate obtained under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85932" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 99" atidlm:xmlId="DLM532557" atidlm:state="UNCHANGED"><atidlm:linkcontent>99</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87453" atidlm:sourceXmlId="DLM532557" atidlm:targetKey="340631" atidlm:targetXmlId="DLM64495" atidlm:targetName="prov 99" atidlm:sourceName="DLM532557" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM64464.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85933" atidlm:branchLabel="_NOVERSION_" atidlm:name="International Crimes and International Criminal Court Act 2000" atidlm:xmlId="DLM532558" atidlm:state="UNCHANGED"><atidlm:linkcontent>International Crimes and International Criminal Court Act 2000</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="87454" atidlm:sourceXmlId="DLM532558" atidlm:targetKey="335400" atidlm:targetXmlId="DLM63090" atidlm:targetName="International Crimes and International Criminal Court Act 2000" atidlm:sourceName="DLM532558" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM63090.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> (including any substitute certificate obtained under that section).</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">4A</label>
              <para xml:lang="en-NZ">
                <text>In the case of a MACM offender (as defined in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85934" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 93" atidlm:xmlId="DLM532559" atidlm:state="UNCHANGED"><atidlm:linkcontent>93</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87455" atidlm:sourceXmlId="DLM532559" atidlm:targetKey="781249" atidlm:targetXmlId="DLM139367" atidlm:targetName="prov 93" atidlm:sourceName="DLM532559" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>), <def-term id="DLM139350" xml:lang="en-NZ">pre-sentence detention</def-term> includes detention in custody overseas, as recorded on a certificate obtained under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85935" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 41B" atidlm:xmlId="DLM532560" atidlm:state="UNCHANGED"><atidlm:linkcontent>41B</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87456" atidlm:sourceXmlId="DLM532560" atidlm:targetKey="1422809" atidlm:targetXmlId="DLM274450" atidlm:targetName="prov 41B" atidlm:sourceName="DLM532560" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM274075.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85936" atidlm:branchLabel="_NOVERSION_" atidlm:name="Mutual Assistance in Criminal Matters Act 1992" atidlm:xmlId="DLM532561" atidlm:state="UNCHANGED"><atidlm:linkcontent>Mutual Assistance in Criminal Matters Act 1992</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="87457" atidlm:sourceXmlId="DLM532561" atidlm:targetKey="1420005" atidlm:targetXmlId="DLM273056" atidlm:targetName="Mutual Assistance in Criminal Matters Act 1992" atidlm:sourceName="DLM532561" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM273056.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> (including any substitute certificate obtained under that section).</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">5</label>
              <para xml:lang="en-NZ">
                <text>Detention that would, under <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85937" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 91" atidlm:xmlId="DLM532562" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87458" atidlm:sourceXmlId="DLM532562" atidlm:targetKey="781228" atidlm:targetXmlId="DLM139346" atidlm:targetName="prov 91" atidlm:sourceName="DLM532562" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85938" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 91" atidlm:xmlId="DLM532563" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87459" atidlm:sourceXmlId="DLM532563" atidlm:targetKey="781228" atidlm:targetXmlId="DLM139346" atidlm:targetName="prov 91" atidlm:sourceName="DLM532563" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, be pre-sentence detention, is not pre-sentence detention for the purposes of <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85939" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 91" atidlm:xmlId="DLM532564" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87460" atidlm:sourceXmlId="DLM532564" atidlm:targetKey="781228" atidlm:targetXmlId="DLM139346" atidlm:targetName="prov 91" atidlm:sourceName="DLM532564" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> if the offender was, during that detention,—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>under legal custody in accordance with the <citation jurisdiction="nz">Corrections Act 2004</citation> and serving a sentence of imprisonment; or</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>in the case of an extradition offender, detained in custody under a sentence for an offence imposed under the law of the country from where the offender was extradited under the request for extradition.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">6</label>
              <para xml:lang="en-NZ">
                <text>In <citation jurisdiction="nz">subsection (5)(a)</citation>, serving a sentence of imprisonment includes time spent in a prison following an application for a recall order, but only if—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>a final recall order is made following that application; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>the offender was not, immediately before the application for the recall order was made, subject to conditions related to the offender's home detention under a direction of the Board given before the commencement of <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="640032" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1045025 - prov 72" atidlm:xmlId="DLM1045467" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 72</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="550996" atidlm:sourceXmlId="DLM1045467" atidlm:targetKey="2567627" atidlm:targetXmlId="DLM411451" atidlm:targetName="prov 72" atidlm:sourceName="DLM1045467" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM411030.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the Parole Amendment Act 2007</citation> or in accordance with that section, whether those conditions were suspended or not.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85941" atidlm:branchLabel="_NOVERSION_" atidlm:name="Criminal Justice Act 1985" atidlm:xmlId="DLM532566" atidlm:state="UNCHANGED"><atidlm:linkcontent>1985 No 120</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="87462" atidlm:sourceXmlId="DLM532566" atidlm:targetKey="442200" atidlm:targetXmlId="DLM76615" atidlm:targetName="Criminal Justice Act 1985" atidlm:sourceName="DLM532566" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM76615.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> ss <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85942" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 81" atidlm:xmlId="DLM532567" atidlm:state="UNCHANGED"><atidlm:linkcontent>81(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87463" atidlm:sourceXmlId="DLM532567" atidlm:targetKey="445869" atidlm:targetXmlId="DLM78205" atidlm:targetName="prov 81" atidlm:sourceName="DLM532567" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM77936.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139318.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139346" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85943" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 81" atidlm:xmlId="DLM532568" atidlm:state="UNCHANGED"><atidlm:linkcontent>(8)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupTyp