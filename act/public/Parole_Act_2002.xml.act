      <prov id="DLM139691" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">110</label>
        <heading xml:lang="en-NZ">Department of Corrections to provide administrative and training support to Board</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The chief executive must ensure that the Board and the chairperson are provided with the administrative and training support necessary to enable them to perform their functions efficiently and effectively.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The support must, without limitation, include the following:</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>support on a regional basis for parole panels operating in each region:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>co-ordination of the timetable for, and membership of, parole panels:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>support for the Board in performing its functions under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86561" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 109" atidlm:xmlId="DLM533084" atidlm:state="UNCHANGED"><atidlm:linkcontent>109(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88011" atidlm:sourceXmlId="DLM533084" atidlm:targetKey="784603" atidlm:targetXmlId="DLM139685" atidlm:targetName="prov 109" atidlm:sourceName="DLM533084" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139691" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>support for the chairperson in performing his or her functions under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86562" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 112" atidlm:xmlId="DLM533085" atidlm:state="UNCHANGED"><atidlm:linkcontent>112</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88012" atidlm:sourceXmlId="DLM533085" atidlm:targetKey="784613" atidlm:targetXmlId="DLM139695" atidlm:targetName="prov 112" atidlm:sourceName="DLM533085" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139691" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">e</label>
                <para xml:lang="en-NZ">
                  <text>support for panel convenors in performing their functions under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86563" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 114" atidlm:xmlId="DLM533086" atidlm:state="UNCHANGED"><atidlm:linkcontent>114(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88013" atidlm:sourceXmlId="DLM533086" atidlm:targetKey="784615" atidlm:targetXmlId="DLM139697" atidlm:targetName="prov 114" atidlm:sourceName="DLM533086" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139691" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">f</label>
                <para xml:lang="en-NZ">
                  <text>making available appropriate induction and training for Board members.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <crosshead id="DLM139692" toc="yes" xml:lang="en-NZ">Membership of Board</crosshead>
      <prov id="DLM139693" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">111</label>
        <heading xml:lang="en-NZ">Membership of Board</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The Board consists of members who are appointed by the Governor-General on the recommendation of the Attorney-General.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The Board must include—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>1 member who is appointed as chairperson; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>at least 9 members who are appointed as panel convenors; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>sufficient other members to enable the Board to carry out its functions efficiently and effectively.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>Before recommending a person as a member, the Attorney-General must be satisfied that the person has—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>knowledge or understanding of the criminal justice system; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the ability to make a balanced and reasonable assessment of the risk an offender may present to the community when released from detention; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>the ability to operate effectively with people from a range of cultures; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>sensitivity to, and understanding of, the impact of crime on victims <insertwords>(as defined in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86564" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 4" atidlm:xmlId="DLM533087" atidlm:state="UNCHANGED"><atidlm:linkcontent>4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88014" atidlm:sourceXmlId="DLM533087" atidlm:targetKey="879436" atidlm:targetXmlId="DLM157820" atidlm:targetName="prov 4" atidlm:sourceName="DLM533087" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM157817.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139693" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86565" atidlm:branchLabel="_NOVERSION_" atidlm:name="Victims' Rights Act 2002" atidlm:xmlId="DLM533088" atidlm:state="UNCHANGED"><atidlm:linkcontent>Victims' Rights Act 2002</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="88015" atidlm:sourceXmlId="DLM533088" atidlm:targetKey="879006" atidlm:targetXmlId="DLM157812" atidlm:targetName="Victims' Rights Act 2002" atidlm:sourceName="DLM533088" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM157812.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139693" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>)</insertwords>.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <notes>
          <history>
            <history-note id="DLM139694">Subsection <amended-provision>(3)(d)</amended-provision> was <amending-operation>amended</amending-operation>, as from <amendment-date>17 December 2002</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86566" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 53" atidlm:xmlId="DLM533089" atidlm:state="UNCHANGED"><atidlm:linkcontent>53</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="88016" atidlm:sourceXmlId="DLM533089" atidlm:targetKey="881812" atidlm:targetXmlId="DLM158562" atidlm:targetName="prov 53" atidlm:sourceName="DLM533089" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM158549.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139694" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Victims' Rights Act 2002</amending-leg> (2002 No 39) <amending-instruction>by inserting the words <quote.in>(as defined in <citation jurisdiction="nz">section 4 of the Victims' Rights Act 2002</citation>)</quote.in></amending-instruction>.</history-note>
          </history>
        </notes>
      </prov>
      <prov id="DLM139695" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">112</label>
        <heading xml:lang="en-NZ">Chairperson of Board</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The member appointed as chairperson must be a Judge of the High Court or a former Judge of the High Court, or a District Court Judge or a former District Court Judge.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The primary function of the chairperson is to ensure that the Board carries out its functions in accordance with this Act in an efficient and effective manner.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>Without limiting the function described in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86567" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 112" atidlm:xmlId="DLM533090" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88017" atidlm:sourceXmlId="DLM533090" atidlm:targetKey="784613" atidlm:targetXmlId="DLM139695" atidlm:targetName="prov 112" atidlm:sourceName="DLM533090" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139695" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, it is also the function of the chairperson to—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>make referrals to the Board for early release under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86568" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 25" atidlm:xmlId="DLM533091" atidlm:state="UNCHANGED"><atidlm:linkcontent>25</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88018" atidlm:sourceXmlId="DLM533091" atidlm:targetKey="781056" atidlm:targetXmlId="DLM138491" atidlm:targetName="prov 25" atidlm:sourceName="DLM533091" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139695" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>make referrals to the Board for compassionate release under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86569" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 41" atidlm:xmlId="DLM533092" atidlm:state="UNCHANGED"><atidlm:linkcontent>41</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88019" atidlm:sourceXmlId="DLM533092" atidlm:targetKey="781097" atidlm:targetXmlId="DLM138832" atidlm:targetName="prov 41" atidlm:sourceName="DLM533092" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139695" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>make interim recall orders under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86570" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 62" atidlm:xmlId="DLM533093" atidlm:state="UNCHANGED"><atidlm:linkcontent>62</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88020" atidlm:sourceXmlId="DLM533093" atidlm:targetKey="781150" atidlm:targetXmlId="DLM138885" atidlm:targetName="prov 62" atidlm:sourceName="DLM533093" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139695" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>allocate panel convenors to undertake reviews of decisions under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86571" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 67" atidlm:xmlId="DLM533094" atidlm:state="UNCHANGED"><atidlm:linkcontent>67</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88021" atidlm:sourceXmlId="DLM533094" atidlm:targetKey="781167" atidlm:targetXmlId="DLM139302" atidlm:targetName="prov 67" atidlm:sourceName="DLM533094" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139695" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">e</label>
                <para xml:lang="en-NZ">
                  <text>do anything else that the chairperson is required to do under this Act or any other enactment.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>The chairperson may sit as a member (including as a panel convenor) at any panel hearing.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM139696" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">113</label>
        <heading xml:lang="en-NZ">Delegation of chairperson's functions, powers, and duties</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The chairperson may delegate any of his or her functions or powers to any 1 or more panel convenors.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Every delegation must be in writing and may be revoked at will, either in writing or orally.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>A delegation under this section may not be further delegated.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>For the purpose of ensuring that the functions and powers of the chairperson are performed or exercised during any period when the chairperson is absent or incapacitated, the chairperson must ensure that at all times he or she has nominated 1 panel convenor to serve as acting chairperson if the need arises.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">5</label>
            <para xml:lang="en-NZ">
              <text>The fact that a person purports to exercise, or to have exercised, any function or power of the chairperson under a delegation or nomination is, in the absence of proof to the contrary, sufficient evidence of the person's authority to do so.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM139697" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">114</label>
        <heading xml:lang="en-NZ">Panel convenors</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Every member who is appointed as a panel convenor must be a District Court Judge, a former District Court Judge, or a barrister or solicitor who has held a practising certificate for at least 7 years.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Despite <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86572" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 114" atidlm:xmlId="DLM533095" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88022" atidlm:sourceXmlId="DLM533095" atidlm:targetKey="784615" atidlm:targetXmlId="DLM139697" atidlm:targetName="prov 114" atidlm:sourceName="DLM533095" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139697" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, at any time there may be appointed as a panel convenor 1 person who is a Judge of the High Court or a former Judge of the High Court.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>The functions of a panel convenor are—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>to make the decision on whether to hold an attended or an unattended hearing for any decision relating to <insertwords>release conditions or parole</insertwords>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>at a hearing,—</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">i</label>
                    <para xml:lang="en-NZ">
                      <text>to preside at the hearing; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">ii</label>
                    <para xml:lang="en-NZ">
                      <text>to determine any matters of procedure that may arise during or in relation to the hearing; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">iii</label>
                    <para xml:lang="en-NZ">
                      <text>to sign the decision of the panel at that hearing; and</text>
                    </para>
                  </label-para>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>to make interim recall orders under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86573" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 62" atidlm:xmlId="DLM533096" atidlm:state="UNCHANGED"><atidlm:linkcontent>62</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88023" atidlm:sourceXmlId="DLM533096" atidlm:targetKey="781150" atidlm:targetXmlId="DLM138885" atidlm:targetName="prov 62" atidlm:sourceName="DLM533096" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139697" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>to undertake reviews <insertwords>under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="640082" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1045076 - prov 46" atidlm:xmlId="DLM1045517" atidlm:state="UNCHANGED"><atidlm:linkcontent>sections 46(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="551048" atidlm:sourceXmlId="DLM1045517" atidlm:targetKey="781111" atidlm:targetXmlId="DLM138846" atidlm:targetName="prov 46" atidlm:sourceName="DLM1045517" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139697" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="640083" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1045077 - prov 67" atidlm:xmlId="DLM1045518" atidlm:state="UNCHANGED"><atidlm:linkcontent>67</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="551049" atidlm:sourceXmlId="DLM1045518" atidlm:targetKey="781167" atidlm:targetXmlId="DLM139302" atidlm:targetName="prov 67" atidlm:sourceName="DLM1045518" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139697" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></insertwords>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">e</label>
                <para xml:lang="en-NZ">
                  <text>to do anything else that a panel convenor is required to do under this Act or any other enactment; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">f</label>
                <para xml:lang="en-NZ">
                  <text>under the direction of the chairperson, to undertake or assist in the exercise of any of his or her functions.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>The chairperson may appoint any member (whether or not qualified to be a panel convenor) as an acting panel convenor in respect of a particular hearing or number of hearings if, for any reason, a panel convenor is not available for that hearing or those hearings.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">5</label>
            <para xml:lang="en-NZ">
              <text>If the chairperson sits on a parole panel, he or she may act as a panel convenor for the purposes of that sitting; and for that purpose the chairperson has all the functions and powers of a panel convenor.</text>
            </para>
          </subprov>
        </prov.body>
        <notes>
          <history>
            <history-note id="DLM1045326"><amended-provision>Section 114(3)(a)</amended-provision>: <amending-operation>amended</amending-operation>, on <amendment-date>1 October 2007</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="640084" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1045078 - prov 68" atidlm:xmlId="DLM1045519" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 68(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="551050" atidlm:sourceXmlId="DLM1045519" atidlm:targetKey="2567612" atidlm:targetXmlId="DLM411442" atidlm:targetName="prov 68" atidlm:sourceName="DLM1045519" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM411030.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1045326" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Parole Amendment Act 2007</amending-leg> (2007 No 28).</history-note>
            <history-note id="DLM1045327"><amended-provision>Section 114(3)(d)</amended-provision>: <amending-operation>amended</amending-operation>, on <amendment-date>1 October 2007</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="640085" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1045078 - prov 68" atidlm:xmlId="DLM1045520" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 68(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="551051" atidlm:sourceXmlId="DLM1045520" atidlm:targetKey="2567612" atidlm:targetXmlId="DLM411442" atidlm:targetName="prov 68" atidlm:sourceName="DLM1045520" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM411030.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1045327" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Parole Amendment Act 2007</amending-leg> (2007 No 28).</history-note>
          </history>
        </notes>
      </prov>
      <crosshead id="DLM139698" toc="yes" xml:lang="en-NZ">How Board performs its functions</crosshead>
      <prov id="DLM139699" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">115</label>
        <heading xml:lang="en-NZ">Parole panels</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The Board must operate in panels of at least 3 members, 1 of whom must be a panel convenor or the chairperson.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The decision of the majority of members on a panel is the decision of the panel.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86575" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 115" atidlm:xmlId="DLM533098" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88025" atidlm:sourceXmlId="DLM533098" atidlm:targetKey="784617" atidlm:targetXmlId="DLM139699" atidlm:targetName="prov 115" atidlm:sourceName="DLM533098" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139699" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is subject to any other provision of this Act that requires or permits the Board to perform its functions other than by way of panel hearings.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>Despite <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86576" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 115" atidlm:xmlId="DLM533099" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88026" atidlm:sourceXmlId="DLM533099" atidlm:targetKey="784617" atidlm:targetXmlId="DLM139699" atidlm:targetName="prov 115" atidlm:sourceName="DLM533099" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139699" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, if a member leaves a hearing for any reason, the remaining 2 members may continue the hearing and, provided the absent member participates in the final decision-making process, the validity of any resulting decision is not affected.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM140100" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">116</label>
        <heading xml:lang="en-NZ">Decisions of Board</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>A decision by a panel acting within its jurisdiction is a decision of the Board.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>A decision by the chairperson or a panel convenor, acting within their respective jurisdictions, is a decision of the Board.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>A decision of the Board on the detention or release of an offender, or on his or her release<struckoutwords/> conditions, must be in writing and include reasons for the decision.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>A copy of every decision of the Board that relates to an offender must be given to—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the offender to whom it relates; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>the Department of Corrections.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <notes>
          <history>
            <history-note id="DLM1045328"><amended-provision>Section 116(3)</amended-provision>: <amending-operation>amended</amending-operation>, on <amendment-date>1 October 2007</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="640086" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1045080 - prov 69" atidlm:xmlId="DLM1045521" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 69</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="551052" atidlm:sourceXmlId="DLM1045521" atidlm:targetKey="2567613" atidlm:targetXmlId="DLM411443" atidlm:targetName="prov 69" atidlm:sourceName="DLM1045521" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM411030.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1045328" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Parole Amendment Act 2007</amending-leg> (2007 No 28).</history-note>
          </history>
        </notes>
      </prov>
      <prov id="DLM140101" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">117</label>
        <heading xml:lang="en-NZ">Information before Board</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>In any hearing before the Board, the Board may receive and take into consideration whatever information it thinks fit, whether or not the information would be admissible as evidence in a court of law.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Information received by the Board may be in a form other than writing, but only if the Board is satisfied that—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>the information adds significantly to the written information available to it; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>it will be possible, in relation to that information, to comply with the rule in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86577" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 13" atidlm:xmlId="DLM533200" atidlm:state="UNCHANGED"><atidlm:linkcontent>13(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88027" atidlm:sourceXmlId="DLM533200" atidlm:targetKey="781021" atidlm:targetXmlId="DLM138456" atidlm:targetName="prov 13" atidlm:sourceName="DLM533200" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM140101" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> about making information available to offenders.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>This section is subject to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="713499" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1096865 - prov 13AA" atidlm:xmlId="DLM1098616" atidlm:state="UNCHANGED"><atidlm:linkcontent>sections 13AA to 13AE</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="576741" atidlm:sourceXmlId="DLM1098616" atidlm:targetKey="781026" atidlm:targetXmlId="DLM138461" atidlm:targetName="prov 13AA" atidlm:sourceName="DLM1098616" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM140101" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86578" atidlm:branchLabel="_NOVERSION_" atidlm:name="Criminal Justice Act 1985" atidlm:xmlId="DLM533201" atidlm:state="UNCHANGED"><atidlm:linkcontent>1985 No 120</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="88028" atidlm:sourceXmlId="DLM533201" atidlm:targetKey="442200" atidlm:targetXmlId="DLM76615" atidlm:targetName="Criminal Justice Act 1985" atidlm:sourceName="DLM533201" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM76615.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM140101" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> ss <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86579" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 106" atidlm:xmlId="DLM533202" atidlm:state="UNCHANGED"><atidlm:linkcontent>106</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88029" atidlm:sourceXmlId="DLM533202" atidlm:targetKey="447891" atidlm:targetXmlId="DLM78443" atidlm:targetName="prov 106" atidlm:sourceName="DLM533202" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM78252.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM140101" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="86580" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 107L" atidlm:xmlId="DLM533203" atidlm:state="UNCHANGED"><atidlm:linkcontent>107L(5)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="88030" atidlm:sourceXmlId="DLM533203" atidlm:targetKey="447950" atidlm:targetXmlId="DLM78502" atidlm:targetName="prov 107L" atidlm:sourceName="DLM533203" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM78252.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM139682.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM140101" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
        </cf>
