kupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="86780" atidlm:sourceXmlId="DLM531918" atidlm:targetKey="447914" atidlm:targetXmlId="DLM78466" atidlm:targetName="prov 107D" atidlm:sourceName="DLM531918" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM78252.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM138471" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
