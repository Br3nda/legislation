rkupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87266" atidlm:sourceXmlId="DLM532453" atidlm:targetKey="447954" atidlm:targetXmlId="DLM78506" atidlm:targetName="prov 107M" atidlm:sourceName="DLM532453" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM78252.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139304" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="85829" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 107M" atidlm:xmlId="DLM532454" atidlm:state="UNCHANGED"><atidlm:linkcontent>(6)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="87267" atidlm:sourceXmlId="DLM532454" atidlm:targetKey="447954" atidlm:targetXmlId="DLM78506" atidlm:targetName="prov 107M" atidlm:sourceName="DLM532454" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM78252.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM138435.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM139304" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
