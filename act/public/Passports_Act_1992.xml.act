                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>The <citation jurisdiction="nz">Passports Amendment Act 1987</citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">e</label>
                <para xml:lang="en-NZ">
