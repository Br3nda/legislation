s/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <struckoutwords/><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="24588" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 120" atidlm:xmlId="DLM471852" atidlm:state="UNCHANGED"><atidlm:linkcontent>120(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25260" atidlm:sourceXmlId="DLM471852" atidlm:targetKey="233427" atidlm:targetXmlId="DLM47116" atidlm:targetName="prov 120" atidlm:sourceName="DLM471852" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="24589" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 122" atidlm:xmlId="DLM471853" atidlm:state="UNCHANGED"><atidlm:linkcontent>122</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25261" atidlm:sourceXmlId="DLM471853" atidlm:targetKey="233429" atidlm:targetXmlId="DLM47118" atidlm:targetName="prov 122" atidlm:sourceName="DLM471853" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="24590" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 133" atidlm:xmlId="DLM471854" atidlm:state="UNCHANGED"><atidlm:linkcontent>133</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25262" atidlm:sourceXmlId="DLM471854" atidlm:targetKey="233442" atidlm:targetXmlId="DLM47131" atidlm:targetName="prov 133" atidlm:sourceName="DLM471854" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="24591" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 134" atidlm:xmlId="DLM471855" atidlm:state="UNCHANGED"><atidlm:linkcontent>134</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25263" atidlm:sourceXmlId="DLM471855" atidlm:targetKey="233444" atidlm:targetXmlId="DLM47133" atidlm:targetName="prov 134" atidlm:sourceName="DLM471855" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The parties to a security agreement may contract out of the debtor's right to—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>receive a statement of account under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="24592" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 116" atidlm:xmlId="DLM471856" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 116</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25264" atidlm:sourceXmlId="DLM471856" atidlm:targetKey="233422" atidlm:targetXmlId="DLM47111" atidlm:targetName="prov 116" atidlm:sourceName="DLM471856" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text/>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>receive notice of a secured party's proposal to retain collateral under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="24594" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 120" atidlm:xmlId="DLM471858" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 120(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25266" atidlm:sourceXmlId="DLM471858" atidlm:targetKey="233427" atidlm:targetXmlId="DLM47116" atidlm:targetName="prov 120" atidlm:sourceName="DLM471858" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">d</label>
                <para xml:lang="en-NZ">
                  <text>object to a secured party's proposal to retain collateral under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="24595" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 121" atidlm:xmlId="DLM471859" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 121</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25267" atidlm:sourceXmlId="DLM471859" atidlm:targetKey="233428" atidlm:targetXmlId="DLM47117" atidlm:targetName="prov 121" atidlm:sourceName="DLM471859" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">e</label>
                <para xml:lang="en-NZ">
                  <text>not have goods damaged when a secured party removes an accession under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="24596" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 125" atidlm:xmlId="DLM471860" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 125</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25268" atidlm:sourceXmlId="DLM471860" atidlm:targetKey="233433" atidlm:targetXmlId="DLM47122" atidlm:targetName="prov 125" atidlm:sourceName="DLM471860" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">f</label>
                <para xml:lang="en-NZ">
                  <text>not be reimbursed for damage caused when a secured party removes an accession under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="24597" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 126" atidlm:xmlId="DLM471861" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 126</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25269" atidlm:sourceXmlId="DLM471861" atidlm:targetKey="233434" atidlm:targetXmlId="DLM47123" atidlm:targetName="prov 126" atidlm:sourceName="DLM471861" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">g</label>
                <para xml:lang="en-NZ">
                  <text>refuse permission to remove an accession under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="24598" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 127" atidlm:xmlId="DLM471862" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 127</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25270" atidlm:sourceXmlId="DLM471862" atidlm:targetKey="233435" atidlm:targetXmlId="DLM47124" atidlm:targetName="prov 127" atidlm:sourceName="DLM471862" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">h</label>
                <para xml:lang="en-NZ">
                  <text>receive notice of the removal of an accession under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="25400" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 129" atidlm:xmlId="DLM471863" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 129</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25271" atidlm:sourceXmlId="DLM471863" atidlm:targetKey="233437" atidlm:targetXmlId="DLM47126" atidlm:targetName="prov 129" atidlm:sourceName="DLM471863" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">i</label>
                <para xml:lang="en-NZ">
                  <text>apply to the court for an order concerning the removal of an accession under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="25401" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 131" atidlm:xmlId="DLM471864" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 131</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25272" atidlm:sourceXmlId="DLM471864" atidlm:targetKey="233439" atidlm:targetXmlId="DLM47128" atidlm:targetName="prov 131" atidlm:sourceName="DLM471864" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                </para>
              </label-para>
              <label-para deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">j</label>
                <para xml:lang="en-NZ">
                  <text/>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>The parties to a security agreement may contract out of the secured party's right to apply to a court for an order in respect of the removal of an accession under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="25403" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 128" atidlm:xmlId="DLM471866" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 128</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="25274" atidlm:sourceXmlId="DLM471866" atidlm:targetKey="233436" atidlm:targetXmlId="DLM47125" atidlm:targetName="prov 128" atidlm:sourceName="DLM471866" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">4</label>
            <para xml:lang="en-NZ">
              <text>The provisions of this section, as in force immediately before the commencement of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1569812" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1845600 - Property Law Act 2007" atidlm:xmlId="DLM1848412" atidlm:state="UNCHANGED"><atidlm:linkcontent>Property Law Act 2007</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="875972" atidlm:sourceXmlId="DLM1848412" atidlm:targetKey="9887400" atidlm:targetXmlId="DLM968961" atidlm:targetName="Property Law Act 2007" atidlm:sourceName="DLM1848412" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM968961.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46798" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, continue to apply to a security agreement that was entered into before that commencement.</text>
            </para>
          </subprov>
        </prov.body>
        <notes>
          <history>
            <history-note id="DLM46799"><amended-provision>Section 107</amended-provision>: <amending-operation>substituted</amending-operation>, on <amendment-date>1 May 2002</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="25404" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 9" atidlm:xmlId="DLM471867" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 9</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="25275" atidlm:sourceXmlId="DLM471867" atidlm:targetKey="490424" atidlm:targetXmlId="DLM90777" atidlm:targetName="prov 9" atidlm:sourceName="DLM471867" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM90753.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM46789.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM46799" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Personal Property Securities Amendment Act 2001</amending-leg> (2001 No 23).</history-note>
