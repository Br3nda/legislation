                    <atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="525761" atidlm:sourceXmlId="DLM985001" atidlm:targetKey="225200" atidlm:targetXmlId="DLM45599" atidlm:targetName="Personal Property Securities Act 1999" atidlm:sourceName="DLM985001" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM45599.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM968448.xml" atidlm:state="UNCHANGED">
