<act date.instructed="2010-05-05" id="DLM3013302" in.amend="false" instructing.office="Police" irdnumbering="no" xml:lang="en-NZ" year.imprint="2010" act.no="43" act.type="public" date.as.at="2010-06-30" date.assent="2010-06-30" date.first.valid="2010-07-01" stage="in-force" year="2010" prospective.consolidation="F">
  <cover id="DLM3013304">
    <title>Policing (Involvement in Local Authority Elections) Amendment Act 2010</title>
    <assent>2010-06-30</assent>
    <commencement>see section 2</commencement>
  </cover>
  <contents>
    <heading>Contents</heading>
    <toc/>
  </contents>
  <front id="DLM3013305">
    <enactment>The Parliament of New Zealand enacts as follows:</enactment>
  </front>
  <body id="DLM3013306" prov-type="clause">
    <prov id="DLM3013307" irdnumbering="no" skeleton="no" toc="yes">
      <label auto.number="no" denominator="yes">1</label>
      <heading>Title</heading>
      <prov.body>
        <subprov skeleton="no">
          <label auto.number="no" denominator="yes"/>
          <para>
            <text>This Act is the Policing (Involvement in Local Authority Elections) Amendment Act 2010.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM3013308" irdnumbering="no" skeleton="no" toc="yes">
      <label auto.number="no" denominator="yes">2</label>
      <heading>Commencement</heading>
      <prov.body>
        <subprov skeleton="no">
          <label auto.number="no" denominator="yes"/>
          <para>
            <text>This Act comes into force on the day after the date on which it receives the Royal assent.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM3013309" irdnumbering="no" skeleton="no" toc="yes">
      <label auto.number="no" denominator="yes">3</label>
      <heading>Principal Act amended</heading>
      <prov.body>
        <subprov skeleton="no">
          <label auto.number="no" denominator="yes"/>
          <para>
            <text>This Act amends the <citation jurisdiction="nz"><extref href="DLM1102100">Policing Act 2008</extref></citation>.</text>
          </para>
        </subprov>
      </prov.body>
      <notes>
        <amends-note>
          <amends-effective>
            <date>2010-07-01</date>
          </amends-effective>
          <amends-affect>
            <citation jurisdiction="nz">
              <extref href="DLM1102100">Policing Act 2008</extref>
            </citation>
          </amends-affect>
        </amends-note>
      </notes>
    </prov>
    <prov id="DLM3013310" irdnumbering="no" skeleton="no" toc="yes">
      <label auto.number="no" denominator="yes">4</label>
      <heading>Police involvement in elections</heading>
      <prov.body>
        <subprov skeleton="no">
          <label auto.number="no" denominator="yes">1</label>
          <para>
            <text><citation jurisdiction="nz"><extref href="DLM1351215">Section 99(1)</extref></citation> is amended by omitting <amend.in quote="1">or a member of a local authority</amend.in>.</text>
          </para>
        </subprov>
        <subprov skeleton="no">
          <label auto.number="no" denominator="yes">2</label>
          <para>
            <text><citation jurisdiction="nz"><extref href="DLM1351215">Section 99(1)(b)</extref></citation> is amended by omitting <amend.in quote="1">or subsections (2) to (4) of this section</amend.in>.</text>
          </para>
        </subprov>
        <subprov skeleton="no">
          <label auto.number="no" denominator="yes">3</label>
          <para>
            <text><citation jurisdiction="nz"><extref href="DLM1351215">Section 99(2) to (4)</extref></citation> are repealed.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM3013311" irdnumbering="no" skeleton="no" toc="yes">
      <label auto.number="no" denominator="yes">5</label>
      <heading>Section 115 repealed</heading>
      <prov.body>
        <subprov skeleton="no">
          <label auto.number="no" denominator="yes"/>
          <para>
            <text><citation jurisdiction="nz"><extref href="DLM1564702">Section 115</extref></citation> is repealed.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
  </body>
