    <schedule date.instructed="2008-07-04" id="DLM1526070" in.amend="false" instructing.office="Office of Treaty Settlements" irdnumbering="no" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" act.no="26" act.type="public" date.as.at="2009-08-04" date.assent="2009-08-04" date.first.valid="2009-08-05" stage="in-force" year="2009">
