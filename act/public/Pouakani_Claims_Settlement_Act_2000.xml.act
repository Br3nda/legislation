      <crosshead id="DLM80012" toc="yes" xml:lang="en-NZ">Memorandum of understanding</crosshead>
      <prov id="DLM80013" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">43</label>
        <heading xml:lang="en-NZ">Authorisation to enter into and amend memorandum of understanding</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The Minister of Conservation (as the Minister of the Crown responsible for the management or administration of the Crown MOU land) has power to enter into, and amend, the memorandum of understanding.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The memorandum of understanding may be amended only by written agreement between the Minister of Conservation and the Pouakani governance entity.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="55642" atidlm:branchLabel="_NOVERSION_" atidlm:name="Ngai Tahu Claims Settlement Act 1998" atidlm:xmlId="DLM502555" atidlm:state="UNCHANGED"><atidlm:linkcontent>1998 No 97</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="56640" atidlm:sourceXmlId="DLM502555" atidlm:targetKey="2393012" atidlm:targetXmlId="DLM429089" atidlm:targetName="Ngai Tahu Claims Settlement Act 1998" atidlm:sourceName="DLM502555" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM429089.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM79875.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[6]/prov[11]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Authorisation to enter into and amend memorandum of understanding" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="43" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM79573" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="90" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Pouakani Claims Settlement Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2000" atidlm:locale="xx_XX" atidlm:state="UNCHA