    <part id="DLM79852" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-10" act.no="90" act.type="public" date.as.at="2007-09-10" date.assent="2000-12-12" stage="reprint" year="2000">
