            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
                <text>TITIRO ARORANGI ATU</text>
              </para>
            </item>
            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
                <text>KI A PUREORA E TU MAI RA</text>
              </para>
            </item>
            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
                <text>EEEEE!</text>
              </para>
            </item>
            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
                <text>KEI RARO RA</text>
              </para>
            </item>
            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
                <text>KO TE HORA-ARUHE</text>
              </para>
            </item>
            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
                <text>KI TE POU-A-KANI</text>
              </para>
            </item>
            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
                <text>TIHEI MAURI ORA</text>
              </para>
            </item>
            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
                <text>TITIRAUPENGA</text>
              </para>
            </item>
            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
                <text>MY BIRD</text>
              </para>
            </item>
            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
                <text>THAT SOARS TO THE SUMMITS</text>
              </para>
            </item>
            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
                <text>OF MY SACRED MOUNTAIN TITIRAUPENGA</text>
              </para>
            </item>
            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
                <text>AND SURVEYS</text>
              </para>
            </item>
            <item>
              <label auto.number="no" denominator="yes"/>
              <para xml:lang="en-NZ">
