      <prov id="DLM297912" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">96</label>
        <heading xml:lang="en-NZ">Proceedings privileged</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>This section applies to—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>The Commissioner; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>Every person engaged or employed in connection with the work of the Commissioner.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Subject to <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="398435" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 96" atidlm:xmlId="DLM842695" atidlm:state="UNCHANGED"><atidlm:linkcontent>(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="405190" atidlm:sourceXmlId="DLM842695" atidlm:targetKey="1546210" atidlm:targetXmlId="DLM297912" atidlm:targetName="prov 96" atidlm:sourceName="DLM842695" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM297902.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM297902.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM297912" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of this section</citation>,—</text>
              <label-para deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>No person to whom this section applies shall be required to give evidence in any court, or in any proceedings of a judicial nature, in respect of anything coming to his or her knowledge in the exercise of his or her functions.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>Nothing in <citation jurisdiction="nz">subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="398436" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 96" atidlm:xmlId="DLM842696" atidlm:state="UNCHANGED"><atidlm:linkcontent>(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="405191" atidlm:sourceXmlId="DLM842696" atidlm:targetKey="1546210" atidlm:targetXmlId="DLM297912" atidlm:targetName="prov 96" atidlm:sourceName="DLM842696" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM297902.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM297902.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM297912" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of this section</citation> applies in respect of proceedings for—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>An offence against <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="398437" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 78" atidlm:xmlId="DLM842697" atidlm:state="UNCHANGED"><atidlm:linkcontent>78</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="405192" atidlm:sourceXmlId="DLM842697" atidlm:targetKey="2137885" atidlm:targetXmlId="DLM328526" atidlm:targetName="prov 78" atidlm:sourceName="DLM842697" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM328518.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM297902.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM297912" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="398438" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 78A" atidlm:xmlId="DLM842698" atidlm:state="UNCHANGED"><atidlm:linkcontent>78A(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="405193" atidlm:sourceXmlId="DLM842698" atidlm:targetKey="2137899" atidlm:targetXmlId="DLM328528" atidlm:targetName="prov 78A" atidlm:sourceName="DLM842698" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM328518.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM297902.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM297912" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="398439" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 105" atidlm:xmlId="DLM842699" atidlm:state="UNCHANGED"><atidlm:linkcontent>105</atidlm:linkcontent><atidlm:resou