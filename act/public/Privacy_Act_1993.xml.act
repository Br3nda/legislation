me="DLM842693" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM101320.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM297902.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM297911" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="398434" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 24" atidlm:xmlId="DLM842694" atidlm:state="UNCHANGED"><atidlm:linkcontent>24(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="405189" atidlm:sourceXmlId="DLM842694" atidlm:targetKey="566032" atidlm:targetXmlId="DLM101352" atidlm:targetName="prov 24" atidlm:sourceName="DLM842694" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM101320.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM297902.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM297911" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; <citation jurisdiction="nz">1991 No 126 ss 26(2), (3), 27</citation></cf>
