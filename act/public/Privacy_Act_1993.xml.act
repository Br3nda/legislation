eOrRef="1" atidlm:seqNum="404197" atidlm:sourceXmlId="DLM841817" atidlm:targetKey="665800" atidlm:targetXmlId="DLM122241" atidlm:targetName="Local Government Official Information and Meetings Act 1987" atidlm:sourceName="DLM841817" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM122241.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM296644.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM296645" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="397255" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 2" atidlm:xmlId="DLM841818" atidlm:state="UNCHANGED"><atidlm:linkcontent>2</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="404198" atidlm:sourceXmlId="DLM841818" atidlm:targetKey="665806" atidlm:targetXmlId="DLM122247" atidlm:targetName="prov 2" atidlm:sourceName="DLM841818" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM122241.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM296644.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM296645" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></cf>
        <notes>
          <history>
            <history-note id="DLM297011">Personal information: this definition was <amending-operation>substituted</amending-operation>, as from <amendment-date>3 September 1996</amendment-date>, by section 2(3) <amending-leg>Privacy Amendment Act 1996</amending-leg> (1996 No 142).</history-note>
            <history-note id="DLM1810525"><amended-provision>Section 2</amended-provision><emphasis style="bold">personal information</emphasis>: <amending-operation>amended</amending-operation>, on <amendment-date>24 January 2009</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1518651" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1807145 - prov 47" atidlm:xmlId="DLM1810561" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 47</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="859412" atidlm:sourceXmlId="DLM1810561" atidlm:targetKey="10431401" atidlm:targetXmlId="DLM1048943" atidlm:targetName="prov 47" atidlm:sourceName="DLM1810561" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1048942.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM296644.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1810525" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <amending-leg>Births, Deaths, Marriages, and Relationships Registration Amendment Act 2008</amending-leg> (2008 No 48).</history-note>
