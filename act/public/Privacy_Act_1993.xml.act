                  <entry colname="col1" colsep="0" rowsep="0"/>
                  <entry colname="col3" rowsep="0"/>
                </row>
                <row>
                  <entry colname="col1" colsep="0" rowsep="0"/>
                  <entry colname="col3" rowsep="0"/>
                </row>
                <row rowsep="0">
                  <entry colname="col1" colsep="0"/>
                  <entry colname="col2" colsep="0"/>
                  <entry colname="col3">
                    <insertwords>Legal Services Agency, for the purpose of determining an application for a grant of legal aid in relation to a criminal matter</insertwords>
                  </entry>
                </row>
                <row>
                  <entry colname="col1" colsep="0">Non-performance of bail conditions</entry>
                  <entry colname="col2" colsep="0">Records relating to failure to comply with bail conditions entered under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="399536" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 38" atidlm:xmlId="DLM843793" atidlm:state="UNCHANGED"><atidlm:linkcontent>38(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="406346" atidlm:sourceXmlId="DLM843793" atidlm:targetKey="360624" atidlm:targetXmlId="DLM68962" atidlm:targetName="prov 38" atidlm:sourceName="DLM843793" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM68938.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM298798.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM299163" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="399537" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 63" atidlm:xmlId="DLM843794" atidlm:state="UNCHANGED"><atidlm:linkcontent>63(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="406347" atidlm:sourceXmlId="DLM843794" atidlm:targetKey="361825" atidlm:targetXmlId="DLM69610" atidlm:targetName="prov 63" atidlm:sourceName="DLM843794" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM68985.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM298798.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM299163" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="399538" atidlm:branchLabel="_NOVERSION_" atidlm:name="Bail Act 2000" atidlm:xmlId="DLM843795" atidlm