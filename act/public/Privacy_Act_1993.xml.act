        <label auto.number="no" denominator="yes">121</label>
        <heading xml:lang="en-NZ">Delegation of functions or powers of Commissioner</heading>
        <prov.body/>
        <notes>
          <history>
            <history-note id="DLM298426"><amended-provision>Section 121</amended-provision>: <amending-operation>repealed</amending-operation>, on <amendment-date>25 January 2005</amendment-date>, by <amending-provision href="DLM331111">section 200</amending-provision> of the <amending-leg>Crown Entities Act 2004</amending-leg> (2004 No 115).</history-note>
          </history>
        </notes>
      </prov>
      <prov deletion-status="repealed" id="DLM298427" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
