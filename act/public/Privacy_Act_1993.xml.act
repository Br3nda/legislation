        <prov deletion-status="repealed" id="DLM298448" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <heading xml:lang="en-NZ">Staff</heading>
          <prov.body/>
