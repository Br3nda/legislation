tidlm:targetName="Local Government Official Information and Meetings Act 1987" atidlm:sourceName="DLM843108" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM122241.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM298408.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM298431" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="398850" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 42" atidlm:xmlId="DLM843109" atidlm:state="UNCHANGED"><atidlm:linkcontent>42</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="405570" atidlm:sourceXmlId="DLM843109" atidlm:targetKey="670002" atidlm:targetXmlId="DLM123059" atidlm:targetName="prov 42" atidlm:sourceName="DLM843109" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM123057.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM298408.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM298431" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></cf>
