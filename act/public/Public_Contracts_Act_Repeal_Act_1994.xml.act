<act id="DLM332304" in.amend="false" instructing.office="Internal Affairs" irdnumbering="no" xml:lang="en-NZ" year.imprint="1994" act.no="55" act.type="public" date.as.at="1994-07-01" date.assent="1994-07-01" date.first.valid="1994-07-01" stage="in-force" year="1994">
  <cover id="DLM332305">
    <title xml:lang="en-NZ">Public Contracts Act Repeal Act 1994</title>
    <assent>1994-07-01</assent>
    <commencement>1 July 1994</commencement>
  </cover>
  <contents>
    <heading xml:lang="en-NZ">Contents</heading>
    <toc/>
  </contents>
  <front id="DLM332306" xml:lang="en-NZ">
    <long-title id="DLM332307" toc="yes" xml:lang="en-NZ">
      <para xml:lang="en-NZ">
        <text>An Act to repeal the Public Contracts Act 1908</text>
      </para>
    </long-title>
    <enactment>BE IT ENACTED by the Parliament of New Zealand as follows:</enactment>
  </front>
  <body id="DLM332308" prov-type="section" xml:lang="en-NZ">
    <prov id="DLM332309" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">1</label>
      <heading xml:lang="en-NZ">Short Title</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text>This Act may be cited as the Public Contracts Act Repeal Act 1994.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM332310" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">2</label>
      <heading xml:lang="en-NZ">Repeal</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text>The Public Contracts Act 1908 is hereby repealed.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
  </body>
  <end id="DLM2105602">
    <admin-office>This Act is administered by the <ministry>Department of Internal Affairs</ministry>.</admin-office>
  </end>
</act>
