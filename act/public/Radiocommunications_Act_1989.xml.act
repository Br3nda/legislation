      <crosshead id="DLM197978" toc="yes" xml:lang="en-NZ">Transitional rights in relation to AMPS B band</crosshead>
      <prov id="DLM197979" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
