            <def-para id="DLM196259" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM196260" xml:lang="en-NZ">radio receiver</def-term> means apparatus designed to receive radio waves for the purpose of radiocommunications</text>
              </para>
            </def-para>
            <def-para id="DLM196261" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM196262" xml:lang="en-NZ">radio transmitter</def-term> means apparatus designed to produce radio waves for the purpose of radiocommunications</text>
              </para>
            </def-para>
            <def-para id="DLM196263" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM196264" xml:lang="en-NZ">radio waves</def-term> means electromagnetic waves of frequencies lower than 3000 gigahertz, propagated in space without artificial guide</text>
              </para>
            </def-para>
