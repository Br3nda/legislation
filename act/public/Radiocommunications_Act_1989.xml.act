      <prov id="DLM197122" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">73</label>
        <heading xml:lang="en-NZ">Mortgage to take effect as security only</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>A mortgage under this Act shall have effect as security, but shall not operate as a transfer of the management rights or <insertwords>spectrum</insertwords> licence charged.</text>
            </para>
          </subprov>
        </prov.body>
