            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>Unless the Court orders otherwise, none of the following persons may be appointed or act as a receiver:</text>
                <label-para skeleton="no" xml:lang="en-NZ">
