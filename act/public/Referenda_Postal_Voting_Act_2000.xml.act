      <prov id="DLM74619" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">43</label>
        <heading xml:lang="en-NZ">Informal voting papers</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>A voting paper is informal if the Returning Officer is satisfied that the voting paper does not clearly indicate the response for which the elector wished to vote.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
