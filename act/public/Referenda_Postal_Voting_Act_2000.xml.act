      <prov id="DLM74648" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">58</label>
        <heading xml:lang="en-NZ">Application to be sent to Returning Officer</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The Registrar of the Court must send a copy of the application to the Returning Officer as soon as practicable after it is filed.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
