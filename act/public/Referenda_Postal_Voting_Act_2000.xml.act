          <empowering-prov>
            <citation jurisdiction="nz">s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="52721" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 49" atidlm:xmlId="DLM499438" atidlm:state="UNCHANGED"><atidlm:linkcontent>49</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="54024" atidlm:sourceXmlId="DLM499438" atidlm:targetKey="389433" atidlm:targetXmlId="DLM74634" atidlm:targetName="prov 49" atidlm:sourceName="DLM499438" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM74601.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM75070.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="form" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[1]/schedule.forms[1]/form[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="form" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Declaration of result of referendum" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="2" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM73880" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="48" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Referenda (Postal Voting) Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
          </empowering-prov>
          <heading xml:lang="en-NZ">Declaration of result of referendum</heading>
          <form.body>
            <para xml:lang="en-NZ">
              <table align="center" fontsize="11.5" frame="topbot" leading="13.5" xml:lang="en-NZ">
                <tgroup cols="3" colsep="0" rowsep="0">
                  <colspec colname="col1" colnum="1" colwidth="2.0*"/>
                  <colspec colname="col2" colnum="2" colwidth="1.5*"/>
                  <colspec colname="col3" colnum="3" colwidth="2.0*"/>
                  <thead>
                    <row rowsep="1">
                      <entry nameend="col3" namest="col1">I declare the result of the referendum taken over the period commencing on [<emphasis style="italic">Date</emphasis>] and ending on [<emphasis style="italic">Date</emphasis>] on the *proposal *precise question—</entry>
                    </row>
                  </thead>
                  <tbody>
                    <row>
                      <entry nameend="col3" namest="col1">[<emphasis style="italic">Specify proposal or precise question</emphasis>]</entry>
                    </row>
                    <row>
                      <entry>to be as follows:</entry>
                      <entry colname="col2"/>
                      <entry colname="col3"/>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">†Votes received for the response [<emphasis style="italic">Identify response</emphasis>]: [<emphasis style="italic">Number</emphasis>]</entry>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">Dated at [<emphasis style="italic">Place</emphasis>] on [<emphasis style="italic">Day</emphasis>] [<emphasis style="italic">Month</emphasis>][<emphasis style="italic">Year</emphasis>]</entry>
                    </row>
                    <row>
                      <entry/>
                      <entry>A.B.</entry>
                      <entry colname="col3"/>
                    </row>
                    <row>
                      <entry colname="col1"/>
                      <entry colname="col2">Returning Officer</entry>
                      <entry colname="col3"/>
                    </row>
                    <row>
                      <entry>NOTE:</entry>
                      <entry colname="col2"/>
                      <entry colname="col3"/>
                    </row>
                    <row>
                      <entry>*Delete if inapplicable.</entry>
                      <entry colname="col2"/>
                      <entry colname="col3"/>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">†Repeat for each response.</entry>
                    </row>
                  </tbody>
                </tgroup>
              </table>
            </para>
          </form.body>
        </form>
        <form id="DLM75073" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">3</label>
          <empowering-prov>
            <citation jurisdiction="nz">s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="52722" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 55" atidlm:xmlId="DLM499439" atidlm:state="UNCHANGED"><atidlm:linkcontent>55</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="54025" atidlm:sourceXmlId="DLM499439" atidlm:targetKey="390207" atidlm:targetXmlId="DLM74645" atidlm:targetName="prov 55" atidlm:sourceName="DLM499439" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM74638.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM75070.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="form" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[1]/schedule.forms[1]/form[3]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="form" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Application for inquiry into result of referendum" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="3" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM73880" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="48" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Referenda (Postal Voting) Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
          </empowering-prov>
          <heading xml:lang="en-NZ">Application for inquiry into result of referendum</heading>
          <form.body>
            <para xml:lang="en-NZ">
              <table align="center" fontsize="11.5" frame="none" leading="13.5" xml:lang="en-NZ">
                <tgroup cols="3" colsep="0" rowsep="0">
                  <colspec colname="col1" colnum="1" colwidth="0.5*"/>
                  <colspec colname="col2" colnum="2" colwidth="2.5*"/>
                  <colspec colname="col3" colnum="3" colwidth="2.5*"/>
                  <thead>
                    <row rowsep="1">
                      <entry nameend="col2" namest="col1" rowsep="0">In the High Court of New Zealand</entry>
                      <entry align="right" rowsep="0" valign="top">No</entry>
                    </row>
                  </thead>
                  <tbody>
                    <row>
                      <entry nameend="col2" namest="col1">Wellington Registry</entry>
                      <entry colname="col3"/>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">In the matter of a referendum held on [<emphasis style="italic">Date</emphasis>] on [<emphasis style="italic">Describe proposal or question</emphasis>].</entry>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">
                        <emphasis style="bold">Applicant</emphasis>
                      </entry>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">1. The applicant is a group of [<emphasis style="italic">Number in group</emphasis>] electors who are dissatisfied with the result of the referendum.</entry>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">2. The names, addresses, electoral districts, and signatures of each member of the group are set out in the appendix to this application.</entry>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">3. The spokesperson for the group is [<emphasis style="italic">Name of spokesperson</emphasis>] of [<emphasis style="italic">Address of spokesperson</emphasis>].</entry>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">4. The applicant acts—</entry>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">(a) through a solicitor, who is [<emphasis style="italic">Name of solicitor</emphasis>], of [<emphasis style="italic">Address of solicitor</emphasis>]; or</entry>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">(b) through its spokesperson.</entry>
                    </row>
                    <row>
                      <entry nameend="col2" namest="col1">[<emphasis style="italic">Delete one</emphasis>]</entry>
                      <entry colname="col3"/>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">
                        <emphasis style="bold">Application</emphasis>
                      </entry>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">5. The applicant asks for an inquiry into—</entry>
                    </row>
                    <row>
                      <entry colname="col1"/>
                      <entry nameend="col3" namest="col2">(a) the conduct of the referendum:</entry>
                    </row>
                    <row>
                      <entry colname="col1"/>
                      <entry nameend="col3" namest="col2">(b) the conduct of [<emphasis style="italic">Name of person complained of</emphasis>], [<emphasis style="italic">Office described in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="52723" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 54" atidlm:xmlId="DLM499440" atidlm:state="UNCHANGED"><atidlm:linkcontent>54</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="54026" atidlm:sourceXmlId="DLM499440" atidlm:targetKey="390205" atidlm:targetXmlId="DLM74643" atidlm:targetName="prov 54" atidlm:sourceName="DLM499440" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM74638.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM75070.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="form" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[1]/schedule.forms[1]/form[3]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="form" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Application for inquiry into result of referendum" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="3" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM73880" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="48" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Referenda (Postal Voting) Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, if relevant</emphasis>], of [<emphasis style="italic">Address of person complained of</emphasis>], who was connected with the referendum.</entry>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">[<emphasis style="italic">Delete those which are inapplicable</emphasis>]</entry>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">6. The specific grounds on which the applicant is dissatisfied with the result of the referendum are as follows: [<emphasis style="italic">State specific grounds</emphasis>].</entry>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">7. The applicant asks the court to—</entry>
                    </row>
                    <row>
                      <entry colname="col1"/>
                      <entry nameend="col3" namest="col2">(a) determine the total number of valid votes recorded for each response; or</entry>
                    </row>
                    <row>
                      <entry colname="col1"/>
                      <entry nameend="col3" namest="col2">(b) declare the referendum void.</entry>
                    </row>
                    <row>
                      <entry nameend="col2" namest="col1">[<emphasis style="italic">Delete one</emphasis>]</entry>
                      <entry colname="col3"/>
                    </row>
                    <row>
                      <entry nameend="col2" namest="col1">
                        <emphasis style="bold">Address for service</emphasis>
                      </entry>
                      <entry colname="col3"/>
                    </row>
                    <row>
                      <entry nameend="col3" namest="col1">8. The applicant's address for service is [<emphasis style="italic">Address</emphasis>].</entry>
                    </row>
                    <row>
                      <entry colname="col1"/>
                      <entry nameend="col3" namest="col2">
                        <emphasis style="bold">Signature</emphasis>
                      </entry>
                    </row>
                    <row>
                      <entry>9.</entry>
                      <entry>Spokesperson for applicant; or</entry>
                      <entry colname="col3"/>
                    </row>
                    <row>
                      <entry colname="col1"/>
                      <entry nameend="col3" namest="col2">Person on behalf of spokesperson for applicant</entry>
                    </row>
                    <row>
                      <entry nameend="col2" namest="col1">[<emphasis style="italic">Delete one</emphasis>]</entry>
                      <entry colname="col3"/>
                    </row>
                  </tbody>
                </tgroup>
              </table>
            </para>
            <para xml:lang="en-NZ">
              <text>
                <emphasis style="bold">APPENDIX</emphasis>
              </text>
            </para>
            <para xml:lang="en-NZ">
              <text>
                <emphasis style="bold">Members of applicant group</emphasis>
              </text>
              <table align="center" fontsize="11.5" leading="13.5" xml:lang="en-NZ">
                <tgroup cols="4" colsep="0" rowsep="0">
