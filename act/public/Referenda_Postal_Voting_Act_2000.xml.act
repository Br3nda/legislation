      <prov id="DLM74594" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">24</label>
        <heading xml:lang="en-NZ">Supply of copy of rolls to Returning Officer</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>At the request of the Returning Officer, the Chief Registrar must supply the Returning Officer with a copy of the referendum roll and the supplementary referendum roll.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="50775" atidlm:branchLabel="_NOVERSION_" atidlm:name="Compulsory Retirement Savings Scheme Referendum Act 1997" atidlm:xmlId="DLM498094" atidlm:state="UNCHANGED"><atidlm:linkcontent>1997 No 13</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="52271" atidlm:sourceXmlId="DLM498094" atidlm:targetKey="2191057" atidlm:targetXmlId="DLM408489" atidlm:targetName="Compulsory Retirement Savings Scheme Referendum Act 1997" atidlm:sourceName="DLM498094" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM408489.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM74578.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/prov[12]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Supply of copy of rolls to Returning Officer" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="24" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM73880" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="48" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Referenda (Postal Voting) Act 2000" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2000" atidlm:locale="xx_XX" a