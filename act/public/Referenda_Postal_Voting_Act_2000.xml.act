          <heading xml:lang="en-NZ">Declaration by Returning Officer or by person employed or engaged for purposes of referendum</heading>
          <form.body>
            <para xml:lang="en-NZ">
              <table align="center" fontsize="11.5" frame="topbot" leading="13.5" xml:lang="en-NZ">
                <tgroup cols="3" colsep="0" rowsep="0">
                  <colspec colname="col1" colnum="1" colwidth="0.66*"/>
                  <colspec colname="col2" colnum="2" colwidth="1.98*"/>
                  <colspec colname="col3" colnum="3" colwidth="2.95*"/>
                  <thead>
                    <row rowsep="1">
                      <entry nameend="col3" namest="col1">I, A.B., [<emphasis style="italic">Insert residence and occupation</emphasis>], solemnly and sincerely declare—</entry>
                    </row>
                  </thead>
                  <tbody>
                    <row>
                      <entry>*(a)</entry>
