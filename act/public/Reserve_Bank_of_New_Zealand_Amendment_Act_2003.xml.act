    <part id="DLM202623" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-03" act.no="46" act.type="public" date.as.at="2007-09-03" date.assent="2003-08-20" stage="reprint" year="2003">
      <label auto.number="no" denominator="yes">2</label>
      <heading xml:lang="en-NZ">New <citation jurisdiction="nz">Parts <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="127021" atidlm:branchLabel="_NOVERSION_" atidlm:name="part 5B Oversight of payment systems" atidlm:parentFolderId="/NZ/NZAct/R/Reserve Bank of New Zealand Amendment Act 2003" atidlm:xmlId="DLM573077" atidlm:state="UNCHANGED"><atidlm:linkcontent>5B</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="128682" atidlm:sourceXmlId="DLM573077" atidlm:targetKey="1092401" atidlm:targetXmlId="DLM201575" atidlm:targetName="part 5B Oversight of payment systems" atidlm:sourceName="DLM573077" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM201575.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM202623.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="part" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[2]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="part" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="New Parts 5B and 5C inserted in principal Act" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="2" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM201751" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="46" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Reserve Bank of New Zealand Amendment Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata ati