    <prov id="DLM968501" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">4</label>
      <heading xml:lang="en-NZ">Application of certain provisions of <citation jurisdiction="nz">Companies Act 1993</citation></heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text><citation jurisdiction="nz">Section 139(1)</citation> is amended by inserting <amend.in>275,</amend.in> before <amend.in>292</amend.in>.</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <para xml:lang="en-NZ">
            <text><citation jurisdiction="nz">Section 139</citation> is amended by adding the following subsection:</text>
            <amend>
              <subprov skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">3</label>
                <para xml:lang="en-NZ">
                  <text>To avoid doubt, the reference in <citation jurisdiction="nz">section 275(4) of the Companies Act 1993</citation> to <citation jurisdiction="nz">clause 1(a) of Schedule 7 of the Companies Act 1993</citation> must be read as a reference to <citation jurisdiction="nz">section 148(1) of this Act</citation>.</text>
                </para>
              </subprov>
            </amend>
          </para>
        </subprov>
      </prov.body>
    </prov>
