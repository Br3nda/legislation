        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM359935" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">11</label>
      <heading xml:lang="en-NZ">Functions of regional councils under this Act</heading>
      <prov.body>
