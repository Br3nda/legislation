    <prov id="DLM191705" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">19</label>
      <heading xml:lang="en-NZ"><citation jurisdiction="nz">Income Tax Act 1976</citation> amended</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text>The <citation jurisdiction="nz">Income Tax Act 1976</citation> is hereby amended by omitting from <citation jurisdiction="nz">Schedule 14</citation> (as substituted by <citation jurisdiction="nz">section 23 of the State Services Conditions of Employment Amendment Act 1987</citation>) the name <emphasis style="bold">Rural Banking and Finance Corporation of New Zealand</emphasis>, and substituting the name <emphasis style="bold">Rural Banking and Finance Corporation of New Zealand Limited</emphasis>.</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
