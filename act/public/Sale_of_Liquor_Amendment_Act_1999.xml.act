      <prov id="DLM33114" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">79</label>
        <heading xml:lang="en-NZ">Allowing unlicensed premises to be used for sale of liquor</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
