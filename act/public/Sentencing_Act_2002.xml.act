        <prov id="DLM136853" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">129</label>
          <heading xml:lang="en-NZ">Confiscation of motor vehicle after second offence</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>This section applies if,—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>on or after 26 July 1996, a person commits an offence (the <emphasis style="bold">first offence</emphasis>) against any of <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84281" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 32" atidlm:xmlId="DLM530912" atidlm:state="UNCHANGED"><atidlm:linkcontent>sections 32(1)(a) or (b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85736" atidlm:sourceXmlId="DLM530912" atidlm:targetKey="2489436" atidlm:targetXmlId="DLM434641" atidlm:targetName="prov 32" atidlm:sourceName="DLM530912" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM434634.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84283" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 35" atidlm:xmlId="DLM530914" atidlm:state="UNCHANGED"><atidlm:linkcontent>35(1)(a) or (b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85738" atidlm:sourceXmlId="DLM530914" atidlm:targetKey="2489442" atidlm:targetXmlId="DLM434650" atidlm:targetName="prov 35" atidlm:sourceName="DLM530914" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM434634.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84285" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 36" atidlm:xmlId="DLM530916" atidlm:state="UNCHANGED"><atidlm:linkcontent>36(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85740" atidlm:sourceXmlId="DLM530916" atidlm:targetKey="2489444" atidlm:targetXmlId="DLM434651" atidlm:targetName="prov 36" atidlm:sourceName="DLM530916" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM434634.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <insertwords><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84286" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 36A" atidlm:xmlId="DLM530917" atidlm:state="UNCHANGED"><atidlm:linkcontent>36A(1)(a) or (c)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85741" atidlm:sourceXmlId="DLM530917" atidlm:targetKey="2489446" atidlm:targetXmlId="DLM434653" atidlm:targetName="prov 36A" atidlm:sourceName="DLM530917" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM434634.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>,</insertwords><insertwords><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84288" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 39" atidlm:xmlId="DLM530919" atidlm:state="UNCHANGED"><atidlm:linkcontent>39(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85743" atidlm:sourceXmlId="DLM530919" atidlm:targetKey="2489454" atidlm:targetXmlId="DLM434662" atidlm:targetName="prov 39" atidlm:sourceName="DLM530919" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM434634.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>,</insertwords><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84289" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 56" atidlm:xmlId="DLM530920" atidlm:state="UNCHANGED"><atidlm:linkcontent>56(1) or (2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85744" atidlm:sourceXmlId="DLM530920" atidlm:targetKey="2490004" atidlm:targetXmlId="DLM434692" atidlm:targetName="prov 56" atidlm:sourceName="DLM530920" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM434690.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84291" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 58" atidlm:xmlId="DLM530922" atidlm:state="UNCHANGED"><atidlm:linkcontent>58(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85746" atidlm:sourceXmlId="DLM530922" atidlm:targetKey="2490009" atidlm:targetXmlId="DLM434697" atidlm:targetName="prov 58" atidlm:sourceName="DLM530922" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM434690.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84292" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 60" atidlm:xmlId="DLM530923" atidlm:state="UNCHANGED"><atidlm:linkcontent>60(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85747" atidlm:sourceXmlId="DLM530923" atidlm:targetKey="2490015" atidlm:targetXmlId="DLM434802" atidlm:targetName="prov 60" atidlm:sourceName="DLM530923" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM434690.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84293" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 61" atidlm:xmlId="DLM530924" atidlm:state="UNCHANGED"><atidlm:linkcontent>61(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85748" atidlm:sourceXmlId="DLM530924" atidlm:targetKey="2490019" atidlm:targetXmlId="DLM434807" atidlm:targetName="prov 61" atidlm:sourceName="DLM530924" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM434690.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link>, <insertwords><citation jurisdiction="nz">61(2)</citation></insertwords> or <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84295" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 62" atidlm:xmlId="DLM530926" atidlm:state="UNCHANGED"><atidlm:linkcontent>62(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85750" atidlm:sourceXmlId="DLM530926" atidlm:targetKey="2490020" atidlm:targetXmlId="DLM434811" atidlm:targetName="prov 62" atidlm:sourceName="DLM530926" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM434690.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the Land Transport Act 1998</citation> (which relate to driving offences) <insertwords>or <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84297" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 171" atidlm:xmlId="DLM530928" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 171</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85752" atidlm:sourceXmlId="DLM530928" atidlm:targetKey="2141510" atidlm:targetXmlId="DLM329319" atidlm:targetName="prov 171" atidlm:sourceName="DLM530928" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM329288.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the Crimes Act 1961</citation> (but only where the manslaughter involved the use of a motor vehicle)</insertwords>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>within 4 years after the date of the commission of that offence, the person commits a further offence (the <def-term id="DLM136854" xml:lang="en-NZ">second offence</def-term>) against any of those provisions of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84299" atidlm:branchLabel="_NOVERSION_" atidlm:name="Land Transport Act 1998" atidlm:xmlId="DLM530930" atidlm:state="UNCHANGED"><atidlm:linkcontent>Land Transport Act 1998</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="85754" atidlm:sourceXmlId="DLM530930" atidlm:targetKey="2480405" atidlm:targetXmlId="DLM433612" atidlm:targetName="Land Transport Act 1998" atidlm:sourceName="DLM530930" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM433612.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>For the purpose of subsection (1), it does not matter whether or not the second offence is of the same kind as the first offence, but it must be an offence that arises from a different incident from the one that gave rise to the first offence.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para xml:lang="en-NZ">
                <text>If the court by or before which the offender is convicted of the second offence is satisfied that any motor vehicle owned by the offender or in which the offender has any interest was being driven by, or in the charge of, the offender at the material time, the court must order that the motor vehicle be confiscated.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">4</label>
              <para xml:lang="en-NZ">
                <text>Despite subsection (3), the court must not make an order under that subsection if it will result in extreme hardship to the offender or undue hardship to any other person.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">5</label>
              <para xml:lang="en-NZ">
                <text>For the purposes of this section, a conviction for an offence against a provision of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84302" atidlm:branchLabel="_NOVERSION_" atidlm:name="Transport Act 1962" atidlm:xmlId="DLM530933" atidlm:state="UNCHANGED"><atidlm:linkcontent>Transport Act 1962</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="85757" atidlm:sourceXmlId="DLM530933" atidlm:targetKey="2228914" atidlm:targetXmlId="DLM341189" atidlm:targetName="Transport Act 1962" atidlm:sourceName="DLM530933" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM341189.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> that corresponds to an offence specified in subsection (1) must be treated as a conviction for an offence specified in that subsection.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
            <citation jurisdiction="nz">1985 No 120 <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="84305" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 84" atidlm:xmlId="DLM530936" atidlm:state="UNCHANGED"><atidlm:linkcontent>s 84(2A), (2AA)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85761" atidlm:sourceXmlId="DLM530936" atidlm:targetKey="446407" atidlm:targetXmlId="DLM78223" atidlm:targetName="prov 84" atidlm:sourceName="DLM530936" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM78216.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136805.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136853" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
          </cf>
