        <prov id="DLM136091" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">75</label>
          <heading xml:lang="en-NZ">Commencement of community-based sentences</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>A community-based sentence commences on the day on which it is imposed.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>Subsection (1) applies—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>subject to <insertwords><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="608905" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1017027 - prov 75A" atidlm:xmlId="DLM1022247" atidlm:state="UNCHANGED"><atidlm:linkcontent>sections 75A</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="541351" atidlm:sourceXmlId="DLM1022247" atidlm:targetKey="764079" atidlm:targetXmlId="DLM136093" atidlm:targetName="prov 75A" atidlm:sourceName="DLM1022247" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136014.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136014.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136091" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="608906" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1017028 - prov 75B" atidlm:xmlId="DLM1022248" atidlm:state="UNCHANGED"><atidlm:linkcontent>75B</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="541352" atidlm:sourceXmlId="DLM1022248" atidlm:targetKey="764080" atidlm:targetXmlId="DLM136094" atidlm:targetName="prov 75B" atidlm:sourceName="DLM1022248" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136014.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136014.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136091" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation></insertwords> and to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="83684" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 137" atidlm:xmlId="DLM530317" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 137</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="85171" atidlm:sourceXmlId="DLM530317" atidlm:targetKey="2043337" atidlm:targetXmlId="DLM312983" atidlm:targetName="prov 137" atidlm:sourceName="DLM530317" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM312656.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136014.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136091" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the Summary Proceedings Act 1957</citation>; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>regardless of whether or not the sentence is imposed in substitution for another sentence.</text>
                  </para>
                </label-para>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2A</label>
              <para xml:lang="en-NZ">
                <text>If the commencement date of a sentence of community work is deferred under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="608907" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1017030 - prov 57A" atidlm:xmlId="DLM1022249" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 57A</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="541353" atidlm:sourceXmlId="DLM1022249" atidlm:targetKey="764033" atidlm:targetXmlId="DLM136047" atidlm:targetName="prov 57A" atidlm:sourceName="DLM1022249" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136014.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM136014.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM136091" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the sentence commences on the date specified in the order of the court.</text>
              </para>
            </subprov>
