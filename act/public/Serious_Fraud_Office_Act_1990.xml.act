      <crosshead id="DLM211457" toc="yes" xml:lang="en-NZ">Miscellaneous provisions</crosshead>
      <prov id="DLM211458" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">25</label>
        <heading xml:lang="en-NZ">Certain provisions not to apply to Police, Inland Revenue, and Statistics Department officers</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
