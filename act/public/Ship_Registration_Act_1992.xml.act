            <def-para id="DLM275925" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text>
                  <def-term id="DLM275926" xml:lang="en-NZ">Superintendent of mercantile marine</def-term>
                </text>
                <notes>
