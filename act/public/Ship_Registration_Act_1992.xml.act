      <prov id="DLM276667" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">85</label>
        <heading xml:lang="en-NZ">Power to appoint or recognise persons as proper officers</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>For the purposes of this Act, the <insertwords>Director</insertwords> may from time to time, by notice in the <emphasis style="italic">Gazette</emphasis>, appoint or recognise any person as a proper officer in relation to a port in a foreign country.</text>
            </para>
          </subprov>
        </prov.body>
