            <def-para id="DLM223516" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM223517" xml:lang="en-NZ">children's product</def-term>—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>means a product (such as comics, games, and toys) marketed primarily for children; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>includes confectionery, ice-cream, soft drinks, and other similar products</text>
                  </para>
