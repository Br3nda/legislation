            <def-para id="DLM223579" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM223580" xml:lang="en-NZ">herbal smoking product</def-term> means a product that—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
                    <text>is or contains vegetable matter; and</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">b</label>
                  <para xml:lang="en-NZ">
                    <text>is intended to be smoked; but</text>
                  </para>
                </label-para>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">c</label>
                  <para xml:lang="en-NZ">
                    <text>contains no tobacco</text>
                  </para>
                </label-para>
