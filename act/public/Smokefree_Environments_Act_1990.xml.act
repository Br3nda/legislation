    <part id="DLM223919" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-03" act.no="108" act.type="public" date.as.at="2007-09-03" date.assent="1990-08-28" stage="reprint" year="1990">
      <label auto.number="no" denominator="yes">1</label>
