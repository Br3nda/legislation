    <part id="DLM85602" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-03" act.no="1" act.type="public" date.as.at="2007-09-03" date.assent="2001-03-07" stage="reprint" year="2001">
