      <schedule.misc>
        <para xml:lang="en-NZ">
          <amend>
            <schedule act.no="28" id="DLM147493" in.amend="true" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" xml:lang="en-NZ" year="2002" irdnumbering="no">
              <label auto.number="no" denominator="yes">3</label>
              <empowering-prov>
                <citation jurisdiction="nz">ss 21, 24</citation>
              </empowering-prov>
              <heading xml:lang="en-NZ">Rates of widows' benefits</heading>
              <schedule.misc>
                <para xml:lang="en-NZ">
                  <table align="center" fontsize="11.5" frame="none" id="DLM147494" leading="13.5" xml:lang="en-NZ">
                    <tgroup cols="4">
                      <colspec colname="col1" colwidth="0.39*"/>
                      <colspec colname="col2" colwidth="0.40*"/>
                      <colspec colname="col3" colwidth="2.15*"/>
                      <colspec colname="col4" colwidth="1.07*"/>
                      <tbody>
                        <row>
                          <entry colsep="0" rowsep="0">1</entry>
                          <entry colsep="0" rowsep="0">(a)</entry>
                          <entry colsep="0" rowsep="0">To a beneficiary with 1 dependent child</entry>
                          <entry colsep="0" rowsep="0">$225.40 a week, subject to Income Test 1</entry>
                        </row>
                        <row>
                          <entry colsep="0" rowsep="0"/>
                          <entry colsep="0" rowsep="0">(b)</entry>
                          <entry colsep="0" rowsep="0">To a beneficiary with 2 or more dependent children</entry>
                          <entry colsep="0" rowsep="0">$245.91 a week, subject to Income Test 1</entry>
                        </row>
                        <row>
                          <entry colsep="0" rowsep="0">2</entry>
                          <entry colsep="0" rowsep="0"/>
                          <entry colsep="0" rowsep="0">To a beneficiary without any dependent children</entry>
                          <entry colsep="0" rowsep="0">$163.92 a week, subject to Income Test 1</entry>
                        </row>
                        <row>
                          <entry colsep="0" rowsep="0">3</entry>
                          <entry colsep="0" rowsep="0"/>
                          <entry colsep="0" rowsep="0">For the purposes of clause 1, the chief executive may, in his or her discretion, disregard up to $20 a week of the beneficiary's personal earnings used to meet the cost of child care for any of the beneficiary's dependent children.</entry>
                          <entry colsep="0" rowsep="0"/>
                        </row>
                      </tbody>
                    </tgroup>
                  </table>
                </para>
              </schedule.misc>
            </schedule>
            <schedule act.no="28" id="DLM147495" in.amend="true" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" xml:lang="en-NZ" year="2002" irdnumbering="no">
