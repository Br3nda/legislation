        <prov id="DLM204914" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">7</label>
          <heading xml:lang="en-NZ">Appointment of deputies</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The Minister may appoint any person to be the deputy of any appointed member of the Board.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>No person shall be appointed under subclause (1) as the deputy of any appointed member of the Board unless that person is eligible to be appointed as an appointed member of the Board.</text>
              </para>
            </subprov>
            <subprov deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">3</label>
              <para>
                <text/>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">4</label>
              <para xml:lang="en-NZ">
                <text>Any person who is appointed under this clause as the deputy of any member of the Board may attend any meeting of the Board in the place of the person whose deputy that person is.</text>
              </para>
            </subprov>
            <subprov deletion-status="repealed" skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">5</label>
              <para>
                <text/>
              </para>
            </subprov>
          </prov.body>
