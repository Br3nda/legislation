      <crosshead id="DLM157616" toc="yes" xml:lang="en-NZ">Chief executive of Agency</crosshead>
      <prov id="DLM157617" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">31</label>
        <heading xml:lang="en-NZ">Appointment of chief executive</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The board must appoint a chief executive.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>The chief executive must not be a member <insertwords>of the board</insertwords>.</text>
            </para>
          </subprov>
        </prov.body>
