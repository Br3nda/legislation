        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text>This Act is the State-Owned Enterprises (AgriQuality Limited and Asure New Zealand Limited) Act 2007.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM409067" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
