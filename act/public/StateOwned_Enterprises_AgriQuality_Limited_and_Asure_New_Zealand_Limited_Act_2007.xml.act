        <prov id="DLM409097" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">8</label>
          <heading xml:lang="en-NZ">Amendments and repeals to enactments resulting from merger of AgriQuality and Asure</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The Governor-General may, by Order in Council made on the recommendation of the responsible Minister, do 1 or more of the following:</text>
                <label-para skeleton="no" xml:lang="en-NZ">
