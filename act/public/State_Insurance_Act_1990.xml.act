    <prov id="DLM210401" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">9</label>
      <heading xml:lang="en-NZ">Books and documents to remain evidence</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text>Any document, matter, or thing, which, if this Act had not been passed, would have been admissible in evidence in respect of any matter for or against the State Insurance General Manager or the State Insurance Office shall, on or after the appointed day, be admissible in evidence in respect of the same matter for or against the company.</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
