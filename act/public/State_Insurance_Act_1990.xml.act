    <prov id="DLM209997" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">6</label>
      <heading xml:lang="en-NZ">Undertaking of State Insurance General Manager and State Insurance Office to vest in company</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="no"/>
          <para xml:lang="en-NZ">
            <text>On a date to be appointed by the Governor-General by Order in Council, the State Insurance undertaking shall vest in the company.</text>
          </para>
        </subprov>
      </prov.body>
