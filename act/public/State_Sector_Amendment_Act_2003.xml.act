      <heading xml:lang="en-NZ">Consequential amendments to enactments arising out of the reorganisation resulting in Ministry of Social Development</heading>
      <schedule.misc>
        <head1 id="DLM201384" toc="yes" type="part" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <heading xml:lang="en-NZ">Acts</heading>
          <head5 id="DLM201385" toc="yes" xml:lang="en-NZ">
            <heading xml:lang="en-NZ"><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="126421" atidlm:branchLabel="_NOVERSION_" atidlm:name="Housing Restructuring and Tenancy Matters Act 1992" atidlm:xmlId="DLM572478" atidlm:state="UNCHANGED"><atidlm:linkcontent>Housing Restructuring Act 1992</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="128237" atidlm:sourceXmlId="DLM572478" atidlm:targetKey="1404819" atidlm:targetXmlId="DLM269427" atidlm:targetName="Housing Restructuring and Tenancy Matters Act 1992" atidlm:sourceName="DLM572478" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM269427.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM201383.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="head5" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[1]/schedule.misc[1]/head1[1]/head5[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="head5" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Housing Restructuring Act 1992 (1992 No 76)" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM201330" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="41" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="State Sector Amendment Act 2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Year" atidlm:value="2003" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Revision_Number" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> (1992 No 76)</heading>
