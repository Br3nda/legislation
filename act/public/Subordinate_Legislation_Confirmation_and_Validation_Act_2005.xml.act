    <part id="DLM363822" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2005-12-22" act.no="125" act.type="public" date.as.at="2006-12-13" date.assent="2005-12-21" date.terminated="2006-12-13" stage="reprint" year="2005">
      <label auto.number="no" denominator="yes">1</label>
      <heading xml:lang="en-NZ">General and technical provisions</heading>
      <prov id="DLM363824" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">3</label>
        <heading xml:lang="en-NZ">Purpose of this Act</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The purpose of this Act is to confirm and validate certain subordinate legislation that, in accordance with confirmation and validation provisions in the Acts under which it is made, lapses at a stated time unless earlier confirmed or validated by Act of Parliament.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
