<act date.instructed="2008-05-27" id="DLM1509400" in.amend="false" instructing.office="Prime Minister and Cabinet" irdnumbering="no" xml:lang="en-NZ" year.imprint="2008" act.no="102" act.type="public" date.as.at="2008-09-29" date.assent="2008-09-29" date.first.valid="2008-09-30" stage="in-force" year="2008">
