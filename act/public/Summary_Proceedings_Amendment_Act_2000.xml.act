    <prov id="DLM78374" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">4</label>
      <heading xml:lang="en-NZ">Consequential amendment and repeals</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text><citation jurisdiction="nz">Section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="54662" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 33" atidlm:xmlId="DLM501377" atidlm:state="UNCHANGED"><atidlm:linkcontent>33</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="55492" atidlm:sourceXmlId="DLM501377" atidlm:targetKey="2038836" atidlm:targetXmlId="DLM311619" atidlm:targetName="prov 33" atidlm:sourceName="DLM501377" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM311185.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM78365.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM78374" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the principal Act</citation> is amended by omitting the word <amend.in>read</amend.in> wherever it appears, and substituting in each case the word <amend.in>given</amend.in>.</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <para xml:lang="en-NZ">
            <text>The following enactments are repealed:</text>
            <label-para skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">a</label>
              <para xml:lang="en-NZ">
                <text><citation jurisdiction="nz">section 7 of the Summary Proceedings Amendment Act 1964</citation>:</text>
              </para>
            </label-para>
            <label-para skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">b</label>
              <para xml:lang="en-NZ">
                <text><citation jurisdiction="nz">section 15 of the Summary Proceedings Amendment Act 1980</citation>:</text>
              </para>
            </label-para>
            <label-para skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">c</label>
              <para xml:lang="en-NZ">
                <text><citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="54663" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 26" atidlm:xmlId="DLM501378" atidlm:state="UNCHANGED"><atidlm:linkcontent>26</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="55493" atidlm:sourceXmlId="DLM501378" atidlm:targetKey="1562037" atidlm:targetXmlId="DLM302511" atidlm:targetName="prov 26" atidlm:sourceName="DLM501378" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM302276.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM78365.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM78374" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="54664" atidlm:branchLabel="_NOVERSION_" atidlm:name="Summary Proceedings Amendment Act 1993" atidlm:xmlId="DLM501379" atidlm:state="UNCHANGED"><atidlm:linkcontent>Summary Proceedings Amendment Act 1993</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="55494" atidlm:sourceXmlId="DLM501379" atidlm:targetKey="1562003" atidlm:targetXmlId="DLM302276" atidlm:targetName="Summary Proceedings Amendment Act 1993" atidlm:sourceName="DLM501379" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM302276.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM78365.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM78374" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
              </para>
            </label-para>
            <label-para skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">d</label>
              <para xml:lang="en-NZ">
                <text><citation jurisdiction="nz">sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="54665" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 38" atidlm:xmlId="DLM501380" atidlm:state="UNCHANGED"><atidlm:linkcontent>38</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="55495" atidlm:sourceXmlId="DLM501380" atidlm:targetKey="2370226" atidlm:targetXmlId="DLM427979" atidlm:targetName="prov 38" atidlm:sourceName="DLM501380" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM427928.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM78365.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM78374" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> and <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="54666" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 39" atidlm:xmlId="DLM501381" atidlm:state="UNCHANGED"><atidlm:linkcontent>39</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="55496" atidlm:sourceXmlId="DLM501381" atidlm:targetKey="2370228" atidlm:targetXmlId="DLM427981" atidlm:targetName="prov 39" atidlm:sourceName="DLM501381" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM427928.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM78365.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM78374" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="54667" atidlm:branchLabel="_NOVERSION_" atidlm:name="Summary Proceedings Amendment Act (No 2) 1998" atidlm:xmlId="DLM501382" atidlm:state="UNCHANGED"><atidlm:linkcontent>Summary Proceedings Amendment Act (No 2) 1998<