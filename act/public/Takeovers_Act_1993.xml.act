          <heading xml:lang="en-NZ">Requirements where appeal allowed</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>To the extent that an appeal in respect of an act or decision of the <insertwords>Panel or</insertwords> Registrar, or any person authorised by the <insertwords>Panel or</insertwords> Registrar, under any of <citation jurisdiction="nz">sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="420657" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 31A" atidlm:xmlId="DLM864755" atidlm:state="UNCHANGED"><atidlm:linkcontent>31A to 31C</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="427787" atidlm:sourceXmlId="DLM864755" atidlm:targetKey="1677694" atidlm:targetXmlId="DLM325890" atidlm:targetName="prov 31A" atidlm:sourceName="DLM864755" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM325885.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM325885.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[3]/subpart[1]/prov[15]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Requirements where appeal allowed" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="31K" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM325508" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="107" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Takeovers Act 1993" atidlm:locale="xx_X