        <prov id="DLM326744" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">44U</label>
          <heading xml:lang="en-NZ">General provisions as to Court's orders</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>An order under this Part may be made on the terms and conditions the Court thinks fit.</text>
              </para>
            </subprov>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">2</label>
              <para xml:lang="en-NZ">
                <text>The Court may revoke, vary, or suspend an order made under this Part on the terms and conditions the Court thinks fit.</text>
              </para>
            </subprov>
          </prov.body>
          <notes>
