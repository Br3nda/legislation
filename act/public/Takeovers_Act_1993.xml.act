        <prov id="DLM326174" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">31W</label>
          <heading xml:lang="en-NZ">Panel to hear proceedings in private</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The Panel may decide whether to hold any meeting or any part of a meeting in public or in private.</text>
              </para>
            </subprov>
          </prov.body>
