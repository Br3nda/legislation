        <prov id="DLM326375" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">43B</label>
          <heading xml:lang="en-NZ">Standard of proof for civil remedies</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The proceedings under this subpart are civil proceedings and the usual rules of the Court and rules of evidence and procedure for civil proceedings apply (including the standard of proof).</text>
              </para>
            </subprov>
          </prov.body>
