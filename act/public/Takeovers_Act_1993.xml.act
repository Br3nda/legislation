      <prov id="DLM325846" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">17</label>
        <heading xml:lang="en-NZ">Annual fee in respect of funding of Panel</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>Each specified company shall pay such annual fee in respect of the funding of the Panel as may be prescribed by regulations made under this Act.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>Any such fee shall be—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>Payable to the Registrar of Companies upon delivery to the Registrar of the company's annual return; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>Payable in addition to any fee payable in respect of the annual return; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>Recoverable by the Registrar in any court of competent jurisdiction as a debt due to the Crown.</text>
                </para>
              </label-para>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">3</label>
            <para xml:lang="en-NZ">
              <text>The Registrar of Companies shall, as soon as practicable, remit to the Panel the fees paid under this section.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov deletion-status="repealed" id="DLM325847" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">17A</label>
        <heading xml:lang="en-NZ">Panel deemed to be public authority</heading>
        <prov.body/>
