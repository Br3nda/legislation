        <prov id="DLM326740" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">44S</label>
          <heading xml:lang="en-NZ">Orders to secure compliance</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The Court may, for the purpose of securing compliance with any order it makes under this Part, direct a person to do or refrain from doing a specified act.</text>
              </para>
            </subprov>
          </prov.body>
