            <def-para id="DLM325534" xml:lang="en-NZ">
              <para xml:lang="en-NZ">
                <text><def-term id="DLM325535" xml:lang="en-NZ">Panel</def-term>—</text>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">a</label>
                  <para xml:lang="en-NZ">
