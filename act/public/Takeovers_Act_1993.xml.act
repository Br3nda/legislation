        <prov id="DLM326750" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">44X</label>
          <heading xml:lang="en-NZ">No pecuniary penalty and fine for same conduct</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>A person cannot be ordered to pay a pecuniary penalty and be liable for a fine under this Act for the same conduct.</text>
              </para>
            </subprov>
          </prov.body>
