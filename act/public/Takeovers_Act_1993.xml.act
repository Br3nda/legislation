        <prov id="DLM326339" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">33P</label>
          <heading xml:lang="en-NZ">Maximum amount of pecuniary penalty</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>The maximum amount of a pecuniary penalty is $500,000 for an individual and $5,000,000 for a body corporate, for each contravention.</text>
              </para>
            </subprov>
          </prov.body>
