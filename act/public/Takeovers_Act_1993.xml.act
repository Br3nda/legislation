        <prov id="DLM326198" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">33B</label>
          <heading xml:lang="en-NZ">No privilege against self-incrimination</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="no"/>
              <para xml:lang="en-NZ">
                <text>No person is excused from answering any question or providing any information or document under this Act on the ground that to do so would or might incriminate or tend to incriminate that person.</text>
              </para>
            </subprov>
          </prov.body>
          <cf>
