        <prov id="DLM326359" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">38</label>
          <heading xml:lang="en-NZ">Court may have regard to determinations and recommendations by Panel</heading>
          <prov.body>
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes">1</label>
              <para xml:lang="en-NZ">
                <text>The Court may, in determining any application under <insertwords>this subpart</insertwords>, have regard to any determination made by the Panel under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="420923" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 32" atidlm:xmlId="DLM865320" atidlm:state="UNCHANGED"><atidlm:linkcontent>32(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="428299" atidlm:sourceXmlId="DLM865320" atidlm:targetKey="1677601" atidlm:targetXmlId="DLM326180" atidlm:targetName="prov 32" atidlm:sourceName="DLM865320" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM325885.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM326305.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[3]/subpart[2]/prov[19]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Court may have regard to determinations and recommendations by Panel" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="38" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM325508" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="107" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Takeovers Act 1993" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><at