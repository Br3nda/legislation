      <prov id="DLM29351" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">123</label>
        <heading xml:lang="en-NZ">Zero-rating</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>After <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="10273" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 11" atidlm:xmlId="DLM457459" atidlm:state="UNCHANGED"><atidlm:linkcontent>11(2)(c)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="10412" atidlm:sourceXmlId="DLM457459" atidlm:targetKey="464736" atidlm:targetXmlId="DLM82734" atidlm:targetName="prov 11" atidlm:sourceName="DLM457459" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM82298.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM29349.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM29351" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the following is inserted:</text>
              <amend>
                <label-para skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes">caa</label>
                  <para xml:lang="en-NZ">
                    <text>The services are provided to overseas postal organisations for the delivery, in New Zealand, of postal articles mailed outside New Zealand; or</text>
                  </para>
                </label-para>
              </amend>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text><citation jurisdiction="nz">Subsection <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="10274" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 123" atidlm:xmlId="DLM457460" atidlm:state="UNCHANGED"><atidlm:linkcontent>(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="10413" atidlm:sourceXmlId="DLM457460" atidlm:targetKey="105802" atidlm:targetXmlId="DLM29351" atidlm:targetName="prov 123" atidlm:sourceName="DLM457460" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM29349.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM29349.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM29351" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> applies on and after 1 April 1999.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
    </part>
