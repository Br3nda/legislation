    <part date.instructed="2009-10-16" id="DLM2546307" in.amend="false" instructing.office="Inland Revenue" irdnumbering="no" toc="yes" date.first.valid="2010-09-07" act.no="109" act.type="public" date.as.at="2010-09-09" date.assent="2010-09-07" stage="in-force" year="2010" prospective.consolidation="F">
      <label auto.number="no" denominator="yes">1</label>
      <heading>Annual rates of income tax</heading>
      <prov id="DLM2546308" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">3</label>
        <heading>Annual rates of income tax for 2010–11 tax year</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="no"/>
            <para>
              <text>Income tax imposed by <citation jurisdiction="nz"><extref href="DLM1512320">section BB 1</extref> of the Income Tax Act 2007</citation> must, for the 2010–11 tax year, be paid at the basic rates specified in <citation jurisdiction="nz"><extref href="DLM1523194">schedule 1</extref> of that Act</citation>.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
    </part>
