          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">3</label>
            <para>
              <text>Subsection (1) applies for the 2005–06 and later income years.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">4</label>
            <para>
              <text>Subsection (2) applies for the 2008–09 and later income years.</text>
            </para>
          </subprov>
