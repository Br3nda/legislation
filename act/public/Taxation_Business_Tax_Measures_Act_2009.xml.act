    <part date.instructed="2009-01-26" id="DLM1826007" in.amend="false" instructing.office="Inland Revenue" irdnumbering="no" toc="yes" date.first.valid="2009-03-30" act.no="5" act.type="public" date.as.at="2009-05-29" date.assent="2009-03-30" stage="reprint" year="2009">
      <label auto.number="no" denominator="yes">1</label>
      <heading>Business tax measures</heading>
      <crosshead id="DLM1826008" toc="yes">Amendments to Income Tax Act 2007</crosshead>
      <prov id="DLM1826009" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">3</label>
        <heading>Income Tax Act 2007</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="yes" denominator="yes"/>
            <para>
              <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659865" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922108 - prov 4" atidlm:xmlId="DLM1922165" atidlm:state="UNCHANGED"><atidlm:linkcontent>Sections 4 to 22</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905826" atidlm:sourceXmlId="DLM1922165" atidlm:targetKey="15083418" atidlm:targetXmlId="DLM1826010" atidlm:targetName="prov 4" atidlm:sourceName="DLM1922165" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1826009" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> amend the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659866" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922111 - Income Tax Act 2007" atidlm:xmlId="DLM1922166" atidlm:state="UNCHANGED"><atidlm:linkcontent>Income Tax Act 2007</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905827" atidlm:sourceXmlId="DLM1922166" atidlm:targetKey="13249608" atidlm:targetXmlId="DLM1512300" atidlm:targetName="Income Tax Act 2007" atidlm:sourceName="DLM1922166" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1512300.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1826009" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM1826010" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">4</label>
        <heading>New heading and new section DB 62</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text>After <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659867" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922112 - prov DB 61" atidlm:xmlId="DLM1922167" atidlm:state="UNCHANGED"><atidlm:linkcontent>section DB 61</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905828" atidlm:sourceXmlId="DLM1922167" atidlm:targetKey="14881004" atidlm:targetXmlId="DLM1793127" atidlm:targetName="prov DB 61" atidlm:sourceName="DLM1922167" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1513570.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1826010" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the following is added:</text>
              <amend quote="1">
                <ird-crosshead id="DLM1826011" quote="1" toc="yes">Legal expenses</ird-crosshead>
                <prov id="DLM1826012" irdnumbering="no" quote="1" skeleton="no" toc="yes">
                  <label auto.number="no" denominator="yes" quote="1">DB 62</label>
                  <heading>Deduction for legal expenses</heading>
                  <prov.body>
                    <subprov.crosshead quote="1">When this section applies</subprov.crosshead>
                    <subprov quote="1" skeleton="no">
                      <label auto.number="no" denominator="yes" quote="1">1</label>
                      <para>
                        <text>This section applies to a person when their total legal expenses for an income year is equal to or less than $10,000.</text>
                      </para>
                    </subprov>
                    <subprov.crosshead quote="1">Deduction</subprov.crosshead>
                    <subprov quote="1" skeleton="no">
                      <label auto.number="no" denominator="yes" quote="1">2</label>
                      <para>
                        <text>The person is allowed a deduction for the legal expenses.</text>
                      </para>
                    </subprov>
                    <subprov.crosshead quote="1">Definition</subprov.crosshead>
                    <subprov quote="1" skeleton="no">
                      <label auto.number="no" denominator="yes" quote="1">3</label>
                      <para>
                        <text>For the purposes of this section, <def-term id="DLM1826013">legal expenses</def-term> means fees for <emphasis style="bold">legal services</emphasis> (as defined in the Lawyers and Conveyancers Act 2006) provided by a person who holds a practising certificate issued by the New Zealand Law Society or an Australian equivalent.</text>
                      </para>
                    </subprov>
                    <subprov.crosshead quote="1">Link with subpart DA</subprov.crosshead>
                    <subprov quote="1" skeleton="no">
                      <label auto.number="no" denominator="yes" quote="1">4</label>
                      <para>
                        <text>This section overrides the capital limitation. The general permission must still be satisfied and the other general limitations still apply.</text>
                      </para>
                    </subprov>
                  </prov.body>
                  <ird.aids id="DLM1826014">
                    <readers-notes quote="1">
                      <para quote="1">
                        <text>Defined in this Act: amount, capital limitation, deduction, general limitation, general permission, income year, legal expenses</text>
                      </para>
                    </readers-notes>
                  </ird.aids>
                </prov>
              </amend>
              <follow-text space-before="no">.</follow-text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
              <text><citation jurisdiction="nz">Subsection (1)</citation> applies for the 2009–10 income year and later income years.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM1825902" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">5</label>
        <heading>Valuing closing stock under $5,000</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text>In the heading to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659868" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922113 - prov EB 23" atidlm:xmlId="DLM1922168" atidlm:state="UNCHANGED"><atidlm:linkcontent>section EB 23</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905829" atidlm:sourceXmlId="DLM1922168" atidlm:targetKey="13264622" atidlm:targetXmlId="DLM1514335" atidlm:targetName="prov EB 23" atidlm:sourceName="DLM1922168" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1514280.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, <amend.in quote="1"><emphasis style="bold">$5,000</emphasis></amend.in> is replaced by <amend.in quote="1"><emphasis style="bold">$10,000</emphasis></amend.in>.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
              <text>In section <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659869" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922114 - prov EB 23" atidlm:xmlId="DLM1922169" atidlm:state="UNCHANGED"><atidlm:linkcontent>EB 23(1)(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905830" atidlm:sourceXmlId="DLM1922169" atidlm:targetKey="13264622" atidlm:targetXmlId="DLM1514335" atidlm:targetName="prov EB 23" atidlm:sourceName="DLM1922169" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1514280.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825902" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, <amend.in quote="1">$5,000</amend.in> is replaced by <amend.in quote="1">$10,000</amend.in>.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">3</label>
            <para>
              <text><citation jurisdiction="nz">Subsections (1) and (2)</citation> apply for the 2009–10 income year and later income years.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM1825903" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">6</label>
        <heading>When use of spreading method not required</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659870" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922115 - prov EW 13" atidlm:xmlId="DLM1922170" atidlm:state="UNCHANGED"><atidlm:linkcontent>Section EW 13(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905831" atidlm:sourceXmlId="DLM1922170" atidlm:targetKey="13267174" atidlm:targetXmlId="DLM1515261" atidlm:targetName="prov EW 13" atidlm:sourceName="DLM1922170" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825903" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, other than the heading, is replaced by the following:</text>
              <amend quote="1">
                <subprov skeleton="no">
                  <label auto.number="yes" denominator="yes" quote="1">2</label>
                  <para>
                    <text>A trustee who holds a financial arrangement in trust to manage compensation paid for personal injury under the Injury Prevention, Rehabilitation, and Compensation Act 2001, the Accident Insurance Act 1988, any of the former Acts as defined in section 13 of the Accident Insurance Act 1998, the Workers' Compensation Act 1956, or a court order does not use any of the spreading methods for the financial arrangement if the trustee is a cash basis person.</text>
                  </para>
                </subprov>
              </amend>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
              <text><citation jurisdiction="nz">Subsection (1)</citation> applies for the 2009–10 income year and later income years.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM1825904" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">7</label>
        <heading>Straight-line method</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text>In <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659871" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922118 - prov EW 17" atidlm:xmlId="DLM1922171" atidlm:state="UNCHANGED"><atidlm:linkcontent>section EW 17(1)(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905832" atidlm:sourceXmlId="DLM1922171" atidlm:targetKey="13267118" atidlm:targetXmlId="DLM1515270" atidlm:targetName="prov EW 17" atidlm:sourceName="DLM1922171" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825904" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, <amend.in quote="1">$1,500,000</amend.in> is replaced by <amend.in quote="1">$1,850,000</amend.in>.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
              <text><citation jurisdiction="nz">Subsection (1)</citation> applies for the 2009–10 income year and later income years.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM1825905" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">8</label>
        <heading>Consistency of use of straight-line method and market valuation method</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text>In the heading to <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659872" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922116 - prov EW 17" atidlm:xmlId="DLM1922172" atidlm:state="UNCHANGED"><atidlm:linkcontent>section EW 25(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905833" atidlm:sourceXmlId="DLM1922172" atidlm:targetKey="13267118" atidlm:targetXmlId="DLM1515270" atidlm:targetName="prov EW 17" atidlm:sourceName="DLM1922172" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825905" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, <amend.in quote="1"><emphasis style="italic">$1,500,000</emphasis></amend.in> is replaced by <amend.in quote="1"><emphasis style="italic">$1,850,000</emphasis></amend.in>.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
              <text>In <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659873" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922117 - prov EW 17" atidlm:xmlId="DLM1922173" atidlm:state="UNCHANGED"><atidlm:linkcontent>section EW 25(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905834" atidlm:sourceXmlId="DLM1922173" atidlm:targetKey="13267118" atidlm:targetXmlId="DLM1515270" atidlm:targetName="prov EW 17" atidlm:sourceName="DLM1922173" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825905" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, <amend.in quote="1">$1,500,000</amend.in> is replaced by <amend.in quote="1">$1,850,000</amend.in>.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">3</label>
            <para>
              <text><citation jurisdiction="nz">Subsections (1) and (2)</citation> apply for the 2009–10 income year and later income years.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM1825906" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">9</label>
        <heading>Section EW 54 replaced</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659874" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922119 - prov EW 54" atidlm:xmlId="DLM1922174" atidlm:state="UNCHANGED"><atidlm:linkcontent>Section EW 54</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905835" atidlm:sourceXmlId="DLM1922174" atidlm:targetKey="13267021" atidlm:targetXmlId="DLM1515373" atidlm:targetName="prov EW 54" atidlm:sourceName="DLM1922174" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825906" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is replaced by the following:</text>
              <amend quote="1">
                <prov id="DLM1825907" irdnumbering="no" quote="1" skeleton="no" toc="yes">
                  <label auto.number="no" denominator="yes" quote="1">EW 54</label>
                  <heading>Meaning of cash basis person</heading>
                  <prov.body>
                    <subprov.crosshead quote="1">Who is cash basis person</subprov.crosshead>
                    <subprov quote="1" skeleton="no">
                      <label auto.number="no" denominator="yes" quote="1">1</label>
                      <para>
                        <text>A person is a <def-term id="DLM1825908">cash basis person</def-term> for an income year if—</text>
                        <label-para quote="1" skeleton="no">
                          <label auto.number="no" denominator="yes" quote="1">a</label>
                          <para>
                            <text>1 of the following applies in the person's case for the income year:</text>
                            <label-para quote="1" skeleton="no">
                              <label auto.number="no" denominator="yes" quote="1">i</label>
                              <para>
                                <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1916804" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922121 - prov EW 57" atidlm:xmlId="DLM2126204" atidlm:state="UNCHANGED"><atidlm:linkcontent>section EW 57(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="987164" atidlm:sourceXmlId="DLM2126204" atidlm:targetKey="13267032" atidlm:targetXmlId="DLM1515380" atidlm:targetName="prov EW 57" atidlm:sourceName="DLM2126204" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825907" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                              </para>
                            </label-para>
                            <label-para quote="1" skeleton="no">
                              <label auto.number="no" denominator="yes" quote="1">ii</label>
                              <para>
                                <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1916805" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922121 - prov EW 57" atidlm:xmlId="DLM2126205" atidlm:state="UNCHANGED"><atidlm:linkcontent>section EW 57(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="987165" atidlm:sourceXmlId="DLM2126205" atidlm:targetKey="13267032" atidlm:targetXmlId="DLM1515380" atidlm:targetName="prov EW 57" atidlm:sourceName="DLM2126205" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825907" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                              </para>
                            </label-para>
                          </para>
                        </label-para>
                        <label-para quote="1" skeleton="no">
                          <label auto.number="no" denominator="yes" quote="1">b</label>
                          <para>
                            <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1916806" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922121 - prov EW 57" atidlm:xmlId="DLM2126206" atidlm:state="UNCHANGED"><atidlm:linkcontent>section EW 57(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="987166" atidlm:sourceXmlId="DLM2126206" atidlm:targetKey="13267032" atidlm:targetXmlId="DLM1515380" atidlm:targetName="prov EW 57" atidlm:sourceName="DLM2126206" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825907" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> applies in the person's case for the income year.</text>
                          </para>
                        </label-para>
                      </para>
                    </subprov>
                    <subprov.crosshead quote="1">Persons excluded by Commissioner</subprov.crosshead>
                    <subprov quote="1" skeleton="no">
                      <label auto.number="no" denominator="yes" quote="1">2</label>
                      <para>
                        <text>A person may be excluded under <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1916807" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922129 - prov EW 59" atidlm:xmlId="DLM2126207" atidlm:state="UNCHANGED"><atidlm:linkcontent>section EW 59</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="987167" atidlm:sourceXmlId="DLM2126207" atidlm:targetKey="13267036" atidlm:targetXmlId="DLM1515389" atidlm:targetName="prov EW 59" atidlm:sourceName="DLM2126207" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825907" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> from being a cash basis person for a class of financial arrangements.</text>
                      </para>
                    </subprov>
                  </prov.body>
                  <ird.aids id="DLM1825909">
                    <readers-notes quote="1">
                      <para quote="1">
                        <text>Defined in this Act: cash basis person, financial arrangement, income year</text>
                      </para>
                    </readers-notes>
                  </ird.aids>
                </prov>
              </amend>
              <follow-text space-before="no">.</follow-text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
              <text><citation jurisdiction="nz">Subsection (1)</citation> applies for the 2009–10 income year and later income years.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM1825910" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">10</label>
        <heading>Section EW 56 repealed</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659875" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922120 - prov EW 56" atidlm:xmlId="DLM1922175" atidlm:state="UNCHANGED"><atidlm:linkcontent>Section EW 56</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905836" atidlm:sourceXmlId="DLM1922175" atidlm:targetKey="13267025" atidlm:targetXmlId="DLM1515378" atidlm:targetName="prov EW 56" atidlm:sourceName="DLM1922175" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825910" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> is repealed.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
              <text><citation jurisdiction="nz">Subsection (1)</citation> applies for the 2009–10 income year and later income years.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM1825911" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">11</label>
        <heading>Thresholds</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text>In <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659876" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922121 - prov EW 57" atidlm:xmlId="DLM1922176" atidlm:state="UNCHANGED"><atidlm:linkcontent>section EW 57(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905837" atidlm:sourceXmlId="DLM1922176" atidlm:targetKey="13267032" atidlm:targetXmlId="DLM1515380" atidlm:targetName="prov EW 57" atidlm:sourceName="DLM1922176" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825911" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, <amend.in quote="1">section EW 56(1)(a)(i)</amend.in> is replaced by <amend.in quote="1">section EW 54(1)(a)(i)</amend.in>.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
              <text>In <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659877" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922122 - prov EW 57" atidlm:xmlId="DLM1922177" atidlm:state="UNCHANGED"><atidlm:linkcontent>section EW 57(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905838" atidlm:sourceXmlId="DLM1922177" atidlm:targetKey="13267032" atidlm:targetXmlId="DLM1515380" atidlm:targetName="prov EW 57" atidlm:sourceName="DLM1922177" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825911" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, <amend.in quote="1">section EW 56(1)(a)(ii)</amend.in> is replaced by <amend.in quote="1">section EW 54(1)(a)(ii)</amend.in>.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">3</label>
            <para>
              <text>In <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659878" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922123 - prov EW 57" atidlm:xmlId="DLM1922178" atidlm:state="UNCHANGED"><atidlm:linkcontent>section EW 57(3)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905840" atidlm:sourceXmlId="DLM1922178" atidlm:targetKey="13267032" atidlm:targetXmlId="DLM1515380" atidlm:targetName="prov EW 57" atidlm:sourceName="DLM1922178" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825911" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, <amend.in quote="1">section EW 56(1)(b)</amend.in> is replaced by <amend.in quote="1">section EW 54(1)(b)</amend.in>.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">4</label>
            <para>
              <text>After <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659879" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922124 - prov EW 57" atidlm:xmlId="DLM1922179" atidlm:state="UNCHANGED"><atidlm:linkcontent>section EW 57(9)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905841" atidlm:sourceXmlId="DLM1922179" atidlm:targetKey="13267032" atidlm:targetXmlId="DLM1515380" atidlm:targetName="prov EW 57" atidlm:sourceName="DLM1922179" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1515229.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1825911" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, the following is added:</text>
              <amend quote="1">
                <subprov.crosshead quote="1">Increase in specified sums</subprov.crosshead>
                <subprov skeleton="no">
                  <label auto.number="yes" denominator="yes" quote="1">10</label>
                  <para>
                    <text>The Governor-General may make an Order in Council increasing a sum specified in any of subsections (1) to (3).</text>
                  </para>
                </subprov>
              </amend>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">5</label>
            <para>
              <text><citation jurisdiction="nz">Subsections (1) to (3)</citation> apply for the 2009–10 income year and later income years.</text>
            </para>
          </subprov>
        </prov.body>
      </prov>
