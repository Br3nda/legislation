    <part date.instructed="2009-01-26" id="DLM1826007" in.amend="false" instructing.office="Inland Revenue" irdnumbering="no" toc="yes" date.first.valid="2009-03-30" act.no="5" act.type="public" date.as.at="2009-03-30" date.assent="2009-03-30" stage="in-force" year="2009">
      <label auto.number="no" denominator="yes">1</label>
      <heading>Business tax measures</heading>
      <crosshead id="DLM1826008" toc="yes">Amendments to Income Tax Act 2007</crosshead>
      <prov id="DLM1826009" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">3</label>
        <heading>Income Tax Act 2007</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="yes" denominator="yes"/>
            <para>
              <text><citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659865" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922108 - prov 4" atidlm:xmlId="DLM1922165" atidlm:state="UNCHANGED"><atidlm:linkcontent>Sections 4</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905826" atidlm:sourceXmlId="DLM1922165" atidlm:targetKey="15083418" atidlm:targetXmlId="DLM1826010" atidlm:targetName="prov 4" atidlm:sourceName="DLM1922165" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1826009" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> to 22</citation> amend the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1659866" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1922111 - Income Tax Act 2007" atidlm:xmlId="DLM1922166" atidlm:state="UNCHANGED"><atidlm:linkcontent>Income Tax Act 2007</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="905827" atidlm:sourceXmlId="DLM1922166" atidlm:targetKey="13249608" atidlm:targetXmlId="DLM1512300" atidlm:targetName="Income Tax Act 2007" atidlm:sourceName="DLM1922166" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1512300.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1826007.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1826009" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
