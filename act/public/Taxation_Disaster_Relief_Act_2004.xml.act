          <citation jurisdiction="nz">
            <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="144973" atidlm:branchLabel="_NOVERSION_" atidlm:name="Tax Administration Act 1994" atidlm:xmlId="DLM591083" atidlm:state="UNCHANGED">
