    <part date.instructed="2008-01-30" id="DLM1401719" in.amend="false" instructing.office="Inland Revenue" irdnumbering="no" toc="yes" date.first.valid="2009-10-06" act.no="34" act.type="public" date.as.at="2009-10-06" date.assent="2009-10-06" stage="in-force" year="2009">
