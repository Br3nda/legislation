      <prov id="DLM1400206" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">55</label>
        <heading>Section CX 39 repealed</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text><citation jurisdiction="nz"><extref href="DLM1513425">Section CX 39</extref></citation> is repealed.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
              <text>Subsection (1) applies––</text>
              <label-para skeleton="no">
                <label auto.number="no" denominator="yes">a</label>
