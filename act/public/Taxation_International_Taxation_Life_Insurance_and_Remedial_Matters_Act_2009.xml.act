      <prov id="DLM2178459" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">93</label>
        <heading>Forestry business on land bought from the Crown, Maori owners, or holding company: no deduction</heading>
        <prov.body>
          <subprov skeleton="no">
