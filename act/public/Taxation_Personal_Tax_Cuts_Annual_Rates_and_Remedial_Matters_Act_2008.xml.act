              <label-para skeleton="no">
                <label auto.number="no" denominator="yes">a</label>
                <para>
                  <text>in subparagraph (iv)(A), <amend.in quote="1">table 1</amend.in> is replaced by <amend.in quote="1">table 2</amend.in>:</text>
                </para>
              </label-para>
              <label-para skeleton="no">
                <label auto.number="no" denominator="yes">b</label>
                <para>
                  <text>in subparagraph (iv)(B), <amend.in quote="1">table 1</amend.in> is replaced by <amend.in quote="1">table 2</amend.in>.</text>
