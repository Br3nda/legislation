    <part id="DLM149584" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-18" act.no="32" act.type="public" date.as.at="2002-10-17" date.assent="2002-10-17" stage="in-force" year="2002">
