        <label auto.number="no" denominator="yes">104</label>
        <heading xml:lang="en-NZ">Application to make single deduction under section 21F</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>In <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="91138" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 21H" atidlm:xmlId="DLM537651" atidlm:state="UNCHANGED"><atidlm:linkcontent>21H(3)(b)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="92588" atidlm:sourceXmlId="DLM537651" atidlm:targetKey="467657" atidlm:targetXmlId="DLM83952" atidlm:targetName="prov 21H" atidlm:sourceName="DLM537651" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM83095.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM149761.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM149774" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, <amend.in><citation jurisdiction="nz">21F(1)</citation></amend.in> is replaced by <amend.in><citation jurisdiction="nz">21G(1)</citation></amend.in>.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>In <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="91139" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 21H" atidlm:xmlId="DLM537652" atidlm:state="UNCHANGED"><atidlm:linkcontent>21H(3)(e)(i)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="92589" atidlm:sourceXmlId="DLM537652" atidlm:targetKey="467657" atidlm:targetXmlId="DLM83952" atidlm:targetName="prov 21H" atidlm:sourceName="DLM537652" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM83095.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM149761.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM149774" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, <amend.in><citation jurisdiction="nz">section 21G</citation></amend.in> is replaced by <amend.in>this section</amend.in>.</text>
