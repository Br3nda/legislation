<act id="DLM431914" in.amend="false" instructing.office="Inland Revenue" irdnumbering="no" year.imprint="2005" xml:lang="en-NZ" act.no="101" act.type="public" date.as.at="2005-04-01" date.assent="1998-10-07" date.first.valid="1998-10-07" stage="in-force" year="1998">
