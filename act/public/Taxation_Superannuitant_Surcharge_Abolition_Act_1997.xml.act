    <title xml:lang="en-NZ">Taxation (Superannuitant Surcharge Abolition) Act 1997</title>
    <assent>1997-08-20</assent>
    <commencement>20 August 1997</commencement>
    <cover.reprint-note>
      <para xml:lang="en-NZ"/>
    </cover.reprint-note>
