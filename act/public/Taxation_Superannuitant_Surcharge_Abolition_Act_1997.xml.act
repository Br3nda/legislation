  <front id="DLM410760" xml:lang="en-NZ">
    <long-title id="DLM410761" toc="yes" xml:lang="en-NZ">
      <para xml:lang="en-NZ">
        <text>An Act to amend the Income Tax Act 1994 and the Tax Administration Act 1994 to abolish the New Zealand superannuitant surcharge</text>
      </para>
    </long-title>
    <enactment>BE IT ENACTED by the Parliament of New Zealand as follows:</enactment>
  </front>
