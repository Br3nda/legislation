      <prov id="DLM1764982" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">36</label>
        <heading>Definitions</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text>This section amends <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="1466898" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1775609 - prov YA 1" atidlm:xmlId="DLM1775698" atidlm:state="UNCHANGED"><atidlm:linkcontent>section YA 1</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="845300" atidlm:sourceXmlId="DLM1775698" atidlm:targetKey="13286206" atidlm:targetXmlId="DLM1520575" atidlm:targetName="prov YA 1" atidlm:sourceName="DLM1775698" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1520574.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1764968.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM1764982" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
