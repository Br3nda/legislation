<act date.instructed="2004-12-23" id="DLM381397" in.amend="false" instructing.office="Office of Treaty Settlements" irdnumbering="no" xml:lang="en-NZ" year.imprint="2008" act.no="43" act.type="public" date.as.at="2008-05-23" date.assent="2006-09-25" date.first.valid="2007-09-18" stage="reprint" year="2006">
