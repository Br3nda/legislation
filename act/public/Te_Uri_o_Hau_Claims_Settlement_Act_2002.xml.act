      <prov id="DLM155565" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">29</label>
        <heading>Hokarako</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
