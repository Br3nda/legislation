    <schedule id="DLM156177" in.amend="false" irdnumbering="no" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" act.no="36" act.type="public" date.as.at="2008-05-23" date.assent="2002-10-17" date.first.valid="2002-10-18" stage="reprint" year="2002">
