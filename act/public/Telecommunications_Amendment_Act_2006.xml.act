<act id="DLM385224" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="53" act.type="public" date.as.at="2007-09-03" date.assent="2006-10-30" date.first.valid="2007-09-18" stage="in-force" year="2006">
  <cover id="DLM385225">
    <title xml:lang="en-NZ">Telecommunications Amendment Act 2006</title>
    <assent>2006-10-30</assent>
    <cover.reprint-note>
      <para xml:lang="en-NZ"/>
      <admin-office>This Act is administered by the <ministry>Ministry of Economic Development</ministry>.</admin-office>
    </cover.reprint-note>
  </cover>
  <contents>
    <heading xml:lang="en-NZ">Contents</heading>
