<act id="DLM362615" in.amend="false" xml:lang="en-NZ" irdnumbering="no" act.no="115" act.type="public" date.as.at="2007-09-03" date.assent="2005-12-14" date.first.valid="2007-09-18" stage="in-force" year="2005">
