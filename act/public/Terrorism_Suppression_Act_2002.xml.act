              <def-para id="DLM152791" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM152792" xml:lang="en-NZ">authorised person</def-term>, <def-term id="DLM152793" xml:lang="en-NZ">Chief Executive</def-term>, <def-term id="DLM152794" xml:lang="en-NZ">the Customs</def-term>, <def-term id="DLM152795" xml:lang="en-NZ">Customs officer</def-term> or <def-term id="DLM152796" xml:lang="en-NZ">officer</def-term>, <def-term id="DLM152797" xml:lang="en-NZ">exportation</def-term>, <def-term id="DLM152798" xml:lang="en-NZ">goods</def-term>, and <def-term id="DLM152799" xml:lang="en-NZ">importation</def-term> have the meanings given to them in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="92369" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 2" atidlm:xmlId="DLM538978" atidlm:state="UNCHANGED"><atidlm:linkcontent>2(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="93996" atidlm:sourceXmlId="DLM538978" atidlm:targetKey="2012308" atidlm:targetXmlId="DLM377342" atidlm:targetName="prov 2" atidlm:sourceName="DLM538978" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM377336.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM152705.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[2]/prov[47]/prov.body[1]/subprov[2]/para[1]/def-para[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="def-para" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="def-para authorised person" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" at