      <heading xml:lang="en-NZ">United Nations Security Council Resolution 1333 (2000)</heading>
      <notes>
        <history>
          <history-note id="DLM1109749"><amended-provision>Schedule 4B</amended-provision>: <amending-operation>inserted</amending-operation>, on <amendment-date>20 November 2007</amendment-date>, by <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="726714" atidlm:branchLabel="_NOVERSION_" atidlm:name="DLM1109000 - prov 43" atidlm:xmlId="DLM1109759" atidlm:state="UNCHANGED"><atidlm:linkcontent>section 43</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="581396" atidlm:sourceXmlId="DLM1109759" atidlm:targetKey="10329281" atidlm:targetXmlId="DLM1034478" atidlm:targetName="prov 43" atidlm:sourceName="DLM1109759" atidlm:targetLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1034397.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM1108956.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/schedule.group[1]/schedule[9]/notes[1]/history[1]/history-note[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM151490" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="34" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Terrorism Suppression Act 2002" atidlm:locale="xx_XX" atidlm:state="UNC