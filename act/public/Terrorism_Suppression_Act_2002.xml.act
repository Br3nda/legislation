    <schedule id="DLM154120" in.amend="false" irdnumbering="no" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" xml:lang="en-NZ" act.no="34" act.type="public" date.as.at="2008-08-01" date.assent="2002-10-17" date.first.valid="2007-09-18" stage="reprint" year="2002">
