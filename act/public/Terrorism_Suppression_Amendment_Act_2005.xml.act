    <part id="DLM359201" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-03" act.no="83" act.type="public" date.as.at="2007-09-03" date.assent="2005-06-21" stage="reprint" year="2005">
