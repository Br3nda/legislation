      <heading xml:lang="en-NZ">Extension of existing designations and consequential amendments</heading>
      <crosshead id="DLM359202" toc="yes" xml:lang="en-NZ">Extension of existing designations under section 22 of principal Act</crosshead>
      <prov id="DLM359203" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">6</label>
        <heading xml:lang="en-NZ">Extension of certain designations under principal Act</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
