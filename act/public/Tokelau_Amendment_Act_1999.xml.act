    <part id="DLM24799" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-03" act.no="49" act.type="public" date.as.at="2007-09-03" date.assent="1999-05-07" stage="reprint" year="1999">
      <heading xml:lang="en-NZ">Tokelau Public Service</heading>
      <prov id="DLM25200" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">2</label>
        <heading xml:lang="en-NZ">New employer for employees of Tokelau Public Service</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The Governor-General may, by Order in Council, recognise any body, institution, or office in Tokelau as successor to the State Services Commissioner as the employer for the Tokelau Public Service.</text>
            </para>
          </subprov>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">2</label>
            <para xml:lang="en-NZ">
              <text>On the commencement of the order,—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>The State Services Commissioner ceases to be the employer of people employed in the Tokelau Public Service immediately before that commencement; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>The body or institution recognised, or the holder of the office recognised, becomes the employer of those employees.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
      </prov>
      <prov id="DLM25201" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">3</label>
