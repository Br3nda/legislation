      <prov id="DLM411291" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">13</label>
        <heading xml:lang="en-NZ">Goods that comply with local law</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Nothing in this Part prevents goods from being sold in New Zealand if (apart from this Act) they comply with the relevant requirements of the law in force in New Zealand.</text>
            </para>
          </subprov>
        </prov.body>
