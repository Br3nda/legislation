      <prov id="DLM411337" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">51</label>
        <heading xml:lang="en-NZ">Remuneration and travelling allowances</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="yes">1</label>
            <para xml:lang="en-NZ">
              <text>The Tribunal is declared to be a statutory board within the meaning of the <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="488289" atidlm:branchLabel="_NOVERSION_" atidlm:name="Fees and Travelling Allowances Act 1951" atidlm:xmlId="DLM931973" atidlm:state="UNCHANGED"><atidlm:linkcontent>Fees and Travelling Allowances Act 1951</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="496476" atidlm:sourceXmlId="DLM931973" atidlm:targetKey="1812408" atidlm:targetXmlId="DLM264952" atidlm:targetName="Fees and Travelling Allowances Act 1951" atidlm:sourceName="DLM931973" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM264952.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM411325.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[4]/prov[11]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="prov" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="Remuneration and travelling allowances" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="51" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM410792" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="60" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Trans-Tasman Mutual Recognition Act 1997" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm