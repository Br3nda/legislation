      <prov id="DLM219719" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">2</label>
        <heading xml:lang="en-NZ">Interpretation</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>In this Part of this Act, unless the context otherwise requires,â€”</text>
              <def-para id="DLM219720" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM219721" xml:lang="en-NZ">Accident</def-term>,â€”</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
