              <def-para id="DLM219735" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM219736" xml:lang="en-NZ">Commission</def-term> means the Transport Accident Investigation Commission established under this Act</text>
                </para>
              </def-para>
              <def-para id="DLM219737" xml:lang="en-NZ">
                <para xml:lang="en-NZ">
                  <text><def-term id="DLM219738" xml:lang="en-NZ">Convention</def-term>â€”</text>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">a</label>
                    <para xml:lang="en-NZ">
                      <text>Means the Convention on International Civil Aviation signed on behalf of the Government of New Zealand in Chicago on the 7th day of December 1944; and</text>
                    </para>
                  </label-para>
                  <label-para skeleton="no" xml:lang="en-NZ">
                    <label auto.number="no" denominator="yes">b</label>
                    <para xml:lang="en-NZ">
                      <text>Includesâ€”</text>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">i</label>
                        <para xml:lang="en-NZ">
                          <text>Any amendment to the Convention which has entered into force under Article 94(a) of the Convention and has been ratified by New Zealand; and</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">ii</label>
                        <para xml:lang="en-NZ">
                          <text>Any Annex or amendment thereto accepted under Article 90 of the Convention; and</text>
                        </para>
                      </label-para>
                      <label-para skeleton="no" xml:lang="en-NZ">
                        <label auto.number="no" denominator="yes">iii</label>
                        <para xml:lang="en-NZ">
                          <text>The international standards and recommended practices from time to time accepted and amended by the International Civil Aviation Organisation pursuant to Article 37 of the Convention:</text>
                        </para>
                      </label-para>
                    </para>
                  </label-para>
                  <notes>
                    <history>
                      <history-note id="DLM219739">Convention: this definition was <amending-operation>inserted</amending-operation>, as from <amendment-date>28 November 1991</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="312943" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 11" atidlm:xmlId="DLM757645" atidlm:state="UNCHANGED"><atidlm:linkcontent>11(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="317958" atidlm:sourceXmlId="DLM757645" atidlm:targetKey="1312222" atidlm:targetXmlId="DLM250180" atidlm:targetName="prov 11" atidlm:sourceName="DLM757645" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM250156.xml" atidlm:sourceLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM219717.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Name" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Amend" atidlm:value="false" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Context" atidlm:value="/act[1]/body[1]/part[1]/prov[1]/prov.body[1]/subprov[1]/para[1]/def-para[6]/para[1]/notes[1]/history[1]/history-note[1]" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Element_Type" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Heading" atidlm:value="history-note" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Label" atidlm:value="" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Id" atidlm:value="DLM219710" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Number" atidlm:value="99" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/><atidlm:metadata atidlm:name="Legislation_Title" atidlm:value="Transport Accident Investigati