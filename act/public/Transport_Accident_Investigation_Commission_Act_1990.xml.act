      <prov id="DLM221886" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">14N</label>
        <heading xml:lang="en-NZ">Admissibility of accident or incident findings, recommendations, or report</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>No findings or recommendations published, or report made after an investigation by the Commission are admissible as evidence in any proceedings exceptâ€”</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>A Coroner's <insertwords>inquiry</insertwords>; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>An application for review of a decision of the Commission.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
