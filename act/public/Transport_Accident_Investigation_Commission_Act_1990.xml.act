              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>The name and the office or offices held by the person; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
