    <prov id="DLM99444" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
      <label auto.number="no" denominator="yes">8</label>
      <heading xml:lang="en-NZ">Validity of certain actions of the Crown and Rakiura before commencement of this Act</heading>
      <prov.body>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">1</label>
          <para xml:lang="en-NZ">
            <text>This section applies to anything done by the Crown or Rakiura before the commencement of this Act that would have been lawful had this Act been in force.</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">2</label>
          <para xml:lang="en-NZ">
            <text>Anything to which this section applies is deemed to be and to have always been lawful.</text>
          </para>
        </subprov>
        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">3</label>
          <para xml:lang="en-NZ">
            <text>This section applies despite any other enactment or rule of law.</text>
          </para>
        </subprov>
      </prov.body>
      <cf>
