    <schedule id="DLM302863" in.amend="false" irdnumbering="no" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" xml:lang="en-NZ" act.no="59" act.type="public" date.as.at="2009-01-24" date.assent="2004-07-01" date.first.valid="2007-09-18" stage="reprint" year="2004">
      <label auto.number="no" denominator="yes">2</label>
      <empowering-prov>
        <citation jurisdiction="nz">s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233897" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 26" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679065" atidlm:state="UNCHANGED"><atidlm:linkcontent>26</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237251" atidlm:sourceXmlId="DLM679065" atidlm:targetKey="1737604" atidlm:targetXmlId="DLM302851" atidlm:targetName="prov 26" atidlm:sourceName="DLM679065" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM302846.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
      </empowering-prov>
      <heading xml:lang="en-NZ">Consequential amendments to other Acts</heading>
      <schedule.misc>
        <head5 id="DLM302864" toc="yes" xml:lang="en-NZ">
          <heading xml:lang="en-NZ">
            <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233898" atidlm:branchLabel="_NOVERSION_" atidlm:name="Armed Forces Discipline Act 1971" atidlm:xmlId="DLM679066" atidlm:state="UNCHANGED"><atidlm:linkcontent>Armed Forces Discipline Act 1971</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="237252" atidlm:sourceXmlId="DLM679066" atidlm:targetKey="2572255" atidlm:targetXmlId="DLM401062" atidlm:targetName="Armed Forces Discipline Act 1971" atidlm:sourceName="DLM679066" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM401062.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302864" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> (1971 No 53)</citation>
          </heading>
        </head5>
        <para xml:lang="en-NZ">
          <text>Repeal the definition of commanding officer in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233899" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 2" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679067" atidlm:state="UNCHANGED"><atidlm:linkcontent>2(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237253" atidlm:sourceXmlId="DLM679067" atidlm:targetKey="2572244" atidlm:targetXmlId="DLM401069" atidlm:targetName="prov 2" atidlm:sourceName="DLM679067" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM401062.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substitute:</text>
          <amend quote="1">
            <def-para id="DLM302865" quote="1" xml:lang="en-NZ">
              <para quote="1" xml:lang="en-NZ">
                <text><def-term id="DLM302866" xml:lang="en-NZ">commanding officer</def-term> means—</text>
                <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">a</label>
                  <para xml:lang="en-NZ">
                    <text>an officer for the time being appointed or authorised to be a commanding officer for the purposes of this Act by an officer authorised to convene courts-martial:</text>
                  </para>
                </label-para>
                <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">b</label>
                  <para xml:lang="en-NZ">
                    <text>an officer who is named as a commanding officer in accordance with <citation jurisdiction="nz">section 16</citation>:</text>
                  </para>
                </label-para>
                <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">c</label>
                  <para xml:lang="en-NZ">
                    <text>the officer who is in command of—</text>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">i</label>
                      <para xml:lang="en-NZ">
                        <text>one of Her Majesty's New Zealand ships in commission (other than a tender or a boat); or</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">ii</label>
                      <para xml:lang="en-NZ">
                        <text>one of Her Majesty's New Zealand naval establishments in commission; or</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">iii</label>
                      <para xml:lang="en-NZ">
                        <text>a ship declared by the Chief of Defence Force, the Chief of Naval Staff, the Chief of General Staff, or the Chief of Air Staff to be a service ship; or</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">iv</label>
                      <para xml:lang="en-NZ">
                        <text>a battalion or regiment:</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">d</label>
                  <para xml:lang="en-NZ">
                    <text>an officer of a force of another State that is for the time being declared to be serving together with a New Zealand force under <citation jurisdiction="nz">section 23B of the Defence Act 1990</citation>, who is for the time being appointed or authorised to be a commanding officer for the purposes of this Act by an officer authorised to convene courts-martial.</text>
                  </para>
                </label-para>
              </para>
            </def-para>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Repeal the definitions of <emphasis style="bold">New Zealand force</emphasis> and <emphasis style="bold">force</emphasis> in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233900" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 2" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679068" atidlm:state="UNCHANGED"><atidlm:linkcontent>2(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237254" atidlm:sourceXmlId="DLM679068" atidlm:targetKey="2572244" atidlm:targetXmlId="DLM401069" atidlm:targetName="prov 2" atidlm:sourceName="DLM679068" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM401062.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substitute:</text>
          <amend quote="1">
            <def-para id="DLM302867" quote="1" xml:lang="en-NZ">
              <para quote="1" xml:lang="en-NZ">
                <text><def-term id="DLM302868" xml:lang="en-NZ">New Zealand force</def-term> or <def-term id="DLM302869" xml:lang="en-NZ">force</def-term> has the same meaning as New Zealand force in the <citation jurisdiction="nz">section 2(1) of the Defence Act 1990</citation>.</text>
              </para>
            </def-para>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Repeal the definition of <emphasis style="bold">superior commander</emphasis> in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233901" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 2" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679069" atidlm:state="UNCHANGED"><atidlm:linkcontent>2(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237255" atidlm:sourceXmlId="DLM679069" atidlm:targetKey="2572244" atidlm:targetXmlId="DLM401069" atidlm:targetName="prov 2" atidlm:sourceName="DLM679069" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM401062.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substitute:</text>
          <amend quote="1">
            <def-para id="DLM302870" quote="1" xml:lang="en-NZ">
              <para quote="1" xml:lang="en-NZ">
                <text><def-term id="DLM302871" xml:lang="en-NZ">superior commander</def-term> means—</text>
                <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">a</label>
                  <para xml:lang="en-NZ">
                    <text>an officer (not below the rank of captain in the Navy, colonel in the Army, or group captain in the Air Force) who—</text>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">i</label>
                      <para xml:lang="en-NZ">
                        <text>is, in accordance with <citation jurisdiction="nz">section 119</citation>, authorised to convene a general court-martial; or</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">ii</label>
                      <para xml:lang="en-NZ">
                        <text>is authorised under the rules of procedure to try summarily or to deal summarily with charges against junior officers and warrant officers of the rank specified in the <citation jurisdiction="nz">Fifth Schedule</citation>; or</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">iii</label>
                      <para xml:lang="en-NZ">
                        <text>is appointed by the Chief of Defence Force, the Chief of Naval Staff, the Chief of General Staff, or the Chief of Air Staff to try summarily or to deal summarily with charges against junior officers and warrant officers of the rank specified in the <citation jurisdiction="nz">Fifth Schedule</citation>; or</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">iv</label>
                      <para xml:lang="en-NZ">
                        <text>if the force is on active service outside New Zealand, is commanding the force (not being its commanding officer) and any officer (not below the rank of captain in the Navy, colonel in the Army, or group captain in the Air Force) appointed by him or her to try summarily or to deal summarily with charges against junior officers and warrant officers of the rank specified in the <citation jurisdiction="nz">Fifth Schedule</citation>; or</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
                <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">b</label>
                  <para xml:lang="en-NZ">
                    <text>an officer (not below the relative rank of captain in the Navy, colonel in the Army, or group captain in the Air Force) of a force of another State that is for the time being declared to be serving together with a New Zealand force under <citation jurisdiction="nz">section 23B of the Defence Act 1990</citation> who—</text>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">i</label>
                      <para xml:lang="en-NZ">
                        <text>is, in accordance with <citation jurisdiction="nz">section 119</citation>, authorised to convene a general court-martial; or</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">ii</label>
                      <para xml:lang="en-NZ">
                        <text>is appointed by the Chief of Defence Force to try summarily or to deal summarily with charges against junior officers and warrant officers of the rank specified in the <citation jurisdiction="nz">Fifth Schedule</citation>.</text>
                      </para>
                    </label-para>
                  </para>
                </label-para>
              </para>
            </def-para>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Insert in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233902" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 2" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679070" atidlm:state="UNCHANGED"><atidlm:linkcontent>2(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237256" atidlm:sourceXmlId="DLM679070" atidlm:targetKey="2572244" atidlm:targetXmlId="DLM401069" atidlm:targetName="prov 2" atidlm:sourceName="DLM679070" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM401062.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, in their appropriate alphabetical order:</text>
          <amend quote="1">
            <def-para id="DLM302872" quote="1" xml:lang="en-NZ">
              <para quote="1" xml:lang="en-NZ">
                <text><def-term id="DLM302873" xml:lang="en-NZ">relative rank</def-term> means the appropriate rank prescribed under <citation jurisdiction="nz">section 17 of the Defence Act 1990</citation></text>
              </para>
            </def-para>
            <def-para id="DLM302874" quote="1" xml:lang="en-NZ">
              <para quote="1" xml:lang="en-NZ">
                <text><def-term id="DLM302875" xml:lang="en-NZ">visiting force</def-term> has the same meaning as in <citation jurisdiction="nz">section 4 of the Visiting Forces Act 2004</citation>.</text>
              </para>
            </def-para>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Add to <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233903" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 7" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679071" atidlm:state="UNCHANGED"><atidlm:linkcontent>7(2)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237257" atidlm:sourceXmlId="DLM679071" atidlm:targetKey="2573651" atidlm:targetXmlId="DLM402326" atidlm:targetName="prov 7" atidlm:sourceName="DLM679071" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM402315.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
          <amend quote="1">
            <label-para quote="1" skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">e</label>
              <para xml:lang="en-NZ">
                <text>members of the armed forces of other States who are attached to the Armed Forces under <citation jurisdiction="nz">section 23A of the Defence Act 1990</citation>.</text>
              </para>
            </label-para>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Repeal <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233904" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 9" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679072" atidlm:state="UNCHANGED"><atidlm:linkcontent>9</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237258" atidlm:sourceXmlId="DLM679072" atidlm:targetKey="2573655" atidlm:targetXmlId="DLM402332" atidlm:targetName="prov 9" atidlm:sourceName="DLM679072" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM402315.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substitute:</text>
          <amend quote="1">
            <prov id="DLM302876" irdnumbering="no" quote="1" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">9</label>
              <heading xml:lang="en-NZ">Members of other forces attached to Armed Forces under <citation jurisdiction="nz">section 23A of Defence Act 1990</citation></heading>
              <prov.body>
                <subprov skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="no"/>
                  <para xml:lang="en-NZ">
                    <text>A member of the armed forces of another State who is attached to the Armed Forces of New Zealand under <citation jurisdiction="nz">section 23A of the Defence Act 1990</citation> is, subject to any express provision in the law of that other State to the contrary, subject to this Act.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Add to <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233905" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 88" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679073" atidlm:state="UNCHANGED"><atidlm:linkcontent>88</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237259" atidlm:sourceXmlId="DLM679073" atidlm:targetKey="2578025" atidlm:targetXmlId="DLM403029" atidlm:targetName="prov 88" atidlm:sourceName="DLM679073" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM403028.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
          <amend quote="1">
            <subprov skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">4</label>
              <para xml:lang="en-NZ">
                <text>A member of a force of another State that is for the time being declared to be serving together with a New Zealand force under <citation jurisdiction="nz">section 23B of the Defence Act 1990</citation> has over members of the New Zealand force the powers of arrest of a member of the Armed Forces of a relative rank.</text>
              </para>
            </subprov>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Insert, after <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233906" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 89" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679074" atidlm:state="UNCHANGED"><atidlm:linkcontent>89</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237260" atidlm:sourceXmlId="DLM679074" atidlm:targetKey="2578027" atidlm:targetXmlId="DLM403033" atidlm:targetName="prov 89" atidlm:sourceName="DLM679074" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM403028.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
          <amend quote="1">
            <prov id="DLM302877" irdnumbering="no" quote="1" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">89A</label>
              <heading xml:lang="en-NZ">Arrest of members of visiting forces</heading>
              <prov.body>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">1</label>
                  <para xml:lang="en-NZ">
                    <text>If the officer commanding a visiting force has reasonable grounds to suspect that a member of the force, a member of its civilian component, or a dependant has committed an offence against the service law of the sending State, the officer may issue a warrant for the arrest of that person.</text>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">2</label>
                  <para xml:lang="en-NZ">
                    <text>If a warrant is issued under <citation jurisdiction="nz">subsection (1)</citation>,—</text>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">a</label>
                      <para xml:lang="en-NZ">
                        <text><citation jurisdiction="nz">subsections (2), (3), (5), and (6) of section 89</citation> apply; and</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">b</label>
                      <para xml:lang="en-NZ">
                        <text>the warrant must specify the maximum punishment for the offence under the service law of the sending State.</text>
                      </para>
                    </label-para>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">3</label>
                  <para xml:lang="en-NZ">
                    <text>A member of the police who arrests a person in execution of a warrant issued under <citation jurisdiction="nz">subsection (1)</citation> must, as soon as practicable, deliver that person into the custody of the visiting force.</text>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">4</label>
                  <para xml:lang="en-NZ">
                    <text>The provisions of this section are in addition to those of <citation jurisdiction="nz">section 92A</citation>.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Insert, after <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233907" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 92" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679075" atidlm:state="UNCHANGED"><atidlm:linkcontent>92</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237261" atidlm:sourceXmlId="DLM679075" atidlm:targetKey="2578038" atidlm:targetXmlId="DLM403042" atidlm:targetName="prov 92" atidlm:sourceName="DLM679075" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM403028.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
          <amend quote="1">
            <prov id="DLM302878" irdnumbering="no" quote="1" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">92A</label>
              <heading xml:lang="en-NZ">Arrest of deserters from other armed forces</heading>
              <prov.body>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">1</label>
                  <para xml:lang="en-NZ">
                    <text>If the Government of a State has specifically requested that a person (other than a New Zealand citizen) who is alleged to be illegally absent from the armed forces of the State be apprehended or dealt with under this section by New Zealand authorities,—</text>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">a</label>
                      <para xml:lang="en-NZ">
                        <text>a warrant for the arrest of the person may be issued under <citation jurisdiction="nz">section 89</citation> as if the person had committed the offence of desertion or absence without leave under this Act:</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">b</label>
                      <para xml:lang="en-NZ">
                        <text><citation jurisdiction="nz">section 89</citation> applies with the necessary modifications:</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">c</label>
                      <para xml:lang="en-NZ">
                        <text>the warrant must specify the maximum punishment for the offence under the service law of the sending State.</text>
                      </para>
                    </label-para>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">2</label>
                  <para xml:lang="en-NZ">
                    <text>A person who is delivered into service custody under a warrant issued under <citation jurisdiction="nz">subsection (1)</citation> must, as soon as practicable, be handed over to the authorities of the other State at such place in New Zealand as may be agreed.</text>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">3</label>
                  <para xml:lang="en-NZ">
                    <text>The authorities of the other State into whose custody a person is delivered under this section may detain the person and may remove the person from New Zealand, but nothing in this subsection limits any other powers that the authority may have in relation to the person.</text>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">4</label>
                  <para xml:lang="en-NZ">
                    <text>The provisions of <citation jurisdiction="nz">subsections (3) to (7) of section 101</citation> do not apply to any person to whom <citation jurisdiction="nz">subsection (2)</citation> applies.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Omit from <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233908" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 93" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679076" atidlm:state="UNCHANGED"><atidlm:linkcontent>93(1)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237262" atidlm:sourceXmlId="DLM679076" atidlm:targetKey="2578046" atidlm:targetXmlId="DLM403048" atidlm:targetName="prov 93" atidlm:sourceName="DLM679076" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM403028.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> the words <amend.in quote="1"><citation jurisdiction="nz">sections 89, 91, and 92 of this Act</citation></amend.in> and substitute the words <amend.in quote="1"><citation jurisdiction="nz">sections 89, 89A, 91, 92, and 92A</citation></amend.in>.</text>
        </para>
        <para xml:lang="en-NZ">
          <text>Add to <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233909" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 93" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679077" atidlm:state="UNCHANGED"><atidlm:linkcontent>93</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237263" atidlm:sourceXmlId="DLM679077" atidlm:targetKey="2578046" atidlm:targetXmlId="DLM403048" atidlm:targetName="prov 93" atidlm:sourceName="DLM679077" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM403028.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
          <amend quote="1">
            <subprov quote="1" skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">3</label>
              <para xml:lang="en-NZ">
                <text>The provisions of <citation jurisdiction="nz">subsection (1)</citation>, with the necessary modifications, apply to any person arrested under <citation jurisdiction="nz">section 89A or section 92A</citation>.</text>
              </para>
            </subprov>
            <subprov quote="1" skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">4</label>
              <para xml:lang="en-NZ">
                <text>The provisions of <citation jurisdiction="nz">subsection (2)</citation>, with the necessary modifications, apply to a member of a visiting force, a member of its civilian component, or a dependant who is in the custody of a visiting force when charged with, or with a view to being charged with, an offence against the service law of the sending State.</text>
              </para>
            </subprov>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Insert, after <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233910" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 93A" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679078" atidlm:state="UNCHANGED"><atidlm:linkcontent>93A</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237264" atidlm:sourceXmlId="DLM679078" atidlm:targetKey="2578053" atidlm:targetXmlId="DLM403054" atidlm:targetName="prov 93A" atidlm:sourceName="DLM679078" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM403028.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
          <amend quote="1">
            <prov id="DLM302879" irdnumbering="no" quote="1" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">93B</label>
              <heading xml:lang="en-NZ">Detention in service custody of members of visiting force</heading>
              <prov.body>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">1</label>
                  <para xml:lang="en-NZ">
                    <text>A person to whom this subsection applies may be delivered into service custody and detained in service custody pending his or her trial.</text>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">2</label>
                  <para xml:lang="en-NZ">
                    <text><citation jurisdiction="nz">Subsection (1)</citation> applies to a member of a visiting force, a member of its civilian component, or a dependant who has been arrested for an offence against—</text>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">a</label>
                      <para xml:lang="en-NZ">
                        <text>the service law of the sending State; or</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">b</label>
                      <para xml:lang="en-NZ">
                        <text>the law of New Zealand.</text>
                      </para>
                    </label-para>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">3</label>
                  <para xml:lang="en-NZ">
                    <text>The provisions of <citation jurisdiction="nz">subsections (3) to (7) of section 101</citation> do not apply to any person to whom <citation jurisdiction="nz">subsection (1)</citation> applies.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
            <prov id="DLM302880" irdnumbering="no" quote="1" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">93C</label>
              <heading xml:lang="en-NZ">Police not to deliver custody of arrested person without consent of Minister of Justice in certain cases</heading>
              <prov.body>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">1</label>
                  <para xml:lang="en-NZ">
                    <text>This section applies if a person is arrested or held in custody by the police undersection 89A or <citation jurisdiction="nz">section 92A or section 93B</citation> in respect of an offence against the law of another State that is subject to—</text>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">a</label>
                      <para xml:lang="en-NZ">
                        <text>a sentence of death; or</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">b</label>
                      <para xml:lang="en-NZ">
                        <text>a punishment that would, if carried out by a member of the Armed Forces of New Zealand, constitute an offence against the <citation jurisdiction="nz">Crimes of Torture Act 1989</citation>.</text>
                      </para>
                    </label-para>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">2</label>
                  <para xml:lang="en-NZ">
                    <text>If this section applies, the police must not deliver that person into the custody of the visiting force or, as the case may be, the authorities of the other State without the written consent of the Minister of Justice.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Repeal <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233911" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 119" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679079" atidlm:state="UNCHANGED"><atidlm:linkcontent>119(1)(d)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237265" atidlm:sourceXmlId="DLM679079" atidlm:targetKey="2581067" atidlm:targetXmlId="DLM403463" atidlm:targetName="prov 119" atidlm:sourceName="DLM679079" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM403459.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substitute:</text>
          <amend quote="1">
            <label-para quote="1" skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">d</label>
              <para xml:lang="en-NZ">
                <text>an officer of a force of another State that is for the time being declared to be serving together with a New Zealand force under <citation jurisdiction="nz">section 23B of the Defence Act 1990</citation> in such circumstances and subject to such conditions as may be specified in a warrant issued to the officer by the Chief of Defence Force.</text>
              </para>
            </label-para>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Repeal <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233912" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 119" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679080" atidlm:state="UNCHANGED"><atidlm:linkcontent>119(2)(c)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237266" atidlm:sourceXmlId="DLM679080" atidlm:targetKey="2581067" atidlm:targetXmlId="DLM403463" atidlm:targetName="prov 119" atidlm:sourceName="DLM679080" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM403459.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substitute:</text>
          <amend quote="1">
            <label-para quote="1" skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">c</label>
              <para xml:lang="en-NZ">
                <text>an officer of a force of another State that is for the time being declared to be serving together with a New Zealand force under <citation jurisdiction="nz">section 23B of the Defence Act 1990</citation> in such circumstances and subject to such conditions as may be specified in a warrant issued to the officer by the Chief of Defence Force.</text>
              </para>
            </label-para>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Repeal <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233913" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 124" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679081" atidlm:state="UNCHANGED"><atidlm:linkcontent>124</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237267" atidlm:sourceXmlId="DLM679081" atidlm:targetKey="2581052" atidlm:targetXmlId="DLM403481" atidlm:targetName="prov 124" atidlm:sourceName="DLM679081" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM403459.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substitute:</text>
          <amend quote="1">
            <prov id="DLM302881" irdnumbering="no" quote="1" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">124</label>
              <heading xml:lang="en-NZ">Eligibility of officers of other forces to be members of court-martial</heading>
              <prov.body>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">1</label>
                  <para xml:lang="en-NZ">
                    <text>If it is necessary to convene a court-martial to try an accused at a place where, or in circumstances in which, in the opinion of the convening officer, the minimum requisite number of eligible officers cannot, having regard to the exigencies of the Armed Forces, be appointed as members of the court, any officer of a force of another State that is for the time being declared to be serving together with a New Zealand force under <citation jurisdiction="nz">section 23B of the Defence Act 1990</citation> is eligible to be appointed as the president or as a member of the court.</text>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">2</label>
                  <para xml:lang="en-NZ">
                    <text><citation jurisdiction="nz">Subsection (1)</citation> is subject to <citation jurisdiction="nz">subsections (3) and (4)</citation>.</text>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">3</label>
                  <para xml:lang="en-NZ">
                    <text>No officer is eligible to be appointed as a member of a court-martial under <citation jurisdiction="nz">subsection (1)</citation> unless—</text>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">a</label>
                      <para xml:lang="en-NZ">
                        <text>that officer has served as an officer for one or more periods totalling not less than 3 years; and</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">b</label>
                      <para xml:lang="en-NZ">
                        <text>the rank held by that officer is not lower than a relative rank to that prescribed by <citation jurisdiction="nz">section 120 or section 121 of this Act</citation> as a qualification for a member of a court-martial to try the accused.</text>
                      </para>
                    </label-para>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">4</label>
                  <para xml:lang="en-NZ">
                    <text>The number of officers appointed to a court-martial under <citation jurisdiction="nz">subsection (1)</citation> must not exceed the number of officers of the Armed Forces of New Zealand who are appointed to the court.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Repeal <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233914" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 172" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679082" atidlm:state="UNCHANGED"><atidlm:linkcontent>172</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237268" atidlm:sourceXmlId="DLM679082" atidlm:targetKey="2584848" atidlm:targetXmlId="DLM403827" atidlm:targetName="prov 172" atidlm:sourceName="DLM679082" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM403803.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substitute:</text>
          <amend quote="1">
            <prov id="DLM302882" irdnumbering="no" quote="1" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">172</label>
              <heading xml:lang="en-NZ">Imprisonment and detention of members of other forces attached to Armed Forces</heading>
              <prov.body>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">1</label>
                  <para xml:lang="en-NZ">
                    <text>This section applies if—</text>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">a</label>
                      <para xml:lang="en-NZ">
                        <text>a member of the armed forces of another State is attached to any Service under <citation jurisdiction="nz">section 23A of the Defence Act 1990</citation> and is sentenced by court-martial under this Act to imprisonment or detention; and</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">b</label>
                      <para xml:lang="en-NZ">
                        <text>an arrangement is for the time being in force with the appropriate authority of that State that enables the return of the member for the purpose of serving any such sentence in that State.</text>
                      </para>
                    </label-para>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">2</label>
                  <para xml:lang="en-NZ">
                    <text>A competent service authority may give directions for the delivery of the member so sentenced into the custody of the forces of that State (whether in New Zealand or elsewhere) and his or her removal to that State for the purpose of serving the sentence.</text>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">3</label>
                  <para xml:lang="en-NZ">
                    <text>Any member of the forces of any State in respect of whom any such directions are given by a competent service authority may, until that member is delivered into the custody of those forces, be kept in service custody or civil custody, or partly in service custody or partly in civil custody.</text>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">4</label>
                  <para xml:lang="en-NZ">
                    <text>Any such member may, by order of a competent service authority, from time to time be transferred from service custody to civil custody or from civil custody to service custody, as the occasion may require.</text>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">5</label>
                  <para xml:lang="en-NZ">
                    <text>Any such member may during his or her transfer from one place to another, whether on board a ship or an aircraft or other means of transport, be subjected only to such restraint as may be necessary to ensure his or her safe conduct and removal.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
          </amend>
        </para>
        <para xml:lang="en-NZ">
          <text>Repeal <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233915" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 173" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679083" atidlm:state="UNCHANGED"><atidlm:linkcontent>173</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237269" atidlm:sourceXmlId="DLM679083" atidlm:targetKey="2584852" atidlm:targetXmlId="DLM403829" atidlm:targetName="prov 173" atidlm:sourceName="DLM679083" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM403803.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substitute:</text>
          <amend quote="1">
            <prov id="DLM302883" irdnumbering="no" quote="1" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">173</label>
              <heading xml:lang="en-NZ">Imprisonment and detention of members of Armed Forces attached to other forces</heading>
              <prov.body>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">1</label>
                  <para xml:lang="en-NZ">
                    <text>This section applies if—</text>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">a</label>
                      <para xml:lang="en-NZ">
                        <text>a member of the Armed Forces is attached to the forces of another State under <citation jurisdiction="nz">section 23 of the Defence Act 1990</citation> and is sentenced by court-martial of those forces to imprisonment or detention; and</text>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">b</label>
                      <para xml:lang="en-NZ">
                        <text>an arrangement is for the time being in force with the appropriate authority in that State that enables the return of the member to serve his or her sentence—</text>
                        <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes" quote="1">i</label>
                          <para xml:lang="en-NZ">
                            <text>in New Zealand; or</text>
                          </para>
                        </label-para>
                        <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                          <label auto.number="no" denominator="yes" quote="1">ii</label>
                          <para xml:lang="en-NZ">
                            <text>in a penal institution, or in a service prison or detention quarter established under this Act (whether in New Zealand or elsewhere); and</text>
                          </para>
                        </label-para>
                      </para>
                    </label-para>
                    <label-para quote="1" skeleton="no" xml:lang="en-NZ">
                      <label auto.number="no" denominator="yes" quote="1">c</label>
                      <para xml:lang="en-NZ">
                        <text>under that arrangement the member is received into the custody of a New Zealand force (whether in New Zealand or elsewhere).</text>
                      </para>
                    </label-para>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">2</label>
                  <para xml:lang="en-NZ">
                    <text>The provisions of this Act apply to that member in all respects, with the necessary modifications, as if the member had been sentenced by court-martial under this Act.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
          </amend>
        </para>
        <head5 id="DLM302884" toc="yes" xml:lang="en-NZ">
          <heading xml:lang="en-NZ">
            <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233916" atidlm:branchLabel="_NOVERSION_" atidlm:name="Arms Act 1983" atidlm:xmlId="DLM679084" atidlm:state="UNCHANGED"><atidlm:linkcontent>Arms Act 1983</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="237270" atidlm:sourceXmlId="DLM679084" atidlm:targetKey="404000" atidlm:targetXmlId="DLM72621" atidlm:targetName="Arms Act 1983" atidlm:sourceName="DLM679084" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM72621.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302884" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> (1983 No 44)</citation>
          </heading>
        </head5>
        <para xml:lang="en-NZ">
          <text>Insert in <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233917" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 3" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679085" atidlm:state="UNCHANGED"><atidlm:linkcontent>3(2)(a)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237271" atidlm:sourceXmlId="DLM679085" atidlm:targetKey="404058" atidlm:targetXmlId="DLM72679" atidlm:targetName="prov 3" atidlm:sourceName="DLM679085" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM72621.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>, after <citation jurisdiction="nz">subparagraph <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233918" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 3" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679086" atidlm:state="UNCHANGED"><atidlm:linkcontent>(i)</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237272" atidlm:sourceXmlId="DLM679086" atidlm:targetKey="404058" atidlm:targetXmlId="DLM72679" atidlm:targetName="prov 3" atidlm:sourceName="DLM679086" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM72621.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>:</text>
          <amend quote="1">
            <label-para quote="1" skeleton="no" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">ia</label>
              <para xml:lang="en-NZ">
                <text>a member of a visiting force under the Visiting Forces Act 2004; or.</text>
              </para>
            </label-para>
          </amend>
        </para>
        <head5 id="DLM302885" toc="yes" xml:lang="en-NZ">
          <heading xml:lang="en-NZ">
            <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233919" atidlm:branchLabel="_NOVERSION_" atidlm:name="Births, Deaths, and Marriages Registration Act 1995" atidlm:xmlId="DLM679087" atidlm:state="UNCHANGED"><atidlm:linkcontent>Births, Deaths, and Marriages Registration Act 1995</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="237273" atidlm:sourceXmlId="DLM679087" atidlm:targetKey="1931208" atidlm:targetXmlId="DLM359368" atidlm:targetName="Births, Deaths, and Marriages Registration Act 1995" atidlm:sourceName="DLM679087" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM359368.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302885" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> (1995 No 16)</citation>
          </heading>
        </head5>
        <para xml:lang="en-NZ">
          <text>Repeal <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="233920" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 34" atidlm:parentFolderId="/NZ/NZAct/V/Visiting Forces Act 2004" atidlm:xmlId="DLM679088" atidlm:state="UNCHANGED"><atidlm:linkcontent>34</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="237274" atidlm:sourceXmlId="DLM679088" atidlm:targetKey="1934401" atidlm:targetXmlId="DLM364158" atidlm:targetName="prov 34" atidlm:sourceName="DLM679088" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM364157.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM302863.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM302863" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation> and substitute:</text>
          <amend quote="1">
            <prov id="DLM302886" irdnumbering="no" quote="1" skeleton="no" toc="yes" xml:lang="en-NZ">
              <label auto.number="no" denominator="yes" quote="1">34</label>
              <heading xml:lang="en-NZ">Deaths in New Zealand to be notified and registered</heading>
              <prov.body>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">1</label>
                  <para xml:lang="en-NZ">
                    <text>Every death in New Zealand must be notified and registered in accordance with this Part.</text>
                  </para>
                </subprov>
                <subprov quote="1" skeleton="no" xml:lang="en-NZ">
                  <label auto.number="no" denominator="yes" quote="1">2</label>
                  <para xml:lang="en-NZ">
                    <text><citation jurisdiction="nz">Subsection (1)</citation> does not apply to a death to which <citation jurisdiction="nz">section 19(1) of the Visiting Forces Act 2004</citation> applies.</text>
                  </para>
                </subprov>
              </prov.body>
            </prov>
          </amend>
        </para>
        <head5 deletion-status="repealed" id="DLM302887" toc="yes" xml:lang="en-NZ">
