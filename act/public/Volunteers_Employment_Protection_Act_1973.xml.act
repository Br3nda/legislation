      <prov id="DLM410459" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">14ZS</label>
        <heading xml:lang="en-NZ">Consequences of demand notice</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The consequences of a demand notice are that it—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>imposes a legal requirement on the employer to comply with it; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>is prima facie evidence that the employee has the entitlement specified in the notice; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">c</label>
                <para xml:lang="en-NZ">
                  <text>may be enforced by the making by the Employment Relations Authority of a compliance order under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="440365" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 137" atidlm:xmlId="DLM884431" atidlm:state="UNCHANGED"><atidlm:linkcontent>137</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="447939" atidlm:sourceXmlId="DLM884431" atidlm:targetKey="322471" atidlm:targetXmlId="DLM60387" atidlm:targetName="prov 137" atidlm:sourceName="DLM884431" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM60316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410427.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410459" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="440366" atidlm:branchLabel="_NOVERSION_" atidlm:name="Employment Relations Act 2000" atidlm:xmlId="DLM884432" atidlm:state="UNCHANGED"><atidlm:linkcontent>Employment Relations Act 2000</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="447940" atidlm:sourceXmlId="DLM884432" atidlm:targetKey="309800" atidlm:targetXmlId="DLM58316" atidlm:targetName="Employment Relations Act 2000" atidlm:sourceName="DLM884432" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM58316.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410427.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410459" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="440367" atidlm:branchLabel="_NOVERSION_" atidlm:name="Parental Leave and Employment Protection Act 1987" atidlm:xmlId="DLM884433" atidlm:state="UNCHANGED"><atidlm:linkcontent>1987 No 129</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="447941" atidlm:sourceXmlId="DLM884433" atidlm:targetKey="652400" atidlm:targetXmlId="DLM120103" atidlm:targetName="Parental Leave and Employment Protection Act 1987" atidlm:sourceName="DLM884433" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM120103.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410427.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410459" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="440368" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 70D" atidlm:xmlId="DLM884434" atidlm:state="UNCHANGED"><atidlm:linkcontent>70D</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="447942" atidlm:sourceXmlId="DLM884434" atidlm:targetKey="661056" atidlm:targetXmlId="DLM121087" atidlm:targetName="prov 70D" atidlm:sourceName="DLM884434" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM121031.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410427.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410459" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
