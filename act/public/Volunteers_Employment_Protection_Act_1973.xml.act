      <prov id="DLM410422" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">14ZC</label>
        <heading xml:lang="en-NZ">Redundancy payments not affected</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Nothing in this Act affects any redundancy payment payable pursuant to the provisions of any Act or of any order or employment agreement.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="440277" atidlm:branchLabel="_NOVERSION_" atidlm:name="Parental Leave and Employment Protection Act 1987" atidlm:xmlId="DLM884343" atidlm:state="UNCHANGED"><atidlm:linkcontent>1987 No 129</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="447849" atidlm:sourceXmlId="DLM884343" atidlm:targetKey="652400" atidlm:targetXmlId="DLM120103" atidlm:targetName="Parental Leave and Employment Protection Act 1987" atidlm:sourceName="DLM884343" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM120103.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410412.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410422" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="440278" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 53" atidlm:xmlId="DLM884344" atidlm:state="UNCHANGED"><atidlm:linkcontent>53</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="447850" atidlm:sourceXmlId="DLM884344" atidlm:targetKey="659606" atidlm:targetXmlId="DLM121025" atidlm:targetName="prov 53" atidlm:sourceName="DLM884344" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM121019.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410412.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410422" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
