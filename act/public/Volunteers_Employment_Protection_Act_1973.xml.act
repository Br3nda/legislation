      <prov id="DLM410178" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">14I</label>
        <heading xml:lang="en-NZ">Duration of leave</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Leave taken under this Part must be taken in one continuous period not exceeding 12 months.</text>
            </para>
          </subprov>
        </prov.body>
