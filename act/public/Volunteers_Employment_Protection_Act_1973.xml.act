      <prov id="DLM410164" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">14C</label>
        <heading xml:lang="en-NZ">Date of commencement of leave</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Leave under this Part begins—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>on the day on which the employee is called out for continuous service, either in New Zealand or elsewhere, as a consequence of a Proclamation under <citation jurisdiction="nz">section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="439625" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 39" atidlm:xmlId="DLM883392" atidlm:state="UNCHANGED"><atidlm:linkcontent>39</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="447071" atidlm:sourceXmlId="DLM883392" atidlm:targetKey="1115255" atidlm:targetXmlId="DLM206060" atidlm:targetName="prov 39" atidlm:sourceName="DLM883392" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM206033.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410158.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410164" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> or section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="439626" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 40" atidlm:xmlId="DLM883393" atidlm:state="UNCHANGED"><atidlm:linkcontent>40</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="447072" atidlm:sourceXmlId="DLM883393" atidlm:targetKey="1115258" atidlm:targetXmlId="DLM206062" atidlm:targetName="prov 40" atidlm:sourceName="DLM883393" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM206033.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410158.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410164" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> of the <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="439627" atidlm:branchLabel="_NOVERSION_" atidlm:name="Defence Act 1990" atidlm:xmlId="DLM883394" atidlm:state="UNCHANGED"><atidlm:linkcontent>Defence Act 1990</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="447073" atidlm:sourceXmlId="DLM883394" atidlm:targetKey="1109810" atidlm:targetXmlId="DLM204972" atidlm:targetName="Defence Act 1990" atidlm:sourceName="DLM883394" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM204972.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410158.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410164" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; or</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>on such earlier or later date as is agreed on by the employee and the employee's employer.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="439628" atidlm:branchLabel="_NOVERSION_" atidlm:name="Parental Leave and Employment Protection Act 1987" atidlm:xmlId="DLM883395" atidlm:state="UNCHANGED"><atidlm:linkcontent>1987 No 129</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="447074" atidlm:sourceXmlId="DLM883395" atidlm:targetKey="652400" atidlm:targetXmlId="DLM120103" atidlm:targetName="Parental Leave and Employment Protection Act 1987" atidlm:sourceName="DLM883395" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM120103.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410158.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410164" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="439629" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 12" atidlm:xmlId="DLM883396" atidlm:state="UNCHANGED"><atidlm:linkcontent>12</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="447075" atidlm:sourceXmlId="DLM883396" atidlm:targetKey="655420" atidlm:targetXmlId="DLM120477" atidlm:targetName="prov 12" atidlm:sourceName="DLM883396" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM120457.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410158.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410164" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
