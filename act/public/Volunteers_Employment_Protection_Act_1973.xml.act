      <prov id="DLM410461" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">14ZT</label>
        <heading xml:lang="en-NZ">Authority to determine entitlement to volunteers leave on objection</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>The function of the Employment Relations Authority in respect of an objection is to determine whether or not the employee has the entitlement specified in the determination or demand notice.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
