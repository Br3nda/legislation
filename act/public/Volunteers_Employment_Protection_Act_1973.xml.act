    <part id="DLM410412" in.amend="false" irdnumbering="no" toc="yes" xml:lang="en-NZ" date.first.valid="2007-09-18" act.no="25" act.type="public" date.as.at="2008-10-01" date.assent="1973-10-02" stage="reprint" year="1973">
      <label auto.number="no" denominator="yes">5</label>
      <heading xml:lang="en-NZ">Protection of employment</heading>
      <notes>
        <history>
          <history-note id="DLM410413">Parts <amended-provision>2 to 7</amended-provision> (comprising sections 14A to 14ZX) were <amending-operation>inserted</amending-operation>, as from <amendment-date>1 April 2004</amendment-date>, by section <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="440255" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 12" atidlm:xmlId="DLM884321" atidlm:state="UNCHANGED"><atidlm:linkcontent>12</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="amending-provision" atidlm:isIncludeOrRef="1" atidlm:seqNum="447786" atidlm:sourceXmlId="DLM884321" atidlm:targetKey="1393002" atidlm:targetXmlId="DLM240731" atidlm:targetName="prov 12" atidlm:sourceName="DLM884321" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM240277.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410412.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410413" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> <amending-leg>Volunteers Employment Protection Amendment Act 2004</amending-leg> (2004 No 12).</history-note>
        </history>
