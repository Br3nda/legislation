      <prov id="DLM410180" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">14J</label>
        <heading xml:lang="en-NZ">Right of employer and employee to determine date of commencement of leave by agreement</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Leave under this Part may, by agreement between the employee and the employee's employer, begin on any date.</text>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="439682" atidlm:branchLabel="_NOVERSION_" atidlm:name="Parental Leave and Employment Protection Act 1987" atidlm:xmlId="DLM883749" atidlm:state="UNCHANGED"><atidlm:linkcontent>1987 No 129</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="447370" atidlm:sourceXmlId="DLM883749" atidlm:targetKey="652400" atidlm:targetXmlId="DLM120103" atidlm:targetName="Parental Leave and Employment Protection Act 1987" atidlm:sourceName="DLM883749" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM120103.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410172.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410180" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="439683" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 12" atidlm:xmlId="DLM883750" atidlm:state="UNCHANGED"><atidlm:linkcontent>12</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="447371" atidlm:sourceXmlId="DLM883750" atidlm:targetKey="655420" atidlm:targetXmlId="DLM120477" atidlm:targetName="prov 12" atidlm:sourceName="DLM883750" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM120457.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410172.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410180" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
