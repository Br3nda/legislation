      <prov id="DLM410439" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">14ZJ</label>
        <heading xml:lang="en-NZ">Duty to promote settlement</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>It is the duty of every party to a volunteers leave complaint—</text>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">a</label>
                <para xml:lang="en-NZ">
                  <text>to promote the settlement of the complaint under the procedures provided in <citation jurisdiction="nz">sections <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="440322" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 14ZH" atidlm:xmlId="DLM884388" atidlm:state="UNCHANGED"><atidlm:linkcontent>14ZH to 14ZL</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="intref" atidlm:isIncludeOrRef="1" atidlm:seqNum="447895" atidlm:sourceXmlId="DLM884388" atidlm:targetKey="2632020" atidlm:targetXmlId="DLM410435" atidlm:targetName="prov 14ZH" atidlm:sourceName="DLM884388" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM410427.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410427.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410439" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>; and</text>
                </para>
              </label-para>
              <label-para skeleton="no" xml:lang="en-NZ">
                <label auto.number="no" denominator="yes">b</label>
                <para xml:lang="en-NZ">
                  <text>to abstain from any action that might impede the effective functioning of the procedures.</text>
                </para>
              </label-para>
            </para>
          </subprov>
        </prov.body>
        <cf>
          <citation jurisdiction="nz"><atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="440323" atidlm:branchLabel="_NOVERSION_" atidlm:name="Parental Leave and Employment Protection Act 1987" atidlm:xmlId="DLM884389" atidlm:state="UNCHANGED"><atidlm:linkcontent>1987 No 129</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="leg-title" atidlm:isIncludeOrRef="1" atidlm:seqNum="447896" atidlm:sourceXmlId="DLM884389" atidlm:targetKey="652400" atidlm:targetXmlId="DLM120103" atidlm:targetName="Parental Leave and Employment Protection Act 1987" atidlm:sourceName="DLM884389" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM120103.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410427.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410439" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link> s <atidlm:link xmlns:atidlm="http://www.arbortext.com/namespace/atidlm" atidlm:key="440324" atidlm:branchLabel="_NOVERSION_" atidlm:name="prov 64" atidlm:xmlId="DLM884390" atidlm:state="UNCHANGED"><atidlm:linkcontent>64</atidlm:linkcontent><atidlm:resourcepair atidlm:markupType="extref" atidlm:isIncludeOrRef="1" atidlm:seqNum="447897" atidlm:sourceXmlId="DLM884390" atidlm:targetKey="661025" atidlm:targetXmlId="DLM121056" atidlm:targetName="prov 64" atidlm:sourceName="DLM884390" atidlm:targetLocator="file:///C:/epicuser/Doctypes/NZAct/fragxyxz_DLM121031.xml" atidlm:sourceLocator="file:///C:/epicuser/doctypes/NZAct/fragxyxz_DLM410427.xml" atidlm:state="UNCHANGED"><atidlm:metadata atidlm:name="Parent_id" atidlm:value="DLM410439" atidlm:locale="xx_XX" atidlm:state="UNCHANGED"/></atidlm:resourcepair></atidlm:link></citation>
