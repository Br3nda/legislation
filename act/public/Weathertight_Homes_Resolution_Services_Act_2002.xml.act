      <prov id="DLM163992" irdnumbering="no" skeleton="no" toc="yes" xml:lang="en-NZ">
        <label auto.number="no" denominator="yes">21</label>
        <heading xml:lang="en-NZ">Other mediation services</heading>
        <prov.body>
          <subprov skeleton="no" xml:lang="en-NZ">
            <label auto.number="no" denominator="no"/>
            <para xml:lang="en-NZ">
              <text>Nothing in this Act prevents any person seeking and using mediation services other than those provided by the chief executive under this Act.</text>
            </para>
          </subprov>
        </prov.body>
