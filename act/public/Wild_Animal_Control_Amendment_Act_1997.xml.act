        <subprov skeleton="no" xml:lang="en-NZ">
          <label auto.number="no" denominator="yes">3</label>
          <para xml:lang="en-NZ">
            <text><citation jurisdiction="nz">Subsection <intref href="DLM413185">(1)</intref></citation> applies only for so long as the conditions (if any) in the licence or permit in force as at 30 September 1997 are complied with.</text>
          </para>
        </subprov>
