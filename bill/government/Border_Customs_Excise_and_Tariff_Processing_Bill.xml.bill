    <schedule id="DLM2376913" in.amend="false" irdnumbering="no" official="yes" prov-type="clause" schedule.heading.continue="no" toc="yes" type="schedule" bill.no="83" bill.type="government" stage="2" year="2009">
      <label auto.number="no" denominator="yes">2 </label>
      <empowering-prov>
