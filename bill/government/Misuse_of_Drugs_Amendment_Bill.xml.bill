          <label auto.number="yes" denominator="yes"/>
          <para>
            <text>This Act is the Misuse of Drugs Amendment Act <changeable><citation jurisdiction="nz">2010</citation></changeable>.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM2827708" irdnumbering="no" skeleton="no" toc="yes">
