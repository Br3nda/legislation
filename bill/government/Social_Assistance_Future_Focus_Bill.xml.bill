      <prov id="DLM2827409" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">40</label>
        <heading>Consequential amendments </heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="yes" denominator="yes"/>
            <para>
              <text>The principal Act is amended in the manner indicated in <changeable><citation jurisdiction="nz">Schedule 1</citation></changeable> of this Act.</text>
            </para>
          </subprov>
