      <prov id="DLM2981106" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">59</label>
        <heading>Child's income</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
