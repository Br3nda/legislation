      <prov id="DLM3123576" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">87</label>
        <heading>Income Tax Act 2004</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes"/>
