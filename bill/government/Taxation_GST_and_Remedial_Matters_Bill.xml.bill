      <prov id="DLM3123517" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">50</label>
        <heading>Determining net amounts and taxable amounts</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text>In section HM 35(8)(a), <amend.in quote="1">investors' interests</amend.in> is replaced by <amend.in quote="1">investor interests</amend.in>.</text>
            </para>
