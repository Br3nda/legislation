      <prov id="DLM3123588" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">94</label>
        <heading>KiwiSaver Act 2006</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="no"/>
            <para>
              <text><changeable><citation jurisdiction="nz">Sections 95 to <struckoutwords amend.level1="struckout-unanimous">100</struckoutwords><insertwords amend.level1="insert-unanimous">100C</insertwords></citation></changeable> amend the KiwiSaver Act 2006.</text>
            </para>
