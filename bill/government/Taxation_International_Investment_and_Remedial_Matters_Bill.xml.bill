      <prov id="DLM3324222" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">139</label>
        <heading>Section 77 repealed</heading>
        <prov.body>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">1</label>
            <para>
              <text>Section 77 is repealed.</text>
            </para>
          </subprov>
          <subprov skeleton="no">
            <label auto.number="no" denominator="yes">2</label>
            <para>
              <text><changeable>Subsection (1)</changeable> applies for returns relating to income years beginning on or after 1 July 2012.</text>
            </para>
