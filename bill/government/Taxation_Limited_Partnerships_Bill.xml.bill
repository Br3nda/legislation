  <body id="DLM1185005" prov-type="section">
    <prov id="DLM1185000" irdnumbering="no" skeleton="no" toc="yes">
      <label auto.number="no" denominator="no">1</label>
      <heading>Title</heading>
      <prov.body>
        <subprov skeleton="no">
          <label auto.number="no" denominator="no"/>
          <para>
            <text>This Act is the Taxation (Limited Partnerships) Act <changeable>2008</changeable>.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <prov id="DLM1185001" irdnumbering="no" skeleton="no" toc="yes">
      <label auto.number="no" denominator="no">2</label>
      <heading>Commencement</heading>
      <prov.body>
        <subprov skeleton="no">
          <label auto.number="no" denominator="no"/>
          <para>
            <text>This Act comes into force on 1 April 2008.</text>
          </para>
        </subprov>
      </prov.body>
    </prov>
    <omitted-provs toc="yes"/>
    <part id="DLM1185006" in.amend="false" irdnumbering="no" official="yes" toc="yes" bill.no="140" bill.type="government" split.letter="A" stage="3" year="2007">
      <label auto.number="no" denominator="yes">5</label>
      <heading>Amendments to Income Tax Act 2007</heading>
      <prov id="DLM1185007" irdnumbering="no" skeleton="no" toc="yes">
        <label auto.number="no" denominator="yes">105</label>
        <heading>Income Tax Act 2007</heading>
        <prov.body>
