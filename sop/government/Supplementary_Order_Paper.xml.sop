                                    <label-para skeleton="no">
                                      <label auto.number="no" denominator="yes">ii</label>
                                      <para>
                                        <text>for the number of journeys specified in the limited purpose visa issued under the former Act and within the period or until the date specified in that visa</text>
                                      </para>
                                    </label-para>
                                  </para>
                                </label-para>
                              </para>
                            </entry>
                          </row>
                          <row>
                            <entry colsep="0" rowsep="0">
                              <para>
                                <text>Temporary permit granted for the purposes of the Mutual Assistance in Criminal Matters Act 1992</text>
                              </para>
                            </entry>
                            <entry colsep="0" rowsep="0"/>
