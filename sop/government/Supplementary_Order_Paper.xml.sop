      <sop.amend id="DLM2246536">
        <clause.ref>Clause 232</clause.ref>
        <sop.para>
          <text><emphasis style="italic">Subclause (1)</emphasis>: to omit <amend.in quote="1"><changeable><citation jurisdiction="nz">239A,</citation></changeable></amend.in> (line 26 on page 256) and substitute <amend.in quote="1"><changeable><citation jurisdiction="nz">239B,</citation></changeable></amend.in>.</text>
        </sop.para>
      </sop.amend>
      <sop.amend id="DLM2246537">
