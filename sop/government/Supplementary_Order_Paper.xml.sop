    <sop.amend id="DLM2571838">
      <clause.ref>New clause 49BA</clause.ref>
      <sop.para>
        <text>To insert, after <emphasis style="italic">clause 49</emphasis> (after line 3 on page 56), the following:</text>
        <instrument.amend close.quote="no" open.quote="no">
          <prov id="DLM2571839" irdnumbering="no" skeleton="no" toc="yes">
            <label auto.number="no" denominator="yes">49BA</label>
