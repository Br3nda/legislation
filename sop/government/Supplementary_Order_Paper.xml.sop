    <sop.amend id="DLM2259444">
      <clause.ref>Clause 107</clause.ref>
      <sop.para>
        <text><emphasis style="italic">Subclause (3)</emphasis>: to omit <emphasis style="italic">subclause (3)</emphasis> (lines 27 to 34 on page 157) and substitute the following:</text>
        <instrument.amend close.quote="yes" open.quote="no">
